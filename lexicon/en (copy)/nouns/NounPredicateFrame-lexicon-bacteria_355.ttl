@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :bacteria_355_sense_1_res ;
  lemon:entry    :bacteria_355 .

:bacteria_355 a  lemon:LexicalEntry ;
