@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :saint_of_210 ;
  lemon:entry    :saint_of_210_form_preposition .

:saint_of_210 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :saint_of_210_form ;
  lemon:otherForm  :saint_of_210_singular ;
  lemon:otherForm  :saint_of_210_plural ;
  lemon:sense          :saint_of_210_sense_1 ;
  lemon:synBehavior    :saint_of_210_nounpp .

:saint_of_210_form a lemon:Form ;

:saint_of_210_singular a    lemon:Form ;
  lemon:writtenRep "Saint"@en ;
  lexinfo:number   lexinfo:singular .

:saint_of_210_plural a   lemon:Form ;
  lemon:writtenRep "Saints"@en ;
  lexinfo:number    lexinfo:plural .

:saint_of_210_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_saint_of_210 ;
  lexinfo:prepositionalAdjunct :arg2_saint_of_210 .

:saint_of_210_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :saint_of_210_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_saint_of_210 ;
  lemon:objOfProp           :arg1_saint_of_210 ;
  lemon:condition           :saint_of_210_sense_1_condition .

:saint_of_210_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Saint> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Saint> .

:arg2_saint_of_210 lemon:marker :saint_of_210_form_preposition .
## Prepositions ##
:saint_of_210_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

