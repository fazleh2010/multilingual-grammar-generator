@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :convention_of_285_sense_1_res ;
  lemon:entry    :convention_of_285 .

:convention_of_285 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :convention_of_285_form ;
  lemon:otherForm  :convention_of_285_singular ;
  lemon:otherForm  :convention_of_285_plural ;
  lemon:sense          :convention_of_285_sense_1 ;
  lemon:synBehavior    :convention_of_285_predFrame .

:convention_of_285_form
 lemon:writtenRep "Convention"@en .

:convention_of_285_singular a    lemon:Form ;
  lemon:writtenRep "Convention"@en ;
  lexinfo:number   lexinfo:singular .

:convention_of_285_plural a   lemon:Form ;
  lemon:writtenRep "Conventions"@en ;
  lexinfo:number    lexinfo:plural .

:convention_of_285_sense_1 a  lemon:LexicalSense ;
  lemon:reference :convention_of_285_sense_1_res ;
  lemon:isA       :convention_of_285_PredSynArg .

:convention_of_285_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Convention> .
