@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_inspire_90 ;
  lemon:entry    :inspire_90ed ;
  lemon:entry    :form_inspire_90_preposition .

:to_inspire_90 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_inspire_90 ;
  lemon:otherForm      :form_inspire_90s ;
  lemon:otherForm      :form_inspire_90ed ;
  lemon:otherForm      :form_inspire_90perfect ;
  lemon:sense          :inspire_90_sense_1 ;
  lemon:synBehavior    :inspire_90_frame_transitive .

:form_inspire_90 a         lemon:Form ;
  lemon:writtenRep     "inspire"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_inspire_90s a    lemon:Form ;
  lemon:writtenRep "inspires"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_inspire_90ed a   lemon:Form ;
  lemon:writtenRep "inspired"@en ;
  lexinfo:tense    lexinfo:past .

:form_inspire_90perfect a   lemon:Form ;
  lemon:writtenRep "inspired"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:inspire_90_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :inspire_90_subj ;
  lexinfo:directObject     :inspire_90_obj .

:inspire_90_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :inspire_90_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/influenced> ;
  lemon:subjOfProp  :inspire_90_obj ;
  lemon:objOfProp   :inspire_90_subj ;
  lemon:condition   :inspire_90_sense_1_condition .


:inspire_90_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:inspire_90_obj lemon:marker :form_inspire_90_preposition .

## Prepositions ##
:form_inspire_90_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

