@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :museum_512_sense_1_res ;
  lemon:entry    :museum_512 .

:museum_512 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :museum_512_form ;
  lemon:otherForm  :museum_512_singular ;
  lemon:otherForm  :museum_512_plural ;
  lemon:sense          :museum_512_sense_1 ;
  lemon:synBehavior    :museum_512_predFrame .

:museum_512_form
 lemon:writtenRep "museum"@en .

:museum_512_singular a    lemon:Form ;
  lemon:writtenRep "museum"@en ;
  lexinfo:number   lexinfo:singular .

:museum_512_plural a   lemon:Form ;
  lemon:writtenRep "museums"@en ;
  lexinfo:number    lexinfo:plural .

:museum_512_sense_1 a  lemon:LexicalSense ;
  lemon:reference :museum_512_sense_1_res ;
  lemon:isA       :museum_512_PredSynArg .

:museum_512_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Museum> .