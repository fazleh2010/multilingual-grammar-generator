@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :genre_215_sense_1_res ;
  lemon:entry    :genre_215 .

:genre_215 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :genre_215_form ;
  lemon:otherForm  :genre_215_singular ;
  lemon:otherForm  :genre_215_plural ;
  lemon:sense          :genre_215_sense_1 ;
  lemon:synBehavior    :genre_215_predFrame .

:genre_215_form
 lemon:writtenRep "genre"@en .

:genre_215_singular a    lemon:Form ;
  lemon:writtenRep "genre"@en ;
  lexinfo:number   lexinfo:singular .

:genre_215_plural a   lemon:Form ;
  lemon:writtenRep "genres"@en ;
  lexinfo:number    lexinfo:plural .

:genre_215_sense_1 a  lemon:LexicalSense ;
  lemon:reference :genre_215_sense_1_res ;
  lemon:isA       :genre_215_PredSynArg .

:genre_215_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Genre> .