@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lastelectiondate-of_415 ;
  lemon:entry    :lastelectiondate-of_415_form_preposition .

:lastelectiondate-of_415 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lastelectiondate-of_415_form ;
  lemon:otherForm  :lastelectiondate-of_415_singular ;
  lemon:otherForm  :lastelectiondate-of_415_plural ;
  lemon:sense          :lastelectiondate-of_415_sense_1 ;
  lemon:synBehavior    :lastelectiondate-of_415_nounpp .

:lastelectiondate-of_415_form a lemon:Form ;
  lemon:writtenRep "last Election Date"@en .

:lastelectiondate-of_415_singular a    lemon:Form ;
  lemon:writtenRep "last Election Date"@en ;
  lexinfo:number   lexinfo:singular .

:lastelectiondate-of_415_plural a   lemon:Form ;
  lemon:writtenRep "last Election Dates"@en ;
  lexinfo:number    lexinfo:plural .

:lastelectiondate-of_415_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_lastelectiondate-of_415 ;
  lexinfo:prepositionalAdjunct :arg2_lastelectiondate-of_415 .

:lastelectiondate-of_415_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :lastelectiondate-of_415_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/lastElectionDate> ;
  lemon:subjOfProp          :arg2_lastelectiondate-of_415 ;
  lemon:objOfProp           :arg1_lastelectiondate-of_415 ;
  lemon:condition           :lastelectiondate-of_415_sense_1_condition .

:lastelectiondate-of_415_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2_lastelectiondate-of_415 lemon:marker :lastelectiondate-of_415_form_preposition .
## Prepositions ##
:lastelectiondate-of_415_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

