@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :website_223_sense_1_res ;
  lemon:entry    :website_223 .

:website_223 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :website_223_form ;
  lemon:otherForm  :website_223_singular ;
  lemon:otherForm  :website_223_plural ;
  lemon:sense          :website_223_sense_1 ;
  lemon:synBehavior    :website_223_predFrame .

:website_223_form
 lemon:writtenRep "website"@en .

:website_223_singular a    lemon:Form ;
  lemon:writtenRep "website"@en ;
  lexinfo:number   lexinfo:singular .

:website_223_plural a   lemon:Form ;
  lemon:writtenRep "websites"@en ;
  lexinfo:number    lexinfo:plural .

:website_223_sense_1 a  lemon:LexicalSense ;
  lemon:reference :website_223_sense_1_res ;
  lemon:isA       :website_223_PredSynArg .

:website_223_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Website> .