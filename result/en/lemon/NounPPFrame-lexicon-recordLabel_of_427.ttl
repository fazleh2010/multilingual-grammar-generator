@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :recordLabel_of_427 ;
  lemon:entry    :recordLabel_of_427_form_preposition .

:recordLabel_of_427 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :recordLabel_of_427_form ;
  lemon:otherForm  :recordLabel_of_427_singular ;
  lemon:otherForm  :recordLabel_of_427_plural ;
  lemon:sense          :recordLabel_of_427_sense_1 ;
  lemon:synBehavior    :recordLabel_of_427_nounpp .

:recordLabel_of_427_form a lemon:Form ;

:recordLabel_of_427_singular a    lemon:Form ;
  lemon:writtenRep "record label"@en ;
  lexinfo:number   lexinfo:singular .

:recordLabel_of_427_plural a   lemon:Form ;
  lemon:writtenRep "record labels"@en ;
  lexinfo:number    lexinfo:plural .

:recordLabel_of_427_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_recordLabel_of_427 ;
  lexinfo:prepositionalAdjunct :arg2_recordLabel_of_427 .

:recordLabel_of_427_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :recordLabel_of_427_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/RecordLabel> ;
  lemon:subjOfProp          :arg2_recordLabel_of_427 ;
  lemon:objOfProp           :arg1_recordLabel_of_427 ;
  lemon:condition           :recordLabel_of_427_sense_1_condition .

:recordLabel_of_427_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Genre> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2_recordLabel_of_427 lemon:marker :recordLabel_of_427_form_preposition .
## Prepositions ##
:recordLabel_of_427_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

