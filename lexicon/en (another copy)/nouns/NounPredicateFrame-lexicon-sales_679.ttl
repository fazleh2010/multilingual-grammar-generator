@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :sales_679_sense_1_res ;
  lemon:entry    :sales_679 .

:sales_679 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :sales_679_form ;
  lemon:otherForm  :sales_679_singular ;
  lemon:otherForm  :sales_679_plural ;
  lemon:sense          :sales_679_sense_1 ;
  lemon:synBehavior    :sales_679_predFrame .

:sales_679_form
 lemon:writtenRep "sales"@en .

:sales_679_singular a    lemon:Form ;
  lemon:writtenRep "sales"@en ;
  lexinfo:number   lexinfo:singular .

:sales_679_plural a   lemon:Form ;
  lemon:writtenRep "sales"@en ;
  lexinfo:number    lexinfo:plural .

:sales_679_sense_1 a  lemon:LexicalSense ;
  lemon:reference :sales_679_sense_1_res ;
  lemon:isA       :sales_679_PredSynArg .

:sales_679_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Sales> .