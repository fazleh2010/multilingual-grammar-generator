@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :location_of_50 ;
  lemon:entry    :location_of_50_form_preposition .

:location_of_50 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :location_of_50_form ;
  lemon:otherForm  :location_of_50_singular ;
  lemon:otherForm  :location_of_50_plural ;
  lemon:sense          :location_of_50_sense_1 ;
  lemon:synBehavior    :location_of_50_nounpp .

:location_of_50_form a lemon:Form ;

:location_of_50_singular a    lemon:Form ;
  lemon:writtenRep "location"@en ;
  lexinfo:number   lexinfo:singular .

:location_of_50_plural a   lemon:Form ;
  lemon:writtenRep "locations"@en ;
  lexinfo:number    lexinfo:plural .

:location_of_50_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_location_of_50 ;
  lexinfo:prepositionalAdjunct :arg2_location_of_50 .

:location_of_50_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :location_of_50_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_location_of_50 ;
  lemon:objOfProp           :arg1_location_of_50 ;
  lemon:condition           :location_of_50_sense_1_condition .

:location_of_50_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Location> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Location> .

:arg2_location_of_50 lemon:marker :location_of_50_form_preposition .
## Prepositions ##
:location_of_50_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

