@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :war_of_74 ;
  lemon:entry    :war_of_74_form_preposition .

:war_of_74 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :war_of_74_form ;
  lemon:otherForm  :war_of_74_singular ;
  lemon:otherForm  :war_of_74_plural ;
  lemon:sense          :war_of_74_sense_1 ;
  lemon:synBehavior    :war_of_74_nounpp .

:war_of_74_form a lemon:Form ;
  lemon:writtenRep "war"@en .

:war_of_74_singular a    lemon:Form ;
  lemon:writtenRep "war"@en ;
  lexinfo:number   lexinfo:singular .

:war_of_74_plural a   lemon:Form ;
  lemon:writtenRep "wars"@en ;
  lexinfo:number    lexinfo:plural .

:war_of_74_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_war_of_74 ;
  lexinfo:prepositionalAdjunct :arg2_war_of_74 .

:war_of_74_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :war_of_74_sense_1 ;
  lemon:reference           <wd:P607> ;
  lemon:subjOfProp          :arg2_war_of_74 ;
  lemon:objOfProp           :arg1_war_of_74 ;
  lemon:condition           :war_of_74_sense_1_condition .

:war_of_74_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <wd:Q19088> ;
  lemon:propertyRange   <wd:Q1656682> .

:arg2_war_of_74 lemon:marker :war_of_74_form_preposition .
## Prepositions ##
:war_of_74_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

