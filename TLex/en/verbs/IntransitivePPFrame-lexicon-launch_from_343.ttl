@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :launch_from_343 ;
  lemon:entry    :form_launch_from_343_preposition .

:launch_from_343 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_launch_from_343 ;
  lemon:otherForm      :form_launch_from_343_present3rd ;
  lemon:otherForm      :form_launch_from_343_past ;
  lemon:otherForm      :form_launch_from_343_perfect ;
  lemon:sense          :launch_from_343_sense_1 ;
  lemon:synBehavior    :launch_from_343_frame .

:form_launch_from_343 a           lemon:Form ;
  lemon:writtenRep     "launch"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_launch_from_343_present3rd a      lemon:Form ;
  lemon:writtenRep "launches"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_launch_from_343_past a lemon:Form ;
  lemon:writtenRep  "launched"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_launch_from_343_perfect a   lemon:Form ;
  lemon:writtenRep "launched"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:launch_from_343_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :launch_from_343_subj ;
  lexinfo:prepositionalAdjunct :launch_from_343_obj .

:launch_from_343_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :launch_from_343_ontomap ;
  lemon:reference   <http://dbpedia.org/property/launchPad> ;
  lemon:subjOfProp  :launch_from_343_obj ;
  lemon:objOfProp   :launch_from_343_subj ;
  lemon:condition   :launch_from_343_sense_1_condition .

:launch_from_343_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/SpaceStation> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Airport> .


:launch_from_343_obj lemon:marker :form_launch_from_343_preposition .

## Prepositions ##
:form_launch_from_343_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "from"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

