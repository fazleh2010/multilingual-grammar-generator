@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :released_on_2_80 ;
  lemon:entry    :form_released_on_2_80_preposition .

:released_on_2_80 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_released_on_2_80 ;
  lemon:otherForm      :form_released_on_2_80_present3rd ;
  lemon:otherForm      :form_released_on_2_80_past ;
  lemon:otherForm      :form_released_on_2_80_perfect ;
  lemon:sense          :released_on_2_80_sense_1 ;
  lemon:synBehavior    :released_on_2_80_frame .

:form_released_on_2_80 a           lemon:Form ;
  lemon:writtenRep     "release"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_released_on_2_80_present3rd a      lemon:Form ;
  lemon:writtenRep "releases"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_released_on_2_80_past a lemon:Form ;
  lemon:writtenRep  "released"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_released_on_2_80_perfect a   lemon:Form ;
  lemon:writtenRep "released"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:released_on_2_80_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :released_on_2_80_subj ;
  lexinfo:prepositionalAdjunct :released_on_2_80_obj .

:released_on_2_80_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :released_on_2_80_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/releaseDate> ;
  lemon:subjOfProp  :released_on_2_80_obj ;
  lemon:objOfProp   :released_on_2_80_subj ;
  lemon:condition   :released_on_2_80_sense_1_condition .

:released_on_2_80_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Song> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:released_on_2_80_obj lemon:marker :form_released_on_2_80_preposition .

## Prepositions ##
:form_released_on_2_80_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "at"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

