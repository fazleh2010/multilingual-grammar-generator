@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :school_of_103_sense_1_res ;
  lemon:entry    :school_of_103 .

:school_of_103 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :school_of_103_form ;
  lemon:otherForm  :school_of_103_singular ;
  lemon:otherForm  :school_of_103_plural ;
  lemon:sense          :school_of_103_sense_1 ;
