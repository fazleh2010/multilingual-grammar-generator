@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :closingDate-of_450 ;
  lemon:entry    :closingDate-of_450_form_preposition .

:closingDate-of_450 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :closingDate-of_450_form ;
  lemon:otherForm  :closingDate-of_450_singular ;
  lemon:otherForm  :closingDate-of_450_plural ;
  lemon:sense          :closingDate-of_450_sense_1 ;
  lemon:synBehavior    :closingDate-of_450_nounpp .

:closingDate-of_450_form a lemon:Form ;

:closingDate-of_450_singular a    lemon:Form ;
  lemon:writtenRep "closing Date"@en ;
  lexinfo:number   lexinfo:singular .

:closingDate-of_450_plural a   lemon:Form ;
  lemon:writtenRep "closing Dates"@en ;
  lexinfo:number    lexinfo:plural .

:closingDate-of_450_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_closingDate-of_450 ;
  lexinfo:prepositionalAdjunct :arg2_closingDate-of_450 .

:closingDate-of_450_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :closingDate-of_450_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/closingDate> ;
  lemon:subjOfProp          :arg2_closingDate-of_450 ;
  lemon:objOfProp           :arg1_closingDate-of_450 ;
  lemon:condition           :closingDate-of_450_sense_1_condition .

:closingDate-of_450_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2_closingDate-of_450 lemon:marker :closingDate-of_450_form_preposition .
## Prepositions ##
:closingDate-of_450_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

