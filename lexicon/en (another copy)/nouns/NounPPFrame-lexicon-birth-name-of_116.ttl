  lemon:otherForm  :birth-name-of_116_singular ;
  lemon:otherForm  :birth-name-of_116_plural ;
  lemon:sense          :birth-name-of_116_sense_1 ;
  lemon:sense          :birth-name-of_116_sense_2 ;
