@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-1652 ;
  lemon:entry    :form_lexicalEntry-1652_preposition .

:lexicalEntry-1652 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-1652 ;
  lemon:otherForm      :form_lexicalEntry-1652_past ;
  lemon:sense          :lexicalEntry-1652_sense_1 ;
  lemon:synBehavior    :lexicalEntry-1652_frame .

:form_lexicalEntry-1652 a           lemon:Form ;
  lemon:writtenRep     "performed in"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-1652 a      lemon:Form ;
  lemon:writtenRep "performed in"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-1652_past a lemon:Form ;
  lemon:writtenRep  "performed in"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-1652perfect a   lemon:Form ;
  lemon:writtenRep "performed in"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-1652_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-1652_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-1652_obj .

:lexicalEntry-1652_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1652_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/language> ;
  lemon:subjOfProp  :lexicalEntry-1652_obj ;
  lemon:objOfProp   :lexicalEntry-1652_subj ;
  lemon:condition   :lexicalEntry-1652_sense_1_condition .

:lexicalEntry-1652_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Language> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Country> .


:lexicalEntry-1652_obj lemon:marker :form_lexicalEntry-1652_preposition .

## Prepositions ##
:form_lexicalEntry-1652_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

