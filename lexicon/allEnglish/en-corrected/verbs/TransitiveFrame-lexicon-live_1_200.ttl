@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_live_1_200 ;
  lemon:entry    :live_1_200ed ;
  lemon:entry    :form_live_1_200_preposition .

:to_live_1_200 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_live_1_200 ;
  lemon:otherForm      :form_live_1_200s ;
  lemon:otherForm      :form_live_1_200ed ;
  lemon:otherForm      :form_live_1_200perfect ;
  lemon:sense          :live_1_200_sense_1 ;
  lemon:synBehavior    :live_1_200_frame_transitive .

:form_live_1_200 a         lemon:Form ;
  lemon:writtenRep     "live"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_live_1_200s a    lemon:Form ;
  lemon:writtenRep "lives"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_live_1_200ed a   lemon:Form ;
  lemon:writtenRep "lived"@en ;
  lexinfo:tense    lexinfo:past .

:form_live_1_200perfect a   lemon:Form ;
  lemon:writtenRep "lived"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:live_1_200_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :live_1_200_subj ;
  lexinfo:directObject     :live_1_200_obj .

:live_1_200_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :live_1_200_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/ethnicGroup> ;
  lemon:subjOfProp  :live_1_200_obj ;
  lemon:objOfProp   :live_1_200_subj ;
  lemon:condition   :live_1_200_sense_1_condition .


:live_1_200_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/EthnicGroup> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Country> .


:live_1_200_obj lemon:marker :form_live_1_200_preposition .

## Prepositions ##
:form_live_1_200_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

