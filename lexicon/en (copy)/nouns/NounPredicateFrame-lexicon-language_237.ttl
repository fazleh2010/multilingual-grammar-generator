@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :language_237_sense_1_res ;
  lemon:entry    :language_237 .

:language_237 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :language_237_form ;
  lemon:otherForm  :language_237_singular ;
  lemon:otherForm  :language_237_plural ;
  lemon:sense          :language_237_sense_1 ;
  lemon:synBehavior    :language_237_predFrame .

:language_237_form
 lemon:writtenRep "language"@en .

:language_237_singular a    lemon:Form ;
  lemon:writtenRep "language"@en ;
  lexinfo:number   lexinfo:singular .

:language_237_plural a   lemon:Form ;
  lemon:writtenRep "languages"@en ;
  lexinfo:number    lexinfo:plural .

:language_237_sense_1 a  lemon:LexicalSense ;
  lemon:reference :language_237_sense_1_res ;
  lemon:isA       :language_237_PredSynArg .

:language_237_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Language> .