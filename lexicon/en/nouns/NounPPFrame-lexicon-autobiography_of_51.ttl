@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :autobiography_of_51 ;
  lemon:entry    :autobiography_of_51_form_preposition .

:autobiography_of_51 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :autobiography_of_51_form ;
  lemon:otherForm  :autobiography_of_51_singular ;
  lemon:otherForm  :autobiography_of_51_plural ;
  lemon:sense          :autobiography_of_51_sense_1 ;
  lemon:synBehavior    :autobiography_of_51_nounpp .

:autobiography_of_51_form a lemon:Form ;
  lemon:writtenRep "autobiography"@en .

:autobiography_of_51_singular a    lemon:Form ;
  lemon:writtenRep "autobiography"@en ;
  lexinfo:number   lexinfo:singular .

:autobiography_of_51_plural a   lemon:Form ;
  lemon:writtenRep "autobiography"@en ;
  lexinfo:number    lexinfo:plural .

:autobiography_of_51_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_autobiography_of_51 ;
  lexinfo:prepositionalAdjunct :arg2_autobiography_of_51 .

:autobiography_of_51_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :autobiography_of_51_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_autobiography_of_51 ;
  lemon:objOfProp           :arg1_autobiography_of_51 ;
  lemon:condition           :autobiography_of_51_sense_1_condition .

:autobiography_of_51_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Autobiography> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Autobiography> .

:arg2_autobiography_of_51 lemon:marker :autobiography_of_51_form_preposition .
## Prepositions ##
:autobiography_of_51_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

