:movie-with_84_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_movie-with_84 ;
  lexinfo:prepositionalAdjunct :arg2_movie-with_84 .

:movie-with_84_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
