@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-436 ;
  lemon:entry    :lexicalEntry-436_form_preposition .

:lexicalEntry-436 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lexicalEntry-436_form ;
  lemon:otherForm  :lexicalEntry-436_singular ;
  lemon:otherForm  :lexicalEntry-436_plural ;
  lemon:sense          :lexicalEntry-436_sense_1 ;
  lemon:synBehavior    :lexicalEntry-436_nounpp .

:lexicalEntry-436_form a lemon:Form ;
  lemon:writtenRep "death"@en .

:lexicalEntry-436_singular a    lemon:Form ;
  lemon:writtenRep "death"@en ;
  lexinfo:number   lexinfo:singular .

:lexicalEntry-436_plural a   lemon:Form ;
  lemon:writtenRep "death"@en ;
  lexinfo:number    lexinfo:plural .

:lexicalEntry-436_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg2 ;
  lexinfo:prepositionalAdjunct :arg1 .

:lexicalEntry-436_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :lexicalEntry-436_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/deathDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :lexicalEntry-436_sense_1_condition .

:lexicalEntry-436_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :lexicalEntry-436_form_preposition .
## Prepositions ##
:lexicalEntry-436_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

