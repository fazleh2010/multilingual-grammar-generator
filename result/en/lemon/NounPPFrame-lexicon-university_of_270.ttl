@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :university_of_270 ;
  lemon:entry    :university_of_270_form_preposition .

:university_of_270 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :university_of_270_form ;
  lemon:otherForm  :university_of_270_singular ;
  lemon:otherForm  :university_of_270_plural ;
  lemon:sense          :university_of_270_sense_1 ;
  lemon:synBehavior    :university_of_270_nounpp .

:university_of_270_form a lemon:Form ;

:university_of_270_singular a    lemon:Form ;
  lemon:writtenRep "University"@en ;
  lexinfo:number   lexinfo:singular .

:university_of_270_plural a   lemon:Form ;
  lemon:writtenRep "Universitys"@en ;
  lexinfo:number    lexinfo:plural .

:university_of_270_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_university_of_270 ;
  lexinfo:prepositionalAdjunct :arg2_university_of_270 .

:university_of_270_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :university_of_270_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_university_of_270 ;
  lemon:objOfProp           :arg1_university_of_270 ;
  lemon:condition           :university_of_270_sense_1_condition .

:university_of_270_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/University> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/University> .

:arg2_university_of_270 lemon:marker :university_of_270_form_preposition .
## Prepositions ##
:university_of_270_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

