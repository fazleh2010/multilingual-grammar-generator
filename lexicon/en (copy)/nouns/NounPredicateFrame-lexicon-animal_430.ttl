@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :animal_430_sense_1_res ;
  lemon:entry    :animal_430 .

:animal_430 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :animal_430_form ;
  lemon:otherForm  :animal_430_singular ;
  lemon:otherForm  :animal_430_plural ;
  lemon:sense          :animal_430_sense_1 ;
  lemon:synBehavior    :animal_430_predFrame .

:animal_430_form
 lemon:writtenRep "animal"@en .

:animal_430_singular a    lemon:Form ;
  lemon:writtenRep "animal"@en ;
  lexinfo:number   lexinfo:singular .

:animal_430_plural a   lemon:Form ;
  lemon:writtenRep "animals"@en ;
  lexinfo:number    lexinfo:plural .

:animal_430_sense_1 a  lemon:LexicalSense ;
  lemon:reference :animal_430_sense_1_res ;
  lemon:isA       :animal_430_PredSynArg .

:animal_430_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Animal> .