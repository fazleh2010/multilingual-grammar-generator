@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :work_of ;
  lemon:entry    :work_of_form_preposition .

:work_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :work_of_form ;
  lemon:otherForm  :work_of_singular ;
  lemon:otherForm  :work_of_plural ;
  lemon:sense          :work_of_sense_1 ;
  lemon:sense          :work_of_sense_2 ;
  lemon:synBehavior    :work_of_nounpp .

:work_of_form a lemon:Form ;
  lemon:writtenRep "work"@en .

:work_of_singular a    lemon:Form ;
  lemon:writtenRep "work"@en ;
  lexinfo:number   lexinfo:singular .

:work_of_plural a   lemon:Form ;
  lemon:writtenRep "works"@en ;
  lexinfo:number    lexinfo:plural .

:work_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:work_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :work_of_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :work_of_sense_1_condition .

:work_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Ship> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Ship> .

:work_of_sense_2 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :work_of_sense_2 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :work_of_sense_2_condition .

:work_of_sense_2_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Work> .

:arg2 lemon:marker :work_of_form_preposition .
## Prepositions ##
:work_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

