@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :australian_23_sense_1_res ;
  lemon:entry    :australian_23 .

:australian_23 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :australian_23_lemma ;
  lemon:sense          :australian_23_sense_1 ;
  lemon:synBehavior    :australian_23_attrFrame, :australian_23_predFrame .

:australian_23_lemma lemon:writtenRep "australian"@en .

:australian_23_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :australian_23_PredSynArg .

:australian_23_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :australian_23_AttrSynArg .

:australian_23_sense_1 a  lemon:LexicalSense ;
  lemon:reference :australian_23_sense_1_res ;
  lemon:isA       :australian_23_AttrSynArg, :australian_23_PredSynArg .

:australian_23_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Australia> .
