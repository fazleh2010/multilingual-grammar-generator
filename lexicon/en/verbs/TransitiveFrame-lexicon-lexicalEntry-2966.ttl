@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-2966 ;
  lemon:entry    :lexicalEntry-2966ed ;
  lemon:entry    :form_lexicalEntry-2966_preposition .

:to_lexicalEntry-2966 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-2966 ;
  lemon:otherForm      :form_lexicalEntry-2966s ;
  lemon:otherForm      :form_lexicalEntry-2966ed ;
  lemon:otherForm      :form_lexicalEntry-2966perfect ;
  lemon:sense          :lexicalEntry-2966_sense_1 ;
  lemon:synBehavior    :lexicalEntry-2966_frame_transitive .

:form_lexicalEntry-2966 a         lemon:Form ;
  lemon:writtenRep     "based in"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-2966s a    lemon:Form ;
  lemon:writtenRep "based in"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-2966ed a   lemon:Form ;
  lemon:writtenRep "based in"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-2966perfect a   lemon:Form ;
  lemon:writtenRep "based in"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-2966_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-2966_obj ;
  lexinfo:directObject     :lexicalEntry-2966_subj .

:lexicalEntry-2966_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2966_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/targetAirport> ;
  lemon:subjOfProp  :lexicalEntry-2966_obj ;
  lemon:objOfProp   :lexicalEntry-2966_subj ;
  lemon:condition   :lexicalEntry-2966_sense_1_condition .


:lexicalEntry-2966_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Airline> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Airport> .

:lexicalEntry-2966ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-2966ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-2966ed_by ;
  lemon:synBehavior    :lexicalEntry-2966ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-2966ed_ontomap .

:form_lexicalEntry-2966ed_canonical a lemon:Form ;
  lemon:writtenRep         "based in"@en .

:form_lexicalEntry-2966ed_by a    lemon:Form ;
  lemon:writtenRep     "based in"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-2966ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-2966ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-2966ed_obj .

:lexicalEntry-2966ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2966ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/targetAirport> ;
  lemon:subjOfProp  :lexicalEntry-2966ed_subj ;
  lemon:objOfProp   :lexicalEntry-2966ed_obj ;
  lemon:condition   :lexicalEntry-2966_sense_1_condition .
:lexicalEntry-2966ed_obj lemon:marker :form_lexicalEntry-2966_preposition .

## Prepositions ##
:form_lexicalEntry-2966_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

