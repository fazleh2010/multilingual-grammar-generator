@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :origin-of_506 ;
  lemon:entry    :origin-of_506_form_preposition .

:origin-of_506 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :origin-of_506_form ;
  lemon:otherForm  :origin-of_506_singular ;
  lemon:otherForm  :origin-of_506_plural ;
  lemon:sense          :origin-of_506_sense_1 ;
  lemon:synBehavior    :origin-of_506_nounpp .

:origin-of_506_form a lemon:Form ;
  lemon:writtenRep "origin"@en .

:origin-of_506_singular a    lemon:Form ;
  lemon:writtenRep "origin"@en ;
  lexinfo:number   lexinfo:singular .

:origin-of_506_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:origin-of_506_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_origin-of_506 ;
  lexinfo:prepositionalAdjunct :arg2_origin-of_506 .

:origin-of_506_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :origin-of_506_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/origin> ;
  lemon:subjOfProp          :arg2_origin-of_506 ;
  lemon:objOfProp           :arg1_origin-of_506 ;
  lemon:condition           :origin-of_506_sense_1_condition .

:origin-of_506_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Device> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2_origin-of_506 lemon:marker :origin-of_506_form_preposition .
## Prepositions ##
:origin-of_506_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

