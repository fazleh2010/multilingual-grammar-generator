@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :library_of_11 ;
  lemon:entry    :library_of_11_form_preposition .

:library_of_11 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :library_of_11_form ;
  lemon:otherForm  :library_of_11_singular ;
  lemon:otherForm  :library_of_11_plural ;
  lemon:sense          :library_of_11_sense_1 ;
  lemon:synBehavior    :library_of_11_nounpp .

:library_of_11_form a lemon:Form ;

:library_of_11_singular a    lemon:Form ;
  lemon:writtenRep "Library"@en ;
  lexinfo:number   lexinfo:singular .

:library_of_11_plural a   lemon:Form ;
  lemon:writtenRep "Librarys"@en ;
  lexinfo:number    lexinfo:plural .

:library_of_11_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_library_of_11 ;
  lexinfo:prepositionalAdjunct :arg2_library_of_11 .

:library_of_11_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :library_of_11_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_library_of_11 ;
  lemon:objOfProp           :arg1_library_of_11 ;
  lemon:condition           :library_of_11_sense_1_condition .

:library_of_11_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Library> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Library> .

:arg2_library_of_11 lemon:marker :library_of_11_form_preposition .
## Prepositions ##
:library_of_11_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

