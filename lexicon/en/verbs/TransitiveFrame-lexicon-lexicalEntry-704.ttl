@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-704 ;
  lemon:entry    :lexicalEntry-704ed ;
  lemon:entry    :form_lexicalEntry-704_preposition .

:to_lexicalEntry-704 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-704 ;
  lemon:otherForm      :form_lexicalEntry-704s ;
  lemon:otherForm      :form_lexicalEntry-704ed ;
  lemon:otherForm      :form_lexicalEntry-704perfect ;
  lemon:sense          :lexicalEntry-704_sense_1 ;
  lemon:synBehavior    :lexicalEntry-704_frame_transitive .

:form_lexicalEntry-704 a         lemon:Form ;
  lemon:writtenRep     "merged into"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-704s a    lemon:Form ;
  lemon:writtenRep "merged into"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-704ed a   lemon:Form ;
  lemon:writtenRep "merged into"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-704perfect a   lemon:Form ;
  lemon:writtenRep "merged into"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-704_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-704_obj ;
  lexinfo:directObject     :lexicalEntry-704_subj .

:lexicalEntry-704_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-704_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/dissolutionDate> ;
  lemon:subjOfProp  :lexicalEntry-704_obj ;
  lemon:objOfProp   :lexicalEntry-704_subj ;
  lemon:condition   :lexicalEntry-704_sense_1_condition .


:lexicalEntry-704_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .

:lexicalEntry-704ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-704ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-704ed_by ;
  lemon:synBehavior    :lexicalEntry-704ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-704ed_ontomap .

:form_lexicalEntry-704ed_canonical a lemon:Form ;
  lemon:writtenRep         "merged into"@en .

:form_lexicalEntry-704ed_by a    lemon:Form ;
  lemon:writtenRep     "merged into"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-704ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-704ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-704ed_obj .

:lexicalEntry-704ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-704ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/dissolutionDate> ;
  lemon:subjOfProp  :lexicalEntry-704ed_subj ;
  lemon:objOfProp   :lexicalEntry-704ed_obj ;
  lemon:condition   :lexicalEntry-704_sense_1_condition .
:lexicalEntry-704ed_obj lemon:marker :form_lexicalEntry-704_preposition .

## Prepositions ##
:form_lexicalEntry-704_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

