@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_develop_1_5 ;
  lemon:entry    :develop_1_5ed ;
  lemon:entry    :form_develop_1_5_preposition .

:to_develop_1_5 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_develop_1_5 ;
  lemon:otherForm      :form_develop_1_5s ;
  lemon:otherForm      :form_develop_1_5ed ;
  lemon:otherForm      :form_develop_1_5perfect ;
  lemon:sense          :develop_1_5_sense_1 ;
  lemon:synBehavior    :develop_1_5_frame_transitive .

:form_develop_1_5 a         lemon:Form ;
  lemon:writtenRep     "develop"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_develop_1_5s a    lemon:Form ;
  lemon:writtenRep "develops"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_develop_1_5ed a   lemon:Form ;
  lemon:writtenRep "developed"@en ;
  lexinfo:tense    lexinfo:past .

:form_develop_1_5perfect a   lemon:Form ;
  lemon:writtenRep "developed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:develop_1_5_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :develop_1_5_obj ;
  lexinfo:directObject     :develop_1_5_subj .

:develop_1_5_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :develop_1_5_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/product> ;
  lemon:subjOfProp  :develop_1_5_obj ;
  lemon:objOfProp   :develop_1_5_subj ;
  lemon:condition   :develop_1_5_sense_1_condition .


:develop_1_5_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Software> .


:develop_1_5_obj lemon:marker :form_develop_1_5_preposition .

## Prepositions ##
:form_develop_1_5_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

