@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :continent_500_sense_1_res ;
  lemon:entry    :continent_500 .

:continent_500 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :continent_500_form ;
  lemon:otherForm  :continent_500_singular ;
  lemon:otherForm  :continent_500_plural ;
  lemon:sense          :continent_500_sense_1 ;
  lemon:synBehavior    :continent_500_predFrame .

:continent_500_form
 lemon:writtenRep "continent"@en .

:continent_500_singular a    lemon:Form ;
  lemon:writtenRep "continent"@en ;
  lexinfo:number   lexinfo:singular .

:continent_500_plural a   lemon:Form ;
  lemon:writtenRep "continents"@en ;
  lexinfo:number    lexinfo:plural .

:continent_500_sense_1 a  lemon:LexicalSense ;
  lemon:reference :continent_500_sense_1_res ;
  lemon:isA       :continent_500_PredSynArg .

:continent_500_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Continent> .