@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_star_3_30 ;
  lemon:entry    :star_3_30ed ;
  lemon:entry    :form_star_3_30_preposition .

:to_star_3_30 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_star_3_30 ;
  lemon:otherForm      :form_star_3_30s ;
  lemon:otherForm      :form_star_3_30ed ;
  lemon:otherForm      :form_star_3_30perfect ;
  lemon:sense          :star_3_30_sense_1 ;
  lemon:synBehavior    :star_3_30_frame_transitive .

:form_star_3_30 a         lemon:Form ;
  lemon:writtenRep     "star"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_star_3_30s a    lemon:Form ;
  lemon:writtenRep "stars"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_star_3_30ed a   lemon:Form ;
  lemon:writtenRep "starred"@en ;
  lexinfo:tense    lexinfo:past .

:form_star_3_30perfect a   lemon:Form ;
  lemon:writtenRep "starred"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:star_3_30_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :star_3_30_subj ;
  lexinfo:directObject     :star_3_30_obj .

:star_3_30_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :star_3_30_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/starring> ;
  lemon:subjOfProp  :star_3_30_obj ;
  lemon:objOfProp   :star_3_30_subj ;
  lemon:condition   :star_3_30_sense_1_condition .


:star_3_30_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Actor> .


:star_3_30_obj lemon:marker :form_star_3_30_preposition .

## Prepositions ##
:form_star_3_30_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

