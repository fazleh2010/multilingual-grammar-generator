@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :region_of_737 ;
  lemon:entry    :region_of_737_form_preposition .

:region_of_737 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :region_of_737_form ;
  lemon:otherForm  :region_of_737_singular ;
  lemon:otherForm  :region_of_737_plural ;
  lemon:sense          :region_of_737_sense_1 ;
  lemon:synBehavior    :region_of_737_nounpp .

:region_of_737_form a lemon:Form ;
  lemon:writtenRep "region"@en .

:region_of_737_singular a    lemon:Form ;
  lemon:writtenRep "region"@en ;
  lexinfo:number   lexinfo:singular .

:region_of_737_plural a   lemon:Form ;
  lemon:writtenRep "regions"@en ;
  lexinfo:number    lexinfo:plural .

:region_of_737_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_region_of_737 ;
  lexinfo:prepositionalAdjunct :arg2_region_of_737 .

:region_of_737_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :region_of_737_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/wineRegion> ;
  lemon:subjOfProp          :arg2_region_of_737 ;
  lemon:objOfProp           :arg1_region_of_737 ;
  lemon:condition           :region_of_737_sense_1_condition .

:region_of_737_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Grape> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Place> .

:arg2_region_of_737 lemon:marker :region_of_737_form_preposition .
## Prepositions ##
:region_of_737_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

