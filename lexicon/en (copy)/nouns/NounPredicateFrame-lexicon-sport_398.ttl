@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :sport_398_sense_1_res ;
  lemon:entry    :sport_398 .

:sport_398 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :sport_398_form ;
  lemon:otherForm  :sport_398_singular ;
  lemon:otherForm  :sport_398_plural ;
  lemon:sense          :sport_398_sense_1 ;
  lemon:synBehavior    :sport_398_predFrame .

:sport_398_form
 lemon:writtenRep "sport"@en .

:sport_398_singular a    lemon:Form ;
  lemon:writtenRep "sport"@en ;
  lexinfo:number   lexinfo:singular .

:sport_398_plural a   lemon:Form ;
  lemon:writtenRep "sports"@en ;
  lexinfo:number    lexinfo:plural .

:sport_398_sense_1 a  lemon:LexicalSense ;
  lemon:reference :sport_398_sense_1_res ;
  lemon:isA       :sport_398_PredSynArg .

:sport_398_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Sport> .