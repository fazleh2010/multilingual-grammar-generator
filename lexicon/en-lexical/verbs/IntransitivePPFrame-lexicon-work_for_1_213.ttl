  lemon:subjOfProp  :work_for_1_213_subj ;
  lemon:objOfProp   :work_for_1_213_obj ;
  lemon:condition   :work_for_1_213_sense_1_condition .

:work_for_1_213_sense_1_condition a      lemon:condition ;
