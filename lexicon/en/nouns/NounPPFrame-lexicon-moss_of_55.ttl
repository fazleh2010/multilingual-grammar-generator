@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :moss_of_55 ;
  lemon:entry    :moss_of_55_form_preposition .

:moss_of_55 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :moss_of_55_form ;
  lemon:otherForm  :moss_of_55_singular ;
  lemon:otherForm  :moss_of_55_plural ;
  lemon:sense          :moss_of_55_sense_1 ;
  lemon:synBehavior    :moss_of_55_nounpp .

:moss_of_55_form a lemon:Form ;
  lemon:writtenRep "Moss"@en .

:moss_of_55_singular a    lemon:Form ;
  lemon:writtenRep "Moss"@en ;
  lexinfo:number   lexinfo:singular .

:moss_of_55_plural a   lemon:Form ;
  lemon:writtenRep "Mosss"@en ;
  lexinfo:number    lexinfo:plural .

:moss_of_55_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_moss_of_55 ;
  lexinfo:prepositionalAdjunct :arg2_moss_of_55 .

:moss_of_55_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :moss_of_55_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_moss_of_55 ;
  lemon:objOfProp           :arg1_moss_of_55 ;
  lemon:condition           :moss_of_55_sense_1_condition .

:moss_of_55_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Moss> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Moss> .

:arg2_moss_of_55 lemon:marker :moss_of_55_form_preposition .
## Prepositions ##
:moss_of_55_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

