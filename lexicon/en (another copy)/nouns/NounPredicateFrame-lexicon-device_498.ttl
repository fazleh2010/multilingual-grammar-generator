@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :device_498_sense_1_res ;
  lemon:entry    :device_498 .

:device_498 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :device_498_form ;
  lemon:otherForm  :device_498_singular ;
  lemon:otherForm  :device_498_plural ;
  lemon:sense          :device_498_sense_1 ;
  lemon:synBehavior    :device_498_predFrame .

:device_498_form
 lemon:writtenRep "device"@en .

:device_498_singular a    lemon:Form ;
  lemon:writtenRep "device"@en ;
  lexinfo:number   lexinfo:singular .

:device_498_plural a   lemon:Form ;
  lemon:writtenRep "devices"@en ;
  lexinfo:number    lexinfo:plural .

:device_498_sense_1 a  lemon:LexicalSense ;
  lemon:reference :device_498_sense_1_res ;
  lemon:isA       :device_498_PredSynArg .

:device_498_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Device> .