@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :restingplace-of_256 ;
  lemon:entry    :restingplace-of_256_form_preposition .

:restingplace-of_256 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :restingplace-of_256_form ;
  lemon:otherForm  :restingplace-of_256_singular ;
  lemon:otherForm  :restingplace-of_256_plural ;
  lemon:sense          :restingplace-of_256_sense_1 ;
  lemon:synBehavior    :restingplace-of_256_nounpp .

:restingplace-of_256_form a lemon:Form ;
  lemon:writtenRep "resting place"@en .

:restingplace-of_256_singular a    lemon:Form ;
  lemon:writtenRep "resting place"@en ;
  lexinfo:number   lexinfo:singular .

:restingplace-of_256_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:restingplace-of_256_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_restingplace-of_256 ;
  lexinfo:prepositionalAdjunct :arg2_restingplace-of_256 .

:restingplace-of_256_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :restingplace-of_256_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/restingPlace> ;
  lemon:subjOfProp          :arg2_restingplace-of_256 ;
  lemon:objOfProp           :arg1_restingplace-of_256 ;
  lemon:condition           :restingplace-of_256_sense_1_condition .

:restingplace-of_256_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2_restingplace-of_256 lemon:marker :restingplace-of_256_form_preposition .
## Prepositions ##
:restingplace-of_256_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

