@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-3431 ;
  lemon:entry    :form_lexicalEntry-3431_preposition .

:lexicalEntry-3431 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-3431 ;
  lemon:otherForm      :form_lexicalEntry-3431_past ;
  lemon:sense          :lexicalEntry-3431_sense_1 ;
  lemon:synBehavior    :lexicalEntry-3431_frame .

:form_lexicalEntry-3431 a           lemon:Form ;
  lemon:writtenRep     "play"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-3431 a      lemon:Form ;
  lemon:writtenRep "playes"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-3431_past a lemon:Form ;
  lemon:writtenRep  "played"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-3431perfect a   lemon:Form ;
  lemon:writtenRep "played"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-3431_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-3431_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-3431_obj .

:lexicalEntry-3431_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3431_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/award> ;
  lemon:subjOfProp  :lexicalEntry-3431_obj ;
  lemon:objOfProp   :lexicalEntry-3431_subj ;
  lemon:condition   :lexicalEntry-3431_sense_1_condition .

:lexicalEntry-3431_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Scientist> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Award> .


:lexicalEntry-3431_obj lemon:marker :form_lexicalEntry-3431_preposition .

## Prepositions ##
:form_lexicalEntry-3431_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

