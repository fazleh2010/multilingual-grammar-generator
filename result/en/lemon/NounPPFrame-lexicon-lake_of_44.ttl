@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lake_of_44 ;
  lemon:entry    :lake_of_44_form_preposition .

:lake_of_44 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lake_of_44_form ;
  lemon:otherForm  :lake_of_44_singular ;
  lemon:otherForm  :lake_of_44_plural ;
  lemon:sense          :lake_of_44_sense_1 ;
  lemon:synBehavior    :lake_of_44_nounpp .

:lake_of_44_form a lemon:Form ;

:lake_of_44_singular a    lemon:Form ;
  lemon:writtenRep "lake"@en ;
  lexinfo:number   lexinfo:singular .

:lake_of_44_plural a   lemon:Form ;
  lemon:writtenRep "lakes"@en ;
  lexinfo:number    lexinfo:plural .

:lake_of_44_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_lake_of_44 ;
  lexinfo:prepositionalAdjunct :arg2_lake_of_44 .

:lake_of_44_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :lake_of_44_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_lake_of_44 ;
  lemon:objOfProp           :arg1_lake_of_44 ;
  lemon:condition           :lake_of_44_sense_1_condition .

:lake_of_44_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Lake> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Lake> .

:arg2_lake_of_44 lemon:marker :lake_of_44_form_preposition .
## Prepositions ##
:lake_of_44_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

