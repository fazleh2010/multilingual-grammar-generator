@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-765 ;
  lemon:entry    :lexicalEntry-765_form_preposition .

:lexicalEntry-765 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lexicalEntry-765_form ;
  lemon:otherForm  :lexicalEntry-765_singular ;
  lemon:otherForm  :lexicalEntry-765_plural ;
  lemon:sense          :lexicalEntry-765_sense_1 ;
  lemon:synBehavior    :lexicalEntry-765_nounpp .

:lexicalEntry-765_form a lemon:Form ;
  lemon:writtenRep "populated place"@en .

:lexicalEntry-765_singular a    lemon:Form ;
  lemon:writtenRep "populated place"@en ;
  lexinfo:number   lexinfo:singular .

:lexicalEntry-765_plural a   lemon:Form ;
  lemon:writtenRep "populated place"@en ;
  lexinfo:number    lexinfo:plural .

:lexicalEntry-765_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:lexicalEntry-765_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :lexicalEntry-765_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/elevation> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :lexicalEntry-765_sense_1_condition .

:lexicalEntry-765_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2 lemon:marker :lexicalEntry-765_form_preposition .
## Prepositions ##
:lexicalEntry-765_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

