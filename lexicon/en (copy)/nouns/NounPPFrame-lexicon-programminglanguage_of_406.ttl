@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :programminglanguage_of_406 ;
  lemon:entry    :programminglanguage_of_406_form_preposition .

:programminglanguage_of_406 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :programminglanguage_of_406_form ;
  lemon:otherForm  :programminglanguage_of_406_singular ;
  lemon:otherForm  :programminglanguage_of_406_plural ;
  lemon:sense          :programminglanguage_of_406_sense_1 ;
  lemon:synBehavior    :programminglanguage_of_406_nounpp .

:programminglanguage_of_406_form a lemon:Form ;
  lemon:writtenRep "programming language"@en .

:programminglanguage_of_406_singular a    lemon:Form ;
  lemon:writtenRep "programming language"@en ;
  lexinfo:number   lexinfo:singular .

:programminglanguage_of_406_plural a   lemon:Form ;
  lemon:writtenRep "programming languages"@en ;
  lexinfo:number    lexinfo:plural .

:programminglanguage_of_406_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :null ;
  lexinfo:prepositionalAdjunct :null .

:programminglanguage_of_406_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :programminglanguage_of_406_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/programmingLanguage> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :programminglanguage_of_406_sense_1_condition .

:programminglanguage_of_406_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Language> .

:null lemon:marker :programminglanguage_of_406_form_preposition .
## Prepositions ##
:programminglanguage_of_406_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

