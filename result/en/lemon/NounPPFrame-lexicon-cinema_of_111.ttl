@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :cinema_of_111 ;
  lemon:entry    :cinema_of_111_form_preposition .

:cinema_of_111 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :cinema_of_111_form ;
  lemon:otherForm  :cinema_of_111_singular ;
  lemon:otherForm  :cinema_of_111_plural ;
  lemon:sense          :cinema_of_111_sense_1 ;
  lemon:synBehavior    :cinema_of_111_nounpp .

:cinema_of_111_form a lemon:Form ;
  lemon:writtenRep "Cinema"@en .

:cinema_of_111_singular a    lemon:Form ;
  lemon:writtenRep "Cinema"@en ;
  lexinfo:number   lexinfo:singular .

:cinema_of_111_plural a   lemon:Form ;
  lemon:writtenRep "Cinemas"@en ;
  lexinfo:number    lexinfo:plural .

:cinema_of_111_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_cinema_of_111 ;
  lexinfo:prepositionalAdjunct :arg2_cinema_of_111 .

:cinema_of_111_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :cinema_of_111_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_cinema_of_111 ;
  lemon:objOfProp           :arg1_cinema_of_111 ;
  lemon:condition           :cinema_of_111_sense_1_condition .

:cinema_of_111_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Cinema> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Cinema> .

:arg2_cinema_of_111 lemon:marker :cinema_of_111_form_preposition .
## Prepositions ##
:cinema_of_111_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

