@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :total-population-of_81 ;
  lemon:entry    :total-population-of_81_form_preposition .

:total-population-of_81 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :total-population-of_81_form ;
  lemon:otherForm  :total-population-of_81_singular ;
  lemon:otherForm  :total-population-of_81_plural ;
  lemon:sense          :total-population-of_81_sense_1 ;
  lemon:synBehavior    :total-population-of_81_nounpp .

:total-population-of_81_form a lemon:Form ;
  lemon:writtenRep "name"@en .

:total-population-of_81_singular a    lemon:Form ;
  lemon:writtenRep "name"@en ;
  lexinfo:number   lexinfo:singular .

:total-population-of_81_plural a   lemon:Form ;
  lemon:writtenRep "names"@en ;
  lexinfo:number    lexinfo:plural .

:total-population-of_81_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_total-population-of_81 ;
  lexinfo:prepositionalAdjunct :arg2_total-population-of_81 .

:total-population-of_81_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :total-population-of_81_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/populationTotal> ;
  lemon:subjOfProp          :arg1_total-population-of_81 ;
  lemon:objOfProp           :arg2_total-population-of_81 ;
  lemon:condition           :total-population-of_81_sense_1_condition .

:total-population-of_81_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#nonNegativeInteger> .

:arg2_total-population-of_81 lemon:marker :total-population-of_81_form_preposition .
## Prepositions ##
:total-population-of_81_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

