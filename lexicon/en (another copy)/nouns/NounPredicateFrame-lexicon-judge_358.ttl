@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :judge_358_sense_1_res ;
  lemon:entry    :judge_358 .

:judge_358 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :judge_358_form ;
  lemon:otherForm  :judge_358_singular ;
  lemon:otherForm  :judge_358_plural ;
  lemon:sense          :judge_358_sense_1 ;
  lemon:synBehavior    :judge_358_predFrame .

:judge_358_form
 lemon:writtenRep "judge"@en .

:judge_358_singular a    lemon:Form ;
  lemon:writtenRep "judge"@en ;
  lexinfo:number   lexinfo:singular .

:judge_358_plural a   lemon:Form ;
  lemon:writtenRep "judges"@en ;
  lexinfo:number    lexinfo:plural .

:judge_358_sense_1 a  lemon:LexicalSense ;
  lemon:reference :judge_358_sense_1_res ;
  lemon:isA       :judge_358_PredSynArg .

:judge_358_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Judge> .