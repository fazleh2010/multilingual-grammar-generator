@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :time-zone_in_508 ;
  lemon:entry    :time-zone_in_508_form_preposition .

:time-zone_in_508 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :time-zone_in_508_form ;
  lemon:otherForm  :time-zone_in_508_singular ;
  lemon:otherForm  :time-zone_in_508_plural ;
  lemon:sense          :time-zone_in_508_sense_1 ;
  lemon:synBehavior    :time-zone_in_508_nounpp .

:time-zone_in_508_form a lemon:Form ;
  lemon:writtenRep "time zone"@en .

:time-zone_in_508_singular a    lemon:Form ;
  lemon:writtenRep "time zone"@en ;
  lexinfo:number   lexinfo:singular .

:time-zone_in_508_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:time-zone_in_508_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_time-zone_in_508 ;
  lexinfo:prepositionalAdjunct :arg2_time-zone_in_508 .

:time-zone_in_508_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :time-zone_in_508_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/timeZone> ;
  lemon:subjOfProp          :arg2_time-zone_in_508 ;
  lemon:objOfProp           :arg1_time-zone_in_508 ;
  lemon:condition           :time-zone_in_508_sense_1_condition .

:time-zone_in_508_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2_time-zone_in_508 lemon:marker :time-zone_in_508_form_preposition .
## Prepositions ##
:time-zone_in_508_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

