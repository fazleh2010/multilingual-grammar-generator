:bone_539_sense_1 a  lemon:LexicalSense ;
  lemon:reference :bone_539_sense_1_res ;
  lemon:isA       :bone_539_PredSynArg .

:bone_539_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
