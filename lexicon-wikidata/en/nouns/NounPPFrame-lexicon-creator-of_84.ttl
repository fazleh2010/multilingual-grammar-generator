@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :creator-of_84 ;
  lemon:entry    :creator-of_84_form_preposition .

:creator-of_84 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :creator-of_84_form ;
  lemon:otherForm  :creator-of_84_singular ;
  lemon:otherForm  :creator-of_84_plural ;
  lemon:sense          :creator-of_84_sense_1 ;
  lemon:synBehavior    :creator-of_84_nounpp .

:creator-of_84_form a lemon:Form ;
  lemon:writtenRep "creator"@en .

:creator-of_84_singular a    lemon:Form ;
  lemon:writtenRep "creator"@en ;
  lexinfo:number   lexinfo:singular .

:creator-of_84_plural a   lemon:Form ;
  lemon:writtenRep "creators"@en ;
  lexinfo:number    lexinfo:plural .

:creator-of_84_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_creator-of_84 ;
  lexinfo:prepositionalAdjunct :arg2_creator-of_84 .

:creator-of_84_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :creator-of_84_sense_1 ;
  lemon:reference           <wd:P170> ;
  lemon:subjOfProp          :arg2_creator-of_84 ;
  lemon:objOfProp           :arg1_creator-of_84 ;
  lemon:condition           :creator-of_84_sense_1_condition .

:creator-of_84_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <wd:Q24229398> ;
  lemon:propertyRange   <wd:Q19088> .

:arg2_creator-of_84 lemon:marker :creator-of_84_form_preposition .
## Prepositions ##
:creator-of_84_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

