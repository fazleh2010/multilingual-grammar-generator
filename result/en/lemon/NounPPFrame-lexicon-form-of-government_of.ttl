@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :form-of-government_of ;
  lemon:entry    :form-of-government_of_form_preposition .

:form-of-government_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :form-of-government_of_form ;
  lemon:otherForm  :form-of-government_of_singular ;
  lemon:otherForm  :form-of-government_of_plural ;
  lemon:sense          :form-of-government_of_sense_1 ;
  lemon:synBehavior    :form-of-government_of_nounpp .

:form-of-government_of_form a lemon:Form ;
  lemon:writtenRep "form of government"@en .

:form-of-government_of_singular a    lemon:Form ;
  lemon:writtenRep "form of government"@en ;
  lexinfo:number   lexinfo:singular .

:form-of-government_of_plural a   lemon:Form ;
  lemon:writtenRep "form of government"@en ;
  lexinfo:number    lexinfo:plural .

:form-of-government_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:form-of-government_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :form-of-government_of_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/governmentType> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :form-of-government_of_sense_1_condition .

:form-of-government_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Organisation> .

:arg2 lemon:marker :form-of-government_of_form_preposition .
## Prepositions ##
:form-of-government_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

