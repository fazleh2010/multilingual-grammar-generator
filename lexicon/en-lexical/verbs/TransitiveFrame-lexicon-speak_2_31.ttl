@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_speak_2_31 ;
  lemon:entry    :speak_2_31ed ;
  lemon:entry    :form_speak_2_31_preposition .

:to_speak_2_31 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_speak_2_31 ;
  lemon:otherForm      :form_speak_2_31s ;
  lemon:otherForm      :form_speak_2_31ed ;
  lemon:otherForm      :form_speak_2_31perfect ;
  lemon:sense          :speak_2_31_sense_1 ;
  lemon:synBehavior    :speak_2_31_frame_transitive .

:form_speak_2_31 a         lemon:Form ;
  lemon:writtenRep     "speak"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_speak_2_31s a    lemon:Form ;
  lemon:writtenRep "speaks"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_speak_2_31ed a   lemon:Form ;
  lemon:writtenRep "spoke"@en ;
  lexinfo:tense    lexinfo:past .

:form_speak_2_31perfect a   lemon:Form ;
  lemon:writtenRep "spoken"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:speak_2_31_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :speak_2_31_subj ;
  lexinfo:directObject     :speak_2_31_obj .

:speak_2_31_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :speak_2_31_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/officialLanguage> ;
  lemon:subjOfProp  :speak_2_31_obj ;
  lemon:objOfProp   :speak_2_31_subj ;
  lemon:condition   :speak_2_31_sense_1_condition .


:speak_2_31_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Language> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/PopulatedPlace> .


:speak_2_31_obj lemon:marker :form_speak_2_31_preposition .

## Prepositions ##
:form_speak_2_31_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

