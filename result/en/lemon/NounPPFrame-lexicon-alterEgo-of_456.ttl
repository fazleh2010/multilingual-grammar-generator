@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :alterEgo-of_456 ;
  lemon:entry    :alterEgo-of_456_form_preposition .

:alterEgo-of_456 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :alterEgo-of_456_form ;
  lemon:otherForm  :alterEgo-of_456_singular ;
  lemon:otherForm  :alterEgo-of_456_plural ;
  lemon:sense          :alterEgo-of_456_sense_1 ;
  lemon:synBehavior    :alterEgo-of_456_nounpp .

:alterEgo-of_456_form a lemon:Form ;

:alterEgo-of_456_singular a    lemon:Form ;
  lemon:writtenRep "alter ego"@en ;
  lexinfo:number   lexinfo:singular .

:alterEgo-of_456_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:alterEgo-of_456_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_alterEgo-of_456 ;
  lexinfo:prepositionalAdjunct :arg2_alterEgo-of_456 .

:alterEgo-of_456_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :alterEgo-of_456_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/alterEgo> ;
  lemon:subjOfProp          :arg2_alterEgo-of_456 ;
  lemon:objOfProp           :arg1_alterEgo-of_456 ;
  lemon:condition           :alterEgo-of_456_sense_1_condition .

:alterEgo-of_456_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ComicsCharacter> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2_alterEgo-of_456 lemon:marker :alterEgo-of_456_form_preposition .
## Prepositions ##
:alterEgo-of_456_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

