@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :comedian_of ;
  lemon:entry    :comedian_of_form_preposition .

:comedian_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :comedian_of_form ;
  lemon:otherForm  :comedian_of_singular ;
  lemon:otherForm  :comedian_of_plural ;
  lemon:sense          :comedian_of_sense_1 ;
  lemon:synBehavior    :comedian_of_nounpp .

:comedian_of_form a lemon:Form ;
  lemon:writtenRep "Comedian"@en .

:comedian_of_singular a    lemon:Form ;
  lemon:writtenRep "Comedian"@en ;
  lexinfo:number   lexinfo:singular .

:comedian_of_plural a   lemon:Form ;
  lemon:writtenRep "Comedians"@en ;
  lexinfo:number    lexinfo:plural .

:comedian_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:comedian_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :comedian_of_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :comedian_of_sense_1_condition .

:comedian_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Comedian> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Comedian> .

:arg2 lemon:marker :comedian_of_form_preposition .
## Prepositions ##
:comedian_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

