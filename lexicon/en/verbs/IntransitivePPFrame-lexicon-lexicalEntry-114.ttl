@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-114 ;
  lemon:entry    :form_lexicalEntry-114_preposition .

:lexicalEntry-114 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-114 ;
  lemon:otherForm      :form_lexicalEntry-114_past ;
  lemon:sense          :lexicalEntry-114_sense_1 ;
  lemon:synBehavior    :lexicalEntry-114_frame .

:form_lexicalEntry-114 a           lemon:Form ;
  lemon:writtenRep     "portrayed"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-114 a      lemon:Form ;
  lemon:writtenRep "portrayed"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-114_past a lemon:Form ;
  lemon:writtenRep  "portrayed"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-114perfect a   lemon:Form ;
  lemon:writtenRep "portrayed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-114_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-114_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-114_obj .

:lexicalEntry-114_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-114_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/child> ;
  lemon:subjOfProp  :lexicalEntry-114_obj ;
  lemon:objOfProp   :lexicalEntry-114_subj ;
  lemon:condition   :lexicalEntry-114_sense_1_condition .

:lexicalEntry-114_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:lexicalEntry-114_obj lemon:marker :form_lexicalEntry-114_preposition .

## Prepositions ##
:form_lexicalEntry-114_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

