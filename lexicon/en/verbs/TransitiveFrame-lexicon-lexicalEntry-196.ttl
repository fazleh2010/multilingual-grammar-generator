@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-196 ;
  lemon:entry    :lexicalEntry-196ed ;
  lemon:entry    :form_lexicalEntry-196_preposition .

:to_lexicalEntry-196 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-196 ;
  lemon:otherForm      :form_lexicalEntry-196s ;
  lemon:otherForm      :form_lexicalEntry-196ed ;
  lemon:otherForm      :form_lexicalEntry-196perfect ;
  lemon:sense          :lexicalEntry-196_sense_1 ;
  lemon:synBehavior    :lexicalEntry-196_frame_transitive .

:form_lexicalEntry-196 a         lemon:Form ;
  lemon:writtenRep     "owned"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-196s a    lemon:Form ;
  lemon:writtenRep "owned"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-196ed a   lemon:Form ;
  lemon:writtenRep "owned"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-196perfect a   lemon:Form ;
  lemon:writtenRep "owned"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-196_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-196_obj ;
  lexinfo:directObject     :lexicalEntry-196_subj .

:lexicalEntry-196_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-196_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/city> ;
  lemon:subjOfProp  :lexicalEntry-196_obj ;
  lemon:objOfProp   :lexicalEntry-196_subj ;
  lemon:condition   :lexicalEntry-196_sense_1_condition .


:lexicalEntry-196_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/City> .

:lexicalEntry-196ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-196ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-196ed_by ;
  lemon:synBehavior    :lexicalEntry-196ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-196ed_ontomap .

:form_lexicalEntry-196ed_canonical a lemon:Form ;
  lemon:writtenRep         "owned"@en .

:form_lexicalEntry-196ed_by a    lemon:Form ;
  lemon:writtenRep     "owned"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-196ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-196ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-196ed_obj .

:lexicalEntry-196ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-196ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/city> ;
  lemon:subjOfProp  :lexicalEntry-196ed_subj ;
  lemon:objOfProp   :lexicalEntry-196ed_obj ;
  lemon:condition   :lexicalEntry-196_sense_1_condition .
:lexicalEntry-196ed_obj lemon:marker :form_lexicalEntry-196_preposition .

## Prepositions ##
:form_lexicalEntry-196_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

