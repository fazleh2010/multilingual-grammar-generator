@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :plant_of ;
  lemon:entry    :plant_of_form_preposition .

:plant_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :plant_of_form ;
  lemon:otherForm  :plant_of_singular ;
  lemon:otherForm  :plant_of_plural ;
  lemon:sense          :plant_of_sense_1 ;
  lemon:sense          :plant_of_sense_2 ;
  lemon:synBehavior    :plant_of_nounpp .

:plant_of_form a lemon:Form ;
  lemon:writtenRep "plant"@en .

:plant_of_singular a    lemon:Form ;
  lemon:writtenRep "plant"@en ;
  lexinfo:number   lexinfo:singular .

:plant_of_plural a   lemon:Form ;
  lemon:writtenRep "planets"@en ;
  lexinfo:number    lexinfo:plural .

:plant_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:plant_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :plant_of_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :plant_of_sense_1_condition .

:plant_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Planet> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Planet> .

:plant_of_sense_2 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :plant_of_sense_2 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :plant_of_sense_2_condition .

:plant_of_sense_2_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Plant> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Plant> .

:arg2 lemon:marker :plant_of_form_preposition .
## Prepositions ##
:plant_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

