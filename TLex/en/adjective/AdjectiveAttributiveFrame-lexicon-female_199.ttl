@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :female_199_sense_1_res ;
  lemon:entry    :female_199 .

:female_199 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :female_199_lemma ;
  lemon:sense          :female_199_sense_1 ;
  lemon:synBehavior    :female_199_attrFrame, :female_199_predFrame .

:female_199_lemma lemon:writtenRep "female"@en .

:female_199_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :female_199_PredSynArg .

:female_199_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :female_199_AttrSynArg .

:female_199_sense_1 a  lemon:LexicalSense ;
  lemon:reference :female_199_sense_1_res ;
  lemon:isA       :female_199_AttrSynArg, :female_199_PredSynArg .

:female_199_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/gender> ;
  owl:hasValue   <http://dbpedia.org/resource/Female> .
