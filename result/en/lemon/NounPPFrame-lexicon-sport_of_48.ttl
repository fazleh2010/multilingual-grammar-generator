@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :sport_of_48 ;
  lemon:entry    :sport_of_48_form_preposition .

:sport_of_48 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :sport_of_48_form ;
  lemon:otherForm  :sport_of_48_singular ;
  lemon:otherForm  :sport_of_48_plural ;
  lemon:sense          :sport_of_48_sense_1 ;
  lemon:synBehavior    :sport_of_48_nounpp .

:sport_of_48_form a lemon:Form ;
  lemon:writtenRep "Sport"@en .

:sport_of_48_singular a    lemon:Form ;
  lemon:writtenRep "Sport"@en ;
  lexinfo:number   lexinfo:singular .

:sport_of_48_plural a   lemon:Form ;
  lemon:writtenRep "Sports"@en ;
  lexinfo:number    lexinfo:plural .

:sport_of_48_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_sport_of_48 ;
  lexinfo:prepositionalAdjunct :arg2_sport_of_48 .

:sport_of_48_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :sport_of_48_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_sport_of_48 ;
  lemon:objOfProp           :arg1_sport_of_48 ;
  lemon:condition           :sport_of_48_sense_1_condition .

:sport_of_48_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Sport> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Sport> .

:arg2_sport_of_48 lemon:marker :sport_of_48_form_preposition .
## Prepositions ##
:sport_of_48_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

