@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :island_of_257_sense_1_res ;
  lemon:entry    :island_of_257 .

:island_of_257 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :island_of_257_form ;
  lemon:otherForm  :island_of_257_singular ;
  lemon:otherForm  :island_of_257_plural ;
  lemon:sense          :island_of_257_sense_1 ;
  lemon:synBehavior    :island_of_257_predFrame .

:island_of_257_form
 lemon:writtenRep "Island"@en .

:island_of_257_singular a    lemon:Form ;
  lemon:writtenRep "Island"@en ;
  lexinfo:number   lexinfo:singular .

:island_of_257_plural a   lemon:Form ;
  lemon:writtenRep "Islands"@en ;
  lexinfo:number    lexinfo:plural .

:island_of_257_sense_1 a  lemon:LexicalSense ;
  lemon:reference :island_of_257_sense_1_res ;
  lemon:isA       :island_of_257_PredSynArg .

:island_of_257_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Island> .
