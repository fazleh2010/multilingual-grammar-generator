@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_cross_7 ;
  lemon:entry    :cross_7ed ;
  lemon:entry    :form_cross_7_preposition .

:to_cross_7 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_cross_7 ;
  lemon:otherForm      :form_cross_7s ;
  lemon:otherForm      :form_cross_7ed ;
  lemon:otherForm      :form_cross_7perfect ;
  lemon:sense          :cross_7_sense_1 ;
  lemon:synBehavior    :cross_7_frame_transitive .

:form_cross_7 a         lemon:Form ;
  lemon:writtenRep     "cross"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_cross_7s a    lemon:Form ;
  lemon:writtenRep "crosses"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_cross_7ed a   lemon:Form ;
  lemon:writtenRep "crossed"@en ;
  lexinfo:tense    lexinfo:past .

:form_cross_7perfect a   lemon:Form ;
  lemon:writtenRep "crossed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:cross_7_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :cross_7_obj ;
  lexinfo:directObject     :cross_7_subj .

:cross_7_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :cross_7_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/crosses> ;
  lemon:subjOfProp  :cross_7_obj ;
  lemon:objOfProp   :cross_7_subj ;
  lemon:condition   :cross_7_sense_1_condition .


:cross_7_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Bridge> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/River> .


:cross_7_obj lemon:marker :form_cross_7_preposition .

## Prepositions ##
:form_cross_7_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

