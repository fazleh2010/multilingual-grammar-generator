@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-204 ;
  lemon:entry    :lexicalEntry-204ed ;
  lemon:entry    :form_lexicalEntry-204_preposition .

:to_lexicalEntry-204 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-204 ;
  lemon:otherForm      :form_lexicalEntry-204s ;
  lemon:otherForm      :form_lexicalEntry-204ed ;
  lemon:otherForm      :form_lexicalEntry-204perfect ;
  lemon:sense          :lexicalEntry-204_sense_1 ;
  lemon:synBehavior    :lexicalEntry-204_frame_transitive .

:form_lexicalEntry-204 a         lemon:Form ;
  lemon:writtenRep     "held"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-204s a    lemon:Form ;
  lemon:writtenRep "held"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-204ed a   lemon:Form ;
  lemon:writtenRep "held"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-204perfect a   lemon:Form ;
  lemon:writtenRep "held"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-204_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-204_obj ;
  lexinfo:directObject     :lexicalEntry-204_subj .

:lexicalEntry-204_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-204_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/city> ;
  lemon:subjOfProp  :lexicalEntry-204_obj ;
  lemon:objOfProp   :lexicalEntry-204_subj ;
  lemon:condition   :lexicalEntry-204_sense_1_condition .


:lexicalEntry-204_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/City> .

:lexicalEntry-204ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-204ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-204ed_by ;
  lemon:synBehavior    :lexicalEntry-204ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-204ed_ontomap .

:form_lexicalEntry-204ed_canonical a lemon:Form ;
  lemon:writtenRep         "held"@en .

:form_lexicalEntry-204ed_by a    lemon:Form ;
  lemon:writtenRep     "held"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-204ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-204ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-204ed_obj .

:lexicalEntry-204ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-204ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/city> ;
  lemon:subjOfProp  :lexicalEntry-204ed_subj ;
  lemon:objOfProp   :lexicalEntry-204ed_obj ;
  lemon:condition   :lexicalEntry-204_sense_1_condition .
:lexicalEntry-204ed_obj lemon:marker :form_lexicalEntry-204_preposition .

## Prepositions ##
:form_lexicalEntry-204_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

