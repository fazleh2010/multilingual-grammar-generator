@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :bird_of_149_sense_1_res ;
  lemon:entry    :bird_of_149 .

:bird_of_149 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :bird_of_149_form ;
  lemon:otherForm  :bird_of_149_singular ;
  lemon:otherForm  :bird_of_149_plural ;
  lemon:sense          :bird_of_149_sense_1 ;
