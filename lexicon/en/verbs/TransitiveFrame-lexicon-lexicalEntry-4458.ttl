@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-4458 ;
  lemon:entry    :lexicalEntry-4458ed ;
  lemon:entry    :form_lexicalEntry-4458_preposition .

:to_lexicalEntry-4458 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-4458 ;
  lemon:otherForm      :form_lexicalEntry-4458s ;
  lemon:otherForm      :form_lexicalEntry-4458ed ;
  lemon:otherForm      :form_lexicalEntry-4458perfect ;
  lemon:sense          :lexicalEntry-4458_sense_1 ;
  lemon:synBehavior    :lexicalEntry-4458_frame_transitive .

:form_lexicalEntry-4458 a         lemon:Form ;
  lemon:writtenRep     "wrestling"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-4458s a    lemon:Form ;
  lemon:writtenRep "wrestling"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-4458ed a   lemon:Form ;
  lemon:writtenRep "wrestling"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-4458perfect a   lemon:Form ;
  lemon:writtenRep "wrestling"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-4458_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-4458_obj ;
  lexinfo:directObject     :lexicalEntry-4458_subj .

:lexicalEntry-4458_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-4458_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/location> ;
  lemon:subjOfProp  :lexicalEntry-4458_obj ;
  lemon:objOfProp   :lexicalEntry-4458_subj ;
  lemon:condition   :lexicalEntry-4458_sense_1_condition .


:lexicalEntry-4458_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Building> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/City> .

:lexicalEntry-4458ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-4458ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-4458ed_by ;
  lemon:synBehavior    :lexicalEntry-4458ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-4458ed_ontomap .

:form_lexicalEntry-4458ed_canonical a lemon:Form ;
  lemon:writtenRep         "wrestling"@en .

:form_lexicalEntry-4458ed_by a    lemon:Form ;
  lemon:writtenRep     "wrestling"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-4458ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-4458ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-4458ed_obj .

:lexicalEntry-4458ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-4458ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/location> ;
  lemon:subjOfProp  :lexicalEntry-4458ed_subj ;
  lemon:objOfProp   :lexicalEntry-4458ed_obj ;
  lemon:condition   :lexicalEntry-4458_sense_1_condition .
:lexicalEntry-4458ed_obj lemon:marker :form_lexicalEntry-4458_preposition .

## Prepositions ##
:form_lexicalEntry-4458_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

