@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :programminglanguage_of_365 ;
  lemon:entry    :programminglanguage_of_365_form_preposition .

:programminglanguage_of_365 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :programminglanguage_of_365_form ;
  lemon:otherForm  :programminglanguage_of_365_singular ;
  lemon:otherForm  :programminglanguage_of_365_plural ;
  lemon:sense          :programminglanguage_of_365_sense_1 ;
  lemon:synBehavior    :programminglanguage_of_365_nounpp .

:programminglanguage_of_365_form a lemon:Form ;
  lemon:writtenRep "programming language"@en .

:programminglanguage_of_365_singular a    lemon:Form ;
  lemon:writtenRep "programming language"@en ;
  lexinfo:number   lexinfo:singular .

:programminglanguage_of_365_plural a   lemon:Form ;
  lemon:writtenRep "programming languages"@en ;
  lexinfo:number    lexinfo:plural .

:programminglanguage_of_365_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_programminglanguage_of_365 ;
  lexinfo:prepositionalAdjunct :arg2_programminglanguage_of_365 .

:programminglanguage_of_365_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :programminglanguage_of_365_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/programmingLanguage> ;
  lemon:subjOfProp          :arg2_programminglanguage_of_365 ;
  lemon:objOfProp           :arg1_programminglanguage_of_365 ;
  lemon:condition           :programminglanguage_of_365_sense_1_condition .

:programminglanguage_of_365_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Language> .

:arg2_programminglanguage_of_365 lemon:marker :programminglanguage_of_365_form_preposition .
## Prepositions ##
:programminglanguage_of_365_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

