@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :loyalty_301 ;
  lemon:entry    :loyalty_301_form_preposition .

:loyalty_301 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :loyalty_301_form ;
  lemon:otherForm  :loyalty_301_singular ;
  lemon:otherForm  :loyalty_301_plural ;
  lemon:sense          :loyalty_301_sense_1 ;
  lemon:synBehavior    :loyalty_301_nounpp .

:loyalty_301_form a lemon:Form ;
  lemon:writtenRep "loyalty"@en .

:loyalty_301_singular a    lemon:Form ;
  lemon:writtenRep "loyalty"@en ;
  lexinfo:number   lexinfo:singular .

:loyalty_301_plural a   lemon:Form ;
  lemon:writtenRep "loyalties"@en ;
  lexinfo:number    lexinfo:plural .

:loyalty_301_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :null ;
  lexinfo:prepositionalAdjunct :null .

:loyalty_301_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :loyalty_301_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/connotation> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :loyalty_301_sense_1_condition .

:loyalty_301_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Loyalty> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Color> .

:null lemon:marker :loyalty_301_form_preposition .
## Prepositions ##
:loyalty_301_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

