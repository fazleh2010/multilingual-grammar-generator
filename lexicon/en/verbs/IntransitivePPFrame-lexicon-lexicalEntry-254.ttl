@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-254 ;
  lemon:entry    :form_lexicalEntry-254_preposition .

:lexicalEntry-254 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-254 ;
  lemon:otherForm      :form_lexicalEntry-254_past ;
  lemon:sense          :lexicalEntry-254_sense_1 ;
  lemon:synBehavior    :lexicalEntry-254_frame .

:form_lexicalEntry-254 a           lemon:Form ;
  lemon:writtenRep     "appearing"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-254 a      lemon:Form ;
  lemon:writtenRep "appearing"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-254_past a lemon:Form ;
  lemon:writtenRep  "appearing"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-254perfect a   lemon:Form ;
  lemon:writtenRep "appearing"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-254_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-254_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-254_obj .

:lexicalEntry-254_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-254_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/creator> ;
  lemon:subjOfProp  :lexicalEntry-254_obj ;
  lemon:objOfProp   :lexicalEntry-254_subj ;
  lemon:condition   :lexicalEntry-254_sense_1_condition .

:lexicalEntry-254_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Agent> .


:lexicalEntry-254_obj lemon:marker :form_lexicalEntry-254_preposition .

## Prepositions ##
:form_lexicalEntry-254_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

