@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :earthquake_of_116 ;
  lemon:entry    :earthquake_of_116_form_preposition .

:earthquake_of_116 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :earthquake_of_116_form ;
  lemon:otherForm  :earthquake_of_116_singular ;
  lemon:otherForm  :earthquake_of_116_plural ;
  lemon:sense          :earthquake_of_116_sense_1 ;
  lemon:synBehavior    :earthquake_of_116_nounpp .

:earthquake_of_116_form a lemon:Form ;

:earthquake_of_116_singular a    lemon:Form ;
  lemon:writtenRep "Earthquake"@en ;
  lexinfo:number   lexinfo:singular .

:earthquake_of_116_plural a   lemon:Form ;
  lemon:writtenRep "Earthquakes"@en ;
  lexinfo:number    lexinfo:plural .

:earthquake_of_116_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_earthquake_of_116 ;
  lexinfo:prepositionalAdjunct :arg2_earthquake_of_116 .

:earthquake_of_116_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :earthquake_of_116_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_earthquake_of_116 ;
  lemon:objOfProp           :arg1_earthquake_of_116 ;
  lemon:condition           :earthquake_of_116_sense_1_condition .

:earthquake_of_116_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Earthquake> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Earthquake> .

:arg2_earthquake_of_116 lemon:marker :earthquake_of_116_form_preposition .
## Prepositions ##
:earthquake_of_116_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

