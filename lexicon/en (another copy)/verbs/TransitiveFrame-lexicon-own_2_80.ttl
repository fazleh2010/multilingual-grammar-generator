@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_own_2_80 ;
  lemon:entry    :own_2_80ed ;
  lemon:entry    :form_own_2_80_preposition .

:to_own_2_80 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_own_2_80 ;
  lemon:otherForm      :form_own_2_80s ;
  lemon:otherForm      :form_own_2_80ed ;
  lemon:otherForm      :form_own_2_80perfect ;
  lemon:sense          :own_2_80_sense_1 ;
  lemon:synBehavior    :own_2_80_frame_transitive .

:form_own_2_80 a         lemon:Form ;
  lemon:writtenRep     "own"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_own_2_80s a    lemon:Form ;
  lemon:writtenRep "owns"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_own_2_80ed a   lemon:Form ;
  lemon:writtenRep "owned"@en ;
  lexinfo:tense    lexinfo:past .

:form_own_2_80perfect a   lemon:Form ;
  lemon:writtenRep "owned"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:own_2_80_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :own_2_80_subj ;
  lexinfo:directObject     :own_2_80_obj .

:own_2_80_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :own_2_80_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/owner> ;
  lemon:subjOfProp  :own_2_80_obj ;
  lemon:objOfProp   :own_2_80_subj ;
  lemon:condition   :own_2_80_sense_1_condition .


:own_2_80_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Animal> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Agent> .


:own_2_80_obj lemon:marker :form_own_2_80_preposition .

## Prepositions ##
:form_own_2_80_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

