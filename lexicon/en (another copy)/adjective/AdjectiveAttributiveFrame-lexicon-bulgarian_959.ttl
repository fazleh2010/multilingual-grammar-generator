@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :bulgarian_959_sense_1_res ;
  lemon:entry    :bulgarian_959 .

:bulgarian_959 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :bulgarian_959_lemma ;
  lemon:sense          :bulgarian_959_sense_1 ;
  lemon:synBehavior    :bulgarian_959_attrFrame, :bulgarian_959_predFrame .

:bulgarian_959_lemma lemon:writtenRep "bulgarian"@en .

:bulgarian_959_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :bulgarian_959_PredSynArg .

:bulgarian_959_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :bulgarian_959_AttrSynArg .

:bulgarian_959_sense_1 a  lemon:LexicalSense ;
  lemon:reference :bulgarian_959_sense_1_res ;
  lemon:isA       :bulgarian_959_AttrSynArg, :bulgarian_959_PredSynArg .

:bulgarian_959_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Bulgaria> .
