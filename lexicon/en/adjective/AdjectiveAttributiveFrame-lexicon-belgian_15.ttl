@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :belgian_15_sense_1_res ;
  lemon:entry    :belgian_15 .

:belgian_15 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :belgian_15_lemma ;
  lemon:sense          :belgian_15_sense_1 ;
  lemon:synBehavior    :belgian_15_attrFrame, :belgian_15_predFrame .

:belgian_15_lemma lemon:writtenRep "belgian"@en .

:belgian_15_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :belgian_15_PredSynArg .

:belgian_15_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :belgian_15_AttrSynArg .

:belgian_15_sense_1 a  lemon:LexicalSense ;
  lemon:reference :belgian_15_sense_1_res ;
  lemon:isA       :belgian_15_AttrSynArg, :belgian_15_PredSynArg .

:belgian_15_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Belgium> .
