@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :province_of_398 ;
  lemon:entry    :province_of_398_form_preposition .

:province_of_398 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :province_of_398_form ;
  lemon:otherForm  :province_of_398_singular ;
  lemon:otherForm  :province_of_398_plural ;
  lemon:sense          :province_of_398_sense_1 ;
  lemon:synBehavior    :province_of_398_nounpp .

:province_of_398_form a lemon:Form ;
  lemon:writtenRep "province"@en .

:province_of_398_singular a    lemon:Form ;
  lemon:writtenRep "province"@en ;
  lexinfo:number   lexinfo:singular .

:province_of_398_plural a   lemon:Form ;
  lemon:writtenRep "provinces"@en ;
  lexinfo:number    lexinfo:plural .

:province_of_398_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :null ;
  lexinfo:prepositionalAdjunct :null .

:province_of_398_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :province_of_398_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/province> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :province_of_398_sense_1_condition .

:province_of_398_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Province> .

:null lemon:marker :province_of_398_form_preposition .
## Prepositions ##
:province_of_398_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

