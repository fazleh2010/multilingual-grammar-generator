@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-350 ;
  lemon:entry    :lexicalEntry-350_form_preposition .

:lexicalEntry-350 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lexicalEntry-350_form ;
  lemon:otherForm  :lexicalEntry-350_singular ;
  lemon:otherForm  :lexicalEntry-350_plural ;
  lemon:sense          :lexicalEntry-350_sense_1 ;
  lemon:synBehavior    :lexicalEntry-350_nounpp .

:lexicalEntry-350_form a lemon:Form ;
  lemon:writtenRep "arts"@en .

:lexicalEntry-350_singular a    lemon:Form ;
  lemon:writtenRep "arts"@en ;
  lexinfo:number   lexinfo:singular .

:lexicalEntry-350_plural a   lemon:Form ;
  lemon:writtenRep "arts"@en ;
  lexinfo:number    lexinfo:plural .

:lexicalEntry-350_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:lexicalEntry-350_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :lexicalEntry-350_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/date> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :lexicalEntry-350_sense_1_condition .

:lexicalEntry-350_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Event> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :lexicalEntry-350_form_preposition .
## Prepositions ##
:lexicalEntry-350_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

