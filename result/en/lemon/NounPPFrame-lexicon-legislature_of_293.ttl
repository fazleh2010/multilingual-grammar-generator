@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :legislature_of_293 ;
  lemon:entry    :legislature_of_293_form_preposition .

:legislature_of_293 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :legislature_of_293_form ;
  lemon:otherForm  :legislature_of_293_singular ;
  lemon:otherForm  :legislature_of_293_plural ;
  lemon:sense          :legislature_of_293_sense_1 ;
  lemon:synBehavior    :legislature_of_293_nounpp .

:legislature_of_293_form a lemon:Form ;

:legislature_of_293_singular a    lemon:Form ;
  lemon:writtenRep "legislature"@en ;
  lexinfo:number   lexinfo:singular .

:legislature_of_293_plural a   lemon:Form ;
  lemon:writtenRep "legislatures"@en ;
  lexinfo:number    lexinfo:plural .

:legislature_of_293_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_legislature_of_293 ;
  lexinfo:prepositionalAdjunct :arg2_legislature_of_293 .

:legislature_of_293_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :legislature_of_293_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_legislature_of_293 ;
  lemon:objOfProp           :arg1_legislature_of_293 ;
  lemon:condition           :legislature_of_293_sense_1_condition .

:legislature_of_293_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Legislature> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Legislature> .

:arg2_legislature_of_293 lemon:marker :legislature_of_293_form_preposition .
## Prepositions ##
:legislature_of_293_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

