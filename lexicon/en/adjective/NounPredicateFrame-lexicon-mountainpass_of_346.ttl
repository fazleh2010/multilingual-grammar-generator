@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :mountainpass_of_346_sense_1_res ;
  lemon:entry    :mountainpass_of_346 .

:mountainpass_of_346 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :mountainpass_of_346_form ;
  lemon:otherForm  :mountainpass_of_346_singular ;
  lemon:otherForm  :mountainpass_of_346_plural ;
  lemon:sense          :mountainpass_of_346_sense_1 ;
  lemon:synBehavior    :mountainpass_of_346_predFrame .

:mountainpass_of_346_form
 lemon:writtenRep "Mountain Pass"@en .

:mountainpass_of_346_singular a    lemon:Form ;
  lemon:writtenRep "Mountain Pass"@en ;
  lexinfo:number   lexinfo:singular .

:mountainpass_of_346_plural a   lemon:Form ;
  lemon:writtenRep "Mountain Passs"@en ;
  lexinfo:number    lexinfo:plural .

:mountainpass_of_346_sense_1 a  lemon:LexicalSense ;
  lemon:reference :mountainpass_of_346_sense_1_res ;
  lemon:isA       :mountainpass_of_346_PredSynArg .

:mountainpass_of_346_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/MountainPass> .
