@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :cricketer_of_103 ;
  lemon:entry    :cricketer_of_103_form_preposition .

:cricketer_of_103 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :cricketer_of_103_form ;
  lemon:otherForm  :cricketer_of_103_singular ;
  lemon:otherForm  :cricketer_of_103_plural ;
  lemon:sense          :cricketer_of_103_sense_1 ;
  lemon:synBehavior    :cricketer_of_103_nounpp .

:cricketer_of_103_form a lemon:Form ;

:cricketer_of_103_singular a    lemon:Form ;
  lemon:writtenRep "Cricketer"@en ;
  lexinfo:number   lexinfo:singular .

:cricketer_of_103_plural a   lemon:Form ;
  lemon:writtenRep "Cricketers"@en ;
  lexinfo:number    lexinfo:plural .

:cricketer_of_103_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_cricketer_of_103 ;
  lexinfo:prepositionalAdjunct :arg2_cricketer_of_103 .

:cricketer_of_103_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :cricketer_of_103_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_cricketer_of_103 ;
  lemon:objOfProp           :arg1_cricketer_of_103 ;
  lemon:condition           :cricketer_of_103_sense_1_condition .

:cricketer_of_103_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Cricketer> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Cricketer> .

:arg2_cricketer_of_103 lemon:marker :cricketer_of_103_form_preposition .
## Prepositions ##
:cricketer_of_103_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

