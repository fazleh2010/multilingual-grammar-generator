@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :areatotal-of_121 ;
  lemon:entry    :areatotal-of_121_form_preposition .

:areatotal-of_121 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :areatotal-of_121_form ;
  lemon:otherForm  :areatotal-of_121_singular ;
  lemon:otherForm  :areatotal-of_121_plural ;
  lemon:sense          :areatotal-of_121_sense_1 ;
  lemon:synBehavior    :areatotal-of_121_nounpp .

:areatotal-of_121_form a lemon:Form ;
  lemon:writtenRep "alma mater"@en .

:areatotal-of_121_singular a    lemon:Form ;
  lemon:writtenRep "alma mater"@en ;
  lexinfo:number   lexinfo:singular .

:areatotal-of_121_plural a   lemon:Form ;
  lemon:writtenRep "alma maters"@en ;
  lexinfo:number    lexinfo:plural .

:areatotal-of_121_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :null ;
  lexinfo:prepositionalAdjunct :null .

:areatotal-of_121_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :areatotal-of_121_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/areaTotal> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :areatotal-of_121_sense_1_condition .

:areatotal-of_121_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:null lemon:marker :areatotal-of_121_form_preposition .
## Prepositions ##
:areatotal-of_121_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

