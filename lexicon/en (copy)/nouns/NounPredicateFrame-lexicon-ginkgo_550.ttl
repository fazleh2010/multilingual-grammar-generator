@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :ginkgo_550_sense_1_res ;
  lemon:entry    :ginkgo_550 .

:ginkgo_550 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :ginkgo_550_form ;
  lemon:otherForm  :ginkgo_550_singular ;
  lemon:otherForm  :ginkgo_550_plural ;
  lemon:sense          :ginkgo_550_sense_1 ;
  lemon:synBehavior    :ginkgo_550_predFrame .

:ginkgo_550_form
 lemon:writtenRep "ginkgo"@en .

:ginkgo_550_singular a    lemon:Form ;
  lemon:writtenRep "ginkgo"@en ;
  lexinfo:number   lexinfo:singular .

:ginkgo_550_plural a   lemon:Form ;
  lemon:writtenRep "ginkgos"@en ;
  lexinfo:number    lexinfo:plural .

:ginkgo_550_sense_1 a  lemon:LexicalSense ;
  lemon:reference :ginkgo_550_sense_1_res ;
  lemon:isA       :ginkgo_550_PredSynArg .

:ginkgo_550_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Ginkgo> .