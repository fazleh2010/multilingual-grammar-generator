@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_produce_1_201 ;
  lemon:entry    :produce_1_201ed ;
  lemon:entry    :form_produce_1_201_preposition .

:to_produce_1_201 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_produce_1_201 ;
  lemon:otherForm      :form_produce_1_201s ;
  lemon:otherForm      :form_produce_1_201ed ;
  lemon:otherForm      :form_produce_1_201perfect ;
  lemon:sense          :produce_1_201_sense_1 ;
  lemon:synBehavior    :produce_1_201_frame_transitive .

:form_produce_1_201 a         lemon:Form ;
  lemon:writtenRep     "produce"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_produce_1_201s a    lemon:Form ;
  lemon:writtenRep "produces"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_produce_1_201ed a   lemon:Form ;
  lemon:writtenRep "produced"@en ;
  lexinfo:tense    lexinfo:past .

:form_produce_1_201perfect a   lemon:Form ;
  lemon:writtenRep "produced"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:produce_1_201_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :produce_1_201_obj ;
  lexinfo:directObject     :produce_1_201_subj .

:produce_1_201_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :produce_1_201_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/product> ;
  lemon:subjOfProp  :produce_1_201_obj ;
  lemon:objOfProp   :produce_1_201_subj ;
  lemon:condition   :produce_1_201_sense_1_condition .


:produce_1_201_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Ship> .


:produce_1_201_obj lemon:marker :form_produce_1_201_preposition .

## Prepositions ##
:form_produce_1_201_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

