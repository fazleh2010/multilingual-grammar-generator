@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :assassinate_in_234 ;
  lemon:entry    :form_assassinate_in_234_preposition .

:assassinate_in_234 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_assassinate_in_234 ;
  lemon:otherForm      :form_assassinate_in_234_present3rd ;
  lemon:otherForm      :form_assassinate_in_234_past ;
  lemon:otherForm      :form_assassinate_in_234_perfect ;
  lemon:sense          :assassinate_in_234_sense_1 ;
  lemon:synBehavior    :assassinate_in_234_frame .

:form_assassinate_in_234 a           lemon:Form ;
  lemon:writtenRep     "assassinate"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_assassinate_in_234_present3rd a      lemon:Form ;
  lemon:writtenRep "assassinates"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_assassinate_in_234_past a lemon:Form ;
  lemon:writtenRep  "assassinated"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_assassinate_in_234_perfect a   lemon:Form ;
  lemon:writtenRep "assassinated"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:assassinate_in_234_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :assassinate_in_234_subj ;
  lexinfo:prepositionalAdjunct :assassinate_in_234_obj .

:assassinate_in_234_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :assassinate_in_234_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/deathPlace> ;
  lemon:subjOfProp  :assassinate_in_234_subj ;
  lemon:objOfProp   :assassinate_in_234_obj ;
  lemon:condition   :assassinate_in_234_sense_1_condition .

:assassinate_in_234_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Place> .


:assassinate_in_234_obj lemon:marker :form_assassinate_in_234_preposition .

## Prepositions ##
:form_assassinate_in_234_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

