@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :astronaut_of_4 ;
  lemon:entry    :astronaut_of_4_form_preposition .

:astronaut_of_4 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :astronaut_of_4_form ;
  lemon:otherForm  :astronaut_of_4_singular ;
  lemon:otherForm  :astronaut_of_4_plural ;
  lemon:sense          :astronaut_of_4_sense_1 ;
  lemon:synBehavior    :astronaut_of_4_nounpp .

:astronaut_of_4_form a lemon:Form ;
  lemon:writtenRep "Astronaut"@en .

:astronaut_of_4_singular a    lemon:Form ;
  lemon:writtenRep "Astronaut"@en ;
  lexinfo:number   lexinfo:singular .

:astronaut_of_4_plural a   lemon:Form ;
  lemon:writtenRep "Astronauts"@en ;
  lexinfo:number    lexinfo:plural .

:astronaut_of_4_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_astronaut_of_4 ;
  lexinfo:prepositionalAdjunct :arg2_astronaut_of_4 .

:astronaut_of_4_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :astronaut_of_4_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_astronaut_of_4 ;
  lemon:objOfProp           :arg1_astronaut_of_4 ;
  lemon:condition           :astronaut_of_4_sense_1_condition .

:astronaut_of_4_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Astronaut> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Astronaut> .

:arg2_astronaut_of_4 lemon:marker :astronaut_of_4_form_preposition .
## Prepositions ##
:astronaut_of_4_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

