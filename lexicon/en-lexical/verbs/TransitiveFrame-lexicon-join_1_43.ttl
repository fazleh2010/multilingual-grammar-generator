@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_join_1_43 ;
  lemon:entry    :join_1_43ed ;
  lemon:entry    :form_join_1_43_preposition .

:to_join_1_43 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_join_1_43 ;
  lemon:otherForm      :form_join_1_43s ;
  lemon:otherForm      :form_join_1_43ed ;
  lemon:otherForm      :form_join_1_43perfect ;
  lemon:sense          :join_1_43_sense_1 ;
  lemon:synBehavior    :join_1_43_frame_transitive .

:form_join_1_43 a         lemon:Form ;
  lemon:writtenRep     "join"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_join_1_43s a    lemon:Form ;
  lemon:writtenRep "joins"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_join_1_43ed a   lemon:Form ;
  lemon:writtenRep "joined"@en ;
  lexinfo:tense    lexinfo:past .

:form_join_1_43perfect a   lemon:Form ;
  lemon:writtenRep "joined"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:join_1_43_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :join_1_43_subj ;
  lexinfo:directObject     :join_1_43_obj .

:join_1_43_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :join_1_43_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/riverMouth> ;
  lemon:subjOfProp  :join_1_43_obj ;
  lemon:objOfProp   :join_1_43_subj ;
  lemon:condition   :join_1_43_sense_1_condition .


:join_1_43_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/BodyOfWater> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/River> .


:join_1_43_obj lemon:marker :form_join_1_43_preposition .

## Prepositions ##
:form_join_1_43_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

