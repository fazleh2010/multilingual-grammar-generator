@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :instrumentalist_294_sense_1_res ;
  lemon:entry    :instrumentalist_294 .

:instrumentalist_294 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
