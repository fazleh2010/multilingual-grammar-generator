@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :algerian_12_sense_1_res ;
  lemon:entry    :algerian_12 .

:algerian_12 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :algerian_12_lemma ;
  lemon:sense          :algerian_12_sense_1 ;
  lemon:synBehavior    :algerian_12_attrFrame, :algerian_12_predFrame .

:algerian_12_lemma lemon:writtenRep "algerian"@en .

:algerian_12_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :algerian_12_PredSynArg .

:algerian_12_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :algerian_12_AttrSynArg .

:algerian_12_sense_1 a  lemon:LexicalSense ;
  lemon:reference :algerian_12_sense_1_res ;
  lemon:isA       :algerian_12_AttrSynArg, :algerian_12_PredSynArg .

:algerian_12_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Algeria> .
