@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "de" ;
  lemon:entry    :to_anfliegen ;
  lemon:entry    :anfliegened ;
  lemon:entry    :form_anfliegen_preposition .

:to_anfliegen a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_anfliegen ;
  lemon:otherForm      :form_anfliegens ;
  lemon:otherForm      :form_anfliegened ;
  lemon:sense          :anfliegen_sense_1 ;
  lemon:synBehavior    :anfliegen_frame_transitive .

:form_anfliegen a         lemon:Form ;
  lemon:writtenRep     "fliegen an"@de ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_anfliegens a    lemon:Form ;
  lemon:writtenRep "fliegt an"@de ;
  lexinfo:tense    lexinfo:past ;
  lexinfo:person   lexinfo:thirdPerson .


:form_anfliegens a    lemon:Form ;
  lemon:writtenRep "flog an"@de ;
  lexinfo:tense    lexinfo:past ;
  lexinfo:person   lexinfo:thirdPerson .

:form_anfliegened a   lemon:Form ;
  lemon:writtenRep "angeflogen"@de ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:anfliegen_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :anfliegen_subj ;
  lexinfo:directObject     :anfliegen_obj .

:anfliegen_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :anfliegen_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/targetAirport> ;
  lemon:subjOfProp  :anfliegen_obj ;
  lemon:objOfProp   :anfliegen_subj ;
  lemon:condition   :anfliegen_sense_1_condition .


:anfliegen_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Airline> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Airport> .

:anfliegened a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_anfliegened_canonical ;
  lemon:otherForm      :form_anfliegened_by ;
  lemon:synBehavior    :anfliegened_frame_adjectivepp ;
  lemon:sense          :anfliegened_ontomap .

:form_anfliegened_canonical a lemon:Form ;
  lemon:writtenRep         "fliegen an"@de .

:form_anfliegened_by a    lemon:Form ;
  lemon:writtenRep     "fliegen an"@de ;
  lexinfo:verbFormMood lexinfo:participle .


:anfliegened_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :anfliegened_subj ;
  lexinfo:prepositionalAdjunct :anfliegened_obj .

:anfliegened_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :anfliegened_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/targetAirport> ;
  lemon:subjOfProp  :anfliegened_subj ;
  lemon:objOfProp   :anfliegened_obj ;
  lemon:condition   :anfliegen_sense_1_condition .
:anfliegened_obj lemon:marker :von .

:arg2 lemon:marker :form_anfliegen_form_preposition .

## Prepositions ##

:form_anfliegen_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "von"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .