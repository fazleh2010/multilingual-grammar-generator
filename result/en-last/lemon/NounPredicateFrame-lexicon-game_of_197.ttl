@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :game_of_197_sense_1_res ;
  lemon:entry    :game_of_197 .

:game_of_197 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :game_of_197_form ;
  lemon:otherForm  :game_of_197_singular ;
  lemon:otherForm  :game_of_197_plural ;
  lemon:sense          :game_of_197_sense_1 ;
  lemon:synBehavior    :game_of_197_predFrame .

:game_of_197_form
 lemon:writtenRep "Game"@en .

:game_of_197_singular a    lemon:Form ;
  lemon:writtenRep "Game"@en ;
  lexinfo:number   lexinfo:singular .

:game_of_197_plural a   lemon:Form ;
  lemon:writtenRep "Games"@en ;
  lexinfo:number    lexinfo:plural .

:game_of_197_sense_1 a  lemon:LexicalSense ;
  lemon:reference :game_of_197_sense_1_res ;
  lemon:isA       :game_of_197_PredSynArg .

:game_of_197_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Game> .
