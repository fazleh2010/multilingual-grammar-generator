@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :height_of_17 ;
  lemon:entry    :height_of_17_form_preposition .

:height_of_17 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :height_of_17_form ;
  lemon:otherForm  :height_of_17_singular ;
  lemon:otherForm  :height_of_17_plural ;
  lemon:sense          :height_of_17_sense_1 ;
  lemon:synBehavior    :height_of_17_nounpp .

:height_of_17_form a lemon:Form ;
  lemon:writtenRep "height"@en .

:height_of_17_singular a    lemon:Form ;
  lemon:writtenRep "height"@en ;
  lexinfo:number   lexinfo:singular .

:height_of_17_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:height_of_17_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_height_of_17 ;
  lexinfo:prepositionalAdjunct :arg2_height_of_17 .

:height_of_17_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :height_of_17_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/height> ;
  lemon:subjOfProp          :arg2_height_of_17 ;
  lemon:objOfProp           :arg1_height_of_17 ;
  lemon:condition           :height_of_17_sense_1_condition .

:height_of_17_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Model> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2_height_of_17 lemon:marker :height_of_17_form_preposition .
## Prepositions ##
:height_of_17_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

