@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :architect_of_1_87 ;
  lemon:entry    :architect_of_1_87_form_preposition .

:architect_of_1_87 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :architect_of_1_87_form ;
  lemon:otherForm  :architect_of_1_87_singular ;
  lemon:otherForm  :architect_of_1_87_plural ;
  lemon:sense          :architect_of_1_87_sense_1 ;
  lemon:synBehavior    :architect_of_1_87_nounpp .

:architect_of_1_87_form a lemon:Form ;
  lemon:writtenRep "architect"@en .

:architect_of_1_87_singular a    lemon:Form ;
  lemon:writtenRep "architect"@en ;
  lexinfo:number   lexinfo:singular .

:architect_of_1_87_plural a   lemon:Form ;
  lemon:writtenRep "architects"@en ;
  lexinfo:number    lexinfo:plural .

:architect_of_1_87_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_architect_of_1_87 ;
  lexinfo:prepositionalAdjunct :arg2_architect_of_1_87 .

:architect_of_1_87_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :architect_of_1_87_sense_1 ;
  lemon:reference           <wd:P84> ;
  lemon:subjOfProp          :arg2_architect_of_1_87 ;
  lemon:objOfProp           :arg1_architect_of_1_87 ;
  lemon:condition           :architect_of_1_87_sense_1_condition .

:architect_of_1_87_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <wd:Q41176> ;
  lemon:propertyRange   <wd:Q19088> .

:arg2_architect_of_1_87 lemon:marker :architect_of_1_87_form_preposition .
## Prepositions ##
:architect_of_1_87_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

