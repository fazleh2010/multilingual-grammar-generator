@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :voiceactor_of_221_sense_1_res ;
  lemon:entry    :voiceactor_of_221 .

:voiceactor_of_221 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :voiceactor_of_221_form ;
  lemon:otherForm  :voiceactor_of_221_singular ;
  lemon:otherForm  :voiceactor_of_221_plural ;
  lemon:sense          :voiceactor_of_221_sense_1 ;
  lemon:synBehavior    :voiceactor_of_221_predFrame .

:voiceactor_of_221_form
 lemon:writtenRep "VoiceActor"@en .

:voiceactor_of_221_singular a    lemon:Form ;
  lemon:writtenRep "VoiceActor"@en ;
  lexinfo:number   lexinfo:singular .

:voiceactor_of_221_plural a   lemon:Form ;
  lemon:writtenRep "VoiceActors"@en ;
  lexinfo:number    lexinfo:plural .

:voiceactor_of_221_sense_1 a  lemon:LexicalSense ;
  lemon:reference :voiceactor_of_221_sense_1_res ;
  lemon:isA       :voiceactor_of_221_PredSynArg .

:voiceactor_of_221_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/VoiceActor> .
