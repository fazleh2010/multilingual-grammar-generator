@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :populated_place_of_41 ;
  lemon:entry    :populated_place_of_41_form_preposition .

:populated_place_of_41 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :populated_place_of_41_form ;
  lemon:otherForm  :populated_place_of_41_singular ;
  lemon:otherForm  :populated_place_of_41_plural ;
  lemon:sense          :populated_place_of_41_sense_1 ;
  lemon:synBehavior    :populated_place_of_41_nounpp .

:populated_place_of_41_form a lemon:Form ;

:populated_place_of_41_singular a    lemon:Form ;
  lemon:writtenRep "populated place"@en ;
  lexinfo:number   lexinfo:singular .

:populated_place_of_41_plural a   lemon:Form ;
  lemon:writtenRep "populated places"@en ;
  lexinfo:number    lexinfo:plural .

:populated_place_of_41_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_populated_place_of_41 ;
  lexinfo:prepositionalAdjunct :arg2_populated_place_of_41 .

:populated_place_of_41_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :populated_place_of_41_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_populated_place_of_41 ;
  lemon:objOfProp           :arg1_populated_place_of_41 ;
  lemon:condition           :populated_place_of_41_sense_1_condition .

:populated_place_of_41_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/PopulatedPlace> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/PopulatedPlace> .

:arg2_populated_place_of_41 lemon:marker :populated_place_of_41_form_preposition .
## Prepositions ##
:populated_place_of_41_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

