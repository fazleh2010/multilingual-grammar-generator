@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :gold_942_sense_1_res ;
  lemon:entry    :gold_942 .

:gold_942 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :gold_942_lemma ;
  lemon:sense          :gold_942_sense_1 ;
  lemon:synBehavior    :gold_942_attrFrame, :gold_942_predFrame .

:gold_942_lemma lemon:writtenRep "gold"@en .

:gold_942_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :gold_942_PredSynArg .

:gold_942_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :gold_942_AttrSynArg .

:gold_942_sense_1 a  lemon:LexicalSense ;
  lemon:reference :gold_942_sense_1_res ;
  lemon:isA       :gold_942_AttrSynArg, :gold_942_PredSynArg .

:gold_942_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/property/mineral> ;
  owl:hasValue   <http://dbpedia.org/resource/Gold> .
