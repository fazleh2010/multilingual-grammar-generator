@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :rugbyleague_of_345 ;
  lemon:entry    :rugbyleague_of_345_form_preposition .

:rugbyleague_of_345 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :rugbyleague_of_345_form ;
  lemon:otherForm  :rugbyleague_of_345_singular ;
  lemon:otherForm  :rugbyleague_of_345_plural ;
  lemon:sense          :rugbyleague_of_345_sense_1 ;
  lemon:synBehavior    :rugbyleague_of_345_nounpp .

:rugbyleague_of_345_form a lemon:Form ;

:rugbyleague_of_345_singular a    lemon:Form ;
  lemon:writtenRep "Rugby League"@en ;
