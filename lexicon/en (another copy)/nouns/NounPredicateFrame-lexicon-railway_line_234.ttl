@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :railway_line_234_sense_1_res ;
  lemon:entry    :railway_line_234 .

:railway_line_234 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :railway_line_234_form ;
  lemon:otherForm  :railway_line_234_singular ;
  lemon:otherForm  :railway_line_234_plural ;
  lemon:sense          :railway_line_234_sense_1 ;
  lemon:synBehavior    :railway_line_234_predFrame .

:railway_line_234_form
 lemon:writtenRep "railway line"@en .

:railway_line_234_singular a    lemon:Form ;
  lemon:writtenRep "railway line"@en ;
  lexinfo:number   lexinfo:singular .

:railway_line_234_plural a   lemon:Form ;
  lemon:writtenRep "railway lines"@en ;
  lexinfo:number    lexinfo:plural .

:railway_line_234_sense_1 a  lemon:LexicalSense ;
  lemon:reference :railway_line_234_sense_1_res ;
  lemon:isA       :railway_line_234_PredSynArg .

:railway_line_234_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/RailwayLine> .