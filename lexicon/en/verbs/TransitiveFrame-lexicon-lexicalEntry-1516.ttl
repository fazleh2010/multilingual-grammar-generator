@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-1516 ;
  lemon:entry    :lexicalEntry-1516ed ;
  lemon:entry    :form_lexicalEntry-1516_preposition .

:to_lexicalEntry-1516 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-1516 ;
  lemon:otherForm      :form_lexicalEntry-1516s ;
  lemon:otherForm      :form_lexicalEntry-1516ed ;
  lemon:otherForm      :form_lexicalEntry-1516perfect ;
  lemon:sense          :lexicalEntry-1516_sense_1 ;
  lemon:synBehavior    :lexicalEntry-1516_frame_transitive .

:form_lexicalEntry-1516 a         lemon:Form ;
  lemon:writtenRep     "serve"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-1516s a    lemon:Form ;
  lemon:writtenRep "serves"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-1516ed a   lemon:Form ;
  lemon:writtenRep "served"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-1516perfect a   lemon:Form ;
  lemon:writtenRep "served"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-1516_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-1516_subj ;
  lexinfo:directObject     :lexicalEntry-1516_obj .

:lexicalEntry-1516_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1516_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/ingredient> ;
  lemon:subjOfProp  :lexicalEntry-1516_obj ;
  lemon:objOfProp   :lexicalEntry-1516_subj ;
  lemon:condition   :lexicalEntry-1516_sense_1_condition .


:lexicalEntry-1516_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Food> ;
  lemon:propertyRange  <http://www.w3.org/2002/07/owl#Thing> .

:lexicalEntry-1516ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-1516ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-1516ed_by ;
  lemon:synBehavior    :lexicalEntry-1516ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-1516ed_ontomap .

:form_lexicalEntry-1516ed_canonical a lemon:Form ;
  lemon:writtenRep         "serve"@en .

:form_lexicalEntry-1516ed_by a    lemon:Form ;
  lemon:writtenRep     "serve"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-1516ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-1516ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-1516ed_obj .

:lexicalEntry-1516ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1516ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/ingredient> ;
  lemon:subjOfProp  :lexicalEntry-1516ed_subj ;
  lemon:objOfProp   :lexicalEntry-1516ed_obj ;
  lemon:condition   :lexicalEntry-1516_sense_1_condition .
:lexicalEntry-1516ed_obj lemon:marker :form_lexicalEntry-1516_preposition .

## Prepositions ##
:form_lexicalEntry-1516_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

