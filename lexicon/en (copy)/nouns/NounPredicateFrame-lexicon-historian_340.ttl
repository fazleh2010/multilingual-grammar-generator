@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :historian_340_sense_1_res ;
  lemon:entry    :historian_340 .

:historian_340 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :historian_340_form ;
  lemon:otherForm  :historian_340_singular ;
  lemon:otherForm  :historian_340_plural ;
  lemon:sense          :historian_340_sense_1 ;
  lemon:synBehavior    :historian_340_predFrame .

:historian_340_form
 lemon:writtenRep "historian"@en .

:historian_340_singular a    lemon:Form ;
  lemon:writtenRep "historian"@en ;
  lexinfo:number   lexinfo:singular .

:historian_340_plural a   lemon:Form ;
  lemon:writtenRep "historians"@en ;
  lexinfo:number    lexinfo:plural .

:historian_340_sense_1 a  lemon:LexicalSense ;
  lemon:reference :historian_340_sense_1_res ;
  lemon:isA       :historian_340_PredSynArg .

:historian_340_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Historian> .