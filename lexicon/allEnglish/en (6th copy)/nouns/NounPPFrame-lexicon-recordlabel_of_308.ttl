@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :recordlabel_of_308 ;
  lemon:entry    :recordlabel_of_308_form_preposition .

:recordlabel_of_308 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :recordlabel_of_308_form ;
  lemon:otherForm  :recordlabel_of_308_singular ;
  lemon:otherForm  :recordlabel_of_308_plural ;
  lemon:sense          :recordlabel_of_308_sense_1 ;
  lemon:synBehavior    :recordlabel_of_308_nounpp .

:recordlabel_of_308_form a lemon:Form ;
  lemon:writtenRep "record label"@en .

:recordlabel_of_308_singular a    lemon:Form ;
  lemon:writtenRep "record label"@en ;
  lexinfo:number   lexinfo:singular .

:recordlabel_of_308_plural a   lemon:Form ;
  lemon:writtenRep "record labels"@en ;
  lexinfo:number    lexinfo:plural .

:recordlabel_of_308_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_recordlabel_of_308 ;
  lexinfo:prepositionalAdjunct :arg2_recordlabel_of_308 .

:recordlabel_of_308_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :recordlabel_of_308_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/RecordLabel> ;
  lemon:subjOfProp          :arg2_recordlabel_of_308 ;
  lemon:objOfProp           :arg1_recordlabel_of_308 ;
  lemon:condition           :recordlabel_of_308_sense_1_condition .

:recordlabel_of_308_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Genre> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2_recordlabel_of_308 lemon:marker :recordlabel_of_308_form_preposition .
## Prepositions ##
:recordlabel_of_308_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

