@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :fictional_character_of_354 ;
  lemon:entry    :fictional_character_of_354_form_preposition .

:fictional_character_of_354 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :fictional_character_of_354_form ;
  lemon:otherForm  :fictional_character_of_354_singular ;
  lemon:otherForm  :fictional_character_of_354_plural ;
  lemon:sense          :fictional_character_of_354_sense_1 ;
  lemon:synBehavior    :fictional_character_of_354_nounpp .

:fictional_character_of_354_form a lemon:Form ;

:fictional_character_of_354_singular a    lemon:Form ;
  lemon:writtenRep "fictional character"@en ;
  lexinfo:number   lexinfo:singular .

:fictional_character_of_354_plural a   lemon:Form ;
  lemon:writtenRep "fictional characters"@en ;
  lexinfo:number    lexinfo:plural .

:fictional_character_of_354_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_fictional_character_of_354 ;
  lexinfo:prepositionalAdjunct :arg2_fictional_character_of_354 .

:fictional_character_of_354_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :fictional_character_of_354_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_fictional_character_of_354 ;
  lemon:objOfProp           :arg1_fictional_character_of_354 ;
  lemon:condition           :fictional_character_of_354_sense_1_condition .

:fictional_character_of_354_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/FictionalCharacter> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/FictionalCharacter> .

:arg2_fictional_character_of_354 lemon:marker :fictional_character_of_354_form_preposition .
## Prepositions ##
:fictional_character_of_354_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

