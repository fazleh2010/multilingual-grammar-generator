@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :music_of_91 ;
  lemon:entry    :music_of_91_form_preposition .

:music_of_91 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :music_of_91_form ;
  lemon:otherForm  :music_of_91_singular ;
  lemon:otherForm  :music_of_91_plural ;
  lemon:sense          :music_of_91_sense_1 ;
  lemon:synBehavior    :music_of_91_nounpp .

:music_of_91_form a lemon:Form ;
  lemon:writtenRep "music"@en .

:music_of_91_singular a    lemon:Form ;
  lemon:writtenRep "music"@en ;
  lexinfo:number   lexinfo:singular .

:music_of_91_plural a   lemon:Form ;
  lemon:writtenRep "musics"@en ;
  lexinfo:number    lexinfo:plural .

:music_of_91_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_music_of_91 ;
  lexinfo:prepositionalAdjunct :arg2_music_of_91 .

:music_of_91_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :music_of_91_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_music_of_91 ;
  lemon:objOfProp           :arg1_music_of_91 ;
  lemon:condition           :music_of_91_sense_1_condition .

:music_of_91_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/MusicalWork> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/MusicalWork> .

:arg2_music_of_91 lemon:marker :music_of_91_form_preposition .
## Prepositions ##
:music_of_91_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

