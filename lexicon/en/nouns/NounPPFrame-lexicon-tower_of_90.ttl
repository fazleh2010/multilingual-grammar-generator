@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :tower_of_90 ;
  lemon:entry    :tower_of_90_form_preposition .

:tower_of_90 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :tower_of_90_form ;
  lemon:otherForm  :tower_of_90_singular ;
  lemon:otherForm  :tower_of_90_plural ;
  lemon:sense          :tower_of_90_sense_1 ;
  lemon:synBehavior    :tower_of_90_nounpp .

:tower_of_90_form a lemon:Form ;
  lemon:writtenRep "tower"@en .

:tower_of_90_singular a    lemon:Form ;
  lemon:writtenRep "tower"@en ;
  lexinfo:number   lexinfo:singular .

:tower_of_90_plural a   lemon:Form ;
  lemon:writtenRep "towers"@en ;
  lexinfo:number    lexinfo:plural .

:tower_of_90_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_tower_of_90 ;
  lexinfo:prepositionalAdjunct :arg2_tower_of_90 .

:tower_of_90_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :tower_of_90_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_tower_of_90 ;
  lemon:objOfProp           :arg1_tower_of_90 ;
  lemon:condition           :tower_of_90_sense_1_condition .

:tower_of_90_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Tower> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Tower> .

:arg2_tower_of_90 lemon:marker :tower_of_90_form_preposition .
## Prepositions ##
:tower_of_90_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

