@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :area_of_94 ;
  lemon:entry    :area_of_94_form_preposition .

:area_of_94 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :area_of_94_form ;
  lemon:otherForm  :area_of_94_singular ;
  lemon:otherForm  :area_of_94_plural ;
  lemon:sense          :area_of_94_sense_1 ;
  lemon:synBehavior    :area_of_94_nounpp .

:area_of_94_form a lemon:Form ;
  lemon:writtenRep "area"@en .

:area_of_94_singular a    lemon:Form ;
  lemon:writtenRep "area"@en ;
  lexinfo:number   lexinfo:singular .

:area_of_94_plural a   lemon:Form ;
  lemon:writtenRep "areas"@en ;
  lexinfo:number    lexinfo:plural .

:area_of_94_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_area_of_94 ;
  lexinfo:prepositionalAdjunct :arg2_area_of_94 .

:area_of_94_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :area_of_94_sense_1 ;
  lemon:reference           <wd:P2046> ;
  lemon:subjOfProp          :arg2_area_of_94 ;
  lemon:objOfProp           :arg1_area_of_94 ;
  lemon:condition           :area_of_94_sense_1_condition .

:area_of_94_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <wd:Q3957> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2_area_of_94 lemon:marker :area_of_94_form_preposition .
## Prepositions ##
:area_of_94_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

