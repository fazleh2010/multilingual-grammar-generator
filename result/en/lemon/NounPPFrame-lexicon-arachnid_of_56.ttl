@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :arachnid_of_56 ;
  lemon:entry    :arachnid_of_56_form_preposition .

:arachnid_of_56 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :arachnid_of_56_form ;
  lemon:otherForm  :arachnid_of_56_singular ;
  lemon:otherForm  :arachnid_of_56_plural ;
  lemon:sense          :arachnid_of_56_sense_1 ;
  lemon:synBehavior    :arachnid_of_56_nounpp .

:arachnid_of_56_form a lemon:Form ;

:arachnid_of_56_singular a    lemon:Form ;
  lemon:writtenRep "Arachnid"@en ;
  lexinfo:number   lexinfo:singular .

:arachnid_of_56_plural a   lemon:Form ;
  lemon:writtenRep "Arachnids"@en ;
  lexinfo:number    lexinfo:plural .

:arachnid_of_56_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_arachnid_of_56 ;
  lexinfo:prepositionalAdjunct :arg2_arachnid_of_56 .

:arachnid_of_56_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :arachnid_of_56_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_arachnid_of_56 ;
  lemon:objOfProp           :arg1_arachnid_of_56 ;
  lemon:condition           :arachnid_of_56_sense_1_condition .

:arachnid_of_56_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Arachnid> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Arachnid> .

:arg2_arachnid_of_56 lemon:marker :arachnid_of_56_form_preposition .
## Prepositions ##
:arachnid_of_56_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

