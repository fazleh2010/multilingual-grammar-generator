@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :produced_in_1_264 ;
  lemon:entry    :form_produced_in_1_264_preposition .

:produced_in_1_264 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_produced_in_1_264 ;
  lemon:otherForm      :form_produced_in_1_264_present3rd ;
  lemon:otherForm      :form_produced_in_1_264_past ;
  lemon:otherForm      :form_produced_in_1_264_perfect ;
  lemon:sense          :produced_in_1_264_sense_1 ;
  lemon:synBehavior    :produced_in_1_264_frame .

:form_produced_in_1_264 a           lemon:Form ;
  lemon:writtenRep     "produce"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_produced_in_1_264_present3rd a      lemon:Form ;
  lemon:writtenRep "produces"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_produced_in_1_264_past a lemon:Form ;
  lemon:writtenRep  "produced"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_produced_in_1_264_perfect a   lemon:Form ;
  lemon:writtenRep "produced"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:produced_in_1_264_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :produced_in_1_264_subj ;
  lexinfo:prepositionalAdjunct :produced_in_1_264_obj .

:produced_in_1_264_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :produced_in_1_264_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/wineProduced> ;
  lemon:subjOfProp  :produced_in_1_264_subj ;
  lemon:objOfProp   :produced_in_1_264_obj ;
  lemon:condition   :produced_in_1_264_sense_1_condition .

:produced_in_1_264_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Location> .


:produced_in_1_264_obj lemon:marker :form_produced_in_1_264_preposition .

## Prepositions ##
:form_produced_in_1_264_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

