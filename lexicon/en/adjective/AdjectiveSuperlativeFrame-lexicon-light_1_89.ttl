@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix oils:     <http://lemon-model.net/oils#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :light_1_89 ;
  lemon:entry    :form_light_1_89_preposition ;
  lemon:entry    :light_1_89_res .

:light_1_89 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_light_1_89 ;
  lemon:otherForm      :form_light_1_89_comperative ;
  lemon:otherForm      :form_light_1_89_superlative ;
  lemon:sense          :light_1_89_sense_1 ;
  lemon:synBehavior    :light_1_89_predFrame .

:form_light_1_89 lemon:writtenRep "light"@en .

:form_light_1_89_comperative lemon:writtenRep "lighter"@en .

:form_light_1_89_superlative lemon:writtenRep "lightest"@en .

:light_1_89_predFrame a        lexinfo:AdjectiveSuperlativeFrame ;
  lexinfo:copulativeSubject :light_1_89_PredSynArg .

:light_1_89_sense_1 a  lemon:LexicalSense ;
  lemon:reference :light_1_89_res ;
  lemon:isA       :light_1_89_PredSynArg ;
  lemon:condition :light_1_89_sense_1_condition .

:light_1_89_res a   oils:CovariantScalar ;
  oils:boundTo  <http://dbpedia.org/ontology/mass> ;
  oils:degree   <http://lemon-model.net/oils/high> .

:light_1_89_sense_1_condition a lemon:condition ;
  lemon:propertyDomain   <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange    <http://www.w3.org/2001/XMLSchema#double> .

## Prepositions ##
:form_light_1_89_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

