@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :island_695_sense_1_res ;
  lemon:entry    :island_695 .

:island_695 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :island_695_form ;
  lemon:otherForm  :island_695_singular ;
  lemon:otherForm  :island_695_plural ;
  lemon:sense          :island_695_sense_1 ;
  lemon:synBehavior    :island_695_predFrame .

:island_695_form
 lemon:writtenRep "island"@en .

:island_695_singular a    lemon:Form ;
  lemon:writtenRep "island"@en ;
  lexinfo:number   lexinfo:singular .

:island_695_plural a   lemon:Form ;
  lemon:writtenRep "islands"@en ;
  lexinfo:number    lexinfo:plural .

:island_695_sense_1 a  lemon:LexicalSense ;
  lemon:reference :island_695_sense_1_res ;
  lemon:isA       :island_695_PredSynArg .

:island_695_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Island> .