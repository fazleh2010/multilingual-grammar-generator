@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-750 ;
  lemon:entry    :form_lexicalEntry-750_preposition .

:lexicalEntry-750 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-750 ;
  lemon:otherForm      :form_lexicalEntry-750_past ;
  lemon:sense          :lexicalEntry-750_sense_1 ;
  lemon:synBehavior    :lexicalEntry-750_frame .

:form_lexicalEntry-750 a           lemon:Form ;
  lemon:writtenRep     "established in"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-750 a      lemon:Form ;
  lemon:writtenRep "established in"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-750_past a lemon:Form ;
  lemon:writtenRep  "established in"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-750perfect a   lemon:Form ;
  lemon:writtenRep "established in"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-750_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-750_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-750_obj .

:lexicalEntry-750_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-750_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/editor> ;
  lemon:subjOfProp  :lexicalEntry-750_obj ;
  lemon:objOfProp   :lexicalEntry-750_subj ;
  lemon:condition   :lexicalEntry-750_sense_1_condition .

:lexicalEntry-750_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:lexicalEntry-750_obj lemon:marker :form_lexicalEntry-750_preposition .

## Prepositions ##
:form_lexicalEntry-750_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

