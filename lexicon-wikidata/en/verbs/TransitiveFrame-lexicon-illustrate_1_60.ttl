@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_illustrate_1_60 ;
  lemon:entry    :illustrate_1_60ed ;
  lemon:entry    :form_illustrate_1_60_preposition .

:to_illustrate_1_60 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_illustrate_1_60 ;
  lemon:otherForm      :form_illustrate_1_60s ;
  lemon:otherForm      :form_illustrate_1_60ed ;
  lemon:otherForm      :form_illustrate_1_60perfect ;
  lemon:sense          :illustrate_1_60_sense_1 ;
  lemon:synBehavior    :illustrate_1_60_frame_transitive .

:form_illustrate_1_60 a         lemon:Form ;
  lemon:writtenRep     "illustrate"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_illustrate_1_60s a    lemon:Form ;
  lemon:writtenRep "illustrates"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_illustrate_1_60ed a   lemon:Form ;
  lemon:writtenRep "illustrated"@en ;
  lexinfo:tense    lexinfo:past .

:form_illustrate_1_60perfect a   lemon:Form ;
  lemon:writtenRep "illustrated"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:illustrate_1_60_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :illustrate_1_60_subj ;
  lexinfo:directObject     :illustrate_1_60_obj .

:illustrate_1_60_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :illustrate_1_60_sense_1 ;
  lemon:reference   <wd:P110> ;
  lemon:subjOfProp  :illustrate_1_60_obj ;
  lemon:objOfProp   :illustrate_1_60_subj ;
  lemon:condition   :illustrate_1_60_sense_1_condition .


:illustrate_1_60_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <wd:Q386724> ;
  lemon:propertyRange  <wd:Q19088> .


:illustrate_1_60_obj lemon:marker :form_illustrate_1_60_preposition .

## Prepositions ##
:form_illustrate_1_60_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

