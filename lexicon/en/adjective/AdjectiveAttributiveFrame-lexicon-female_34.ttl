@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :female_34_sense_1_res ;
  lemon:entry    :female_34 .

:female_34 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :female_34_lemma ;
  lemon:sense          :female_34_sense_1 ;
  lemon:synBehavior    :female_34_attrFrame, :female_34_predFrame .

:female_34_lemma lemon:writtenRep "female"@en .

:female_34_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :female_34_PredSynArg .

:female_34_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :female_34_AttrSynArg .

:female_34_sense_1 a  lemon:LexicalSense ;
  lemon:reference :female_34_sense_1_res ;
  lemon:isA       :female_34_AttrSynArg, :female_34_PredSynArg .

:female_34_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/gender> ;
  owl:hasValue   <http://dbpedia.org/resource/Female> .
