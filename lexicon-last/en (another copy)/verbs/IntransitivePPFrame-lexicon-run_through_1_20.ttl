@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :run_through_1_20 ;
  lemon:entry    :form_run_through_1_20_preposition .

:run_through_1_20 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_run_through_1_20 ;
  lemon:otherForm      :form_run_through_1_20_present3rd ;
  lemon:otherForm      :form_run_through_1_20_past ;
  lemon:otherForm      :form_run_through_1_20_perfect ;
  lemon:sense          :run_through_1_20_sense_1 ;
  lemon:synBehavior    :run_through_1_20_frame .

:form_run_through_1_20 a           lemon:Form ;
  lemon:writtenRep     "runs"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_run_through_1_20_present3rd a      lemon:Form ;
  lemon:writtenRep "ran"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_run_through_1_20_past a lemon:Form ;
  lemon:writtenRep  "run"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_run_through_1_20_perfect a   lemon:Form ;
  lemon:writtenRep "run"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:run_through_1_20_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :run_through_1_20_subj ;
  lexinfo:prepositionalAdjunct :run_through_1_20_obj .

:run_through_1_20_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :run_through_1_20_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/country> ;
  lemon:subjOfProp  :run_through_1_20_subj ;
  lemon:objOfProp   :run_through_1_20_obj ;
  lemon:condition   :run_through_1_20_sense_1_condition .

:run_through_1_20_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Place> .


:run_through_1_20_obj lemon:marker :form_run_through_1_20_preposition .

## Prepositions ##
:form_run_through_1_20_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "through"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

