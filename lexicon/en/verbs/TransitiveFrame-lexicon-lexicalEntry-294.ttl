@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-294 ;
  lemon:entry    :lexicalEntry-294ed ;
  lemon:entry    :form_lexicalEntry-294_preposition .

:to_lexicalEntry-294 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-294 ;
  lemon:otherForm      :form_lexicalEntry-294s ;
  lemon:otherForm      :form_lexicalEntry-294ed ;
  lemon:otherForm      :form_lexicalEntry-294perfect ;
  lemon:sense          :lexicalEntry-294_sense_1 ;
  lemon:synBehavior    :lexicalEntry-294_frame_transitive .

:form_lexicalEntry-294 a         lemon:Form ;
  lemon:writtenRep     "carries"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-294s a    lemon:Form ;
  lemon:writtenRep "carries"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-294ed a   lemon:Form ;
  lemon:writtenRep "carries"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-294perfect a   lemon:Form ;
  lemon:writtenRep "carries"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-294_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-294_obj ;
  lexinfo:directObject     :lexicalEntry-294_subj .

:lexicalEntry-294_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-294_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/crosses> ;
  lemon:subjOfProp  :lexicalEntry-294_obj ;
  lemon:objOfProp   :lexicalEntry-294_subj ;
  lemon:condition   :lexicalEntry-294_sense_1_condition .


:lexicalEntry-294_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Bridge> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/River> .

:lexicalEntry-294ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-294ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-294ed_by ;
  lemon:synBehavior    :lexicalEntry-294ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-294ed_ontomap .

:form_lexicalEntry-294ed_canonical a lemon:Form ;
  lemon:writtenRep         "carries"@en .

:form_lexicalEntry-294ed_by a    lemon:Form ;
  lemon:writtenRep     "carries"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-294ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-294ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-294ed_obj .

:lexicalEntry-294ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-294ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/crosses> ;
  lemon:subjOfProp  :lexicalEntry-294ed_subj ;
  lemon:objOfProp   :lexicalEntry-294ed_obj ;
  lemon:condition   :lexicalEntry-294_sense_1_condition .
:lexicalEntry-294ed_obj lemon:marker :form_lexicalEntry-294_preposition .

## Prepositions ##
:form_lexicalEntry-294_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

