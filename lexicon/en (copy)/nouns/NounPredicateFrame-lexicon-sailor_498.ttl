@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :sailor_498_sense_1_res ;
  lemon:entry    :sailor_498 .

:sailor_498 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :sailor_498_form ;
  lemon:otherForm  :sailor_498_singular ;
  lemon:otherForm  :sailor_498_plural ;
  lemon:sense          :sailor_498_sense_1 ;
  lemon:synBehavior    :sailor_498_predFrame .

:sailor_498_form
 lemon:writtenRep "sailor"@en .

:sailor_498_singular a    lemon:Form ;
  lemon:writtenRep "sailor"@en ;
  lexinfo:number   lexinfo:singular .

:sailor_498_plural a   lemon:Form ;
  lemon:writtenRep "sailors"@en ;
  lexinfo:number    lexinfo:plural .

:sailor_498_sense_1 a  lemon:LexicalSense ;
  lemon:reference :sailor_498_sense_1_res ;
  lemon:isA       :sailor_498_PredSynArg .

:sailor_498_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Sailor> .