@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :player_of_401_sense_1_res ;
  lemon:entry    :player_of_401 .

:player_of_401 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :player_of_401_form ;
  lemon:otherForm  :player_of_401_singular ;
  lemon:otherForm  :player_of_401_plural ;
  lemon:sense          :player_of_401_sense_1 ;
  lemon:synBehavior    :player_of_401_predFrame .

:player_of_401_form
 lemon:writtenRep "player"@en .

:player_of_401_singular a    lemon:Form ;
  lemon:writtenRep "player"@en ;
  lexinfo:number   lexinfo:singular .

:player_of_401_plural a   lemon:Form ;
  lemon:writtenRep "players"@en ;
  lexinfo:number    lexinfo:plural .

:player_of_401_sense_1 a  lemon:LexicalSense ;
  lemon:reference :player_of_401_sense_1_res ;
  lemon:isA       :player_of_401_PredSynArg .

:player_of_401_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/VolleyballPlayer> .
