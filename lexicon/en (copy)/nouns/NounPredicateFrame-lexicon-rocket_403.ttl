@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :rocket_403_sense_1_res ;
  lemon:entry    :rocket_403 .

:rocket_403 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :rocket_403_form ;
  lemon:otherForm  :rocket_403_singular ;
  lemon:otherForm  :rocket_403_plural ;
  lemon:sense          :rocket_403_sense_1 ;
  lemon:synBehavior    :rocket_403_predFrame .

:rocket_403_form
 lemon:writtenRep "rocket"@en .

:rocket_403_singular a    lemon:Form ;
  lemon:writtenRep "rocket"@en ;
  lexinfo:number   lexinfo:singular .

:rocket_403_plural a   lemon:Form ;
  lemon:writtenRep "rockets"@en ;
  lexinfo:number    lexinfo:plural .

:rocket_403_sense_1 a  lemon:LexicalSense ;
  lemon:reference :rocket_403_sense_1_res ;
  lemon:isA       :rocket_403_PredSynArg .

:rocket_403_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Rocket> .