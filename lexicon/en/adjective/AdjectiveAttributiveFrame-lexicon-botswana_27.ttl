@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :botswana_27_sense_1_res ;
  lemon:entry    :botswana_27 .

:botswana_27 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :botswana_27_lemma ;
  lemon:sense          :botswana_27_sense_1 ;
  lemon:synBehavior    :botswana_27_attrFrame, :botswana_27_predFrame .

:botswana_27_lemma lemon:writtenRep "botswana"@en .

:botswana_27_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :botswana_27_PredSynArg .

:botswana_27_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :botswana_27_AttrSynArg .

:botswana_27_sense_1 a  lemon:LexicalSense ;
  lemon:reference :botswana_27_sense_1_res ;
  lemon:isA       :botswana_27_AttrSynArg, :botswana_27_PredSynArg .

:botswana_27_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Botswana> .
