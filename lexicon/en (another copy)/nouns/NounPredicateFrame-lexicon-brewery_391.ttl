@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :brewery_391_sense_1_res ;
  lemon:entry    :brewery_391 .

:brewery_391 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :brewery_391_form ;
  lemon:otherForm  :brewery_391_singular ;
  lemon:otherForm  :brewery_391_plural ;
  lemon:sense          :brewery_391_sense_1 ;
  lemon:synBehavior    :brewery_391_predFrame .

:brewery_391_form
 lemon:writtenRep "brewery"@en .

:brewery_391_singular a    lemon:Form ;
  lemon:writtenRep "brewery"@en ;
  lexinfo:number   lexinfo:singular .

:brewery_391_plural a   lemon:Form ;
  lemon:writtenRep "breweries"@en ;
  lexinfo:number    lexinfo:plural .

:brewery_391_sense_1 a  lemon:LexicalSense ;
  lemon:reference :brewery_391_sense_1_res ;
  lemon:isA       :brewery_391_PredSynArg .

:brewery_391_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Brewery> .