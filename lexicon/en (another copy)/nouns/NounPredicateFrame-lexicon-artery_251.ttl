@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :artery_251_sense_1_res ;
  lemon:entry    :artery_251 .

:artery_251 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :artery_251_form ;
  lemon:otherForm  :artery_251_singular ;
  lemon:otherForm  :artery_251_plural ;
  lemon:sense          :artery_251_sense_1 ;
  lemon:synBehavior    :artery_251_predFrame .

:artery_251_form
 lemon:writtenRep "artery"@en .

:artery_251_singular a    lemon:Form ;
  lemon:writtenRep "artery"@en ;
  lexinfo:number   lexinfo:singular .

:artery_251_plural a   lemon:Form ;
  lemon:writtenRep "arteries"@en ;
  lexinfo:number    lexinfo:plural .

:artery_251_sense_1 a  lemon:LexicalSense ;
  lemon:reference :artery_251_sense_1_res ;
  lemon:isA       :artery_251_PredSynArg .

:artery_251_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Artery> .