@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :magazine_281_sense_1_res ;
  lemon:entry    :magazine_281 .

:magazine_281 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :magazine_281_form ;
  lemon:otherForm  :magazine_281_singular ;
  lemon:otherForm  :magazine_281_plural ;
  lemon:sense          :magazine_281_sense_1 ;
  lemon:synBehavior    :magazine_281_predFrame .

:magazine_281_form
 lemon:writtenRep "magazine"@en .

:magazine_281_singular a    lemon:Form ;
  lemon:writtenRep "magazine"@en ;
  lexinfo:number   lexinfo:singular .

:magazine_281_plural a   lemon:Form ;
  lemon:writtenRep "magazines"@en ;
  lexinfo:number    lexinfo:plural .

:magazine_281_sense_1 a  lemon:LexicalSense ;
  lemon:reference :magazine_281_sense_1_res ;
  lemon:isA       :magazine_281_PredSynArg .

:magazine_281_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Magazine> .