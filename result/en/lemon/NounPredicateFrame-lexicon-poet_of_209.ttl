@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :poet_of_209_sense_1_res ;
  lemon:entry    :poet_of_209 .

:poet_of_209 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :poet_of_209_form ;
  lemon:otherForm  :poet_of_209_singular ;
  lemon:otherForm  :poet_of_209_plural ;
  lemon:sense          :poet_of_209_sense_1 ;
