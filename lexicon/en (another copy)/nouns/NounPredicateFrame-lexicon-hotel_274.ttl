@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :hotel_274_sense_1_res ;
  lemon:entry    :hotel_274 .

:hotel_274 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :hotel_274_form ;
  lemon:otherForm  :hotel_274_singular ;
  lemon:otherForm  :hotel_274_plural ;
  lemon:sense          :hotel_274_sense_1 ;
  lemon:synBehavior    :hotel_274_predFrame .

:hotel_274_form
 lemon:writtenRep "hotel"@en .

:hotel_274_singular a    lemon:Form ;
  lemon:writtenRep "hotel"@en ;
  lexinfo:number   lexinfo:singular .

:hotel_274_plural a   lemon:Form ;
  lemon:writtenRep "hotels"@en ;
  lexinfo:number    lexinfo:plural .

:hotel_274_sense_1 a  lemon:LexicalSense ;
  lemon:reference :hotel_274_sense_1_res ;
  lemon:isA       :hotel_274_PredSynArg .

:hotel_274_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Hotel> .