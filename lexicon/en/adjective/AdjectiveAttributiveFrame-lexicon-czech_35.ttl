@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :czech_35_sense_1_res ;
  lemon:entry    :czech_35 .

:czech_35 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :czech_35_lemma ;
  lemon:sense          :czech_35_sense_1 ;
  lemon:synBehavior    :czech_35_attrFrame, :czech_35_predFrame .

:czech_35_lemma lemon:writtenRep "czech"@en .

:czech_35_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :czech_35_PredSynArg .

:czech_35_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :czech_35_AttrSynArg .

:czech_35_sense_1 a  lemon:LexicalSense ;
  lemon:reference :czech_35_sense_1_res ;
  lemon:isA       :czech_35_AttrSynArg, :czech_35_PredSynArg .

:czech_35_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Czech_Republic> .
