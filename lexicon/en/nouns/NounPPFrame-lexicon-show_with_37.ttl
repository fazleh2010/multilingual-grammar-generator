@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :show_with_37 ;
  lemon:entry    :show_with_37_form_preposition .

:show_with_37 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :show_with_37_form ;
  lemon:otherForm  :show_with_37_singular ;
  lemon:otherForm  :show_with_37_plural ;
  lemon:sense          :show_with_37_sense_1 ;
  lemon:synBehavior    :show_with_37_nounpp .

:show_with_37_form a lemon:Form ;
  lemon:writtenRep "show"@en .

:show_with_37_singular a    lemon:Form ;
  lemon:writtenRep "show"@en ;
  lexinfo:number   lexinfo:singular .

:show_with_37_plural a   lemon:Form ;
  lemon:writtenRep "shows"@en ;
  lexinfo:number    lexinfo:plural .

:show_with_37_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_show_with_37 ;
  lexinfo:prepositionalAdjunct :arg2_show_with_37 .

:show_with_37_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :show_with_37_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/starring> ;
  lemon:subjOfProp          :arg2_show_with_37 ;
  lemon:objOfProp           :arg1_show_with_37 ;
  lemon:condition           :show_with_37_sense_1_condition .

:show_with_37_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Actor> .

:arg2_show_with_37 lemon:marker :show_with_37_form_preposition .
## Prepositions ##
:show_with_37_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

