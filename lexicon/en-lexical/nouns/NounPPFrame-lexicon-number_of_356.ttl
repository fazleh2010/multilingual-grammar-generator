@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :number_of_356 ;
  lemon:entry    :number_of_356_form_preposition .

:number_of_356 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :number_of_356_form ;
  lemon:otherForm  :number_of_356_singular ;
  lemon:otherForm  :number_of_356_plural ;
  lemon:sense          :number_of_356_sense_1 ;
  lemon:synBehavior    :number_of_356_nounpp .

:number_of_356_form a lemon:Form ;
  lemon:writtenRep "number of locations"@en .

:number_of_356_singular a    lemon:Form ;
  lemon:writtenRep "number of locations"@en ;
  lexinfo:number   lexinfo:singular .

:number_of_356_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:number_of_356_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :null ;
  lexinfo:prepositionalAdjunct :null .

:number_of_356_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :number_of_356_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/numberOfLocations> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :number_of_356_sense_1_condition .

:number_of_356_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#nonNegativeInteger> .

:null lemon:marker :number_of_356_form_preposition .
## Prepositions ##
:number_of_356_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

