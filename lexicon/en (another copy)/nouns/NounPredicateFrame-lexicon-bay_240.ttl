@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :bay_240_sense_1_res ;
  lemon:entry    :bay_240 .

:bay_240 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :bay_240_form ;
  lemon:otherForm  :bay_240_singular ;
  lemon:otherForm  :bay_240_plural ;
  lemon:sense          :bay_240_sense_1 ;
  lemon:synBehavior    :bay_240_predFrame .

:bay_240_form
 lemon:writtenRep "bay"@en .

:bay_240_singular a    lemon:Form ;
  lemon:writtenRep "bay"@en ;
  lexinfo:number   lexinfo:singular .

:bay_240_plural a   lemon:Form ;
  lemon:writtenRep "bays"@en ;
  lexinfo:number    lexinfo:plural .

:bay_240_sense_1 a  lemon:LexicalSense ;
  lemon:reference :bay_240_sense_1_res ;
  lemon:isA       :bay_240_PredSynArg .

:bay_240_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Bay> .