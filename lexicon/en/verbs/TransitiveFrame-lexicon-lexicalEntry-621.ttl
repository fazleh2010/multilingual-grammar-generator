@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-621 ;
  lemon:entry    :lexicalEntry-621ed ;
  lemon:entry    :form_lexicalEntry-621_preposition .

:to_lexicalEntry-621 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-621 ;
  lemon:otherForm      :form_lexicalEntry-621s ;
  lemon:otherForm      :form_lexicalEntry-621ed ;
  lemon:otherForm      :form_lexicalEntry-621perfect ;
  lemon:sense          :lexicalEntry-621_sense_1 ;
  lemon:synBehavior    :lexicalEntry-621_frame_transitive .

:form_lexicalEntry-621 a         lemon:Form ;
  lemon:writtenRep     "directed by"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-621s a    lemon:Form ;
  lemon:writtenRep "directed by"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-621ed a   lemon:Form ;
  lemon:writtenRep "directed by"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-621perfect a   lemon:Form ;
  lemon:writtenRep "directed by"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-621_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-621_obj ;
  lexinfo:directObject     :lexicalEntry-621_subj .

:lexicalEntry-621_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-621_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/director> ;
  lemon:subjOfProp  :lexicalEntry-621_obj ;
  lemon:objOfProp   :lexicalEntry-621_subj ;
  lemon:condition   :lexicalEntry-621_sense_1_condition .


:lexicalEntry-621_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Film> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .

:lexicalEntry-621ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-621ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-621ed_by ;
  lemon:synBehavior    :lexicalEntry-621ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-621ed_ontomap .

:form_lexicalEntry-621ed_canonical a lemon:Form ;
  lemon:writtenRep         "directed by"@en .

:form_lexicalEntry-621ed_by a    lemon:Form ;
  lemon:writtenRep     "directed by"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-621ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-621ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-621ed_obj .

:lexicalEntry-621ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-621ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/director> ;
  lemon:subjOfProp  :lexicalEntry-621ed_subj ;
  lemon:objOfProp   :lexicalEntry-621ed_obj ;
  lemon:condition   :lexicalEntry-621_sense_1_condition .
:lexicalEntry-621ed_obj lemon:marker :form_lexicalEntry-621_preposition .

## Prepositions ##
:form_lexicalEntry-621_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

