  lemon:subjOfProp          :arg2_study_in_2_80 ;
  lemon:objOfProp           :arg1_study_in_2_80 ;
  lemon:condition           :study_in_2_80_sense_1_condition .

:study_in_2_80_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Agent> .

:arg2_study_in_2_80 lemon:marker :study_in_2_80_form_preposition .
## Prepositions ##
:study_in_2_80_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
