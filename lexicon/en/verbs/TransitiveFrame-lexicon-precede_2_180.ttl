@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_precede_2_180 ;
  lemon:entry    :precede_2_180ed ;
  lemon:entry    :form_precede_2_180_preposition .

:to_precede_2_180 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_precede_2_180 ;
  lemon:otherForm      :form_precede_2_180s ;
  lemon:otherForm      :form_precede_2_180ed ;
  lemon:otherForm      :form_precede_2_180perfect ;
  lemon:sense          :precede_2_180_sense_1 ;
  lemon:synBehavior    :precede_2_180_frame_transitive .

:form_precede_2_180 a         lemon:Form ;
  lemon:writtenRep     "precede"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_precede_2_180s a    lemon:Form ;
  lemon:writtenRep "precedes"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_precede_2_180ed a   lemon:Form ;
  lemon:writtenRep "preceded"@en ;
  lexinfo:tense    lexinfo:past .

:form_precede_2_180perfect a   lemon:Form ;
  lemon:writtenRep "preceded"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:precede_2_180_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :precede_2_180_subj ;
  lexinfo:directObject     :precede_2_180_obj .

:precede_2_180_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :precede_2_180_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/previousWork> ;
  lemon:subjOfProp  :precede_2_180_obj ;
  lemon:objOfProp   :precede_2_180_subj ;
  lemon:condition   :precede_2_180_sense_1_condition .


:precede_2_180_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Work> .


:precede_2_180_obj lemon:marker :form_precede_2_180_preposition .

## Prepositions ##
:form_precede_2_180_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

