@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-2226 ;
  lemon:entry    :lexicalEntry-2226ed ;
  lemon:entry    :form_lexicalEntry-2226_preposition .

:to_lexicalEntry-2226 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-2226 ;
  lemon:otherForm      :form_lexicalEntry-2226s ;
  lemon:otherForm      :form_lexicalEntry-2226ed ;
  lemon:otherForm      :form_lexicalEntry-2226perfect ;
  lemon:sense          :lexicalEntry-2226_sense_1 ;
  lemon:synBehavior    :lexicalEntry-2226_frame_transitive .

:form_lexicalEntry-2226 a         lemon:Form ;
  lemon:writtenRep     "aired"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-2226s a    lemon:Form ;
  lemon:writtenRep "aired"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-2226ed a   lemon:Form ;
  lemon:writtenRep "aired"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-2226perfect a   lemon:Form ;
  lemon:writtenRep "aired"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-2226_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-2226_obj ;
  lexinfo:directObject     :lexicalEntry-2226_subj .

:lexicalEntry-2226_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2226_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/numberOfSeasons> ;
  lemon:subjOfProp  :lexicalEntry-2226_obj ;
  lemon:objOfProp   :lexicalEntry-2226_subj ;
  lemon:condition   :lexicalEntry-2226_sense_1_condition .


:lexicalEntry-2226_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#nonNegativeInteger> .

:lexicalEntry-2226ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-2226ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-2226ed_by ;
  lemon:synBehavior    :lexicalEntry-2226ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-2226ed_ontomap .

:form_lexicalEntry-2226ed_canonical a lemon:Form ;
  lemon:writtenRep         "aired"@en .

:form_lexicalEntry-2226ed_by a    lemon:Form ;
  lemon:writtenRep     "aired"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-2226ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-2226ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-2226ed_obj .

:lexicalEntry-2226ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2226ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/numberOfSeasons> ;
  lemon:subjOfProp  :lexicalEntry-2226ed_subj ;
  lemon:objOfProp   :lexicalEntry-2226ed_obj ;
  lemon:condition   :lexicalEntry-2226_sense_1_condition .
:lexicalEntry-2226ed_obj lemon:marker :form_lexicalEntry-2226_preposition .

## Prepositions ##
:form_lexicalEntry-2226_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

