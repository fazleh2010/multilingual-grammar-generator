@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-3135 ;
  lemon:entry    :form_lexicalEntry-3135_preposition .

:lexicalEntry-3135 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-3135 ;
  lemon:otherForm      :form_lexicalEntry-3135_past ;
  lemon:sense          :lexicalEntry-3135_sense_1 ;
  lemon:synBehavior    :lexicalEntry-3135_frame .

:form_lexicalEntry-3135 a           lemon:Form ;
  lemon:writtenRep     "appeared"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-3135 a      lemon:Form ;
  lemon:writtenRep "appeared"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-3135_past a lemon:Form ;
  lemon:writtenRep  "appeared"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-3135perfect a   lemon:Form ;
  lemon:writtenRep "appeared"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-3135_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-3135_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-3135_obj .

:lexicalEntry-3135_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3135_ontomap ;
  lemon:reference   <http://dbpedia.org/property/alterEgo> ;
  lemon:subjOfProp  :lexicalEntry-3135_obj ;
  lemon:objOfProp   :lexicalEntry-3135_subj ;
  lemon:condition   :lexicalEntry-3135_sense_1_condition .

:lexicalEntry-3135_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/ComicsCharacter> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person/xsd> .


:lexicalEntry-3135_obj lemon:marker :form_lexicalEntry-3135_preposition .

## Prepositions ##
:form_lexicalEntry-3135_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

