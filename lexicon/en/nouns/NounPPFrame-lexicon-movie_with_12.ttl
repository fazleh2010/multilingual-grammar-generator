@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :movie_with_12 ;
  lemon:entry    :movie_with_12_form_preposition .

:movie_with_12 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :movie_with_12_form ;
  lemon:otherForm  :movie_with_12_singular ;
  lemon:otherForm  :movie_with_12_plural ;
  lemon:sense          :movie_with_12_sense_1 ;
  lemon:synBehavior    :movie_with_12_nounpp .

:movie_with_12_form a lemon:Form ;
  lemon:writtenRep "movie"@en .

:movie_with_12_singular a    lemon:Form ;
  lemon:writtenRep "movie"@en ;
  lexinfo:number   lexinfo:singular .

:movie_with_12_plural a   lemon:Form ;
  lemon:writtenRep "movies"@en ;
  lexinfo:number    lexinfo:plural .

:movie_with_12_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg2_movie_with_12 ;
  lexinfo:prepositionalAdjunct :arg1_movie_with_12 .

:movie_with_12_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :movie_with_12_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/starring> ;
  lemon:subjOfProp          :arg1_movie_with_12 ;
  lemon:objOfProp           :arg2_movie_with_12 ;
  lemon:condition           :movie_with_12_sense_1_condition .

:movie_with_12_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Film> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg1_movie_with_12 lemon:marker :movie_with_12_form_preposition .
## Prepositions ##
:movie_with_12_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "with"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

