@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :species_405_sense_1_res ;
  lemon:entry    :species_405 .

:species_405 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :species_405_form ;
  lemon:otherForm  :species_405_singular ;
  lemon:otherForm  :species_405_plural ;
  lemon:sense          :species_405_sense_1 ;
  lemon:synBehavior    :species_405_predFrame .

:species_405_form
 lemon:writtenRep "species"@en .

:species_405_singular a    lemon:Form ;
  lemon:writtenRep "species"@en ;
  lexinfo:number   lexinfo:singular .

:species_405_plural a   lemon:Form ;
  lemon:writtenRep "species"@en ;
  lexinfo:number    lexinfo:plural .

:species_405_sense_1 a  lemon:LexicalSense ;
  lemon:reference :species_405_sense_1_res ;
  lemon:isA       :species_405_PredSynArg .

:species_405_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Species> .