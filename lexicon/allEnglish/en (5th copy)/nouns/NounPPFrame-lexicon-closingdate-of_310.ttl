@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :closingdate-of_310 ;
  lemon:entry    :closingdate-of_310_form_preposition .

:closingdate-of_310 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :closingdate-of_310_form ;
  lemon:otherForm  :closingdate-of_310_singular ;
  lemon:otherForm  :closingdate-of_310_plural ;
  lemon:sense          :closingdate-of_310_sense_1 ;
  lemon:synBehavior    :closingdate-of_310_nounpp .

:closingdate-of_310_form a lemon:Form ;
  lemon:writtenRep "closing Date"@en .

:closingdate-of_310_singular a    lemon:Form ;
  lemon:writtenRep "closing Date"@en ;
  lexinfo:number   lexinfo:singular .

:closingdate-of_310_plural a   lemon:Form ;
  lemon:writtenRep "closing Dates"@en ;
  lexinfo:number    lexinfo:plural .

:closingdate-of_310_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_closingdate-of_310 ;
  lexinfo:prepositionalAdjunct :arg2_closingdate-of_310 .

:closingdate-of_310_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :closingdate-of_310_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/closingDate> ;
  lemon:subjOfProp          :arg2_closingdate-of_310 ;
  lemon:objOfProp           :arg1_closingdate-of_310 ;
  lemon:condition           :closingdate-of_310_sense_1_condition .

:closingdate-of_310_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2_closingdate-of_310 lemon:marker :closingdate-of_310_form_preposition .
## Prepositions ##
:closingdate-of_310_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

