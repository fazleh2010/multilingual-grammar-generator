@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :article_of_17_sense_1_res ;
  lemon:entry    :article_of_17 .

:article_of_17 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :article_of_17_form ;
  lemon:otherForm  :article_of_17_singular ;
  lemon:otherForm  :article_of_17_plural ;
  lemon:sense          :article_of_17_sense_1 ;
  lemon:synBehavior    :article_of_17_predFrame .

:article_of_17_form
 lemon:writtenRep "article"@en .

:article_of_17_singular a    lemon:Form ;
  lemon:writtenRep "article"@en ;
  lexinfo:number   lexinfo:singular .

:article_of_17_plural a   lemon:Form ;
  lemon:writtenRep "articles"@en ;
  lexinfo:number    lexinfo:plural .

:article_of_17_sense_1 a  lemon:LexicalSense ;
  lemon:reference :article_of_17_sense_1_res ;
  lemon:isA       :article_of_17_PredSynArg .

:article_of_17_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Article> .
