@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :bird_300_sense_1_res ;
  lemon:entry    :bird_300 .

:bird_300 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :bird_300_form ;
  lemon:otherForm  :bird_300_singular ;
  lemon:otherForm  :bird_300_plural ;
  lemon:sense          :bird_300_sense_1 ;
  lemon:synBehavior    :bird_300_predFrame .

:bird_300_form
 lemon:writtenRep "bird"@en .

:bird_300_singular a    lemon:Form ;
  lemon:writtenRep "bird"@en ;
  lexinfo:number   lexinfo:singular .

:bird_300_plural a   lemon:Form ;
  lemon:writtenRep "birds"@en ;
  lexinfo:number    lexinfo:plural .

:bird_300_sense_1 a  lemon:LexicalSense ;
  lemon:reference :bird_300_sense_1_res ;
  lemon:isA       :bird_300_PredSynArg .

:bird_300_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Bird> .