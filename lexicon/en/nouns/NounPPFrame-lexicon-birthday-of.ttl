@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :birthday-of ;
  lemon:entry    :birthday-of_form_preposition .

:birthday-of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :birthday-of_form ;
  lemon:otherForm  :birthday-of_singular ;
  lemon:otherForm  :birthday-of_plural ;
  lemon:sense          :birthday-of_sense_1 ;
  lemon:synBehavior    :birthday-of_nounpp .

:birthday-of_form a lemon:Form ;
  lemon:writtenRep "birthday"@en .

:birthday-of_singular a    lemon:Form ;
  lemon:writtenRep "birthday"@en ;
  lexinfo:number   lexinfo:singular .

:birthday-of_plural a   lemon:Form ;
  lemon:writtenRep "birthdays"@en ;
  lexinfo:number    lexinfo:plural .

:birthday-of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:birthday-of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :birthday-of_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/birthDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :birthday-of_sense_1_condition .

:birthday-of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Agent> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :birthday-of_form_preposition .
## Prepositions ##
:birthday-of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

