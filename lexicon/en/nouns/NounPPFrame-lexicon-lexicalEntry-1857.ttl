@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-1857 ;
  lemon:entry    :lexicalEntry-1857_form_preposition .

:lexicalEntry-1857 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lexicalEntry-1857_form ;
  lemon:otherForm  :lexicalEntry-1857_singular ;
  lemon:otherForm  :lexicalEntry-1857_plural ;
  lemon:sense          :lexicalEntry-1857_sense_1 ;
  lemon:synBehavior    :lexicalEntry-1857_nounpp .

:lexicalEntry-1857_form a lemon:Form ;
  lemon:writtenRep "short story"@en .

:lexicalEntry-1857_singular a    lemon:Form ;
  lemon:writtenRep "short story"@en ;
  lexinfo:number   lexinfo:singular .

:lexicalEntry-1857_plural a   lemon:Form ;
  lemon:writtenRep "short story"@en ;
  lexinfo:number    lexinfo:plural .

:lexicalEntry-1857_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg2 ;
  lexinfo:prepositionalAdjunct :arg1 .

:lexicalEntry-1857_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :lexicalEntry-1857_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/literaryGenre> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :lexicalEntry-1857_sense_1_condition .

:lexicalEntry-1857_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Autobiography> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Author> .

:arg2 lemon:marker :lexicalEntry-1857_form_preposition .
## Prepositions ##
:lexicalEntry-1857_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

