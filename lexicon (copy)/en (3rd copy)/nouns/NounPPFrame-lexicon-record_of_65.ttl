@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :record_of_65 ;
  lemon:entry    :record_of_65_form_preposition .

:record_of_65 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :record_of_65_form ;
  lemon:otherForm  :record_of_65_singular ;
  lemon:otherForm  :record_of_65_plural ;
  lemon:sense          :record_of_65_sense_1 ;
  lemon:synBehavior    :record_of_65_nounpp .

:record_of_65_form a lemon:Form ;
  lemon:writtenRep "record"@en .

:record_of_65_singular a    lemon:Form ;
  lemon:writtenRep "record"@en ;
  lexinfo:number   lexinfo:singular .

:record_of_65_plural a   lemon:Form ;
  lemon:writtenRep "records"@en ;
  lexinfo:number    lexinfo:plural .

:record_of_65_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_record_of_65 ;
  lexinfo:prepositionalAdjunct :arg2_record_of_65 .

:record_of_65_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :record_of_65_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/releaseDate> ;
  lemon:subjOfProp          :arg2_record_of_65 ;
  lemon:objOfProp           :arg1_record_of_65 ;
  lemon:condition           :record_of_65_sense_1_condition .

:record_of_65_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2_record_of_65 lemon:marker :record_of_65_form_preposition .
## Prepositions ##
:record_of_65_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

