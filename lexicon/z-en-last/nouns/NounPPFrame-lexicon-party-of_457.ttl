@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :party-of_457 ;
  lemon:entry    :party-of_457_form_preposition .

:party-of_457 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :party-of_457_form ;
  lemon:otherForm  :party-of_457_singular ;
  lemon:otherForm  :party-of_457_plural ;
  lemon:sense          :party-of_457_sense_1 ;
  lemon:sense          :party-of_457_sense_2 ;
  lemon:synBehavior    :party-of_457_nounpp .

:party-of_457_form a lemon:Form ;
  lemon:writtenRep "party"@en .

:party-of_457_singular a    lemon:Form ;
  lemon:writtenRep "party"@en ;
  lexinfo:number   lexinfo:singular .

:party-of_457_plural a   lemon:Form ;
  lemon:writtenRep "parties"@en ;
  lexinfo:number    lexinfo:plural .

:party-of_457_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :null ;
  lexinfo:prepositionalAdjunct :null .

:party-of_457_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :party-of_457_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/party> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :party-of_457_sense_1_condition .

:party-of_457_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Politician> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:party-of_457_sense_2 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :party-of_457_sense_2 ;
  lemon:reference           <http://dbpedia.org/ontology/party> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :party-of_457_sense_2_condition .

:party-of_457_sense_2_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Politician> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:null lemon:marker :party-of_457_form_preposition .
## Prepositions ##
:party-of_457_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

