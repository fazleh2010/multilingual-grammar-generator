@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :density_1_329 ;
  lemon:entry    :density_1_329_form_preposition .

:density_1_329 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :density_1_329_form ;
  lemon:otherForm  :density_1_329_singular ;
  lemon:otherForm  :density_1_329_plural ;
  lemon:sense          :density_1_329_sense_1 ;
  lemon:synBehavior    :density_1_329_nounpp .

:density_1_329_form a lemon:Form ;
  lemon:writtenRep "density"@en .

:density_1_329_singular a    lemon:Form ;
  lemon:writtenRep "density"@en ;
  lexinfo:number   lexinfo:singular .

:density_1_329_plural a   lemon:Form ;
  lemon:writtenRep "densities"@en ;
  lexinfo:number    lexinfo:plural .

:density_1_329_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :null ;
  lexinfo:prepositionalAdjunct :null .

:density_1_329_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :density_1_329_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/densityrank> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :density_1_329_sense_1_condition .

:density_1_329_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#nonNegativeInteger> .

:null lemon:marker :density_1_329_form_preposition .
## Prepositions ##
:density_1_329_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

