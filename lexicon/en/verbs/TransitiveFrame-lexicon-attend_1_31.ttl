@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_attend_1_31 ;
  lemon:entry    :attend_1_31ed ;
  lemon:entry    :form_attend_1_31_preposition .

:to_attend_1_31 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_attend_1_31 ;
  lemon:otherForm      :form_attend_1_31s ;
  lemon:otherForm      :form_attend_1_31ed ;
  lemon:otherForm      :form_attend_1_31perfect ;
  lemon:sense          :attend_1_31_sense_1 ;
  lemon:synBehavior    :attend_1_31_frame_transitive .

:form_attend_1_31 a         lemon:Form ;
  lemon:writtenRep     "attend"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_attend_1_31s a    lemon:Form ;
  lemon:writtenRep "attends"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_attend_1_31ed a   lemon:Form ;
  lemon:writtenRep "attended"@en ;
  lexinfo:tense    lexinfo:past .

:form_attend_1_31perfect a   lemon:Form ;
  lemon:writtenRep "attended"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:attend_1_31_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :attend_1_31_obj ;
  lexinfo:directObject     :attend_1_31_subj .

:attend_1_31_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :attend_1_31_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/almaMater> ;
  lemon:subjOfProp  :attend_1_31_obj ;
  lemon:objOfProp   :attend_1_31_subj ;
  lemon:condition   :attend_1_31_sense_1_condition .


:attend_1_31_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Agent> .


:attend_1_31_obj lemon:marker :form_attend_1_31_preposition .

## Prepositions ##
:form_attend_1_31_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

