@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :aircraft_of_104 ;
  lemon:entry    :aircraft_of_104_form_preposition .

:aircraft_of_104 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :aircraft_of_104_form ;
  lemon:otherForm  :aircraft_of_104_singular ;
  lemon:otherForm  :aircraft_of_104_plural ;
  lemon:sense          :aircraft_of_104_sense_1 ;
  lemon:synBehavior    :aircraft_of_104_nounpp .

:aircraft_of_104_form a lemon:Form ;
  lemon:writtenRep "Aircraft"@en .

:aircraft_of_104_singular a    lemon:Form ;
  lemon:writtenRep "Aircraft"@en ;
  lexinfo:number   lexinfo:singular .

:aircraft_of_104_plural a   lemon:Form ;
  lemon:writtenRep "Aircrafts"@en ;
  lexinfo:number    lexinfo:plural .

:aircraft_of_104_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_aircraft_of_104 ;
  lexinfo:prepositionalAdjunct :arg2_aircraft_of_104 .

:aircraft_of_104_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :aircraft_of_104_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_aircraft_of_104 ;
  lemon:objOfProp           :arg1_aircraft_of_104 ;
  lemon:condition           :aircraft_of_104_sense_1_condition .

:aircraft_of_104_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Aircraft> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Aircraft> .

:arg2_aircraft_of_104 lemon:marker :aircraft_of_104_form_preposition .
## Prepositions ##
:aircraft_of_104_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

