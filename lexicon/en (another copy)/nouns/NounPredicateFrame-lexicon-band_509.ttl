@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :band_509_sense_1_res ;
  lemon:entry    :band_509 .

:band_509 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :band_509_form ;
  lemon:otherForm  :band_509_singular ;
  lemon:otherForm  :band_509_plural ;
  lemon:sense          :band_509_sense_1 ;
  lemon:synBehavior    :band_509_predFrame .

:band_509_form
 lemon:writtenRep "band"@en .

:band_509_singular a    lemon:Form ;
  lemon:writtenRep "band"@en ;
  lexinfo:number   lexinfo:singular .

:band_509_plural a   lemon:Form ;
  lemon:writtenRep "bands"@en ;
  lexinfo:number    lexinfo:plural .

:band_509_sense_1 a  lemon:LexicalSense ;
  lemon:reference :band_509_sense_1_res ;
  lemon:isA       :band_509_PredSynArg .

:band_509_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Band> .