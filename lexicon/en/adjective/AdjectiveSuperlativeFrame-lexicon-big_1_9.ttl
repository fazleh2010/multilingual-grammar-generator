@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix oils:     <http://lemon-model.net/oils#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :big_1_9 ;
  lemon:entry    :form_big_1_9_preposition ;
  lemon:entry    :big_1_9_res .

:big_1_9 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_big_1_9 ;
  lemon:otherForm      :form_big_1_9_comperative ;
  lemon:otherForm      :form_big_1_9_superlative ;
  lemon:sense          :big_1_9_sense_1 ;
  lemon:synBehavior    :big_1_9_predFrame .

:form_big_1_9 lemon:writtenRep "big"@en .

:form_big_1_9_comperative lemon:writtenRep "bigger"@en .

:form_big_1_9_superlative lemon:writtenRep "biggest"@en .

:big_1_9_predFrame a        lexinfo:AdjectiveSuperlativeFrame ;
  lexinfo:copulativeSubject :big_1_9_PredSynArg .

:big_1_9_sense_1 a  lemon:LexicalSense ;
  lemon:reference :big_1_9_res ;
  lemon:isA       :big_1_9_PredSynArg ;
  lemon:condition :big_1_9_sense_1_condition .

:big_1_9_res a   oils:CovariantScalar ;
  oils:boundTo  <http://dbpedia.org/ontology/seatingCapacity> ;
  oils:degree   <http://lemon-model.net/oils/high> .

:big_1_9_sense_1_condition a lemon:condition ;
  lemon:propertyDomain   <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange    <http://dbpedia.org/ontology/Stadium> .

## Prepositions ##
:form_big_1_9_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

