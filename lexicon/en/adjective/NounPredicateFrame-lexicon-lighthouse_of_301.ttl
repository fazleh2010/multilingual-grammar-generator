@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lighthouse_of_301_sense_1_res ;
  lemon:entry    :lighthouse_of_301 .

:lighthouse_of_301 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :lighthouse_of_301_form ;
  lemon:otherForm  :lighthouse_of_301_singular ;
  lemon:otherForm  :lighthouse_of_301_plural ;
  lemon:sense          :lighthouse_of_301_sense_1 ;
  lemon:synBehavior    :lighthouse_of_301_predFrame .

:lighthouse_of_301_form
 lemon:writtenRep "Lighthouse"@en .

:lighthouse_of_301_singular a    lemon:Form ;
  lemon:writtenRep "Lighthouse"@en ;
  lexinfo:number   lexinfo:singular .

:lighthouse_of_301_plural a   lemon:Form ;
  lemon:writtenRep "Lighthouses"@en ;
  lexinfo:number    lexinfo:plural .

:lighthouse_of_301_sense_1 a  lemon:LexicalSense ;
  lemon:reference :lighthouse_of_301_sense_1_res ;
  lemon:isA       :lighthouse_of_301_PredSynArg .

:lighthouse_of_301_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Lighthouse> .
