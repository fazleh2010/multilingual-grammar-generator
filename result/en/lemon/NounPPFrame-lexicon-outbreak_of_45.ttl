@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :outbreak_of_45 ;
  lemon:entry    :outbreak_of_45_form_preposition .

:outbreak_of_45 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :outbreak_of_45_form ;
  lemon:otherForm  :outbreak_of_45_singular ;
  lemon:otherForm  :outbreak_of_45_plural ;
  lemon:sense          :outbreak_of_45_sense_1 ;
  lemon:synBehavior    :outbreak_of_45_nounpp .

:outbreak_of_45_form a lemon:Form ;
  lemon:writtenRep "Outbreak"@en .

:outbreak_of_45_singular a    lemon:Form ;
  lemon:writtenRep "Outbreak"@en ;
  lexinfo:number   lexinfo:singular .

:outbreak_of_45_plural a   lemon:Form ;
  lemon:writtenRep "Outbreaks"@en ;
  lexinfo:number    lexinfo:plural .

:outbreak_of_45_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_outbreak_of_45 ;
  lexinfo:prepositionalAdjunct :arg2_outbreak_of_45 .

:outbreak_of_45_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :outbreak_of_45_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_outbreak_of_45 ;
  lemon:objOfProp           :arg1_outbreak_of_45 ;
  lemon:condition           :outbreak_of_45_sense_1_condition .

:outbreak_of_45_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Outbreak> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Outbreak> .

:arg2_outbreak_of_45 lemon:marker :outbreak_of_45_form_preposition .
## Prepositions ##
:outbreak_of_45_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

