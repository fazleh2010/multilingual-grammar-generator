@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :sumowrestler_of_9 ;
  lemon:entry    :sumowrestler_of_9_form_preposition .

:sumowrestler_of_9 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :sumowrestler_of_9_form ;
  lemon:otherForm  :sumowrestler_of_9_singular ;
  lemon:otherForm  :sumowrestler_of_9_plural ;
  lemon:sense          :sumowrestler_of_9_sense_1 ;
  lemon:synBehavior    :sumowrestler_of_9_nounpp .

:sumowrestler_of_9_form a lemon:Form ;
  lemon:writtenRep "Sumo Wrestler"@en .

:sumowrestler_of_9_singular a    lemon:Form ;
  lemon:writtenRep "Sumo Wrestler"@en ;
  lexinfo:number   lexinfo:singular .

:sumowrestler_of_9_plural a   lemon:Form ;
  lemon:writtenRep "Sumo Wrestlers"@en ;
