  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_restaurant_of_282 ;
  lemon:objOfProp           :arg1_restaurant_of_282 ;
  lemon:condition           :restaurant_of_282_sense_1_condition .

:restaurant_of_282_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Restaurant> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Restaurant> .

:arg2_restaurant_of_282 lemon:marker :restaurant_of_282_form_preposition .
## Prepositions ##
:restaurant_of_282_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

