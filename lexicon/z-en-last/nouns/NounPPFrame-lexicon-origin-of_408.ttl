@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :origin-of_408 ;
  lemon:entry    :origin-of_408_form_preposition .

:origin-of_408 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :origin-of_408_form ;
  lemon:otherForm  :origin-of_408_singular ;
  lemon:otherForm  :origin-of_408_plural ;
  lemon:sense          :origin-of_408_sense_1 ;
  lemon:sense          :origin-of_408_sense_2 ;
  lemon:synBehavior    :origin-of_408_nounpp .

:origin-of_408_form a lemon:Form ;
  lemon:writtenRep "origin"@en .

:origin-of_408_singular a    lemon:Form ;
  lemon:writtenRep "origin"@en ;
  lexinfo:number   lexinfo:singular .

:origin-of_408_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:origin-of_408_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :null ;
  lexinfo:prepositionalAdjunct :null .

:origin-of_408_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :origin-of_408_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/origin> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :origin-of_408_sense_1_condition .

:origin-of_408_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Device> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:origin-of_408_sense_2 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :origin-of_408_sense_2 ;
  lemon:reference           <http://dbpedia.org/ontology/origin> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :origin-of_408_sense_2_condition .

:origin-of_408_sense_2_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Device> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:null lemon:marker :origin-of_408_form_preposition .
## Prepositions ##
:origin-of_408_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

