@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :entomologist_270_sense_1_res ;
  lemon:entry    :entomologist_270 .

:entomologist_270 a  lemon:LexicalEntry ;
