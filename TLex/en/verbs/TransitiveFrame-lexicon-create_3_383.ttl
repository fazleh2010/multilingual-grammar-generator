@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_create_3_383 ;
  lemon:entry    :create_3_383ed ;
  lemon:entry    :form_create_3_383_preposition .

:to_create_3_383 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_create_3_383 ;
  lemon:otherForm      :form_create_3_383s ;
  lemon:otherForm      :form_create_3_383ed ;
  lemon:otherForm      :form_create_3_383perfect ;
  lemon:sense          :create_3_383_sense_1 ;
  lemon:synBehavior    :create_3_383_frame_transitive .

:form_create_3_383 a         lemon:Form ;
  lemon:writtenRep     "create"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_create_3_383s a    lemon:Form ;
  lemon:writtenRep "creates"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_create_3_383ed a   lemon:Form ;
  lemon:writtenRep "created"@en ;
  lexinfo:tense    lexinfo:past .

:form_create_3_383perfect a   lemon:Form ;
  lemon:writtenRep "created"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:create_3_383_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :create_3_383_subj ;
  lexinfo:directObject     :create_3_383_obj .

:create_3_383_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :create_3_383_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/notableWork> ;
  lemon:subjOfProp  :create_3_383_obj ;
  lemon:objOfProp   :create_3_383_subj ;
  lemon:condition   :create_3_383_sense_1_condition .


:create_3_383_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:create_3_383_obj lemon:marker :form_create_3_383_preposition .

## Prepositions ##
:form_create_3_383_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

