@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :mythologicalfigure_of_260 ;
  lemon:entry    :mythologicalfigure_of_260_form_preposition .

:mythologicalfigure_of_260 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :mythologicalfigure_of_260_form ;
  lemon:otherForm  :mythologicalfigure_of_260_singular ;
  lemon:otherForm  :mythologicalfigure_of_260_plural ;
  lemon:sense          :mythologicalfigure_of_260_sense_1 ;
  lemon:synBehavior    :mythologicalfigure_of_260_nounpp .

:mythologicalfigure_of_260_form a lemon:Form ;
  lemon:writtenRep "Mythological Figure"@en .

:mythologicalfigure_of_260_singular a    lemon:Form ;
  lemon:writtenRep "Mythological Figure"@en ;
  lexinfo:number   lexinfo:singular .

:mythologicalfigure_of_260_plural a   lemon:Form ;
  lemon:writtenRep "Mythological Figures"@en ;
  lexinfo:number    lexinfo:plural .

:mythologicalfigure_of_260_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_mythologicalfigure_of_260 ;
  lexinfo:prepositionalAdjunct :arg2_mythologicalfigure_of_260 .

:mythologicalfigure_of_260_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :mythologicalfigure_of_260_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_mythologicalfigure_of_260 ;
  lemon:objOfProp           :arg1_mythologicalfigure_of_260 ;
  lemon:condition           :mythologicalfigure_of_260_sense_1_condition .

:mythologicalfigure_of_260_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/MythologicalFigure> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/MythologicalFigure> .

:arg2_mythologicalfigure_of_260 lemon:marker :mythologicalfigure_of_260_form_preposition .
## Prepositions ##
:mythologicalfigure_of_260_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

