@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :admitted_as_110 ;
  lemon:entry    :form_admitted_as_110_preposition .

:admitted_as_110 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_admitted_as_110 ;
  lemon:otherForm      :form_admitted_as_110_present3rd ;
  lemon:otherForm      :form_admitted_as_110_past ;
  lemon:otherForm      :form_admitted_as_110_perfect ;
  lemon:sense          :admitted_as_110_sense_1 ;
  lemon:synBehavior    :admitted_as_110_frame .

:form_admitted_as_110 a           lemon:Form ;
  lemon:writtenRep     "admit"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_admitted_as_110_present3rd a      lemon:Form ;
  lemon:writtenRep "admits"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_admitted_as_110_past a lemon:Form ;
  lemon:writtenRep  "admitted"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_admitted_as_110_perfect a   lemon:Form ;
  lemon:writtenRep "admitted"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:admitted_as_110_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :admitted_as_110_subj ;
  lexinfo:prepositionalAdjunct :admitted_as_110_obj .

:admitted_as_110_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :admitted_as_110_ontomap ;
  lemon:reference   <http://dbpedia.org/property/admittancedate> ;
  lemon:subjOfProp  :admitted_as_110_obj ;
  lemon:objOfProp   :admitted_as_110_subj ;
  lemon:condition   :admitted_as_110_sense_1_condition .

:admitted_as_110_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Settlement> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:admitted_as_110_obj lemon:marker :form_admitted_as_110_preposition .

## Prepositions ##
:form_admitted_as_110_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "as"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

