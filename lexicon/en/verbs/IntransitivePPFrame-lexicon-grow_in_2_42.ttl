@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :grow_in_2_42 ;
  lemon:entry    :form_grow_in_2_42_preposition .

:grow_in_2_42 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_grow_in_2_42 ;
  lemon:otherForm      :form_grow_in_2_42_present3rd ;
  lemon:otherForm      :form_grow_in_2_42_past ;
  lemon:otherForm      :form_grow_in_2_42_perfect ;
  lemon:sense          :grow_in_2_42_sense_1 ;
  lemon:synBehavior    :grow_in_2_42_frame .

:form_grow_in_2_42 a           lemon:Form ;
  lemon:writtenRep     "grow"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_grow_in_2_42_present3rd a      lemon:Form ;
  lemon:writtenRep "grows"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_grow_in_2_42_past a lemon:Form ;
  lemon:writtenRep  "grew"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_grow_in_2_42_perfect a   lemon:Form ;
  lemon:writtenRep "grown"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:grow_in_2_42_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :grow_in_2_42_subj ;
  lexinfo:prepositionalAdjunct :grow_in_2_42_obj .

:grow_in_2_42_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :grow_in_2_42_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/growingGrape> ;
  lemon:subjOfProp  :grow_in_2_42_obj ;
  lemon:objOfProp   :grow_in_2_42_subj ;
  lemon:condition   :grow_in_2_42_sense_1_condition .

:grow_in_2_42_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/WineRegion> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Grape> .


:grow_in_2_42_obj lemon:marker :form_grow_in_2_42_preposition .

## Prepositions ##
:form_grow_in_2_42_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

