:study_in_2_80_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_study_in_2_80 ;
  lexinfo:prepositionalAdjunct :arg2_study_in_2_80 .

:study_in_2_80_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :study_in_2_80_sense_1 ;
