:poem_523_form
 lemon:writtenRep "poem"@en .

:poem_523_singular a    lemon:Form ;
  lemon:writtenRep "poem"@en ;
  lexinfo:number   lexinfo:singular .

:poem_523_plural a   lemon:Form ;
  lemon:writtenRep "poems"@en ;
