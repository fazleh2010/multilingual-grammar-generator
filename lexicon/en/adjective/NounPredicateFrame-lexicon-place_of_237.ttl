@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :place_of_237_sense_1_res ;
  lemon:entry    :place_of_237 .

:place_of_237 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :place_of_237_form ;
  lemon:otherForm  :place_of_237_singular ;
  lemon:otherForm  :place_of_237_plural ;
  lemon:sense          :place_of_237_sense_1 ;
  lemon:synBehavior    :place_of_237_predFrame .

:place_of_237_form
 lemon:writtenRep "place"@en .

:place_of_237_singular a    lemon:Form ;
  lemon:writtenRep "place"@en ;
  lexinfo:number   lexinfo:singular .

:place_of_237_plural a   lemon:Form ;
  lemon:writtenRep "places"@en ;
  lexinfo:number    lexinfo:plural .

:place_of_237_sense_1 a  lemon:LexicalSense ;
  lemon:reference :place_of_237_sense_1_res ;
  lemon:isA       :place_of_237_PredSynArg .

:place_of_237_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Place> .
