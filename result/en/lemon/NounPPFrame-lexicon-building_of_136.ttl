@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :building_of_136 ;
  lemon:entry    :building_of_136_form_preposition .

:building_of_136 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :building_of_136_form ;
  lemon:otherForm  :building_of_136_singular ;
  lemon:otherForm  :building_of_136_plural ;
  lemon:sense          :building_of_136_sense_1 ;
  lemon:synBehavior    :building_of_136_nounpp .

:building_of_136_form a lemon:Form ;

:building_of_136_singular a    lemon:Form ;
  lemon:writtenRep "Building"@en ;
  lexinfo:number   lexinfo:singular .

:building_of_136_plural a   lemon:Form ;
  lemon:writtenRep "dbo:Buildings"@en ;
  lexinfo:number    lexinfo:plural .

:building_of_136_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_building_of_136 ;
  lexinfo:prepositionalAdjunct :arg2_building_of_136 .

:building_of_136_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :building_of_136_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_building_of_136 ;
  lemon:objOfProp           :arg1_building_of_136 ;
  lemon:condition           :building_of_136_sense_1_condition .

:building_of_136_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Building> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Building> .

:arg2_building_of_136 lemon:marker :building_of_136_form_preposition .
## Prepositions ##
:building_of_136_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

