@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_marry_6 ;
  lemon:entry    :marry_6ed ;
  lemon:entry    :form_marry_6_preposition .

:to_marry_6 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_marry_6 ;
  lemon:otherForm      :form_marry_6s ;
  lemon:otherForm      :form_marry_6ed ;
  lemon:otherForm      :form_marry_6perfect ;
  lemon:sense          :marry_6_sense_1 ;
  lemon:synBehavior    :marry_6_frame_transitive .

:form_marry_6 a         lemon:Form ;
  lemon:writtenRep     "marry"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_marry_6s a    lemon:Form ;
  lemon:writtenRep "marries"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_marry_6ed a   lemon:Form ;
  lemon:writtenRep "married"@en ;
  lexinfo:tense    lexinfo:past .

:form_marry_6perfect a   lemon:Form ;
  lemon:writtenRep "married"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:marry_6_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :marry_6_subj ;
  lexinfo:directObject     :marry_6_obj .

:marry_6_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :marry_6_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/spouse> ;
  lemon:subjOfProp  :marry_6_obj ;
  lemon:objOfProp   :marry_6_subj ;
  lemon:condition   :marry_6_sense_1_condition .


:marry_6_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:marry_6_obj lemon:marker :form_marry_6_preposition .

## Prepositions ##
:form_marry_6_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "to"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

