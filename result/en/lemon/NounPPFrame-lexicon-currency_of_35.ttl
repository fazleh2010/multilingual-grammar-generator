@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :currency_of_35 ;
  lemon:entry    :currency_of_35_form_preposition .

:currency_of_35 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :currency_of_35_form ;
  lemon:otherForm  :currency_of_35_singular ;
  lemon:otherForm  :currency_of_35_plural ;
  lemon:sense          :currency_of_35_sense_1 ;
  lemon:synBehavior    :currency_of_35_nounpp .

:currency_of_35_form a lemon:Form ;

:currency_of_35_singular a    lemon:Form ;
  lemon:writtenRep "currency"@en ;
  lexinfo:number   lexinfo:singular .

:currency_of_35_plural a   lemon:Form ;
  lemon:writtenRep "currencies"@en ;
  lexinfo:number    lexinfo:plural .

:currency_of_35_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_currency_of_35 ;
  lexinfo:prepositionalAdjunct :arg2_currency_of_35 .

:currency_of_35_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :currency_of_35_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_currency_of_35 ;
  lemon:objOfProp           :arg1_currency_of_35 ;
  lemon:condition           :currency_of_35_sense_1_condition .

:currency_of_35_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Currency> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Currency> .

:arg2_currency_of_35 lemon:marker :currency_of_35_form_preposition .
## Prepositions ##
:currency_of_35_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

