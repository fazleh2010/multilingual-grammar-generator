@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-3217 ;
  lemon:entry    :lexicalEntry-3217ed ;
  lemon:entry    :form_lexicalEntry-3217_preposition .

:to_lexicalEntry-3217 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-3217 ;
  lemon:otherForm      :form_lexicalEntry-3217s ;
  lemon:otherForm      :form_lexicalEntry-3217ed ;
  lemon:otherForm      :form_lexicalEntry-3217perfect ;
  lemon:sense          :lexicalEntry-3217_sense_1 ;
  lemon:synBehavior    :lexicalEntry-3217_frame_transitive .

:form_lexicalEntry-3217 a         lemon:Form ;
  lemon:writtenRep     "held"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-3217s a    lemon:Form ;
  lemon:writtenRep "held"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-3217ed a   lemon:Form ;
  lemon:writtenRep "held"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-3217perfect a   lemon:Form ;
  lemon:writtenRep "held"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-3217_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-3217_subj ;
  lexinfo:directObject     :lexicalEntry-3217_obj .

:lexicalEntry-3217_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3217_sense_1 ;
  lemon:reference   <http://dbpedia.org/property/country> ;
  lemon:subjOfProp  :lexicalEntry-3217_obj ;
  lemon:objOfProp   :lexicalEntry-3217_subj ;
  lemon:condition   :lexicalEntry-3217_sense_1_condition .


:lexicalEntry-3217_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Country> .

:lexicalEntry-3217ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-3217ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-3217ed_by ;
  lemon:synBehavior    :lexicalEntry-3217ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-3217ed_ontomap .

:form_lexicalEntry-3217ed_canonical a lemon:Form ;
  lemon:writtenRep         "held"@en .

:form_lexicalEntry-3217ed_by a    lemon:Form ;
  lemon:writtenRep     "held"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-3217ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-3217ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-3217ed_obj .

:lexicalEntry-3217ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3217ed_ontomap ;
  lemon:reference   <http://dbpedia.org/property/country> ;
  lemon:subjOfProp  :lexicalEntry-3217ed_subj ;
  lemon:objOfProp   :lexicalEntry-3217ed_obj ;
  lemon:condition   :lexicalEntry-3217_sense_1_condition .
:lexicalEntry-3217ed_obj lemon:marker :form_lexicalEntry-3217_preposition .

## Prepositions ##
:form_lexicalEntry-3217_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

