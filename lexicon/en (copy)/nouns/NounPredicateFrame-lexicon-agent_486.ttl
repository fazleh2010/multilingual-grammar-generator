@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :agent_486_sense_1_res ;
  lemon:entry    :agent_486 .

:agent_486 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :agent_486_form ;
  lemon:otherForm  :agent_486_singular ;
  lemon:otherForm  :agent_486_plural ;
  lemon:sense          :agent_486_sense_1 ;
  lemon:synBehavior    :agent_486_predFrame .

:agent_486_form
 lemon:writtenRep "agent"@en .

:agent_486_singular a    lemon:Form ;
  lemon:writtenRep "agent"@en ;
  lexinfo:number   lexinfo:singular .

:agent_486_plural a   lemon:Form ;
  lemon:writtenRep "agents"@en ;
  lexinfo:number    lexinfo:plural .

:agent_486_sense_1 a  lemon:LexicalSense ;
  lemon:reference :agent_486_sense_1_res ;
  lemon:isA       :agent_486_PredSynArg .

:agent_486_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Agent> .