@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_graduated_from_51 ;
  lemon:entry    :graduated_from_51ed ;
  lemon:entry    :form_graduated_from_51_preposition .

:to_graduated_from_51 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_graduated_from_51 ;
  lemon:otherForm      :form_graduated_from_51s ;
  lemon:otherForm      :form_graduated_from_51ed ;
  lemon:otherForm      :form_graduated_from_51perfect ;
  lemon:sense          :graduated_from_51_sense_1 ;
  lemon:synBehavior    :graduated_from_51_frame_transitive .

:form_graduated_from_51 a         lemon:Form ;
  lemon:writtenRep     "graduate from"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_graduated_from_51s a    lemon:Form ;
  lemon:writtenRep "graduates from"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_graduated_from_51ed a   lemon:Form ;
  lemon:writtenRep "graduated from"@en ;
  lexinfo:tense    lexinfo:past .

:form_graduated_from_51perfect a   lemon:Form ;
  lemon:writtenRep "graduated from"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:graduated_from_51_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :graduated_from_51_subj ;
  lexinfo:directObject     :graduated_from_51_obj .

:graduated_from_51_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :graduated_from_51_sense_1 ;
  lemon:reference   <wd:P69> ;
  lemon:subjOfProp  :graduated_from_51_obj ;
  lemon:objOfProp   :graduated_from_51_subj ;
  lemon:condition   :graduated_from_51_sense_1_condition .


:graduated_from_51_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <wd:Q19088> ;
  lemon:propertyRange  <wd:Q2385804> .


:graduated_from_51_obj lemon:marker :form_graduated_from_51_preposition .

## Prepositions ##
:form_graduated_from_51_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

