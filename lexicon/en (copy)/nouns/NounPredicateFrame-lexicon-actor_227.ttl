@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :actor_227_sense_1_res ;
  lemon:entry    :actor_227 .

:actor_227 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :actor_227_form ;
  lemon:otherForm  :actor_227_singular ;
  lemon:otherForm  :actor_227_plural ;
  lemon:sense          :actor_227_sense_1 ;
  lemon:synBehavior    :actor_227_predFrame .

:actor_227_form
 lemon:writtenRep "actor"@en .

:actor_227_singular a    lemon:Form ;
  lemon:writtenRep "actor"@en ;
  lexinfo:number   lexinfo:singular .

:actor_227_plural a   lemon:Form ;
  lemon:writtenRep "actors"@en ;
  lexinfo:number    lexinfo:plural .

:actor_227_sense_1 a  lemon:LexicalSense ;
  lemon:reference :actor_227_sense_1_res ;
  lemon:isA       :actor_227_PredSynArg .

:actor_227_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Actor> .