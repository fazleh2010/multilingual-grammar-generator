@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :actor_of_1_42 ;
  lemon:entry    :actor_of_1_42_form_preposition .

:actor_of_1_42 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :actor_of_1_42_form ;
  lemon:otherForm  :actor_of_1_42_singular ;
  lemon:otherForm  :actor_of_1_42_plural ;
  lemon:sense          :actor_of_1_42_sense_1 ;
  lemon:synBehavior    :actor_of_1_42_nounpp .

:actor_of_1_42_form a lemon:Form ;
  lemon:writtenRep "actor"@en .

:actor_of_1_42_singular a    lemon:Form ;
  lemon:writtenRep "actor"@en ;
  lexinfo:number   lexinfo:singular .

:actor_of_1_42_plural a   lemon:Form ;
  lemon:writtenRep "actors"@en ;
  lexinfo:number    lexinfo:plural .

:actor_of_1_42_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_actor_of_1_42 ;
  lexinfo:prepositionalAdjunct :arg2_actor_of_1_42 .

:actor_of_1_42_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :actor_of_1_42_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/starring> ;
  lemon:subjOfProp          :arg2_actor_of_1_42 ;
  lemon:objOfProp           :arg1_actor_of_1_42 ;
  lemon:condition           :actor_of_1_42_sense_1_condition .

:actor_of_1_42_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2_actor_of_1_42 lemon:marker :actor_of_1_42_form_preposition .
## Prepositions ##
:actor_of_1_42_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

