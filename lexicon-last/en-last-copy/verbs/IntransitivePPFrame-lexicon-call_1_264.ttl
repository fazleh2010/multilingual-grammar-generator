@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :call_1_264 ;
  lemon:entry    :form_call_1_264_preposition .

:call_1_264 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_call_1_264 ;
  lemon:otherForm      :form_call_1_264_present3rd ;
  lemon:otherForm      :form_call_1_264_past ;
  lemon:otherForm      :form_call_1_264_perfect ;
  lemon:sense          :call_1_264_sense_1 ;
  lemon:synBehavior    :call_1_264_frame .

:form_call_1_264 a           lemon:Form ;
  lemon:writtenRep     "call"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_call_1_264_present3rd a      lemon:Form ;
  lemon:writtenRep "calls"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_call_1_264_past a lemon:Form ;
  lemon:writtenRep  "called"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_call_1_264_perfect a   lemon:Form ;
  lemon:writtenRep "called"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:call_1_264_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :call_1_264_subj ;
  lexinfo:prepositionalAdjunct :call_1_264_obj .

:call_1_264_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :call_1_264_ontomap ;
  lemon:reference   <http://dbpedia.org/property/nickname> ;
  lemon:subjOfProp  :call_1_264_subj ;
  lemon:objOfProp   :call_1_264_obj ;
  lemon:condition   :call_1_264_sense_1_condition .

:call_1_264_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#String> .


:call_1_264_obj lemon:marker :form_call_1_264_preposition .

## Prepositions ##
:form_call_1_264_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

