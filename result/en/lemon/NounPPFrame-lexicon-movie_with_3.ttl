@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :movie_with_3 ;
  lemon:entry    :movie_with_3_form_preposition .

:movie_with_3 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :movie_with_3_form ;
  lemon:otherForm  :movie_with_3_singular ;
  lemon:otherForm  :movie_with_3_plural ;
  lemon:sense          :movie_with_3_sense_1 ;
  lemon:synBehavior    :movie_with_3_nounpp .

:movie_with_3_form a lemon:Form ;
  lemon:writtenRep "movie"@en .

:movie_with_3_singular a    lemon:Form ;
  lemon:writtenRep "movie"@en ;
  lexinfo:number   lexinfo:singular .

:movie_with_3_plural a   lemon:Form ;
  lemon:writtenRep "movies"@en ;
  lexinfo:number    lexinfo:plural .

:movie_with_3_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_movie_with_3 ;
  lexinfo:prepositionalAdjunct :arg2_movie_with_3 .

:movie_with_3_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :movie_with_3_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/alias> ;
  lemon:subjOfProp          :arg2_movie_with_3 ;
  lemon:objOfProp           :arg1_movie_with_3 ;
  lemon:condition           :movie_with_3_sense_1_condition .

:movie_with_3_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#String> .

:arg2_movie_with_3 lemon:marker :movie_with_3_form_preposition .
## Prepositions ##
:movie_with_3_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

