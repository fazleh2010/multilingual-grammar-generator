@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-1510 ;
  lemon:entry    :lexicalEntry-1510ed ;
  lemon:entry    :form_lexicalEntry-1510_preposition .

:to_lexicalEntry-1510 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-1510 ;
  lemon:otherForm      :form_lexicalEntry-1510s ;
  lemon:otherForm      :form_lexicalEntry-1510ed ;
  lemon:otherForm      :form_lexicalEntry-1510perfect ;
  lemon:sense          :lexicalEntry-1510_sense_1 ;
  lemon:synBehavior    :lexicalEntry-1510_frame_transitive .

:form_lexicalEntry-1510 a         lemon:Form ;
  lemon:writtenRep     "made"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-1510s a    lemon:Form ;
  lemon:writtenRep "made"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-1510ed a   lemon:Form ;
  lemon:writtenRep "made"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-1510perfect a   lemon:Form ;
  lemon:writtenRep "made"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-1510_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-1510_subj ;
  lexinfo:directObject     :lexicalEntry-1510_obj .

:lexicalEntry-1510_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1510_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/ingredient> ;
  lemon:subjOfProp  :lexicalEntry-1510_obj ;
  lemon:objOfProp   :lexicalEntry-1510_subj ;
  lemon:condition   :lexicalEntry-1510_sense_1_condition .


:lexicalEntry-1510_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Food> ;
  lemon:propertyRange  <http://www.w3.org/2002/07/owl#Thing> .

:lexicalEntry-1510ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-1510ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-1510ed_by ;
  lemon:synBehavior    :lexicalEntry-1510ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-1510ed_ontomap .

:form_lexicalEntry-1510ed_canonical a lemon:Form ;
  lemon:writtenRep         "made"@en .

:form_lexicalEntry-1510ed_by a    lemon:Form ;
  lemon:writtenRep     "made"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-1510ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-1510ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-1510ed_obj .

:lexicalEntry-1510ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1510ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/ingredient> ;
  lemon:subjOfProp  :lexicalEntry-1510ed_subj ;
  lemon:objOfProp   :lexicalEntry-1510ed_obj ;
  lemon:condition   :lexicalEntry-1510_sense_1_condition .
:lexicalEntry-1510ed_obj lemon:marker :form_lexicalEntry-1510_preposition .

## Prepositions ##
:form_lexicalEntry-1510_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

