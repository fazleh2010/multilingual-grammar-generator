:location-of_2_409_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :null ;
  lexinfo:prepositionalAdjunct :null .

:location-of_2_409_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :location-of_2_409_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/locatedInArea> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :location-of_2_409_sense_1_condition .

:location-of_2_409_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:location-of_2_409_sense_2 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :location-of_2_409_sense_2 ;
  lemon:reference           <http://dbpedia.org/ontology/locatedInArea> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
