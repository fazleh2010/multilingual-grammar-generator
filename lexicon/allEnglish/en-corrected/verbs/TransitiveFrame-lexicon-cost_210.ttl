@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_cost_210 ;
  lemon:entry    :cost_210ed ;
  lemon:entry    :form_cost_210_preposition .

:to_cost_210 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_cost_210 ;
  lemon:otherForm      :form_cost_210s ;
  lemon:otherForm      :form_cost_210ed ;
  lemon:otherForm      :form_cost_210perfect ;
  lemon:sense          :cost_210_sense_1 ;
  lemon:synBehavior    :cost_210_frame_transitive .

:form_cost_210 a         lemon:Form ;
  lemon:writtenRep     "cost"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_cost_210s a    lemon:Form ;
  lemon:writtenRep "costs"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_cost_210ed a   lemon:Form ;
  lemon:writtenRep "costed"@en ;
  lexinfo:tense    lexinfo:past .

:form_cost_210perfect a   lemon:Form ;
  lemon:writtenRep "costed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:cost_210_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :cost_210_subj ;
  lexinfo:directObject     :cost_210_obj .

:cost_210_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :cost_210_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/budget> ;
  lemon:subjOfProp  :cost_210_obj ;
  lemon:objOfProp   :cost_210_subj ;
  lemon:condition   :cost_210_sense_1_condition .


:cost_210_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Film> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#double> .


:cost_210_obj lemon:marker :form_cost_210_preposition .

## Prepositions ##
:form_cost_210_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

