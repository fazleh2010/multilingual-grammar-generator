@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :destroy_on_1_50 ;
  lemon:entry    :form_destroy_on_1_50_preposition .

:destroy_on_1_50 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_destroy_on_1_50 ;
  lemon:otherForm      :form_destroy_on_1_50_present3rd ;
  lemon:otherForm      :form_destroy_on_1_50_past ;
  lemon:otherForm      :form_destroy_on_1_50_perfect ;
  lemon:sense          :destroy_on_1_50_sense_1 ;
  lemon:synBehavior    :destroy_on_1_50_frame .

:form_destroy_on_1_50 a           lemon:Form ;
  lemon:writtenRep     "destroy"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_destroy_on_1_50_present3rd a      lemon:Form ;
  lemon:writtenRep "destroys"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_destroy_on_1_50_past a lemon:Form ;
  lemon:writtenRep  "destroyed"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_destroy_on_1_50_perfect a   lemon:Form ;
  lemon:writtenRep "destroyed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:destroy_on_1_50_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :destroy_on_1_50_subj ;
  lexinfo:prepositionalAdjunct :destroy_on_1_50_obj .

:destroy_on_1_50_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :destroy_on_1_50_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/demolitionDate> ;
  lemon:subjOfProp  :destroy_on_1_50_obj ;
  lemon:objOfProp   :destroy_on_1_50_subj ;
  lemon:condition   :destroy_on_1_50_sense_1_condition .

:destroy_on_1_50_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:destroy_on_1_50_obj lemon:marker :form_destroy_on_1_50_preposition .

## Prepositions ##
:form_destroy_on_1_50_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "on"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

