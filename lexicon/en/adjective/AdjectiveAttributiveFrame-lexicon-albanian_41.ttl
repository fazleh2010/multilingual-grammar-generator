@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :albanian_41_sense_1_res ;
  lemon:entry    :albanian_41 .

:albanian_41 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :albanian_41_lemma ;
  lemon:sense          :albanian_41_sense_1 ;
  lemon:synBehavior    :albanian_41_attrFrame, :albanian_41_predFrame .

:albanian_41_lemma lemon:writtenRep "albanian"@en .

:albanian_41_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :albanian_41_PredSynArg .

:albanian_41_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :albanian_41_AttrSynArg .

:albanian_41_sense_1 a  lemon:LexicalSense ;
  lemon:reference :albanian_41_sense_1_res ;
  lemon:isA       :albanian_41_AttrSynArg, :albanian_41_PredSynArg .

:albanian_41_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Albania> .
