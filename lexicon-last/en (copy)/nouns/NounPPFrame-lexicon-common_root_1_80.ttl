@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :common_root_1_80 ;
  lemon:entry    :common_root_1_80_form_preposition .

:common_root_1_80 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :common_root_1_80_form ;
  lemon:otherForm  :common_root_1_80_singular ;
  lemon:otherForm  :common_root_1_80_plural ;
  lemon:sense          :common_root_1_80_sense_1 ;
  lemon:synBehavior    :common_root_1_80_nounpp .

:common_root_1_80_form a lemon:Form ;
  lemon:writtenRep "common root"@en .

:common_root_1_80_singular a    lemon:Form ;
  lemon:writtenRep "common root"@en ;
  lexinfo:number   lexinfo:singular .

:common_root_1_80_plural a   lemon:Form ;
  lemon:writtenRep "common roots"@en ;
  lexinfo:number    lexinfo:plural .

:common_root_1_80_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_common_root_1_80 ;
  lexinfo:prepositionalAdjunct :arg2_common_root_1_80 .

:common_root_1_80_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :common_root_1_80_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/languageFamily> ;
  lemon:subjOfProp          :arg2_common_root_1_80 ;
  lemon:objOfProp           :arg1_common_root_1_80 ;
  lemon:condition           :common_root_1_80_sense_1_condition .

:common_root_1_80_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Language> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Language> .

:arg2_common_root_1_80 lemon:marker :common_root_1_80_form_preposition .
## Prepositions ##
:common_root_1_80_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

