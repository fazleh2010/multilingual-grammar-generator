@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :brazilian_208_sense_1_res ;
  lemon:entry    :brazilian_208 .

:brazilian_208 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :brazilian_208_lemma ;
  lemon:sense          :brazilian_208_sense_1 ;
  lemon:synBehavior    :brazilian_208_attrFrame, :brazilian_208_predFrame .

:brazilian_208_lemma lemon:writtenRep "brazilian"@en .

:brazilian_208_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :brazilian_208_PredSynArg .

:brazilian_208_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :brazilian_208_AttrSynArg .

:brazilian_208_sense_1 a  lemon:LexicalSense ;
  lemon:reference :brazilian_208_sense_1_res ;
  lemon:isA       :brazilian_208_AttrSynArg, :brazilian_208_PredSynArg .

:brazilian_208_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Brazil> .
