@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :open_on_1_201 ;
  lemon:entry    :form_open_on_1_201_preposition .

:open_on_1_201 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_open_on_1_201 ;
  lemon:otherForm      :form_open_on_1_201_present3rd ;
  lemon:otherForm      :form_open_on_1_201_past ;
  lemon:otherForm      :form_open_on_1_201_perfect ;
  lemon:sense          :open_on_1_201_sense_1 ;
  lemon:synBehavior    :open_on_1_201_frame .

:form_open_on_1_201 a           lemon:Form ;
  lemon:writtenRep     "open"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_open_on_1_201_present3rd a      lemon:Form ;
  lemon:writtenRep "opens"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_open_on_1_201_past a lemon:Form ;
  lemon:writtenRep  "opened"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_open_on_1_201_perfect a   lemon:Form ;
  lemon:writtenRep "opened"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:open_on_1_201_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :open_on_1_201_subj ;
  lexinfo:prepositionalAdjunct :open_on_1_201_obj .

:open_on_1_201_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :open_on_1_201_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/openingDate> ;
  lemon:subjOfProp  :open_on_1_201_obj ;
  lemon:objOfProp   :open_on_1_201_subj ;
  lemon:condition   :open_on_1_201_sense_1_condition .

:open_on_1_201_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Bridge> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:open_on_1_201_obj lemon:marker :form_open_on_1_201_preposition .

## Prepositions ##
:form_open_on_1_201_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "on"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

