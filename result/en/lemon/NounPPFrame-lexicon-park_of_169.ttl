@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :park_of_169 ;
  lemon:entry    :park_of_169_form_preposition .

:park_of_169 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :park_of_169_form ;
  lemon:otherForm  :park_of_169_singular ;
  lemon:otherForm  :park_of_169_plural ;
  lemon:sense          :park_of_169_sense_1 ;
  lemon:synBehavior    :park_of_169_nounpp .

:park_of_169_form a lemon:Form ;

:park_of_169_singular a    lemon:Form ;
  lemon:writtenRep "Park"@en ;
  lexinfo:number   lexinfo:singular .

:park_of_169_plural a   lemon:Form ;
  lemon:writtenRep "Parks"@en ;
  lexinfo:number    lexinfo:plural .

:park_of_169_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_park_of_169 ;
  lexinfo:prepositionalAdjunct :arg2_park_of_169 .

:park_of_169_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :park_of_169_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_park_of_169 ;
  lemon:objOfProp           :arg1_park_of_169 ;
  lemon:condition           :park_of_169_sense_1_condition .

:park_of_169_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Park> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Park> .

:arg2_park_of_169 lemon:marker :park_of_169_form_preposition .
## Prepositions ##
:park_of_169_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

