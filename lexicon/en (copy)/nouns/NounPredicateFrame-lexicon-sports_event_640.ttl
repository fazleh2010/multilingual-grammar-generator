@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :sports_event_640_sense_1_res ;
  lemon:entry    :sports_event_640 .

:sports_event_640 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :sports_event_640_form ;
  lemon:otherForm  :sports_event_640_singular ;
  lemon:otherForm  :sports_event_640_plural ;
  lemon:sense          :sports_event_640_sense_1 ;
  lemon:synBehavior    :sports_event_640_predFrame .

:sports_event_640_form
