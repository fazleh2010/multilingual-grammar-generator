@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :astronaut_354_sense_1_res ;
  lemon:entry    :astronaut_354 .

:astronaut_354 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :astronaut_354_form ;
  lemon:otherForm  :astronaut_354_singular ;
  lemon:otherForm  :astronaut_354_plural ;
  lemon:sense          :astronaut_354_sense_1 ;
  lemon:synBehavior    :astronaut_354_predFrame .

:astronaut_354_form
 lemon:writtenRep "astronaut"@en .

:astronaut_354_singular a    lemon:Form ;
  lemon:writtenRep "astronaut"@en ;
  lexinfo:number   lexinfo:singular .

:astronaut_354_plural a   lemon:Form ;
  lemon:writtenRep "astronauts"@en ;
  lexinfo:number    lexinfo:plural .

:astronaut_354_sense_1 a  lemon:LexicalSense ;
  lemon:reference :astronaut_354_sense_1_res ;
  lemon:isA       :astronaut_354_PredSynArg .

:astronaut_354_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Astronaut> .