@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :company_of_299 ;
  lemon:entry    :company_of_299_form_preposition .

:company_of_299 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :company_of_299_form ;
  lemon:otherForm  :company_of_299_singular ;
  lemon:otherForm  :company_of_299_plural ;
  lemon:sense          :company_of_299_sense_1 ;
  lemon:synBehavior    :company_of_299_nounpp .

:company_of_299_form a lemon:Form ;
  lemon:writtenRep "company"@en .

:company_of_299_singular a    lemon:Form ;
  lemon:writtenRep "company"@en ;
  lexinfo:number   lexinfo:singular .

:company_of_299_plural a   lemon:Form ;
  lemon:writtenRep "company"@en ;
  lexinfo:number    lexinfo:plural .

:company_of_299_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_company_of_299 ;
  lexinfo:prepositionalAdjunct :arg2_company_of_299 .

:company_of_299_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :company_of_299_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_company_of_299 ;
  lemon:objOfProp           :arg1_company_of_299 ;
  lemon:condition           :company_of_299_sense_1_condition .

:company_of_299_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Company> .

:arg2_company_of_299 lemon:marker :company_of_299_form_preposition .
## Prepositions ##
:company_of_299_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

