@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :railwayline_of_284 ;
  lemon:entry    :railwayline_of_284_form_preposition .

:railwayline_of_284 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :railwayline_of_284_form ;
  lemon:otherForm  :railwayline_of_284_singular ;
  lemon:otherForm  :railwayline_of_284_plural ;
  lemon:sense          :railwayline_of_284_sense_1 ;
  lemon:synBehavior    :railwayline_of_284_nounpp .

:railwayline_of_284_form a lemon:Form ;
  lemon:writtenRep "Railway Line"@en .

:railwayline_of_284_singular a    lemon:Form ;
  lemon:writtenRep "Railway Line"@en ;
  lexinfo:number   lexinfo:singular .

:railwayline_of_284_plural a   lemon:Form ;
  lemon:writtenRep "Railway Lines"@en ;
  lexinfo:number    lexinfo:plural .

:railwayline_of_284_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_railwayline_of_284 ;
  lexinfo:prepositionalAdjunct :arg2_railwayline_of_284 .

:railwayline_of_284_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :railwayline_of_284_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_railwayline_of_284 ;
  lemon:objOfProp           :arg1_railwayline_of_284 ;
  lemon:condition           :railwayline_of_284_sense_1_condition .

:railwayline_of_284_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/RailwayLine> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/RailwayLine> .

:arg2_railwayline_of_284 lemon:marker :railwayline_of_284_form_preposition .
## Prepositions ##
:railwayline_of_284_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

