@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_shot_42 ;
  lemon:entry    :shot_42ed ;
  lemon:entry    :form_shot_42_preposition .

:to_shot_42 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_shot_42 ;
  lemon:otherForm      :form_shot_42s ;
  lemon:otherForm      :form_shot_42ed ;
  lemon:otherForm      :form_shot_42perfect ;
  lemon:sense          :shot_42_sense_1 ;
  lemon:synBehavior    :shot_42_frame_transitive .

:form_shot_42 a         lemon:Form ;
  lemon:writtenRep     "shot"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_shot_42s a    lemon:Form ;
  lemon:writtenRep "shots"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_shot_42ed a   lemon:Form ;
  lemon:writtenRep "shot"@en ;
  lexinfo:tense    lexinfo:past .

:form_shot_42perfect a   lemon:Form ;
  lemon:writtenRep "shot"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:shot_42_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :shot_42_subj ;
  lexinfo:directObject     :shot_42_obj .

:shot_42_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :shot_42_sense_1 ;
  lemon:reference   <wd:P570> ;
  lemon:subjOfProp  :shot_42_obj ;
  lemon:objOfProp   :shot_42_subj ;
  lemon:condition   :shot_42_sense_1_condition .


:shot_42_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <wd:Q19088> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:shot_42_obj lemon:marker :form_shot_42_preposition .

## Prepositions ##
:form_shot_42_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

