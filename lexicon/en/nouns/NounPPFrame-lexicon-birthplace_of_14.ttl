@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :birthplace_of_14 ;
  lemon:entry    :birthplace_of_14_form_preposition .

:birthplace_of_14 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :birthplace_of_14_form ;
  lemon:otherForm  :birthplace_of_14_singular ;
  lemon:otherForm  :birthplace_of_14_plural ;
  lemon:sense          :birthplace_of_14_sense_1 ;
  lemon:synBehavior    :birthplace_of_14_nounpp .

:birthplace_of_14_form a lemon:Form ;
  lemon:writtenRep "birthplace"@en .

:birthplace_of_14_singular a    lemon:Form ;
  lemon:writtenRep "birthplace"@en ;
  lexinfo:number   lexinfo:singular .

:birthplace_of_14_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:birthplace_of_14_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :null ;
  lexinfo:prepositionalAdjunct :null .

:birthplace_of_14_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :birthplace_of_14_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/birthPlace> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :birthplace_of_14_sense_1_condition .

:birthplace_of_14_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Place> .

:null lemon:marker :birthplace_of_14_form_preposition .
## Prepositions ##
:birthplace_of_14_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

