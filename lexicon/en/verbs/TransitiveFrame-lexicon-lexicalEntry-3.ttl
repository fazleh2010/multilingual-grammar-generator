@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-3 ;
  lemon:entry    :lexicalEntry-3ed ;
  lemon:entry    :form_lexicalEntry-3_preposition .

:to_lexicalEntry-3 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-3 ;
  lemon:otherForm      :form_lexicalEntry-3s ;
  lemon:otherForm      :form_lexicalEntry-3ed ;
  lemon:otherForm      :form_lexicalEntry-3perfect ;
  lemon:sense          :lexicalEntry-3_sense_1 ;
  lemon:synBehavior    :lexicalEntry-3_frame_transitive .

:form_lexicalEntry-3 a         lemon:Form ;
  lemon:writtenRep     "create"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-3s a    lemon:Form ;
  lemon:writtenRep "creates"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-3ed a   lemon:Form ;
  lemon:writtenRep "created"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-3perfect a   lemon:Form ;
  lemon:writtenRep "created"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-3_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-3_subj ;
  lexinfo:directObject     :lexicalEntry-3_obj .

:lexicalEntry-3_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/knownFor> ;
  lemon:subjOfProp  :lexicalEntry-3_obj ;
  lemon:objOfProp   :lexicalEntry-3_subj ;
  lemon:condition   :lexicalEntry-3_sense_1_condition .


:lexicalEntry-3_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Award> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .

:lexicalEntry-3ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-3ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-3ed_by ;
  lemon:synBehavior    :lexicalEntry-3ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-3ed_ontomap .

:form_lexicalEntry-3ed_canonical a lemon:Form ;
  lemon:writtenRep         "create"@en .

:form_lexicalEntry-3ed_by a    lemon:Form ;
  lemon:writtenRep     "create"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-3ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-3ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-3ed_obj .

:lexicalEntry-3ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/knownFor> ;
  lemon:subjOfProp  :lexicalEntry-3ed_subj ;
  lemon:objOfProp   :lexicalEntry-3ed_obj ;
  lemon:condition   :lexicalEntry-3_sense_1_condition .
:lexicalEntry-3ed_obj lemon:marker :form_lexicalEntry-3_preposition .

## Prepositions ##
:form_lexicalEntry-3_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

