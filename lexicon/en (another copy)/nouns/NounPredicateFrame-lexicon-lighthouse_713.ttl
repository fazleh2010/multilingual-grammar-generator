@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lighthouse_713_sense_1_res ;
  lemon:entry    :lighthouse_713 .

:lighthouse_713 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lighthouse_713_form ;
  lemon:otherForm  :lighthouse_713_singular ;
  lemon:otherForm  :lighthouse_713_plural ;
  lemon:sense          :lighthouse_713_sense_1 ;
  lemon:synBehavior    :lighthouse_713_predFrame .

:lighthouse_713_form
 lemon:writtenRep "lighthouse"@en .

:lighthouse_713_singular a    lemon:Form ;
  lemon:writtenRep "lighthouse"@en ;
  lexinfo:number   lexinfo:singular .

:lighthouse_713_plural a   lemon:Form ;
  lemon:writtenRep "lighthouses"@en ;
  lexinfo:number    lexinfo:plural .

:lighthouse_713_sense_1 a  lemon:LexicalSense ;
  lemon:reference :lighthouse_713_sense_1_res ;
  lemon:isA       :lighthouse_713_PredSynArg .

:lighthouse_713_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Lighthouse> .