  lexinfo:prepositionalAdjunct :arg2_cyclist_of_74 .

:cyclist_of_74_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :cyclist_of_74_sense_1 ;
