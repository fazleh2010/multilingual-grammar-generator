@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :cyclist_of_74 ;
  lemon:entry    :cyclist_of_74_form_preposition .

:cyclist_of_74 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :cyclist_of_74_form ;
  lemon:otherForm  :cyclist_of_74_singular ;
  lemon:otherForm  :cyclist_of_74_plural ;
  lemon:sense          :cyclist_of_74_sense_1 ;
  lemon:synBehavior    :cyclist_of_74_nounpp .

:cyclist_of_74_form a lemon:Form ;

:cyclist_of_74_singular a    lemon:Form ;
  lemon:writtenRep "Cyclist"@en ;
