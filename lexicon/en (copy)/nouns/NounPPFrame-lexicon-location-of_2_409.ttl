:location-of_2_409_form a lemon:Form ;
  lemon:writtenRep "location"@en .

:location-of_2_409_singular a    lemon:Form ;
  lemon:writtenRep "location"@en ;
  lexinfo:number   lexinfo:singular .

:location-of_2_409_plural a   lemon:Form ;
