@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-3959 ;
  lemon:entry    :lexicalEntry-3959ed ;
  lemon:entry    :form_lexicalEntry-3959_preposition .

:to_lexicalEntry-3959 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-3959 ;
  lemon:otherForm      :form_lexicalEntry-3959s ;
  lemon:otherForm      :form_lexicalEntry-3959ed ;
  lemon:otherForm      :form_lexicalEntry-3959perfect ;
  lemon:sense          :lexicalEntry-3959_sense_1 ;
  lemon:synBehavior    :lexicalEntry-3959_frame_transitive .

:form_lexicalEntry-3959 a         lemon:Form ;
  lemon:writtenRep     "hosted"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-3959s a    lemon:Form ;
  lemon:writtenRep "hosted"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-3959ed a   lemon:Form ;
  lemon:writtenRep "hosted"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-3959perfect a   lemon:Form ;
  lemon:writtenRep "hosted"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-3959_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-3959_obj ;
  lexinfo:directObject     :lexicalEntry-3959_subj .

:lexicalEntry-3959_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3959_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/presenter> ;
  lemon:subjOfProp  :lexicalEntry-3959_obj ;
  lemon:objOfProp   :lexicalEntry-3959_subj ;
  lemon:condition   :lexicalEntry-3959_sense_1_condition .


:lexicalEntry-3959_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .

:lexicalEntry-3959ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-3959ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-3959ed_by ;
  lemon:synBehavior    :lexicalEntry-3959ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-3959ed_ontomap .

:form_lexicalEntry-3959ed_canonical a lemon:Form ;
  lemon:writtenRep         "hosted"@en .

:form_lexicalEntry-3959ed_by a    lemon:Form ;
  lemon:writtenRep     "hosted"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-3959ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-3959ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-3959ed_obj .

:lexicalEntry-3959ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3959ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/presenter> ;
  lemon:subjOfProp  :lexicalEntry-3959ed_subj ;
  lemon:objOfProp   :lexicalEntry-3959ed_obj ;
  lemon:condition   :lexicalEntry-3959_sense_1_condition .
:lexicalEntry-3959ed_obj lemon:marker :form_lexicalEntry-3959_preposition .

## Prepositions ##
:form_lexicalEntry-3959_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

