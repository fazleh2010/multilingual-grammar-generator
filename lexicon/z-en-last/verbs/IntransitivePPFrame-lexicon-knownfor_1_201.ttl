@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :knownfor_1_201 ;
  lemon:entry    :form_knownfor_1_201_preposition .

:knownfor_1_201 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_knownfor_1_201 ;
  lemon:otherForm      :form_knownfor_1_201_present3rd ;
  lemon:otherForm      :form_knownfor_1_201_past ;
  lemon:otherForm      :form_knownfor_1_201_perfect ;
  lemon:sense          :knownfor_1_201_sense_1 ;
  lemon:synBehavior    :knownfor_1_201_frame .

:form_knownfor_1_201 a           lemon:Form ;
  lemon:writtenRep     "known"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_knownfor_1_201_present3rd a      lemon:Form ;
  lemon:writtenRep "known"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_knownfor_1_201_past a lemon:Form ;
  lemon:writtenRep  "known"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_knownfor_1_201_perfect a   lemon:Form ;
  lemon:writtenRep "known"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:knownfor_1_201_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :knownfor_1_201_subj ;
  lexinfo:prepositionalAdjunct :knownfor_1_201_obj .

:knownfor_1_201_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :knownfor_1_201_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/knownFor> ;
  lemon:subjOfProp  :knownfor_1_201_subj ;
  lemon:objOfProp   :knownfor_1_201_obj ;
  lemon:condition   :knownfor_1_201_sense_1_condition .

:knownfor_1_201_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Scientist> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/MilitaryUnit> .


:knownfor_1_201_obj lemon:marker :form_knownfor_1_201_preposition .

## Prepositions ##
:form_knownfor_1_201_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "for"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

