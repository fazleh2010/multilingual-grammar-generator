  lexinfo:number    lexinfo:plural .

:year_231_sense_1 a  lemon:LexicalSense ;
  lemon:reference :year_231_sense_1_res ;
  lemon:isA       :year_231_PredSynArg .

:year_231_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
