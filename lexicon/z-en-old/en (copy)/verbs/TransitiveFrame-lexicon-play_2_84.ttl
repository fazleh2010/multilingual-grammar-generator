@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_play_2_84 ;
  lemon:entry    :play_2_84ed ;
  lemon:entry    :form_play_2_84_preposition .

:to_play_2_84 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_play_2_84 ;
  lemon:otherForm      :form_play_2_84s ;
  lemon:otherForm      :form_play_2_84ed ;
  lemon:otherForm      :form_play_2_84perfect ;
  lemon:sense          :play_2_84_sense_1 ;
  lemon:synBehavior    :play_2_84_frame_transitive .

:form_play_2_84 a         lemon:Form ;
  lemon:writtenRep     "play"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_play_2_84s a    lemon:Form ;
  lemon:writtenRep "plays"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_play_2_84ed a   lemon:Form ;
  lemon:writtenRep "played"@en ;
  lexinfo:tense    lexinfo:past .

:form_play_2_84perfect a   lemon:Form ;
  lemon:writtenRep "played"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:play_2_84_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :play_2_84_subj ;
  lexinfo:directObject     :play_2_84_obj .

:play_2_84_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :play_2_84_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/portrayer> ;
  lemon:subjOfProp  :play_2_84_obj ;
  lemon:objOfProp   :play_2_84_subj ;
  lemon:condition   :play_2_84_sense_1_condition .


:play_2_84_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Actor> .


:play_2_84_obj lemon:marker :form_play_2_84_preposition .

## Prepositions ##
:form_play_2_84_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

