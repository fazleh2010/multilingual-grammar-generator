@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :monarch_263_sense_1_res ;
  lemon:entry    :monarch_263 .

:monarch_263 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :monarch_263_form ;
  lemon:otherForm  :monarch_263_singular ;
  lemon:otherForm  :monarch_263_plural ;
  lemon:sense          :monarch_263_sense_1 ;
  lemon:synBehavior    :monarch_263_predFrame .

:monarch_263_form
 lemon:writtenRep "monarch"@en .

:monarch_263_singular a    lemon:Form ;
  lemon:writtenRep "monarch"@en ;
  lexinfo:number   lexinfo:singular .

:monarch_263_plural a   lemon:Form ;
  lemon:writtenRep "monarchs"@en ;
  lexinfo:number    lexinfo:plural .

:monarch_263_sense_1 a  lemon:LexicalSense ;
  lemon:reference :monarch_263_sense_1_res ;
  lemon:isA       :monarch_263_PredSynArg .

:monarch_263_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Monarch> .