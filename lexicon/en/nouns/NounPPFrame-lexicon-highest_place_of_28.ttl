@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :highest_place_of_28 ;
  lemon:entry    :highest_place_of_28_form_preposition .

:highest_place_of_28 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :highest_place_of_28_form ;
  lemon:otherForm  :highest_place_of_28_singular ;
  lemon:otherForm  :highest_place_of_28_plural ;
  lemon:sense          :highest_place_of_28_sense_1 ;
  lemon:synBehavior    :highest_place_of_28_nounpp .

:highest_place_of_28_form a lemon:Form ;
  lemon:writtenRep "highest place"@en .

:highest_place_of_28_singular a    lemon:Form ;
  lemon:writtenRep "highest place"@en ;
  lexinfo:number   lexinfo:singular .

:highest_place_of_28_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:highest_place_of_28_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_highest_place_of_28 ;
  lexinfo:prepositionalAdjunct :arg2_highest_place_of_28 .

:highest_place_of_28_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :highest_place_of_28_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/highest> ;
  lemon:subjOfProp          :arg2_highest_place_of_28 ;
  lemon:objOfProp           :arg1_highest_place_of_28 ;
  lemon:condition           :highest_place_of_28_sense_1_condition .

:highest_place_of_28_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2_highest_place_of_28 lemon:marker :highest_place_of_28_form_preposition .
## Prepositions ##
:highest_place_of_28_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

