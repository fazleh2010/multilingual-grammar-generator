@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-2223 ;
  lemon:entry    :lexicalEntry-2223_form_preposition .

:lexicalEntry-2223 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lexicalEntry-2223_form ;
  lemon:otherForm  :lexicalEntry-2223_singular ;
  lemon:otherForm  :lexicalEntry-2223_plural ;
  lemon:sense          :lexicalEntry-2223_sense_1 ;
  lemon:synBehavior    :lexicalEntry-2223_nounpp .

:lexicalEntry-2223_form a lemon:Form ;
  lemon:writtenRep "the series"@en .

:lexicalEntry-2223_singular a    lemon:Form ;
  lemon:writtenRep "the series"@en ;
  lexinfo:number   lexinfo:singular .

:lexicalEntry-2223_plural a   lemon:Form ;
  lemon:writtenRep "the series"@en ;
  lexinfo:number    lexinfo:plural .

:lexicalEntry-2223_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:lexicalEntry-2223_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :lexicalEntry-2223_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/numberOfSeasons> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :lexicalEntry-2223_sense_1_condition .

:lexicalEntry-2223_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#nonNegativeInteger> .

:arg2 lemon:marker :lexicalEntry-2223_form_preposition .
## Prepositions ##
:lexicalEntry-2223_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

