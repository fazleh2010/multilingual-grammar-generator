@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lawfirm_of ;
  lemon:entry    :lawfirm_of_form_preposition .

:lawfirm_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lawfirm_of_form ;
  lemon:otherForm  :lawfirm_of_singular ;
  lemon:otherForm  :lawfirm_of_plural ;
  lemon:sense          :lawfirm_of_sense_1 ;
  lemon:synBehavior    :lawfirm_of_nounpp .

:lawfirm_of_form a lemon:Form ;
  lemon:writtenRep "Law Firm"@en .

:lawfirm_of_singular a    lemon:Form ;
  lemon:writtenRep "Law Firm"@en ;
  lexinfo:number   lexinfo:singular .

:lawfirm_of_plural a   lemon:Form ;
  lemon:writtenRep "Law Firms"@en ;
  lexinfo:number    lexinfo:plural .

:lawfirm_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:lawfirm_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :lawfirm_of_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :lawfirm_of_sense_1_condition .

:lawfirm_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/LawFirm> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/LawFirm> .

:arg2 lemon:marker :lawfirm_of_form_preposition .
## Prepositions ##
:lawfirm_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

