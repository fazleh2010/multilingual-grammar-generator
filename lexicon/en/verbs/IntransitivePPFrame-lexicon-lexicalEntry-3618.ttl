@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-3618 ;
  lemon:entry    :form_lexicalEntry-3618_preposition .

:lexicalEntry-3618 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-3618 ;
  lemon:otherForm      :form_lexicalEntry-3618_past ;
  lemon:sense          :lexicalEntry-3618_sense_1 ;
  lemon:synBehavior    :lexicalEntry-3618_frame .

:form_lexicalEntry-3618 a           lemon:Form ;
  lemon:writtenRep     "operate"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-3618 a      lemon:Form ;
  lemon:writtenRep "operates"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-3618_past a lemon:Form ;
  lemon:writtenRep  "operated"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-3618perfect a   lemon:Form ;
  lemon:writtenRep "operated"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-3618_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-3618_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-3618_obj .

:lexicalEntry-3618_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3618_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/closingDate> ;
  lemon:subjOfProp  :lexicalEntry-3618_obj ;
  lemon:objOfProp   :lexicalEntry-3618_subj ;
  lemon:condition   :lexicalEntry-3618_sense_1_condition .

:lexicalEntry-3618_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:lexicalEntry-3618_obj lemon:marker :form_lexicalEntry-3618_preposition .

## Prepositions ##
:form_lexicalEntry-3618_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

