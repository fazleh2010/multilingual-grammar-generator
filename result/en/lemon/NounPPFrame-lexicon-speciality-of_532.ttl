@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :speciality-of_532 ;
  lemon:entry    :speciality-of_532_form_preposition .

:speciality-of_532 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :speciality-of_532_form ;
  lemon:otherForm  :speciality-of_532_singular ;
  lemon:otherForm  :speciality-of_532_plural ;
  lemon:sense          :speciality-of_532_sense_1 ;
  lemon:synBehavior    :speciality-of_532_nounpp .

:speciality-of_532_form a lemon:Form ;
  lemon:writtenRep "speciality"@en .

:speciality-of_532_singular a    lemon:Form ;
  lemon:writtenRep "speciality"@en ;
  lexinfo:number   lexinfo:singular .

:speciality-of_532_plural a   lemon:Form ;
  lemon:writtenRep "specialities"@en ;
  lexinfo:number    lexinfo:plural .

:speciality-of_532_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_speciality-of_532 ;
  lexinfo:prepositionalAdjunct :arg2_speciality-of_532 .

:speciality-of_532_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :speciality-of_532_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/title> ;
  lemon:subjOfProp          :arg2_speciality-of_532 ;
  lemon:objOfProp           :arg1_speciality-of_532 ;
  lemon:condition           :speciality-of_532_sense_1_condition .

:speciality-of_532_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2_speciality-of_532 lemon:marker :speciality-of_532_form_preposition .
## Prepositions ##
:speciality-of_532_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

