@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :produced_on_2_122 ;
  lemon:entry    :form_produced_on_2_122_preposition .

:produced_on_2_122 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_produced_on_2_122 ;
  lemon:otherForm      :form_produced_on_2_122_present3rd ;
  lemon:otherForm      :form_produced_on_2_122_past ;
  lemon:otherForm      :form_produced_on_2_122_perfect ;
  lemon:sense          :produced_on_2_122_sense_1 ;
  lemon:synBehavior    :produced_on_2_122_frame .

:form_produced_on_2_122 a           lemon:Form ;
  lemon:writtenRep     "produce"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_produced_on_2_122_present3rd a      lemon:Form ;
  lemon:writtenRep "produced"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_produced_on_2_122_past a lemon:Form ;
  lemon:writtenRep  "produced"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_produced_on_2_122_perfect a   lemon:Form ;
  lemon:writtenRep "produced"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:produced_on_2_122_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :produced_on_2_122_subj ;
  lexinfo:prepositionalAdjunct :produced_on_2_122_obj .

:produced_on_2_122_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :produced_on_2_122_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/productionDate> ;
  lemon:subjOfProp  :produced_on_2_122_obj ;
  lemon:objOfProp   :produced_on_2_122_subj ;
  lemon:condition   :produced_on_2_122_sense_1_condition .

:produced_on_2_122_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Album> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:produced_on_2_122_obj lemon:marker :form_produced_on_2_122_preposition .

## Prepositions ##
:form_produced_on_2_122_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "on"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

