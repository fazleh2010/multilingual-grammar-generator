@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :part-of ;
  lemon:entry    :part-of_form_preposition .

:part-of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :part-of_form ;
  lemon:otherForm  :part-of_singular ;
  lemon:otherForm  :part-of_plural ;
  lemon:sense          :part-of_sense_1 ;
  lemon:synBehavior    :part-of_nounpp .

:part-of_form a lemon:Form ;
  lemon:writtenRep "part"@en .

:part-of_singular a    lemon:Form ;
  lemon:writtenRep "part"@en ;
  lexinfo:number   lexinfo:singular .

:part-of_plural a   lemon:Form ;
  lemon:writtenRep "parts"@en ;
  lexinfo:number    lexinfo:plural .

:part-of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:part-of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :part-of_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/alliance> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :part-of_sense_1_condition .

:part-of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Agent> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Company> .

:arg2 lemon:marker :part-of_form_preposition .
## Prepositions ##
:part-of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

