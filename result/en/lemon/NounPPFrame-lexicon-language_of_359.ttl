@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :language_of_359 ;
  lemon:entry    :language_of_359_form_preposition .

:language_of_359 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :language_of_359_form ;
  lemon:otherForm  :language_of_359_singular ;
  lemon:otherForm  :language_of_359_plural ;
  lemon:sense          :language_of_359_sense_1 ;
  lemon:synBehavior    :language_of_359_nounpp .

:language_of_359_form a lemon:Form ;

:language_of_359_singular a    lemon:Form ;
  lemon:writtenRep "language"@en ;
  lexinfo:number   lexinfo:singular .

:language_of_359_plural a   lemon:Form ;
  lemon:writtenRep "languages"@en ;
  lexinfo:number    lexinfo:plural .

:language_of_359_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_language_of_359 ;
  lexinfo:prepositionalAdjunct :arg2_language_of_359 .

:language_of_359_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :language_of_359_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_language_of_359 ;
  lemon:objOfProp           :arg1_language_of_359 ;
  lemon:condition           :language_of_359_sense_1_condition .

:language_of_359_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Language> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Language> .

:arg2_language_of_359 lemon:marker :language_of_359_form_preposition .
## Prepositions ##
:language_of_359_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

