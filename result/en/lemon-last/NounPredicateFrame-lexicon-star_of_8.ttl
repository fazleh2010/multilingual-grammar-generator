@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :star_of_8_sense_1_res ;
  lemon:entry    :star_of_8 .

:star_of_8 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :star_of_8_form ;
  lemon:otherForm  :star_of_8_singular ;
  lemon:otherForm  :star_of_8_plural ;
  lemon:sense          :star_of_8_sense_1 ;
  lemon:synBehavior    :star_of_8_predFrame .

:star_of_8_form
 lemon:writtenRep "Star"@en .

:star_of_8_singular a    lemon:Form ;
  lemon:writtenRep "Star"@en ;
  lexinfo:number   lexinfo:singular .

:star_of_8_plural a   lemon:Form ;
  lemon:writtenRep "Stars"@en ;
  lexinfo:number    lexinfo:plural .

:star_of_8_sense_1 a  lemon:LexicalSense ;
  lemon:reference :star_of_8_sense_1_res ;
  lemon:isA       :star_of_8_PredSynArg .

:star_of_8_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Star> .
