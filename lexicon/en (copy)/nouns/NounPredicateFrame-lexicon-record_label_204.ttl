@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :record_label_204_sense_1_res ;
  lemon:entry    :record_label_204 .

:record_label_204 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :record_label_204_form ;
  lemon:otherForm  :record_label_204_singular ;
  lemon:otherForm  :record_label_204_plural ;
  lemon:sense          :record_label_204_sense_1 ;
  lemon:synBehavior    :record_label_204_predFrame .

:record_label_204_form
 lemon:writtenRep "record label"@en .

:record_label_204_singular a    lemon:Form ;
  lemon:writtenRep "record label"@en ;
  lexinfo:number   lexinfo:singular .

:record_label_204_plural a   lemon:Form ;
  lemon:writtenRep "record labels"@en ;
  lexinfo:number    lexinfo:plural .

:record_label_204_sense_1 a  lemon:LexicalSense ;
  lemon:reference :record_label_204_sense_1_res ;
  lemon:isA       :record_label_204_PredSynArg .

:record_label_204_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/RecordLabel> .