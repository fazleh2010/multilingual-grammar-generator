@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :launch_pad_of_201 ;
  lemon:entry    :launch_pad_of_201_form_preposition .

:launch_pad_of_201 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :launch_pad_of_201_form ;
  lemon:otherForm  :launch_pad_of_201_singular ;
  lemon:otherForm  :launch_pad_of_201_plural ;
  lemon:sense          :launch_pad_of_201_sense_1 ;
  lemon:synBehavior    :launch_pad_of_201_nounpp .

:launch_pad_of_201_form a lemon:Form ;
  lemon:writtenRep "launch pad"@en .

:launch_pad_of_201_singular a    lemon:Form ;
  lemon:writtenRep "launch pad"@en ;
  lexinfo:number   lexinfo:singular .

:launch_pad_of_201_plural a   lemon:Form ;
  lemon:writtenRep "launch pads"@en ;
  lexinfo:number    lexinfo:plural .

:launch_pad_of_201_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_launch_pad_of_201 ;
  lexinfo:prepositionalAdjunct :arg2_launch_pad_of_201 .

:launch_pad_of_201_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :launch_pad_of_201_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_launch_pad_of_201 ;
  lemon:objOfProp           :arg1_launch_pad_of_201 ;
  lemon:condition           :launch_pad_of_201_sense_1_condition .

:launch_pad_of_201_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/LaunchPad> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/LaunchPad> .

:arg2_launch_pad_of_201 lemon:marker :launch_pad_of_201_form_preposition .
## Prepositions ##
:launch_pad_of_201_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

