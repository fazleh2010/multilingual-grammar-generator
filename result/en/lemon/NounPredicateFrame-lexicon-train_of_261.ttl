@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :train_of_261_sense_1_res ;
  lemon:entry    :train_of_261 .

:train_of_261 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :train_of_261_form ;
  lemon:otherForm  :train_of_261_singular ;
  lemon:otherForm  :train_of_261_plural ;
  lemon:sense          :train_of_261_sense_1 ;
  lemon:synBehavior    :train_of_261_predFrame .

:train_of_261_form
 lemon:writtenRep "Train"@en .

:train_of_261_singular a    lemon:Form ;
  lemon:writtenRep "Train"@en ;
  lexinfo:number   lexinfo:singular .

:train_of_261_plural a   lemon:Form ;
  lemon:writtenRep "Trains"@en ;
  lexinfo:number    lexinfo:plural .

:train_of_261_sense_1 a  lemon:LexicalSense ;
  lemon:reference :train_of_261_sense_1_res ;
  lemon:isA       :train_of_261_PredSynArg .

:train_of_261_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Train> .
