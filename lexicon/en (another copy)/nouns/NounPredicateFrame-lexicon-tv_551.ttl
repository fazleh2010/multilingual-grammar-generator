@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :tv_551_sense_1_res ;
  lemon:entry    :tv_551 .

:tv_551 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :tv_551_form ;
  lemon:otherForm  :tv_551_singular ;
  lemon:otherForm  :tv_551_plural ;
  lemon:sense          :tv_551_sense_1 ;
  lemon:synBehavior    :tv_551_predFrame .

:tv_551_form
 lemon:writtenRep "television show"@en .

:tv_551_singular a    lemon:Form ;
  lemon:writtenRep "television show"@en ;
  lexinfo:number   lexinfo:singular .

:tv_551_plural a   lemon:Form ;
  lemon:writtenRep "television shows"@en ;
  lexinfo:number    lexinfo:plural .

:tv_551_sense_1 a  lemon:LexicalSense ;
  lemon:reference :tv_551_sense_1_res ;
  lemon:isA       :tv_551_PredSynArg .

:tv_551_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/TelevisionShow> .