@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-2289 ;
  lemon:entry    :lexicalEntry-2289ed ;
  lemon:entry    :form_lexicalEntry-2289_preposition .

:to_lexicalEntry-2289 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-2289 ;
  lemon:otherForm      :form_lexicalEntry-2289s ;
  lemon:otherForm      :form_lexicalEntry-2289ed ;
  lemon:otherForm      :form_lexicalEntry-2289perfect ;
  lemon:sense          :lexicalEntry-2289_sense_1 ;
  lemon:synBehavior    :lexicalEntry-2289_frame_transitive .

:form_lexicalEntry-2289 a         lemon:Form ;
  lemon:writtenRep     "serve"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-2289s a    lemon:Form ;
  lemon:writtenRep "serves"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-2289ed a   lemon:Form ;
  lemon:writtenRep "served"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-2289perfect a   lemon:Form ;
  lemon:writtenRep "served"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-2289_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-2289_obj ;
  lexinfo:directObject     :lexicalEntry-2289_subj .

:lexicalEntry-2289_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2289_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/office> ;
  lemon:subjOfProp  :lexicalEntry-2289_obj ;
  lemon:objOfProp   :lexicalEntry-2289_subj ;
  lemon:condition   :lexicalEntry-2289_sense_1_condition .


:lexicalEntry-2289_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#string> .

:lexicalEntry-2289ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-2289ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-2289ed_by ;
  lemon:synBehavior    :lexicalEntry-2289ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-2289ed_ontomap .

:form_lexicalEntry-2289ed_canonical a lemon:Form ;
  lemon:writtenRep         "serve"@en .

:form_lexicalEntry-2289ed_by a    lemon:Form ;
  lemon:writtenRep     "serve"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-2289ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-2289ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-2289ed_obj .

:lexicalEntry-2289ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2289ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/office> ;
  lemon:subjOfProp  :lexicalEntry-2289ed_subj ;
  lemon:objOfProp   :lexicalEntry-2289ed_obj ;
  lemon:condition   :lexicalEntry-2289_sense_1_condition .
:lexicalEntry-2289ed_obj lemon:marker :form_lexicalEntry-2289_preposition .

## Prepositions ##
:form_lexicalEntry-2289_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

