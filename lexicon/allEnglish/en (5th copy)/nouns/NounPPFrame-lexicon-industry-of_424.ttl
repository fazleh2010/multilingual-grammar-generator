  lemon:otherForm  :industry-of_424_plural ;
  lemon:sense          :industry-of_424_sense_1 ;
  lemon:synBehavior    :industry-of_424_nounpp .

:industry-of_424_form a lemon:Form ;
  lemon:writtenRep "industry"@en .

