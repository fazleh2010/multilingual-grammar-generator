:form_work_for_1_213_present3rd a      lemon:Form ;
  lemon:writtenRep "works"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_work_for_1_213_past a lemon:Form ;
