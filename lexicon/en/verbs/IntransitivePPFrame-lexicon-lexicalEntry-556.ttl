@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-556 ;
  lemon:entry    :form_lexicalEntry-556_preposition .

:lexicalEntry-556 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-556 ;
  lemon:otherForm      :form_lexicalEntry-556_past ;
  lemon:sense          :lexicalEntry-556_sense_1 ;
  lemon:synBehavior    :lexicalEntry-556_frame .

:form_lexicalEntry-556 a           lemon:Form ;
  lemon:writtenRep     "publish"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-556 a      lemon:Form ;
  lemon:writtenRep "publishes"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-556_past a lemon:Form ;
  lemon:writtenRep  "published"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-556perfect a   lemon:Form ;
  lemon:writtenRep "published"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-556_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-556_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-556_obj .

:lexicalEntry-556_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-556_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/developer> ;
  lemon:subjOfProp  :lexicalEntry-556_obj ;
  lemon:objOfProp   :lexicalEntry-556_subj ;
  lemon:condition   :lexicalEntry-556_sense_1_condition .

:lexicalEntry-556_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/VideoGame> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Agent> .


:lexicalEntry-556_obj lemon:marker :form_lexicalEntry-556_preposition .

## Prepositions ##
:form_lexicalEntry-556_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

