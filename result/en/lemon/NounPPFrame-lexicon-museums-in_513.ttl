@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :museums-in_513 ;
  lemon:entry    :museums-in_513_form_preposition .

:museums-in_513 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :museums-in_513_form ;
  lemon:otherForm  :museums-in_513_singular ;
  lemon:otherForm  :museums-in_513_plural ;
  lemon:sense          :museums-in_513_sense_1 ;
  lemon:synBehavior    :museums-in_513_nounpp .

:museums-in_513_form a lemon:Form ;

:museums-in_513_singular a    lemon:Form ;
  lemon:writtenRep "museum"@en ;
  lexinfo:number   lexinfo:singular .

:museums-in_513_plural a   lemon:Form ;
  lemon:writtenRep "museums"@en ;
  lexinfo:number    lexinfo:plural .

:museums-in_513_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_museums-in_513 ;
  lexinfo:prepositionalAdjunct :arg2_museums-in_513 .

:museums-in_513_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :museums-in_513_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/location> ;
  lemon:subjOfProp          :arg2_museums-in_513 ;
  lemon:objOfProp           :arg1_museums-in_513 ;
  lemon:condition           :museums-in_513_sense_1_condition .

:museums-in_513_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Settlement> .

:arg2_museums-in_513 lemon:marker :museums-in_513_form_preposition .
## Prepositions ##
:museums-in_513_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

