@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :area_of_284 ;
  lemon:entry    :area_of_284_form_preposition .

:area_of_284 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :area_of_284_form ;
  lemon:otherForm  :area_of_284_singular ;
  lemon:otherForm  :area_of_284_plural ;
  lemon:sense          :area_of_284_sense_1 ;
  lemon:synBehavior    :area_of_284_nounpp .

:area_of_284_form a lemon:Form ;
  lemon:writtenRep "area"@en .

:area_of_284_singular a    lemon:Form ;
  lemon:writtenRep "area"@en ;
  lexinfo:number   lexinfo:singular .

:area_of_284_plural a   lemon:Form ;
  lemon:writtenRep "areas"@en ;
  lexinfo:number    lexinfo:plural .

:area_of_284_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :null ;
  lexinfo:prepositionalAdjunct :null .

:area_of_284_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :area_of_284_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/areaTotal> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :area_of_284_sense_1_condition .

:area_of_284_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:null lemon:marker :area_of_284_form_preposition .
## Prepositions ##
:area_of_284_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

