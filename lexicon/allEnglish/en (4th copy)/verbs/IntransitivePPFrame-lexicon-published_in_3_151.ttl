@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :published_in_3_151 ;
  lemon:entry    :form_published_in_3_151_preposition .

:published_in_3_151 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_published_in_3_151 ;
  lemon:otherForm      :form_published_in_3_151_present3rd ;
  lemon:otherForm      :form_published_in_3_151_past ;
  lemon:otherForm      :form_published_in_3_151_perfect ;
  lemon:sense          :published_in_3_151_sense_1 ;
  lemon:synBehavior    :published_in_3_151_frame .

:form_published_in_3_151 a           lemon:Form ;
  lemon:writtenRep     "publish"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_published_in_3_151_present3rd a      lemon:Form ;
  lemon:writtenRep "publishes"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_published_in_3_151_past a lemon:Form ;
  lemon:writtenRep  "published"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_published_in_3_151_perfect a   lemon:Form ;
  lemon:writtenRep "published"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:published_in_3_151_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :published_in_3_151_subj ;
  lexinfo:prepositionalAdjunct :published_in_3_151_obj .

:published_in_3_151_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :published_in_3_151_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/publicationDate> ;
  lemon:subjOfProp  :published_in_3_151_subj ;
  lemon:objOfProp   :published_in_3_151_obj ;
  lemon:condition   :published_in_3_151_sense_1_condition .

:published_in_3_151_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Book> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#gYear> .


:published_in_3_151_obj lemon:marker :form_published_in_3_151_preposition .

## Prepositions ##
:form_published_in_3_151_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

