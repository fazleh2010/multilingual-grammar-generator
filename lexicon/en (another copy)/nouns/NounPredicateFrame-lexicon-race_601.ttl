@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :race_601_sense_1_res ;
  lemon:entry    :race_601 .

:race_601 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :race_601_form ;
  lemon:otherForm  :race_601_singular ;
  lemon:otherForm  :race_601_plural ;
  lemon:sense          :race_601_sense_1 ;
  lemon:synBehavior    :race_601_predFrame .

:race_601_form
 lemon:writtenRep "race"@en .

:race_601_singular a    lemon:Form ;
  lemon:writtenRep "race"@en ;
  lexinfo:number   lexinfo:singular .

:race_601_plural a   lemon:Form ;
  lemon:writtenRep "races"@en ;
  lexinfo:number    lexinfo:plural .

:race_601_sense_1 a  lemon:LexicalSense ;
  lemon:reference :race_601_sense_1_res ;
  lemon:isA       :race_601_PredSynArg .

:race_601_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Race> .