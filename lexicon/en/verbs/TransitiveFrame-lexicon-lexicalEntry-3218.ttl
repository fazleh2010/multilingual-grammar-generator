@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-3218 ;
  lemon:entry    :lexicalEntry-3218ed ;
  lemon:entry    :form_lexicalEntry-3218_preposition .

:to_lexicalEntry-3218 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-3218 ;
  lemon:otherForm      :form_lexicalEntry-3218s ;
  lemon:otherForm      :form_lexicalEntry-3218ed ;
  lemon:otherForm      :form_lexicalEntry-3218perfect ;
  lemon:sense          :lexicalEntry-3218_sense_1 ;
  lemon:synBehavior    :lexicalEntry-3218_frame_transitive .

:form_lexicalEntry-3218 a         lemon:Form ;
  lemon:writtenRep     "held"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-3218s a    lemon:Form ;
  lemon:writtenRep "held"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-3218ed a   lemon:Form ;
  lemon:writtenRep "held"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-3218perfect a   lemon:Form ;
  lemon:writtenRep "held"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-3218_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-3218_obj ;
  lexinfo:directObject     :lexicalEntry-3218_subj .

:lexicalEntry-3218_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3218_sense_1 ;
  lemon:reference   <http://dbpedia.org/property/country> ;
  lemon:subjOfProp  :lexicalEntry-3218_obj ;
  lemon:objOfProp   :lexicalEntry-3218_subj ;
  lemon:condition   :lexicalEntry-3218_sense_1_condition .


:lexicalEntry-3218_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Country> .

:lexicalEntry-3218ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-3218ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-3218ed_by ;
  lemon:synBehavior    :lexicalEntry-3218ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-3218ed_ontomap .

:form_lexicalEntry-3218ed_canonical a lemon:Form ;
  lemon:writtenRep         "held"@en .

:form_lexicalEntry-3218ed_by a    lemon:Form ;
  lemon:writtenRep     "held"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-3218ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-3218ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-3218ed_obj .

:lexicalEntry-3218ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3218ed_ontomap ;
  lemon:reference   <http://dbpedia.org/property/country> ;
  lemon:subjOfProp  :lexicalEntry-3218ed_subj ;
  lemon:objOfProp   :lexicalEntry-3218ed_obj ;
  lemon:condition   :lexicalEntry-3218_sense_1_condition .
:lexicalEntry-3218ed_obj lemon:marker :form_lexicalEntry-3218_preposition .

## Prepositions ##
:form_lexicalEntry-3218_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

