@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :writer_of ;
  lemon:entry    :writer_of_form_preposition .

:writer_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :writer_of_form ;
  lemon:otherForm  :writer_of_singular ;
  lemon:otherForm  :writer_of_plural ;
  lemon:sense          :writer_of_sense_1 ;
  lemon:synBehavior    :writer_of_nounpp .

:writer_of_form a lemon:Form ;
  lemon:writtenRep "writer"@en .

:writer_of_singular a    lemon:Form ;
  lemon:writtenRep "writer"@en ;
  lexinfo:number   lexinfo:singular .

:writer_of_plural a   lemon:Form ;
  lemon:writtenRep "writers"@en ;
  lexinfo:number    lexinfo:plural .

:writer_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:writer_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :writer_of_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/writer> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :writer_of_sense_1_condition .

:writer_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/MusicalWork> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Actor> .

:arg2 lemon:marker :writer_of_form_preposition .
## Prepositions ##
:writer_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

