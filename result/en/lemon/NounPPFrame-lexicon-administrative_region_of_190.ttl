@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :administrative_region_of_190 ;
  lemon:entry    :administrative_region_of_190_form_preposition .

:administrative_region_of_190 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :administrative_region_of_190_form ;
  lemon:otherForm  :administrative_region_of_190_singular ;
  lemon:otherForm  :administrative_region_of_190_plural ;
  lemon:sense          :administrative_region_of_190_sense_1 ;
  lemon:synBehavior    :administrative_region_of_190_nounpp .

:administrative_region_of_190_form a lemon:Form ;

:administrative_region_of_190_singular a    lemon:Form ;
  lemon:writtenRep "administrative region"@en ;
  lexinfo:number   lexinfo:singular .

:administrative_region_of_190_plural a   lemon:Form ;
  lemon:writtenRep "administrative regions"@en ;
  lexinfo:number    lexinfo:plural .

:administrative_region_of_190_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_administrative_region_of_190 ;
  lexinfo:prepositionalAdjunct :arg2_administrative_region_of_190 .

:administrative_region_of_190_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :administrative_region_of_190_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_administrative_region_of_190 ;
  lemon:objOfProp           :arg1_administrative_region_of_190 ;
  lemon:condition           :administrative_region_of_190_sense_1_condition .

:administrative_region_of_190_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/AdministrativeRegion> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/AdministrativeRegion> .

:arg2_administrative_region_of_190 lemon:marker :administrative_region_of_190_form_preposition .
## Prepositions ##
:administrative_region_of_190_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

