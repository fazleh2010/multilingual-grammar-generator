@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :play_for_2_4 ;
  lemon:entry    :form_play_for_2_4_preposition .

:play_for_2_4 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_play_for_2_4 ;
  lemon:otherForm      :form_play_for_2_4_present3rd ;
  lemon:otherForm      :form_play_for_2_4_past ;
  lemon:otherForm      :form_play_for_2_4_perfect ;
  lemon:sense          :play_for_2_4_sense_1 ;
  lemon:synBehavior    :play_for_2_4_frame .

:form_play_for_2_4 a           lemon:Form ;
  lemon:writtenRep     "play"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_play_for_2_4_present3rd a      lemon:Form ;
  lemon:writtenRep "plays"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_play_for_2_4_past a lemon:Form ;
  lemon:writtenRep  "played"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_play_for_2_4_perfect a   lemon:Form ;
  lemon:writtenRep "played"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:play_for_2_4_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :play_for_2_4_subj ;
  lexinfo:prepositionalAdjunct :play_for_2_4_obj .

:play_for_2_4_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :play_for_2_4_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/team> ;
  lemon:subjOfProp  :play_for_2_4_subj ;
  lemon:objOfProp   :play_for_2_4_obj ;
  lemon:condition   :play_for_2_4_sense_1_condition .

:play_for_2_4_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/SportsClub> .


:play_for_2_4_obj lemon:marker :form_play_for_2_4_preposition .

## Prepositions ##
:form_play_for_2_4_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "for"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

