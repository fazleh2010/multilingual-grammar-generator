@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_publish_1_45 ;
  lemon:entry    :publish_1_45ed ;
  lemon:entry    :form_publish_1_45_preposition .

:to_publish_1_45 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_publish_1_45 ;
  lemon:otherForm      :form_publish_1_45s ;
  lemon:otherForm      :form_publish_1_45ed ;
  lemon:otherForm      :form_publish_1_45perfect ;
  lemon:sense          :publish_1_45_sense_1 ;
  lemon:synBehavior    :publish_1_45_frame_transitive .

:form_publish_1_45 a         lemon:Form ;
  lemon:writtenRep     "publish"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_publish_1_45s a    lemon:Form ;
  lemon:writtenRep "publishes"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_publish_1_45ed a   lemon:Form ;
  lemon:writtenRep "published"@en ;
  lexinfo:tense    lexinfo:past .

:form_publish_1_45perfect a   lemon:Form ;
  lemon:writtenRep "published"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:publish_1_45_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :publish_1_45_subj ;
  lexinfo:directObject     :publish_1_45_obj .

:publish_1_45_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :publish_1_45_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/publisher> ;
  lemon:subjOfProp  :publish_1_45_obj ;
  lemon:objOfProp   :publish_1_45_subj ;
  lemon:condition   :publish_1_45_sense_1_condition .


:publish_1_45_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Book> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Publisher> .


:publish_1_45_obj lemon:marker :form_publish_1_45_preposition .

## Prepositions ##
:form_publish_1_45_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

