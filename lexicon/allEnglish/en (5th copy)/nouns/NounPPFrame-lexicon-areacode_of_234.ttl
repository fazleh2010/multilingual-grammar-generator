@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :areacode_of_234 ;
  lemon:entry    :areacode_of_234_form_preposition .

:areacode_of_234 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :areacode_of_234_form ;
  lemon:otherForm  :areacode_of_234_singular ;
  lemon:otherForm  :areacode_of_234_plural ;
  lemon:sense          :areacode_of_234_sense_1 ;
  lemon:synBehavior    :areacode_of_234_nounpp .

:areacode_of_234_form a lemon:Form ;
  lemon:writtenRep "area code"@en .

:areacode_of_234_singular a    lemon:Form ;
