@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :firstseason_of_345 ;
  lemon:entry    :firstseason_of_345_form_preposition .

:firstseason_of_345 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :firstseason_of_345_form ;
  lemon:otherForm  :firstseason_of_345_singular ;
  lemon:otherForm  :firstseason_of_345_plural ;
  lemon:sense          :firstseason_of_345_sense_1 ;
  lemon:synBehavior    :firstseason_of_345_nounpp .

:firstseason_of_345_form a lemon:Form ;
  lemon:writtenRep "first season"@en .

:firstseason_of_345_singular a    lemon:Form ;
  lemon:writtenRep "first season"@en ;
  lexinfo:number   lexinfo:singular .

:firstseason_of_345_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:firstseason_of_345_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_firstseason_of_345 ;
  lexinfo:prepositionalAdjunct :arg2_firstseason_of_345 .

:firstseason_of_345_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :firstseason_of_345_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/seasonNumber> ;
  lemon:subjOfProp          :arg2_firstseason_of_345 ;
  lemon:objOfProp           :arg1_firstseason_of_345 ;
  lemon:condition           :firstseason_of_345_sense_1_condition .

:firstseason_of_345_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#nonNegativeInteger> .

:arg2_firstseason_of_345 lemon:marker :firstseason_of_345_form_preposition .
## Prepositions ##
:firstseason_of_345_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

