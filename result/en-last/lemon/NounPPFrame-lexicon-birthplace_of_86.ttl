@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :birthplace_of_86 ;
  lemon:entry    :birthplace_of_86_form_preposition .

:birthplace_of_86 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :birthplace_of_86_form ;
  lemon:otherForm  :birthplace_of_86_singular ;
  lemon:otherForm  :birthplace_of_86_plural ;
  lemon:sense          :birthplace_of_86_sense_1 ;
  lemon:sense          :birthplace_of_86_sense_2 ;
  lemon:synBehavior    :birthplace_of_86_nounpp .

:birthplace_of_86_form a lemon:Form ;
  lemon:writtenRep "birth place"@en .

:birthplace_of_86_singular a    lemon:Form ;
  lemon:writtenRep "birth place"@en ;
  lexinfo:number   lexinfo:singular .

:birthplace_of_86_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:birthplace_of_86_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_birthplace_of_86 ;
  lexinfo:prepositionalAdjunct :arg2_birthplace_of_86 .

:birthplace_of_86_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :birthplace_of_86_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/birthPlace> ;
  lemon:subjOfProp          :arg2_birthplace_of_86 ;
  lemon:objOfProp           :arg1_birthplace_of_86 ;
  lemon:condition           :birthplace_of_86_sense_1_condition .

:birthplace_of_86_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:birthplace_of_86_sense_2 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :birthplace_of_86_sense_2 ;
  lemon:reference           <http://dbpedia.org/ontology/birthPlace> ;
  lemon:subjOfProp          :arg2_birthplace_of_86 ;
  lemon:objOfProp           :arg1_birthplace_of_86 ;
  lemon:condition           :birthplace_of_86_sense_2_condition .

:birthplace_of_86_sense_2_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2_birthplace_of_86 lemon:marker :birthplace_of_86_form_preposition .
## Prepositions ##
:birthplace_of_86_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

