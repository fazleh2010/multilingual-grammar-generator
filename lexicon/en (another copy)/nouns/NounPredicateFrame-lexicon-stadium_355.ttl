@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :stadium_355_sense_1_res ;
  lemon:entry    :stadium_355 .

:stadium_355 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :stadium_355_form ;
  lemon:otherForm  :stadium_355_singular ;
  lemon:otherForm  :stadium_355_plural ;
  lemon:sense          :stadium_355_sense_1 ;
  lemon:synBehavior    :stadium_355_predFrame .

:stadium_355_form
 lemon:writtenRep "stadium"@en .

:stadium_355_singular a    lemon:Form ;
  lemon:writtenRep "stadium"@en ;
  lexinfo:number   lexinfo:singular .

:stadium_355_plural a   lemon:Form ;
  lemon:writtenRep "stadiums"@en ;
  lexinfo:number    lexinfo:plural .

:stadium_355_sense_1 a  lemon:LexicalSense ;
  lemon:reference :stadium_355_sense_1_res ;
  lemon:isA       :stadium_355_PredSynArg .

:stadium_355_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Stadium> .