@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :aircraft_390_sense_1_res ;
  lemon:entry    :aircraft_390 .

:aircraft_390 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :aircraft_390_form ;
  lemon:otherForm  :aircraft_390_singular ;
  lemon:otherForm  :aircraft_390_plural ;
  lemon:sense          :aircraft_390_sense_1 ;
  lemon:synBehavior    :aircraft_390_predFrame .

:aircraft_390_form
 lemon:writtenRep "aircraft"@en .

:aircraft_390_singular a    lemon:Form ;
  lemon:writtenRep "aircraft"@en ;
  lexinfo:number   lexinfo:singular .

:aircraft_390_plural a   lemon:Form ;
  lemon:writtenRep "aircrafts"@en ;
  lexinfo:number    lexinfo:plural .

:aircraft_390_sense_1 a  lemon:LexicalSense ;
  lemon:reference :aircraft_390_sense_1_res ;
  lemon:isA       :aircraft_390_PredSynArg .

:aircraft_390_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Aircraft> .