@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :settlement_385_sense_1_res ;
  lemon:entry    :settlement_385 .

:settlement_385 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :settlement_385_form ;
  lemon:otherForm  :settlement_385_singular ;
  lemon:otherForm  :settlement_385_plural ;
  lemon:sense          :settlement_385_sense_1 ;
  lemon:synBehavior    :settlement_385_predFrame .

:settlement_385_form
 lemon:writtenRep "settlement"@en .

:settlement_385_singular a    lemon:Form ;
  lemon:writtenRep "settlement"@en ;
  lexinfo:number   lexinfo:singular .

:settlement_385_plural a   lemon:Form ;
  lemon:writtenRep "settlements"@en ;
  lexinfo:number    lexinfo:plural .

:settlement_385_sense_1 a  lemon:LexicalSense ;
  lemon:reference :settlement_385_sense_1_res ;
  lemon:isA       :settlement_385_PredSynArg .

:settlement_385_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Settlement> .