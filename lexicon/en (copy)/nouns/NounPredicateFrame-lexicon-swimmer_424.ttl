@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :swimmer_424_sense_1_res ;
  lemon:entry    :swimmer_424 .

:swimmer_424 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :swimmer_424_form ;
  lemon:otherForm  :swimmer_424_singular ;
  lemon:otherForm  :swimmer_424_plural ;
  lemon:sense          :swimmer_424_sense_1 ;
  lemon:synBehavior    :swimmer_424_predFrame .

:swimmer_424_form
 lemon:writtenRep "swimmer"@en .

:swimmer_424_singular a    lemon:Form ;
  lemon:writtenRep "swimmer"@en ;
  lexinfo:number   lexinfo:singular .

:swimmer_424_plural a   lemon:Form ;
  lemon:writtenRep "swimmers"@en ;
  lexinfo:number    lexinfo:plural .

:swimmer_424_sense_1 a  lemon:LexicalSense ;
  lemon:reference :swimmer_424_sense_1_res ;
  lemon:isA       :swimmer_424_PredSynArg .

:swimmer_424_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Swimmer> .