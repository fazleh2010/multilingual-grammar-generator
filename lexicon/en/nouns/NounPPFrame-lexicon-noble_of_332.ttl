@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :noble_of_332 ;
  lemon:entry    :noble_of_332_form_preposition .

:noble_of_332 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :noble_of_332_form ;
  lemon:otherForm  :noble_of_332_singular ;
  lemon:otherForm  :noble_of_332_plural ;
  lemon:sense          :noble_of_332_sense_1 ;
  lemon:synBehavior    :noble_of_332_nounpp .

:noble_of_332_form a lemon:Form ;
  lemon:writtenRep "Noble"@en .

:noble_of_332_singular a    lemon:Form ;
  lemon:writtenRep "Noble"@en ;
  lexinfo:number   lexinfo:singular .

:noble_of_332_plural a   lemon:Form ;
  lemon:writtenRep "Nobles"@en ;
  lexinfo:number    lexinfo:plural .

:noble_of_332_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_noble_of_332 ;
  lexinfo:prepositionalAdjunct :arg2_noble_of_332 .

:noble_of_332_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :noble_of_332_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_noble_of_332 ;
  lemon:objOfProp           :arg1_noble_of_332 ;
  lemon:condition           :noble_of_332_sense_1_condition .

:noble_of_332_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Noble> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Noble> .

:arg2_noble_of_332 lemon:marker :noble_of_332_form_preposition .
## Prepositions ##
:noble_of_332_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

