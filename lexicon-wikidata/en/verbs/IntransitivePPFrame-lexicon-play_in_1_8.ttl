@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :play_in_1_8 ;
  lemon:entry    :form_play_in_1_8_preposition .

:play_in_1_8 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_play_in_1_8 ;
  lemon:otherForm      :form_play_in_1_8_present3rd ;
  lemon:otherForm      :form_play_in_1_8_past ;
  lemon:otherForm      :form_play_in_1_8_perfect ;
  lemon:sense          :play_in_1_8_sense_1 ;
  lemon:synBehavior    :play_in_1_8_frame .

:form_play_in_1_8 a           lemon:Form ;
  lemon:writtenRep     "play"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_play_in_1_8_present3rd a      lemon:Form ;
  lemon:writtenRep "plays"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_play_in_1_8_past a lemon:Form ;
  lemon:writtenRep  "played"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_play_in_1_8_perfect a   lemon:Form ;
  lemon:writtenRep "played"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:play_in_1_8_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :play_in_1_8_subj ;
  lexinfo:prepositionalAdjunct :play_in_1_8_obj .

:play_in_1_8_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :play_in_1_8_ontomap ;
  lemon:reference   <wd:P463> ;
  lemon:subjOfProp  :play_in_1_8_subj ;
  lemon:objOfProp   :play_in_1_8_obj ;
  lemon:condition   :play_in_1_8_sense_1_condition .

:play_in_1_8_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <wd:Q19088> ;
  lemon:propertyRange  <wd:Q24229398> .


:play_in_1_8_obj lemon:marker :form_play_in_1_8_preposition .

## Prepositions ##
:form_play_in_1_8_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

