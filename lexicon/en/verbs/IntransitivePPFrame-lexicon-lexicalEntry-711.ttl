@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-711 ;
  lemon:entry    :form_lexicalEntry-711_preposition .

:lexicalEntry-711 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-711 ;
  lemon:otherForm      :form_lexicalEntry-711_past ;
  lemon:sense          :lexicalEntry-711_sense_1 ;
  lemon:synBehavior    :lexicalEntry-711_frame .

:form_lexicalEntry-711 a           lemon:Form ;
  lemon:writtenRep     "born"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-711 a      lemon:Form ;
  lemon:writtenRep "born"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-711_past a lemon:Form ;
  lemon:writtenRep  "born"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-711perfect a   lemon:Form ;
  lemon:writtenRep "born"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-711_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-711_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-711_obj .

:lexicalEntry-711_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-711_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/doctoralAdvisor> ;
  lemon:subjOfProp  :lexicalEntry-711_obj ;
  lemon:objOfProp   :lexicalEntry-711_subj ;
  lemon:condition   :lexicalEntry-711_sense_1_condition .

:lexicalEntry-711_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Scientist> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:lexicalEntry-711_obj lemon:marker :form_lexicalEntry-711_preposition .

## Prepositions ##
:form_lexicalEntry-711_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

