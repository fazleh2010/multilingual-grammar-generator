:birth-name-of_116_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_birth-name-of_116 ;
  lexinfo:prepositionalAdjunct :arg2_birth-name-of_116 .

:birth-name-of_116_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
