@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-4017 ;
  lemon:entry    :form_lexicalEntry-4017_preposition .

:lexicalEntry-4017 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-4017 ;
  lemon:otherForm      :form_lexicalEntry-4017_past ;
  lemon:sense          :lexicalEntry-4017_sense_1 ;
  lemon:synBehavior    :lexicalEntry-4017_frame .

:form_lexicalEntry-4017 a           lemon:Form ;
  lemon:writtenRep     "released"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-4017 a      lemon:Form ;
  lemon:writtenRep "released"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-4017_past a lemon:Form ;
  lemon:writtenRep  "released"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-4017perfect a   lemon:Form ;
  lemon:writtenRep "released"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-4017_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-4017_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-4017_obj .

:lexicalEntry-4017_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-4017_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/producer> ;
  lemon:subjOfProp  :lexicalEntry-4017_obj ;
  lemon:objOfProp   :lexicalEntry-4017_subj ;
  lemon:condition   :lexicalEntry-4017_sense_1_condition .

:lexicalEntry-4017_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Film> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Company> .


:lexicalEntry-4017_obj lemon:marker :form_lexicalEntry-4017_preposition .

## Prepositions ##
:form_lexicalEntry-4017_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

