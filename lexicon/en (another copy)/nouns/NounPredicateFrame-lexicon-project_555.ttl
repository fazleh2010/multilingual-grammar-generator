@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :project_555_sense_1_res ;
  lemon:entry    :project_555 .

:project_555 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :project_555_form ;
  lemon:otherForm  :project_555_singular ;
  lemon:otherForm  :project_555_plural ;
  lemon:sense          :project_555_sense_1 ;
  lemon:synBehavior    :project_555_predFrame .

:project_555_form
 lemon:writtenRep "project"@en .

:project_555_singular a    lemon:Form ;
  lemon:writtenRep "project"@en ;
  lexinfo:number   lexinfo:singular .

:project_555_plural a   lemon:Form ;
  lemon:writtenRep "projects"@en ;
  lexinfo:number    lexinfo:plural .

:project_555_sense_1 a  lemon:LexicalSense ;
  lemon:reference :project_555_sense_1_res ;
  lemon:isA       :project_555_PredSynArg .

:project_555_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Project> .