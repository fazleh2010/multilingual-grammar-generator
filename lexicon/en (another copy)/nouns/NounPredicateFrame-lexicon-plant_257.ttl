@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :plant_257_sense_1_res ;
  lemon:entry    :plant_257 .

:plant_257 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :plant_257_form ;
  lemon:otherForm  :plant_257_singular ;
  lemon:otherForm  :plant_257_plural ;
  lemon:sense          :plant_257_sense_1 ;
  lemon:synBehavior    :plant_257_predFrame .

:plant_257_form
 lemon:writtenRep "plant"@en .

:plant_257_singular a    lemon:Form ;
  lemon:writtenRep "plant"@en ;
  lexinfo:number   lexinfo:singular .

:plant_257_plural a   lemon:Form ;
  lemon:writtenRep "plants"@en ;
  lexinfo:number    lexinfo:plural .

:plant_257_sense_1 a  lemon:LexicalSense ;
  lemon:reference :plant_257_sense_1_res ;
  lemon:isA       :plant_257_PredSynArg .

:plant_257_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Plant> .