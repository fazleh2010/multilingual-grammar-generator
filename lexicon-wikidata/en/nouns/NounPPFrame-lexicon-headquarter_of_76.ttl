@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :headquarter_of_76 ;
  lemon:entry    :headquarter_of_76_form_preposition .

:headquarter_of_76 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :headquarter_of_76_form ;
  lemon:otherForm  :headquarter_of_76_singular ;
  lemon:otherForm  :headquarter_of_76_plural ;
  lemon:sense          :headquarter_of_76_sense_1 ;
  lemon:sense          :headquarter_of_76_sense_2 ;
  lemon:synBehavior    :headquarter_of_76_nounpp .

:headquarter_of_76_form a lemon:Form ;
  lemon:writtenRep "headquarter"@en .

:headquarter_of_76_singular a    lemon:Form ;
  lemon:writtenRep "headquarter"@en ;
  lexinfo:number   lexinfo:singular .

:headquarter_of_76_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:headquarter_of_76_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_headquarter_of_76 ;
  lexinfo:prepositionalAdjunct :arg2_headquarter_of_76 .

:headquarter_of_76_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :headquarter_of_76_sense_1 ;
  lemon:reference           <wd:P159> ;
  lemon:subjOfProp          :arg2_headquarter_of_76 ;
  lemon:objOfProp           :arg1_headquarter_of_76 ;
  lemon:condition           :headquarter_of_76_sense_1_condition .

:headquarter_of_76_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <wd:Q476028> ;
  lemon:propertyRange   <wd:Q3957> .

:headquarter_of_76_sense_2 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :headquarter_of_76_sense_2 ;
  lemon:reference           <wd:P159> ;
  lemon:subjOfProp          :arg2_headquarter_of_76 ;
  lemon:objOfProp           :arg1_headquarter_of_76 ;
  lemon:condition           :headquarter_of_76_sense_2_condition .

:headquarter_of_76_sense_2_condition a lemon:condition ;
  lemon:propertyDomain  <wd:Q476028> ;
  lemon:propertyRange   <wd:Q3957> .

:arg2_headquarter_of_76 lemon:marker :headquarter_of_76_form_preposition .
## Prepositions ##
:headquarter_of_76_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

