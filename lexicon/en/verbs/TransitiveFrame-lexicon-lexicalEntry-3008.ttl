@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-3008 ;
  lemon:entry    :lexicalEntry-3008ed ;
  lemon:entry    :form_lexicalEntry-3008_preposition .

:to_lexicalEntry-3008 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-3008 ;
  lemon:otherForm      :form_lexicalEntry-3008s ;
  lemon:otherForm      :form_lexicalEntry-3008ed ;
  lemon:otherForm      :form_lexicalEntry-3008perfect ;
  lemon:sense          :lexicalEntry-3008_sense_1 ;
  lemon:synBehavior    :lexicalEntry-3008_frame_transitive .

:form_lexicalEntry-3008 a         lemon:Form ;
  lemon:writtenRep     "serve"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-3008s a    lemon:Form ;
  lemon:writtenRep "serves"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-3008ed a   lemon:Form ;
  lemon:writtenRep "served"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-3008perfect a   lemon:Form ;
  lemon:writtenRep "served"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-3008_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-3008_obj ;
  lexinfo:directObject     :lexicalEntry-3008_subj .

:lexicalEntry-3008_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3008_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/vicePresident> ;
  lemon:subjOfProp  :lexicalEntry-3008_obj ;
  lemon:objOfProp   :lexicalEntry-3008_subj ;
  lemon:condition   :lexicalEntry-3008_sense_1_condition .


:lexicalEntry-3008_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Politician> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Politician> .

:lexicalEntry-3008ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-3008ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-3008ed_by ;
  lemon:synBehavior    :lexicalEntry-3008ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-3008ed_ontomap .

:form_lexicalEntry-3008ed_canonical a lemon:Form ;
  lemon:writtenRep         "serve"@en .

:form_lexicalEntry-3008ed_by a    lemon:Form ;
  lemon:writtenRep     "serve"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-3008ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-3008ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-3008ed_obj .

:lexicalEntry-3008ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3008ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/vicePresident> ;
  lemon:subjOfProp  :lexicalEntry-3008ed_subj ;
  lemon:objOfProp   :lexicalEntry-3008ed_obj ;
  lemon:condition   :lexicalEntry-3008_sense_1_condition .
:lexicalEntry-3008ed_obj lemon:marker :form_lexicalEntry-3008_preposition .

## Prepositions ##
:form_lexicalEntry-3008_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

