@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :astronaut_of_15 ;
  lemon:entry    :astronaut_of_15_form_preposition .

:astronaut_of_15 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :astronaut_of_15_form ;
  lemon:otherForm  :astronaut_of_15_singular ;
  lemon:otherForm  :astronaut_of_15_plural ;
  lemon:sense          :astronaut_of_15_sense_1 ;
  lemon:synBehavior    :astronaut_of_15_nounpp .

:astronaut_of_15_form a lemon:Form ;
  lemon:writtenRep "astronaut "@en .

:astronaut_of_15_singular a    lemon:Form ;
  lemon:writtenRep "astronaut "@en ;
  lexinfo:number   lexinfo:singular .

:astronaut_of_15_plural a   lemon:Form ;
  lemon:writtenRep "astronauts"@en ;
  lexinfo:number    lexinfo:plural .

:astronaut_of_15_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_astronaut_of_15 ;
  lexinfo:prepositionalAdjunct :arg2_astronaut_of_15 .

:astronaut_of_15_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :astronaut_of_15_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/mission> ;
  lemon:subjOfProp          :arg2_astronaut_of_15 ;
  lemon:objOfProp           :arg1_astronaut_of_15 ;
  lemon:condition           :astronaut_of_15_sense_1_condition .

:astronaut_of_15_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/SpaceMission> .

:arg2_astronaut_of_15 lemon:marker :astronaut_of_15_form_preposition .
## Prepositions ##
:astronaut_of_15_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

