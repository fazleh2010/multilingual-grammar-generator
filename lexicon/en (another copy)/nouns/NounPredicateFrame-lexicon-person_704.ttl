@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :person_704_sense_1_res ;
  lemon:entry    :person_704 .

:person_704 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :person_704_form ;
  lemon:otherForm  :person_704_singular ;
  lemon:otherForm  :person_704_plural ;
  lemon:sense          :person_704_sense_1 ;
  lemon:synBehavior    :person_704_predFrame .

:person_704_form
 lemon:writtenRep "person"@en .

:person_704_singular a    lemon:Form ;
  lemon:writtenRep "person"@en ;
  lexinfo:number   lexinfo:singular .

:person_704_plural a   lemon:Form ;
  lemon:writtenRep "persons"@en ;
  lexinfo:number    lexinfo:plural .

:person_704_sense_1 a  lemon:LexicalSense ;
  lemon:reference :person_704_sense_1_res ;
  lemon:isA       :person_704_PredSynArg .

:person_704_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Person> .