@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :greek_12_sense_1_res ;
  lemon:entry    :greek_12 .

:greek_12 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :greek_12_lemma ;
  lemon:sense          :greek_12_sense_1 ;
  lemon:synBehavior    :greek_12_attrFrame, :greek_12_predFrame .

:greek_12_lemma lemon:writtenRep "greek"@en .

:greek_12_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :greek_12_PredSynArg .

:greek_12_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :greek_12_AttrSynArg .

:greek_12_sense_1 a  lemon:LexicalSense ;
  lemon:reference :greek_12_sense_1_res ;
  lemon:isA       :greek_12_AttrSynArg, :greek_12_PredSynArg .

:greek_12_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Greece> .
