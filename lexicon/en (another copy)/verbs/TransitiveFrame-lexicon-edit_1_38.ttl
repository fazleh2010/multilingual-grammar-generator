@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_edit_1_38 ;
  lemon:entry    :edit_1_38ed ;
  lemon:entry    :form_edit_1_38_preposition .

:to_edit_1_38 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_edit_1_38 ;
  lemon:otherForm      :form_edit_1_38s ;
  lemon:otherForm      :form_edit_1_38ed ;
  lemon:otherForm      :form_edit_1_38perfect ;
  lemon:sense          :edit_1_38_sense_1 ;
  lemon:synBehavior    :edit_1_38_frame_transitive .

:form_edit_1_38 a         lemon:Form ;
  lemon:writtenRep     "edit"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_edit_1_38s a    lemon:Form ;
  lemon:writtenRep "edits"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_edit_1_38ed a   lemon:Form ;
  lemon:writtenRep "edited"@en ;
  lexinfo:tense    lexinfo:past .

:form_edit_1_38perfect a   lemon:Form ;
  lemon:writtenRep "edited"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:edit_1_38_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :edit_1_38_subj ;
  lexinfo:directObject     :edit_1_38_obj .

:edit_1_38_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :edit_1_38_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/editor> ;
  lemon:subjOfProp  :edit_1_38_obj ;
  lemon:objOfProp   :edit_1_38_subj ;
  lemon:condition   :edit_1_38_sense_1_condition .


:edit_1_38_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/AcademicJournal> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Agent> .


:edit_1_38_obj lemon:marker :form_edit_1_38_preposition .

## Prepositions ##
:form_edit_1_38_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

