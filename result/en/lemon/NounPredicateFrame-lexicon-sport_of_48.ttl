@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :sport_of_48_sense_1_res ;
  lemon:entry    :sport_of_48 .

:sport_of_48 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :sport_of_48_form ;
  lemon:otherForm  :sport_of_48_singular ;
  lemon:otherForm  :sport_of_48_plural ;
  lemon:sense          :sport_of_48_sense_1 ;
  lemon:synBehavior    :sport_of_48_predFrame .

:sport_of_48_form
 lemon:writtenRep "Sport"@en .

:sport_of_48_singular a    lemon:Form ;
  lemon:writtenRep "Sport"@en ;
  lexinfo:number   lexinfo:singular .

:sport_of_48_plural a   lemon:Form ;
  lemon:writtenRep "Sports"@en ;
  lexinfo:number    lexinfo:plural .

:sport_of_48_sense_1 a  lemon:LexicalSense ;
  lemon:reference :sport_of_48_sense_1_res ;
  lemon:isA       :sport_of_48_PredSynArg .

:sport_of_48_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Sport> .
