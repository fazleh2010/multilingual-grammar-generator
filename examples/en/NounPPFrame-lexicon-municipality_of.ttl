@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :municipality_of ;
  lemon:entry    :municipality_of_form_preposition .

:municipality_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :municipality_of_form ;
  lemon:otherForm  :municipality_of_singular ;
  lemon:otherForm  :municipality_of_plural ;
  lemon:sense          :municipality_of_sense_1 ;
  lemon:synBehavior    :municipality_of_nounpp .

:municipality_of_form a lemon:Form ;
  lemon:writtenRep "municipality"@en .

:municipality_of_singular a    lemon:Form ;
  lemon:writtenRep "municipality"@en ;
  lexinfo:number   lexinfo:singular .

:municipality_of_plural a   lemon:Form ;
  lemon:writtenRep "municipalities"@en ;
  lexinfo:number    lexinfo:plural .

:municipality_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:municipality_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :municipality_of_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/municipality> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :municipality_of_sense_1_condition .

:municipality_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :municipality_of_form_preposition .
## Prepositions ##
:municipality_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

