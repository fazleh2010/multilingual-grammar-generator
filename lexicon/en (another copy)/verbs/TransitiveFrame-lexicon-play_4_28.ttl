@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_play_4_28 ;
  lemon:entry    :play_4_28ed ;
  lemon:entry    :form_play_4_28_preposition .

:to_play_4_28 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_play_4_28 ;
  lemon:otherForm      :form_play_4_28s ;
  lemon:otherForm      :form_play_4_28ed ;
  lemon:otherForm      :form_play_4_28perfect ;
  lemon:sense          :play_4_28_sense_1 ;
  lemon:synBehavior    :play_4_28_frame_transitive .

:form_play_4_28 a         lemon:Form ;
  lemon:writtenRep     "play"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_play_4_28s a    lemon:Form ;
  lemon:writtenRep "playes"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_play_4_28ed a   lemon:Form ;
  lemon:writtenRep "played"@en ;
  lexinfo:tense    lexinfo:past .

:form_play_4_28perfect a   lemon:Form ;
  lemon:writtenRep "played"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:play_4_28_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :play_4_28_obj ;
  lexinfo:directObject     :play_4_28_subj .

:play_4_28_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :play_4_28_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/genre> ;
  lemon:subjOfProp  :play_4_28_obj ;
  lemon:objOfProp   :play_4_28_subj ;
  lemon:condition   :play_4_28_sense_1_condition .


:play_4_28_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/MusicalWork> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:play_4_28_obj lemon:marker :form_play_4_28_preposition .

## Prepositions ##
:form_play_4_28_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

