@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :river_of_234_sense_1_res ;
  lemon:entry    :river_of_234_sense_2_res ;
  lemon:entry    :river_of_234 .

:river_of_234 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :river_of_234_form ;
  lemon:otherForm  :river_of_234_singular ;
  lemon:otherForm  :river_of_234_plural ;
  lemon:sense          :river_of_234_sense_1 ;
  lemon:sense          :river_of_234_sense_2 ;
  lemon:synBehavior    :river_of_234_predFrame .

:river_of_234_form
 lemon:writtenRep "river"@en .

:river_of_234_singular a    lemon:Form ;
  lemon:writtenRep "river"@en ;
  lexinfo:number   lexinfo:singular .

:river_of_234_plural a   lemon:Form ;
  lemon:writtenRep "rivers"@en ;
  lexinfo:number    lexinfo:plural .

:river_of_234_sense_1 a  lemon:LexicalSense ;
  lemon:reference :river_of_234_sense_1_res ;
  lemon:isA       :river_of_234_PredSynArg .

:river_of_234_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/BodyOfWater> .
:river_of_234_sense_2 a  lemon:LexicalSense ;
  lemon:reference :river_of_234_sense_2_res ;
  lemon:isA       :river_of_234_PredSynArg .

:river_of_234_sense_2_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/River> .
