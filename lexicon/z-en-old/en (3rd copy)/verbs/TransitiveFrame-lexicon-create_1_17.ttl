@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_create_1_17 ;
  lemon:entry    :create_1_17ed ;
  lemon:entry    :form_create_1_17_preposition .

:to_create_1_17 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_create_1_17 ;
  lemon:otherForm      :form_create_1_17s ;
  lemon:otherForm      :form_create_1_17ed ;
  lemon:otherForm      :form_create_1_17perfect ;
  lemon:sense          :create_1_17_sense_1 ;
  lemon:sense          :create_1_17_sense_2 ;
  lemon:synBehavior    :create_1_17_frame_transitive .

:form_create_1_17 a         lemon:Form ;
  lemon:writtenRep     "create"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_create_1_17s a    lemon:Form ;
  lemon:writtenRep "creates"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_create_1_17ed a   lemon:Form ;
  lemon:writtenRep "created"@en ;
  lexinfo:tense    lexinfo:past .

:form_create_1_17perfect a   lemon:Form ;
  lemon:writtenRep "created"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:create_1_17_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :create_1_17_subj ;
  lexinfo:directObject     :create_1_17_obj .

:create_1_17_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :create_1_17_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/creator> ;
  lemon:subjOfProp  :create_1_17_obj ;
  lemon:objOfProp   :create_1_17_subj ;
  lemon:condition   :create_1_17_sense_1_condition .


:create_1_17_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Agent> .

:create_1_17_sense_2 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :create_1_17_sense_2 ;
  lemon:reference   <http://dbpedia.org/ontology/author> ;
  lemon:subjOfProp  :create_1_17_obj ;
  lemon:objOfProp   :create_1_17_subj ;
  lemon:condition   :create_1_17_sense_2_condition .


:create_1_17_sense_2_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Book> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:create_1_17_obj lemon:marker :form_create_1_17_preposition .

## Prepositions ##
:form_create_1_17_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

