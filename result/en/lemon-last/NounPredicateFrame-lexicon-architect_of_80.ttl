@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :architect_of_80_sense_1_res ;
  lemon:entry    :architect_of_80 .

:architect_of_80 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :architect_of_80_form ;
  lemon:otherForm  :architect_of_80_singular ;
  lemon:otherForm  :architect_of_80_plural ;
  lemon:sense          :architect_of_80_sense_1 ;
  lemon:synBehavior    :architect_of_80_predFrame .

:architect_of_80_form
 lemon:writtenRep "architect"@en .

:architect_of_80_singular a    lemon:Form ;
  lemon:writtenRep "architect"@en ;
  lexinfo:number   lexinfo:singular .

:architect_of_80_plural a   lemon:Form ;
  lemon:writtenRep "architects"@en ;
  lexinfo:number    lexinfo:plural .

:architect_of_80_sense_1 a  lemon:LexicalSense ;
  lemon:reference :architect_of_80_sense_1_res ;
  lemon:isA       :architect_of_80_PredSynArg .

:architect_of_80_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Architect> .
