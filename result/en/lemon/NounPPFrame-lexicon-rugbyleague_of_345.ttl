:rugbyleague_of_345_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_rugbyleague_of_345 ;
  lexinfo:prepositionalAdjunct :arg2_rugbyleague_of_345 .

:rugbyleague_of_345_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :rugbyleague_of_345_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_rugbyleague_of_345 ;
  lemon:objOfProp           :arg1_rugbyleague_of_345 ;
  lemon:condition           :rugbyleague_of_345_sense_1_condition .

:rugbyleague_of_345_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/RugbyLeague> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/RugbyLeague> .

:arg2_rugbyleague_of_345 lemon:marker :rugbyleague_of_345_form_preposition .
## Prepositions ##
:rugbyleague_of_345_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

