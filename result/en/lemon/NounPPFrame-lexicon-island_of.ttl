@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :island_of ;
  lemon:entry    :island_of_form_preposition .

:island_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :island_of_form ;
  lemon:otherForm  :island_of_singular ;
  lemon:otherForm  :island_of_plural ;
  lemon:sense          :island_of_sense_1 ;
  lemon:synBehavior    :island_of_nounpp .

:island_of_form a lemon:Form ;
  lemon:writtenRep "Island"@en .

:island_of_singular a    lemon:Form ;
  lemon:writtenRep "Island"@en ;
  lexinfo:number   lexinfo:singular .

:island_of_plural a   lemon:Form ;
  lemon:writtenRep "Islands"@en ;
  lexinfo:number    lexinfo:plural .

:island_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:island_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :island_of_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :island_of_sense_1_condition .

:island_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Island> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Island> .

:arg2 lemon:marker :island_of_form_preposition .
## Prepositions ##
:island_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

