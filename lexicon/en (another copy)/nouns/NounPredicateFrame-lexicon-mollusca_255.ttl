@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :mollusca_255_sense_1_res ;
  lemon:entry    :mollusca_255 .

:mollusca_255 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :mollusca_255_form ;
  lemon:otherForm  :mollusca_255_singular ;
  lemon:otherForm  :mollusca_255_plural ;
  lemon:sense          :mollusca_255_sense_1 ;
  lemon:synBehavior    :mollusca_255_predFrame .

:mollusca_255_form
 lemon:writtenRep "mollusca"@en .

:mollusca_255_singular a    lemon:Form ;
  lemon:writtenRep "mollusca"@en ;
  lexinfo:number   lexinfo:singular .

:mollusca_255_plural a   lemon:Form ;
  lemon:writtenRep "molluscas"@en ;
  lexinfo:number    lexinfo:plural .

:mollusca_255_sense_1 a  lemon:LexicalSense ;
  lemon:reference :mollusca_255_sense_1_res ;
  lemon:isA       :mollusca_255_PredSynArg .

:mollusca_255_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Mollusca> .