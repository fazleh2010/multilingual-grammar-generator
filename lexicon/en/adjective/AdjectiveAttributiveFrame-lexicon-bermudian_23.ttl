@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :bermudian_23_sense_1_res ;
  lemon:entry    :bermudian_23 .

:bermudian_23 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :bermudian_23_lemma ;
  lemon:sense          :bermudian_23_sense_1 ;
  lemon:synBehavior    :bermudian_23_attrFrame, :bermudian_23_predFrame .

:bermudian_23_lemma lemon:writtenRep "bermudian"@en .

:bermudian_23_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :bermudian_23_PredSynArg .

:bermudian_23_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :bermudian_23_AttrSynArg .

:bermudian_23_sense_1 a  lemon:LexicalSense ;
  lemon:reference :bermudian_23_sense_1_res ;
  lemon:isA       :bermudian_23_AttrSynArg, :bermudian_23_PredSynArg .

:bermudian_23_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Bermuda> .
