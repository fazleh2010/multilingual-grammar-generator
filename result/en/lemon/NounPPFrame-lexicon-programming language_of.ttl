@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :programming language_of ;
  lemon:entry    :programming language_of_form_preposition .

:programming language_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :programming language_of_form ;
  lemon:otherForm  :programming language_of_singular ;
  lemon:otherForm  :programming language_of_plural ;
  lemon:sense          :programming language_of_sense_1 ;
  lemon:synBehavior    :programming language_of_nounpp .

:programming language_of_form a lemon:Form ;
  lemon:writtenRep "programming language"@en .

:programming language_of_singular a    lemon:Form ;
  lemon:writtenRep "programming language"@en ;
  lexinfo:number   lexinfo:singular .

:programming language_of_plural a   lemon:Form ;
  lemon:writtenRep "programming languages"@en ;
  lexinfo:number    lexinfo:plural .

:programming language_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:programming language_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :programming language_of_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :programming language_of_sense_1_condition .

:programming language_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ProgrammingLanguage> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/ProgrammingLanguage> .

:arg2 lemon:marker :programming language_of_form_preposition .
## Prepositions ##
:programming language_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

