@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-2288 ;
  lemon:entry    :lexicalEntry-2288ed ;
  lemon:entry    :form_lexicalEntry-2288_preposition .

:to_lexicalEntry-2288 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-2288 ;
  lemon:otherForm      :form_lexicalEntry-2288s ;
  lemon:otherForm      :form_lexicalEntry-2288ed ;
  lemon:otherForm      :form_lexicalEntry-2288perfect ;
  lemon:sense          :lexicalEntry-2288_sense_1 ;
  lemon:synBehavior    :lexicalEntry-2288_frame_transitive .

:form_lexicalEntry-2288 a         lemon:Form ;
  lemon:writtenRep     "serve"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-2288s a    lemon:Form ;
  lemon:writtenRep "serves"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-2288ed a   lemon:Form ;
  lemon:writtenRep "served"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-2288perfect a   lemon:Form ;
  lemon:writtenRep "served"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-2288_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-2288_subj ;
  lexinfo:directObject     :lexicalEntry-2288_obj .

:lexicalEntry-2288_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2288_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/office> ;
  lemon:subjOfProp  :lexicalEntry-2288_obj ;
  lemon:objOfProp   :lexicalEntry-2288_subj ;
  lemon:condition   :lexicalEntry-2288_sense_1_condition .


:lexicalEntry-2288_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#string> .

:lexicalEntry-2288ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-2288ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-2288ed_by ;
  lemon:synBehavior    :lexicalEntry-2288ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-2288ed_ontomap .

:form_lexicalEntry-2288ed_canonical a lemon:Form ;
  lemon:writtenRep         "serve"@en .

:form_lexicalEntry-2288ed_by a    lemon:Form ;
  lemon:writtenRep     "serve"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-2288ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-2288ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-2288ed_obj .

:lexicalEntry-2288ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2288ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/office> ;
  lemon:subjOfProp  :lexicalEntry-2288ed_subj ;
  lemon:objOfProp   :lexicalEntry-2288ed_obj ;
  lemon:condition   :lexicalEntry-2288_sense_1_condition .
:lexicalEntry-2288ed_obj lemon:marker :form_lexicalEntry-2288_preposition .

## Prepositions ##
:form_lexicalEntry-2288_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

