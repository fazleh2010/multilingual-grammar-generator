@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :squashplayer_of_37 ;
  lemon:entry    :squashplayer_of_37_form_preposition .

:squashplayer_of_37 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :squashplayer_of_37_form ;
  lemon:otherForm  :squashplayer_of_37_singular ;
  lemon:otherForm  :squashplayer_of_37_plural ;
  lemon:sense          :squashplayer_of_37_sense_1 ;
  lemon:synBehavior    :squashplayer_of_37_nounpp .

:squashplayer_of_37_form a lemon:Form ;

:squashplayer_of_37_singular a    lemon:Form ;
  lemon:writtenRep "SquashPlayer"@en ;
  lexinfo:number   lexinfo:singular .

:squashplayer_of_37_plural a   lemon:Form ;
  lemon:writtenRep "SquashPlayers"@en ;
  lexinfo:number    lexinfo:plural .

:squashplayer_of_37_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_squashplayer_of_37 ;
  lexinfo:prepositionalAdjunct :arg2_squashplayer_of_37 .

:squashplayer_of_37_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :squashplayer_of_37_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_squashplayer_of_37 ;
  lemon:objOfProp           :arg1_squashplayer_of_37 ;
  lemon:condition           :squashplayer_of_37_sense_1_condition .

:squashplayer_of_37_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/SquashPlayer> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/SquashPlayer> .

:arg2_squashplayer_of_37 lemon:marker :squashplayer_of_37_form_preposition .
## Prepositions ##
:squashplayer_of_37_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

