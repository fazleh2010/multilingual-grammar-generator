@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :mountain_of_348 ;
  lemon:entry    :mountain_of_348_form_preposition .

:mountain_of_348 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :mountain_of_348_form ;
  lemon:otherForm  :mountain_of_348_singular ;
  lemon:otherForm  :mountain_of_348_plural ;
  lemon:sense          :mountain_of_348_sense_1 ;
  lemon:synBehavior    :mountain_of_348_nounpp .

:mountain_of_348_form a lemon:Form ;

:mountain_of_348_singular a    lemon:Form ;
  lemon:writtenRep "mountain"@en ;
  lexinfo:number   lexinfo:singular .

:mountain_of_348_plural a   lemon:Form ;
  lemon:writtenRep "mountains"@en ;
  lexinfo:number    lexinfo:plural .

:mountain_of_348_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_mountain_of_348 ;
  lexinfo:prepositionalAdjunct :arg2_mountain_of_348 .

:mountain_of_348_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :mountain_of_348_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_mountain_of_348 ;
  lemon:objOfProp           :arg1_mountain_of_348 ;
  lemon:condition           :mountain_of_348_sense_1_condition .

:mountain_of_348_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Mountain> .

:arg2_mountain_of_348 lemon:marker :mountain_of_348_form_preposition .
## Prepositions ##
:mountain_of_348_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

