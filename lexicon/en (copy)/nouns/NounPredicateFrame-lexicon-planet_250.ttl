@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :planet_250_sense_1_res ;
  lemon:entry    :planet_250 .

:planet_250 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :planet_250_form ;
  lemon:otherForm  :planet_250_singular ;
  lemon:otherForm  :planet_250_plural ;
  lemon:sense          :planet_250_sense_1 ;
  lemon:synBehavior    :planet_250_predFrame .

:planet_250_form
 lemon:writtenRep "planet"@en .

:planet_250_singular a    lemon:Form ;
  lemon:writtenRep "planet"@en ;
  lexinfo:number   lexinfo:singular .

:planet_250_plural a   lemon:Form ;
  lemon:writtenRep "planets"@en ;
  lexinfo:number    lexinfo:plural .

:planet_250_sense_1 a  lemon:LexicalSense ;
  lemon:reference :planet_250_sense_1_res ;
  lemon:isA       :planet_250_PredSynArg .

:planet_250_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Planet> .