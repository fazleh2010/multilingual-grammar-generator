@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix oils:     <http://lemon-model.net/oils#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :big_2_88 ;
  lemon:entry    :form_big_2_88_preposition ;
  lemon:entry    :big_2_88_res .

:big_2_88 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_big_2_88 ;
  lemon:otherForm      :form_big_2_88_comperative ;
  lemon:otherForm      :form_big_2_88_superlative ;
  lemon:sense          :big_2_88_sense_1 ;
  lemon:synBehavior    :big_2_88_predFrame .

:form_big_2_88 lemon:writtenRep "big"@en .

:form_big_2_88_comperative lemon:writtenRep "bigger"@en .

:form_big_2_88_superlative lemon:writtenRep "biggest"@en .

:big_2_88_predFrame a        lexinfo:AdjectiveSuperlativeFrame ;
  lexinfo:copulativeSubject :big_2_88_PredSynArg .

:big_2_88_sense_1 a  lemon:LexicalSense ;
  lemon:reference :big_2_88_res ;
  lemon:isA       :big_2_88_PredSynArg ;
  lemon:condition :big_2_88_sense_1_condition .

:big_2_88_res a   oils:CovariantScalar ;
  oils:boundTo  <http://dbpedia.org/ontology/areaTotal> ;
  oils:degree   <http://lemon-model.net/oils/high> .

:big_2_88_sense_1_condition a lemon:condition ;
  lemon:propertyDomain   <http://dbpedia.org/ontology/Lake> ;
  lemon:propertyRange    <http://www.w3.org/2001/XMLSchema#double> .

## Prepositions ##
:form_big_2_88_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

