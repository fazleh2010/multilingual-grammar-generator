@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :organization_of_272 ;
  lemon:entry    :organization_of_272_form_preposition .

:organization_of_272 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :organization_of_272_form ;
  lemon:otherForm  :organization_of_272_singular ;
  lemon:otherForm  :organization_of_272_plural ;
  lemon:sense          :organization_of_272_sense_1 ;
  lemon:synBehavior    :organization_of_272_nounpp .

:organization_of_272_form a lemon:Form ;
  lemon:writtenRep "organization"@en .

:organization_of_272_singular a    lemon:Form ;
  lemon:writtenRep "organization"@en ;
  lexinfo:number   lexinfo:singular .

:organization_of_272_plural a   lemon:Form ;
  lemon:writtenRep "organizations"@en ;
  lexinfo:number    lexinfo:plural .

:organization_of_272_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_organization_of_272 ;
  lexinfo:prepositionalAdjunct :arg2_organization_of_272 .

:organization_of_272_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :organization_of_272_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_organization_of_272 ;
  lemon:objOfProp           :arg1_organization_of_272 ;
  lemon:condition           :organization_of_272_sense_1_condition .

:organization_of_272_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Organisation> .

:arg2_organization_of_272 lemon:marker :organization_of_272_form_preposition .
## Prepositions ##
:organization_of_272_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

