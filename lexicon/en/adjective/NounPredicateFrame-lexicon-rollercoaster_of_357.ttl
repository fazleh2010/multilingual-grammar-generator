@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :rollercoaster_of_357_sense_1_res ;
  lemon:entry    :rollercoaster_of_357 .

:rollercoaster_of_357 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :rollercoaster_of_357_form ;
  lemon:otherForm  :rollercoaster_of_357_singular ;
  lemon:otherForm  :rollercoaster_of_357_plural ;
  lemon:sense          :rollercoaster_of_357_sense_1 ;
  lemon:synBehavior    :rollercoaster_of_357_predFrame .

:rollercoaster_of_357_form
 lemon:writtenRep "Roller Coaster"@en .

:rollercoaster_of_357_singular a    lemon:Form ;
  lemon:writtenRep "Roller Coaster"@en ;
  lexinfo:number   lexinfo:singular .

:rollercoaster_of_357_plural a   lemon:Form ;
  lemon:writtenRep "Roller Coasters"@en ;
  lexinfo:number    lexinfo:plural .

:rollercoaster_of_357_sense_1 a  lemon:LexicalSense ;
  lemon:reference :rollercoaster_of_357_sense_1_res ;
  lemon:isA       :rollercoaster_of_357_PredSynArg .

:rollercoaster_of_357_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/RollerCoaster> .
