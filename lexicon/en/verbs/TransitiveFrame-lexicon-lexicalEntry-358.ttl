@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-358 ;
  lemon:entry    :lexicalEntry-358ed ;
  lemon:entry    :form_lexicalEntry-358_preposition .

:to_lexicalEntry-358 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-358 ;
  lemon:otherForm      :form_lexicalEntry-358s ;
  lemon:otherForm      :form_lexicalEntry-358ed ;
  lemon:otherForm      :form_lexicalEntry-358perfect ;
  lemon:sense          :lexicalEntry-358_sense_1 ;
  lemon:synBehavior    :lexicalEntry-358_frame_transitive .

:form_lexicalEntry-358 a         lemon:Form ;
  lemon:writtenRep     "wrestling"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-358s a    lemon:Form ;
  lemon:writtenRep "wrestling"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-358ed a   lemon:Form ;
  lemon:writtenRep "wrestling"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-358perfect a   lemon:Form ;
  lemon:writtenRep "wrestling"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-358_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-358_subj ;
  lexinfo:directObject     :lexicalEntry-358_obj .

:lexicalEntry-358_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-358_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/date> ;
  lemon:subjOfProp  :lexicalEntry-358_obj ;
  lemon:objOfProp   :lexicalEntry-358_subj ;
  lemon:condition   :lexicalEntry-358_sense_1_condition .


:lexicalEntry-358_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Event> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .

:lexicalEntry-358ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-358ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-358ed_by ;
  lemon:synBehavior    :lexicalEntry-358ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-358ed_ontomap .

:form_lexicalEntry-358ed_canonical a lemon:Form ;
  lemon:writtenRep         "wrestling"@en .

:form_lexicalEntry-358ed_by a    lemon:Form ;
  lemon:writtenRep     "wrestling"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-358ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-358ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-358ed_obj .

:lexicalEntry-358ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-358ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/date> ;
  lemon:subjOfProp  :lexicalEntry-358ed_subj ;
  lemon:objOfProp   :lexicalEntry-358ed_obj ;
  lemon:condition   :lexicalEntry-358_sense_1_condition .
:lexicalEntry-358ed_obj lemon:marker :form_lexicalEntry-358_preposition .

## Prepositions ##
:form_lexicalEntry-358_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

