@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :extend_to_1_7 ;
  lemon:entry    :form_extend_to_1_7_preposition .

:extend_to_1_7 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_extend_to_1_7 ;
  lemon:otherForm      :form_extend_to_1_7_present3rd ;
  lemon:otherForm      :form_extend_to_1_7_past ;
  lemon:otherForm      :form_extend_to_1_7_perfect ;
  lemon:sense          :extend_to_1_7_sense_1 ;
  lemon:synBehavior    :extend_to_1_7_frame .

:form_extend_to_1_7 a           lemon:Form ;
  lemon:writtenRep     "extend"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_extend_to_1_7_present3rd a      lemon:Form ;
  lemon:writtenRep "extends"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_extend_to_1_7_past a lemon:Form ;
  lemon:writtenRep  "extended"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_extend_to_1_7_perfect a   lemon:Form ;
  lemon:writtenRep "extended"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:extend_to_1_7_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :extend_to_1_7_subj ;
  lexinfo:prepositionalAdjunct :extend_to_1_7_obj .

:extend_to_1_7_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :extend_to_1_7_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/country> ;
  lemon:subjOfProp  :extend_to_1_7_subj ;
  lemon:objOfProp   :extend_to_1_7_obj ;
  lemon:condition   :extend_to_1_7_sense_1_condition .

:extend_to_1_7_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Country> .


:extend_to_1_7_obj lemon:marker :form_extend_to_1_7_preposition .

## Prepositions ##
:form_extend_to_1_7_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

