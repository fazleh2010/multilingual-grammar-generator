@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :birthYear_of_433 ;
  lemon:entry    :birthYear_of_433_form_preposition .

:birthYear_of_433 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :birthYear_of_433_form ;
  lemon:otherForm  :birthYear_of_433_singular ;
  lemon:otherForm  :birthYear_of_433_plural ;
  lemon:sense          :birthYear_of_433_sense_1 ;
  lemon:synBehavior    :birthYear_of_433_nounpp .

:birthYear_of_433_form a lemon:Form ;

:birthYear_of_433_singular a    lemon:Form ;
  lemon:writtenRep "birth year"@en ;
  lexinfo:number   lexinfo:singular .

:birthYear_of_433_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:birthYear_of_433_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_birthYear_of_433 ;
  lexinfo:prepositionalAdjunct :arg2_birthYear_of_433 .

:birthYear_of_433_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :birthYear_of_433_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/birthYear> ;
  lemon:subjOfProp          :arg2_birthYear_of_433 ;
  lemon:objOfProp           :arg1_birthYear_of_433 ;
  lemon:condition           :birthYear_of_433_sense_1_condition .

:birthYear_of_433_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#gYear> .

:arg2_birthYear_of_433 lemon:marker :birthYear_of_433_form_preposition .
## Prepositions ##
:birthYear_of_433_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

