@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_win_3_15 ;
  lemon:entry    :win_3_15ed ;
  lemon:entry    :form_win_3_15_preposition .

:to_win_3_15 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_win_3_15 ;
  lemon:otherForm      :form_win_3_15s ;
  lemon:otherForm      :form_win_3_15ed ;
  lemon:otherForm      :form_win_3_15perfect ;
  lemon:sense          :win_3_15_sense_1 ;
  lemon:synBehavior    :win_3_15_frame_transitive .

:form_win_3_15 a         lemon:Form ;
  lemon:writtenRep     "win"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_win_3_15s a    lemon:Form ;
  lemon:writtenRep "wins"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_win_3_15ed a   lemon:Form ;
  lemon:writtenRep "won"@en ;
  lexinfo:tense    lexinfo:past .

:form_win_3_15perfect a   lemon:Form ;
  lemon:writtenRep "won"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:win_3_15_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :win_3_15_subj ;
  lexinfo:directObject     :win_3_15_obj .

:win_3_15_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :win_3_15_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/award> ;
  lemon:subjOfProp  :win_3_15_obj ;
  lemon:objOfProp   :win_3_15_subj ;
  lemon:condition   :win_3_15_sense_1_condition .


:win_3_15_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:win_3_15_obj lemon:marker :form_win_3_15_preposition .

## Prepositions ##
:form_win_3_15_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

