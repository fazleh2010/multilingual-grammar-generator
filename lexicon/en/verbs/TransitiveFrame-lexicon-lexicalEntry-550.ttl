@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-550 ;
  lemon:entry    :lexicalEntry-550ed ;
  lemon:entry    :form_lexicalEntry-550_preposition .

:to_lexicalEntry-550 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-550 ;
  lemon:otherForm      :form_lexicalEntry-550s ;
  lemon:otherForm      :form_lexicalEntry-550ed ;
  lemon:otherForm      :form_lexicalEntry-550perfect ;
  lemon:sense          :lexicalEntry-550_sense_1 ;
  lemon:synBehavior    :lexicalEntry-550_frame_transitive .

:form_lexicalEntry-550 a         lemon:Form ;
  lemon:writtenRep     "develop"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-550s a    lemon:Form ;
  lemon:writtenRep "develops"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-550ed a   lemon:Form ;
  lemon:writtenRep "developed"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-550perfect a   lemon:Form ;
  lemon:writtenRep "developed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-550_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-550_obj ;
  lexinfo:directObject     :lexicalEntry-550_subj .

:lexicalEntry-550_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-550_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/developer> ;
  lemon:subjOfProp  :lexicalEntry-550_obj ;
  lemon:objOfProp   :lexicalEntry-550_subj ;
  lemon:condition   :lexicalEntry-550_sense_1_condition .


:lexicalEntry-550_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/VideoGame> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Agent> .

:lexicalEntry-550ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-550ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-550ed_by ;
  lemon:synBehavior    :lexicalEntry-550ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-550ed_ontomap .

:form_lexicalEntry-550ed_canonical a lemon:Form ;
  lemon:writtenRep         "develop"@en .

:form_lexicalEntry-550ed_by a    lemon:Form ;
  lemon:writtenRep     "develop"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-550ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-550ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-550ed_obj .

:lexicalEntry-550ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-550ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/developer> ;
  lemon:subjOfProp  :lexicalEntry-550ed_subj ;
  lemon:objOfProp   :lexicalEntry-550ed_obj ;
  lemon:condition   :lexicalEntry-550_sense_1_condition .
:lexicalEntry-550ed_obj lemon:marker :form_lexicalEntry-550_preposition .

## Prepositions ##
:form_lexicalEntry-550_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

