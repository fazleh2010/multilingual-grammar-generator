@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :bolivian_960_sense_1_res ;
  lemon:entry    :bolivian_960 .

:bolivian_960 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :bolivian_960_lemma ;
  lemon:sense          :bolivian_960_sense_1 ;
  lemon:synBehavior    :bolivian_960_attrFrame, :bolivian_960_predFrame .

:bolivian_960_lemma lemon:writtenRep "bolivian"@en .

:bolivian_960_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :bolivian_960_PredSynArg .

:bolivian_960_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :bolivian_960_AttrSynArg .

:bolivian_960_sense_1 a  lemon:LexicalSense ;
  lemon:reference :bolivian_960_sense_1_res ;
  lemon:isA       :bolivian_960_AttrSynArg, :bolivian_960_PredSynArg .

:bolivian_960_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Bolivia> .
