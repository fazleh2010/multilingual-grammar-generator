@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_perform_1_51 ;
  lemon:entry    :perform_1_51ed ;
  lemon:entry    :form_perform_1_51_preposition .

:to_perform_1_51 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_perform_1_51 ;
  lemon:otherForm      :form_perform_1_51s ;
  lemon:otherForm      :form_perform_1_51ed ;
  lemon:otherForm      :form_perform_1_51perfect ;
  lemon:sense          :perform_1_51_sense_1 ;
  lemon:synBehavior    :perform_1_51_frame_transitive .

:form_perform_1_51 a         lemon:Form ;
  lemon:writtenRep     "perform"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_perform_1_51s a    lemon:Form ;
  lemon:writtenRep "performs"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_perform_1_51ed a   lemon:Form ;
  lemon:writtenRep "performed"@en ;
  lexinfo:tense    lexinfo:past .

:form_perform_1_51perfect a   lemon:Form ;
  lemon:writtenRep "performed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:perform_1_51_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :perform_1_51_subj ;
  lexinfo:directObject     :perform_1_51_obj .

:perform_1_51_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :perform_1_51_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/artist> ;
  lemon:subjOfProp  :perform_1_51_obj ;
  lemon:objOfProp   :perform_1_51_subj ;
  lemon:condition   :perform_1_51_sense_1_condition .


:perform_1_51_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/MusicalWork> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Agent> .


:perform_1_51_obj lemon:marker :form_perform_1_51_preposition .

## Prepositions ##
:form_perform_1_51_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

