  lemon:subjOfProp          :arg2_foundingyear_of_840 ;
  lemon:objOfProp           :arg1_foundingyear_of_840 ;
  lemon:condition           :foundingyear_of_840_sense_1_condition .

:foundingyear_of_840_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
