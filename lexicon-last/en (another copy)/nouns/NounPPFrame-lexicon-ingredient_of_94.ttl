@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :ingredient_of_94 ;
  lemon:entry    :ingredient_of_94_form_preposition .

:ingredient_of_94 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :ingredient_of_94_form ;
  lemon:otherForm  :ingredient_of_94_singular ;
  lemon:otherForm  :ingredient_of_94_plural ;
  lemon:sense          :ingredient_of_94_sense_1 ;
  lemon:synBehavior    :ingredient_of_94_nounpp .

:ingredient_of_94_form a lemon:Form ;
  lemon:writtenRep "ingredient"@en .

:ingredient_of_94_singular a    lemon:Form ;
  lemon:writtenRep "ingredient"@en ;
  lexinfo:number   lexinfo:singular .

:ingredient_of_94_plural a   lemon:Form ;
  lemon:writtenRep "ingredients"@en ;
  lexinfo:number    lexinfo:plural .

:ingredient_of_94_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_ingredient_of_94 ;
  lexinfo:prepositionalAdjunct :arg2_ingredient_of_94 .

:ingredient_of_94_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :ingredient_of_94_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/ingredient> ;
  lemon:subjOfProp          :arg1_ingredient_of_94 ;
  lemon:objOfProp           :arg2_ingredient_of_94 ;
  lemon:condition           :ingredient_of_94_sense_1_condition .

:ingredient_of_94_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Food> ;
  lemon:propertyRange   <http://www.w3.org/2002/07/owl#Thing> .

:arg2_ingredient_of_94 lemon:marker :ingredient_of_94_form_preposition .
## Prepositions ##
:ingredient_of_94_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

