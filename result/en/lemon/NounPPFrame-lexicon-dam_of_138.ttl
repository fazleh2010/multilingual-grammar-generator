@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :dam_of_138 ;
  lemon:entry    :dam_of_138_form_preposition .

:dam_of_138 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :dam_of_138_form ;
  lemon:otherForm  :dam_of_138_singular ;
  lemon:otherForm  :dam_of_138_plural ;
  lemon:sense          :dam_of_138_sense_1 ;
  lemon:synBehavior    :dam_of_138_nounpp .

:dam_of_138_form a lemon:Form ;

:dam_of_138_singular a    lemon:Form ;
  lemon:writtenRep "Dam"@en ;
  lexinfo:number   lexinfo:singular .

:dam_of_138_plural a   lemon:Form ;
  lemon:writtenRep "Dams"@en ;
  lexinfo:number    lexinfo:plural .

:dam_of_138_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_dam_of_138 ;
  lexinfo:prepositionalAdjunct :arg2_dam_of_138 .

:dam_of_138_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :dam_of_138_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_dam_of_138 ;
  lemon:objOfProp           :arg1_dam_of_138 ;
  lemon:condition           :dam_of_138_sense_1_condition .

:dam_of_138_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Dam> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Dam> .

:arg2_dam_of_138 lemon:marker :dam_of_138_form_preposition .
## Prepositions ##
:dam_of_138_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

