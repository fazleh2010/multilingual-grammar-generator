@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :grand-children_of_101 ;
  lemon:entry    :grand-children_of_101_form_preposition .

:grand-children_of_101 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :grand-children_of_101_form ;
  lemon:otherForm  :grand-children_of_101_singular ;
  lemon:otherForm  :grand-children_of_101_plural ;
  lemon:sense          :grand-children_of_101_sense_1 ;
  lemon:synBehavior    :grand-children_of_101_nounpp .

:grand-children_of_101_form a lemon:Form ;
  lemon:writtenRep "grand child"@en .

:grand-children_of_101_singular a    lemon:Form ;
  lemon:writtenRep "grand child"@en ;
  lexinfo:number   lexinfo:singular .

:grand-children_of_101_plural a   lemon:Form ;
  lemon:writtenRep "grand children"@en ;
  lexinfo:number    lexinfo:plural .

:grand-children_of_101_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_grand-children_of_101 ;
  lexinfo:prepositionalAdjunct :arg2_grand-children_of_101 .

:grand-children_of_101_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :grand-children_of_101_sense_1 ;
  lemon:reference           <wd:P40> ;
  lemon:subjOfProp          :arg2_grand-children_of_101 ;
  lemon:objOfProp           :arg1_grand-children_of_101 ;
  lemon:condition           :grand-children_of_101_sense_1_condition .

:grand-children_of_101_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <wd:Q19088> ;
  lemon:propertyRange   <wd:Q19088> .

:arg2_grand-children_of_101 lemon:marker :grand-children_of_101_form_preposition .
## Prepositions ##
:grand-children_of_101_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

