@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :timezone-of_537 ;
  lemon:entry    :timezone-of_537_form_preposition .

:timezone-of_537 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :timezone-of_537_form ;
  lemon:otherForm  :timezone-of_537_singular ;
  lemon:otherForm  :timezone-of_537_plural ;
  lemon:sense          :timezone-of_537_sense_1 ;
  lemon:synBehavior    :timezone-of_537_nounpp .

:timezone-of_537_form a lemon:Form ;
  lemon:writtenRep "timezone"@en .

:timezone-of_537_singular a    lemon:Form ;
  lemon:writtenRep "timezone"@en ;
  lexinfo:number   lexinfo:singular .

:timezone-of_537_plural a   lemon:Form ;
  lemon:writtenRep "timezones"@en ;
  lexinfo:number    lexinfo:plural .

:timezone-of_537_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :null ;
  lexinfo:prepositionalAdjunct :null .

:timezone-of_537_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :timezone-of_537_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/timeZone> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :timezone-of_537_sense_1_condition .

:timezone-of_537_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/City> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:null lemon:marker :timezone-of_537_form_preposition .
## Prepositions ##
:timezone-of_537_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

