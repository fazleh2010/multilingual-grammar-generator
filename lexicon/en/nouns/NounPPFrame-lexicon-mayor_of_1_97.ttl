:mayor_of_1_97_sense_2 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :mayor_of_1_97_sense_2 ;
  lemon:reference           <http://dbpedia.org/ontology/mayor> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
