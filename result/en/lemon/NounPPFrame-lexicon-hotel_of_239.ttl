@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :hotel_of_239 ;
  lemon:entry    :hotel_of_239_form_preposition .

:hotel_of_239 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :hotel_of_239_form ;
  lemon:otherForm  :hotel_of_239_singular ;
  lemon:otherForm  :hotel_of_239_plural ;
  lemon:sense          :hotel_of_239_sense_1 ;
  lemon:synBehavior    :hotel_of_239_nounpp .

:hotel_of_239_form a lemon:Form ;

:hotel_of_239_singular a    lemon:Form ;
  lemon:writtenRep "Hotel"@en ;
  lexinfo:number   lexinfo:singular .

:hotel_of_239_plural a   lemon:Form ;
  lemon:writtenRep "Hotels"@en ;
  lexinfo:number    lexinfo:plural .

:hotel_of_239_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_hotel_of_239 ;
  lexinfo:prepositionalAdjunct :arg2_hotel_of_239 .

:hotel_of_239_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :hotel_of_239_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_hotel_of_239 ;
  lemon:objOfProp           :arg1_hotel_of_239 ;
  lemon:condition           :hotel_of_239_sense_1_condition .

:hotel_of_239_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Hotel> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Hotel> .

:arg2_hotel_of_239 lemon:marker :hotel_of_239_form_preposition .
## Prepositions ##
:hotel_of_239_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

