@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-3262 ;
  lemon:entry    :lexicalEntry-3262ed ;
  lemon:entry    :form_lexicalEntry-3262_preposition .

:to_lexicalEntry-3262 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-3262 ;
  lemon:otherForm      :form_lexicalEntry-3262s ;
  lemon:otherForm      :form_lexicalEntry-3262ed ;
  lemon:otherForm      :form_lexicalEntry-3262perfect ;
  lemon:sense          :lexicalEntry-3262_sense_1 ;
  lemon:synBehavior    :lexicalEntry-3262_frame_transitive .

:form_lexicalEntry-3262 a         lemon:Form ;
  lemon:writtenRep     "wrestling"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-3262s a    lemon:Form ;
  lemon:writtenRep "wrestling"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-3262ed a   lemon:Form ;
  lemon:writtenRep "wrestling"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-3262perfect a   lemon:Form ;
  lemon:writtenRep "wrestling"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-3262_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-3262_obj ;
  lexinfo:directObject     :lexicalEntry-3262_subj .

:lexicalEntry-3262_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3262_sense_1 ;
  lemon:reference   <http://dbpedia.org/property/date> ;
  lemon:subjOfProp  :lexicalEntry-3262_obj ;
  lemon:objOfProp   :lexicalEntry-3262_subj ;
  lemon:condition   :lexicalEntry-3262_sense_1_condition .


:lexicalEntry-3262_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/MilitaryConflict> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .

:lexicalEntry-3262ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-3262ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-3262ed_by ;
  lemon:synBehavior    :lexicalEntry-3262ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-3262ed_ontomap .

:form_lexicalEntry-3262ed_canonical a lemon:Form ;
  lemon:writtenRep         "wrestling"@en .

:form_lexicalEntry-3262ed_by a    lemon:Form ;
  lemon:writtenRep     "wrestling"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-3262ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-3262ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-3262ed_obj .

:lexicalEntry-3262ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3262ed_ontomap ;
  lemon:reference   <http://dbpedia.org/property/date> ;
  lemon:subjOfProp  :lexicalEntry-3262ed_subj ;
  lemon:objOfProp   :lexicalEntry-3262ed_obj ;
  lemon:condition   :lexicalEntry-3262_sense_1_condition .
:lexicalEntry-3262ed_obj lemon:marker :form_lexicalEntry-3262_preposition .

## Prepositions ##
:form_lexicalEntry-3262_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

