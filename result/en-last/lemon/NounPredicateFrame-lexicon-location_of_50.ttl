@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :location_of_50_sense_1_res ;
  lemon:entry    :location_of_50 .

:location_of_50 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :location_of_50_form ;
  lemon:otherForm  :location_of_50_singular ;
  lemon:otherForm  :location_of_50_plural ;
  lemon:sense          :location_of_50_sense_1 ;
  lemon:synBehavior    :location_of_50_predFrame .

:location_of_50_form
 lemon:writtenRep "location"@en .

:location_of_50_singular a    lemon:Form ;
  lemon:writtenRep "location"@en ;
  lexinfo:number   lexinfo:singular .

:location_of_50_plural a   lemon:Form ;
  lemon:writtenRep "locations"@en ;
  lexinfo:number    lexinfo:plural .

:location_of_50_sense_1 a  lemon:LexicalSense ;
  lemon:reference :location_of_50_sense_1_res ;
  lemon:isA       :location_of_50_PredSynArg .

:location_of_50_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Location> .
