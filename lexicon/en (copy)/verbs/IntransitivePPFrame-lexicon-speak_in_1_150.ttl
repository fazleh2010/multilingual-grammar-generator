@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :speak_in_1_150 ;
  lemon:entry    :form_speak_in_1_150_preposition .

:speak_in_1_150 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_speak_in_1_150 ;
  lemon:otherForm      :form_speak_in_1_150_present3rd ;
  lemon:otherForm      :form_speak_in_1_150_past ;
  lemon:otherForm      :form_speak_in_1_150_perfect ;
  lemon:sense          :speak_in_1_150_sense_1 ;
  lemon:synBehavior    :speak_in_1_150_frame .

:form_speak_in_1_150 a           lemon:Form ;
  lemon:writtenRep     "speak"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_speak_in_1_150_present3rd a      lemon:Form ;
  lemon:writtenRep "speaks"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_speak_in_1_150_past a lemon:Form ;
  lemon:writtenRep  "spoken"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_speak_in_1_150_perfect a   lemon:Form ;
  lemon:writtenRep "spoken"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:speak_in_1_150_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :speak_in_1_150_subj ;
  lexinfo:prepositionalAdjunct :speak_in_1_150_obj .

:speak_in_1_150_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :speak_in_1_150_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/spokenIn> ;
  lemon:subjOfProp  :speak_in_1_150_subj ;
  lemon:objOfProp   :speak_in_1_150_obj ;
  lemon:condition   :speak_in_1_150_sense_1_condition .

:speak_in_1_150_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Language> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Place> .


:speak_in_1_150_obj lemon:marker :form_speak_in_1_150_preposition .

## Prepositions ##
:form_speak_in_1_150_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

