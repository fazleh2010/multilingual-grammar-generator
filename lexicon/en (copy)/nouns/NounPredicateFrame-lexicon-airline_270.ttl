@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :airline_270_sense_1_res ;
  lemon:entry    :airline_270 .

:airline_270 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :airline_270_form ;
  lemon:otherForm  :airline_270_singular ;
  lemon:otherForm  :airline_270_plural ;
  lemon:sense          :airline_270_sense_1 ;
  lemon:synBehavior    :airline_270_predFrame .

:airline_270_form
 lemon:writtenRep "airline"@en .

:airline_270_singular a    lemon:Form ;
  lemon:writtenRep "airline"@en ;
  lexinfo:number   lexinfo:singular .

:airline_270_plural a   lemon:Form ;
  lemon:writtenRep "airlines"@en ;
  lexinfo:number    lexinfo:plural .

:airline_270_sense_1 a  lemon:LexicalSense ;
  lemon:reference :airline_270_sense_1_res ;
  lemon:isA       :airline_270_PredSynArg .

:airline_270_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Airline> .