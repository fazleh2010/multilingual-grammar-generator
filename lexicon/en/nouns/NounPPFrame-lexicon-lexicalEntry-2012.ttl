@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-2012 ;
  lemon:entry    :lexicalEntry-2012_form_preposition .

:lexicalEntry-2012 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lexicalEntry-2012_form ;
  lemon:otherForm  :lexicalEntry-2012_singular ;
  lemon:otherForm  :lexicalEntry-2012_plural ;
  lemon:sense          :lexicalEntry-2012_sense_1 ;
  lemon:synBehavior    :lexicalEntry-2012_nounpp .

:lexicalEntry-2012_form a lemon:Form ;
  lemon:writtenRep "american"@en .

:lexicalEntry-2012_singular a    lemon:Form ;
  lemon:writtenRep "american"@en ;
  lexinfo:number   lexinfo:singular .

:lexicalEntry-2012_plural a   lemon:Form ;
  lemon:writtenRep "american"@en ;
  lexinfo:number    lexinfo:plural .

:lexicalEntry-2012_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg2 ;
  lexinfo:prepositionalAdjunct :arg1 .

:lexicalEntry-2012_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :lexicalEntry-2012_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/movement> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :lexicalEntry-2012_sense_1_condition .

:lexicalEntry-2012_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Artist> ;
  lemon:propertyRange   <http://www.w3.org/2002/07/owl#Thing> .

:arg2 lemon:marker :lexicalEntry-2012_form_preposition .
## Prepositions ##
:lexicalEntry-2012_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

