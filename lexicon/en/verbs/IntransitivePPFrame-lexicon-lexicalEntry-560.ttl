@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-560 ;
  lemon:entry    :form_lexicalEntry-560_preposition .

:lexicalEntry-560 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-560 ;
  lemon:otherForm      :form_lexicalEntry-560_past ;
  lemon:sense          :lexicalEntry-560_sense_1 ;
  lemon:synBehavior    :lexicalEntry-560_frame .

:form_lexicalEntry-560 a           lemon:Form ;
  lemon:writtenRep     "published by"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-560 a      lemon:Form ;
  lemon:writtenRep "published by"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-560_past a lemon:Form ;
  lemon:writtenRep  "published by"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-560perfect a   lemon:Form ;
  lemon:writtenRep "published by"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-560_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-560_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-560_obj .

:lexicalEntry-560_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-560_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/developer> ;
  lemon:subjOfProp  :lexicalEntry-560_obj ;
  lemon:objOfProp   :lexicalEntry-560_subj ;
  lemon:condition   :lexicalEntry-560_sense_1_condition .

:lexicalEntry-560_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/VideoGame> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Agent> .


:lexicalEntry-560_obj lemon:marker :form_lexicalEntry-560_preposition .

## Prepositions ##
:form_lexicalEntry-560_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

