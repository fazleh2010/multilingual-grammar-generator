@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :cyclist_450_sense_1_res ;
  lemon:entry    :cyclist_450 .

:cyclist_450 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :cyclist_450_form ;
  lemon:otherForm  :cyclist_450_singular ;
  lemon:otherForm  :cyclist_450_plural ;
  lemon:sense          :cyclist_450_sense_1 ;
  lemon:synBehavior    :cyclist_450_predFrame .

:cyclist_450_form
 lemon:writtenRep "cyclist"@en .

:cyclist_450_singular a    lemon:Form ;
  lemon:writtenRep "cyclist"@en ;
  lexinfo:number   lexinfo:singular .

:cyclist_450_plural a   lemon:Form ;
  lemon:writtenRep "cyclists"@en ;
  lexinfo:number    lexinfo:plural .

:cyclist_450_sense_1 a  lemon:LexicalSense ;
  lemon:reference :cyclist_450_sense_1_res ;
  lemon:isA       :cyclist_450_PredSynArg .

:cyclist_450_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Cyclist> .