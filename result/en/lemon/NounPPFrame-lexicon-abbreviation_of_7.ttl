@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :abbreviation_of_7 ;
  lemon:entry    :abbreviation_of_7_form_preposition .

:abbreviation_of_7 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :abbreviation_of_7_form ;
  lemon:otherForm  :abbreviation_of_7_singular ;
  lemon:otherForm  :abbreviation_of_7_plural ;
  lemon:sense          :abbreviation_of_7_sense_1 ;
  lemon:synBehavior    :abbreviation_of_7_nounpp .

:abbreviation_of_7_form a lemon:Form ;
  lemon:writtenRep "abbreviation"@en .

:abbreviation_of_7_singular a    lemon:Form ;
  lemon:writtenRep "abbreviation"@en ;
  lexinfo:number   lexinfo:singular .

:abbreviation_of_7_plural a   lemon:Form ;
  lemon:writtenRep "abbreviations"@en ;
  lexinfo:number    lexinfo:plural .

:abbreviation_of_7_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg2_abbreviation_of_7 ;
  lexinfo:prepositionalAdjunct :arg1_abbreviation_of_7 .

:abbreviation_of_7_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :abbreviation_of_7_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/postalabbreviation> ;
  lemon:subjOfProp          :arg1_abbreviation_of_7 ;
  lemon:objOfProp           :arg2_abbreviation_of_7 ;
  lemon:condition           :abbreviation_of_7_sense_1_condition .

:abbreviation_of_7_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg1_abbreviation_of_7 lemon:marker :abbreviation_of_7_form_preposition .
## Prepositions ##
:abbreviation_of_7_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

