@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-4174 ;
  lemon:entry    :form_lexicalEntry-4174_preposition .

:lexicalEntry-4174 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-4174 ;
  lemon:otherForm      :form_lexicalEntry-4174_past ;
  lemon:sense          :lexicalEntry-4174_sense_1 ;
  lemon:synBehavior    :lexicalEntry-4174_frame .

:form_lexicalEntry-4174 a           lemon:Form ;
  lemon:writtenRep     "it flows"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-4174 a      lemon:Form ;
  lemon:writtenRep "it flows"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-4174_past a lemon:Form ;
  lemon:writtenRep  "it flows"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-4174perfect a   lemon:Form ;
  lemon:writtenRep "it flows"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-4174_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-4174_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-4174_obj .

:lexicalEntry-4174_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-4174_ontomap ;
  lemon:reference   <http://dbpedia.org/property/mouthLocation> ;
  lemon:subjOfProp  :lexicalEntry-4174_obj ;
  lemon:objOfProp   :lexicalEntry-4174_subj ;
  lemon:condition   :lexicalEntry-4174_sense_1_condition .

:lexicalEntry-4174_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/River> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Country> .


:lexicalEntry-4174_obj lemon:marker :form_lexicalEntry-4174_preposition .

## Prepositions ##
:form_lexicalEntry-4174_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

