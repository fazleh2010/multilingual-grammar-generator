@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :belarusian_45_sense_1_res ;
  lemon:entry    :belarusian_45 .

:belarusian_45 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :belarusian_45_lemma ;
  lemon:sense          :belarusian_45_sense_1 ;
  lemon:synBehavior    :belarusian_45_attrFrame, :belarusian_45_predFrame .

:belarusian_45_lemma lemon:writtenRep "belarusian"@en .

:belarusian_45_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :belarusian_45_PredSynArg .

:belarusian_45_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :belarusian_45_AttrSynArg .

:belarusian_45_sense_1 a  lemon:LexicalSense ;
  lemon:reference :belarusian_45_sense_1_res ;
  lemon:isA       :belarusian_45_AttrSynArg, :belarusian_45_PredSynArg .

:belarusian_45_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Belarus> .
