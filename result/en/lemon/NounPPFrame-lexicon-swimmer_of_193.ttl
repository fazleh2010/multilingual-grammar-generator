@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :swimmer_of_193 ;
  lemon:entry    :swimmer_of_193_form_preposition .

:swimmer_of_193 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :swimmer_of_193_form ;
  lemon:otherForm  :swimmer_of_193_singular ;
  lemon:otherForm  :swimmer_of_193_plural ;
  lemon:sense          :swimmer_of_193_sense_1 ;
  lemon:synBehavior    :swimmer_of_193_nounpp .

:swimmer_of_193_form a lemon:Form ;
  lemon:writtenRep "swimmer"@en .

:swimmer_of_193_singular a    lemon:Form ;
  lemon:writtenRep "swimmer"@en ;
  lexinfo:number   lexinfo:singular .

:swimmer_of_193_plural a   lemon:Form ;
  lemon:writtenRep "swimmers"@en ;
  lexinfo:number    lexinfo:plural .

:swimmer_of_193_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_swimmer_of_193 ;
  lexinfo:prepositionalAdjunct :arg2_swimmer_of_193 .

:swimmer_of_193_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :swimmer_of_193_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_swimmer_of_193 ;
  lemon:objOfProp           :arg1_swimmer_of_193 ;
  lemon:condition           :swimmer_of_193_sense_1_condition .

:swimmer_of_193_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Swimmer> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Swimmer> .

:arg2_swimmer_of_193 lemon:marker :swimmer_of_193_form_preposition .
## Prepositions ##
:swimmer_of_193_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

