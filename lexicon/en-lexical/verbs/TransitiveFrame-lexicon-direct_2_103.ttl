@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_direct_2_103 ;
  lemon:entry    :direct_2_103ed ;
  lemon:entry    :form_direct_2_103_preposition .

:to_direct_2_103 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_direct_2_103 ;
  lemon:otherForm      :form_direct_2_103s ;
  lemon:otherForm      :form_direct_2_103ed ;
  lemon:otherForm      :form_direct_2_103perfect ;
  lemon:sense          :direct_2_103_sense_1 ;
  lemon:synBehavior    :direct_2_103_frame_transitive .

:form_direct_2_103 a         lemon:Form ;
  lemon:writtenRep     "direct"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_direct_2_103s a    lemon:Form ;
  lemon:writtenRep "directs"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_direct_2_103ed a   lemon:Form ;
  lemon:writtenRep "directed"@en ;
  lexinfo:tense    lexinfo:past .

:form_direct_2_103perfect a   lemon:Form ;
  lemon:writtenRep "directed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:direct_2_103_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :direct_2_103_subj ;
  lexinfo:directObject     :direct_2_103_obj .

:direct_2_103_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :direct_2_103_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/director> ;
  lemon:subjOfProp  :direct_2_103_obj ;
  lemon:objOfProp   :direct_2_103_subj ;
  lemon:condition   :direct_2_103_sense_1_condition .


:direct_2_103_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Software> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:direct_2_103_obj lemon:marker :form_direct_2_103_preposition .

## Prepositions ##
:form_direct_2_103_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

