@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-3607 ;
  lemon:entry    :lexicalEntry-3607ed ;
  lemon:entry    :form_lexicalEntry-3607_preposition .

:to_lexicalEntry-3607 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-3607 ;
  lemon:otherForm      :form_lexicalEntry-3607s ;
  lemon:otherForm      :form_lexicalEntry-3607ed ;
  lemon:otherForm      :form_lexicalEntry-3607perfect ;
  lemon:sense          :lexicalEntry-3607_sense_1 ;
  lemon:synBehavior    :lexicalEntry-3607_frame_transitive .

:form_lexicalEntry-3607 a         lemon:Form ;
  lemon:writtenRep     "closed"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-3607s a    lemon:Form ;
  lemon:writtenRep "closed"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-3607ed a   lemon:Form ;
  lemon:writtenRep "closed"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-3607perfect a   lemon:Form ;
  lemon:writtenRep "closed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-3607_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-3607_subj ;
  lexinfo:directObject     :lexicalEntry-3607_obj .

:lexicalEntry-3607_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3607_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/closingDate> ;
  lemon:subjOfProp  :lexicalEntry-3607_obj ;
  lemon:objOfProp   :lexicalEntry-3607_subj ;
  lemon:condition   :lexicalEntry-3607_sense_1_condition .


:lexicalEntry-3607_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .

:lexicalEntry-3607ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-3607ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-3607ed_by ;
  lemon:synBehavior    :lexicalEntry-3607ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-3607ed_ontomap .

:form_lexicalEntry-3607ed_canonical a lemon:Form ;
  lemon:writtenRep         "closed"@en .

:form_lexicalEntry-3607ed_by a    lemon:Form ;
  lemon:writtenRep     "closed"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-3607ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-3607ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-3607ed_obj .

:lexicalEntry-3607ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3607ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/closingDate> ;
  lemon:subjOfProp  :lexicalEntry-3607ed_subj ;
  lemon:objOfProp   :lexicalEntry-3607ed_obj ;
  lemon:condition   :lexicalEntry-3607_sense_1_condition .
:lexicalEntry-3607ed_obj lemon:marker :form_lexicalEntry-3607_preposition .

## Prepositions ##
:form_lexicalEntry-3607_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

