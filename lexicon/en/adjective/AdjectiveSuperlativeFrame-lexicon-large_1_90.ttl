@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix oils:     <http://lemon-model.net/oils#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :large_1_90 ;
  lemon:entry    :form_large_1_90_preposition ;
  lemon:entry    :large_1_90_res .

:large_1_90 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_large_1_90 ;
  lemon:otherForm      :form_large_1_90_comperative ;
  lemon:otherForm      :form_large_1_90_superlative ;
  lemon:sense          :large_1_90_sense_1 ;
  lemon:synBehavior    :large_1_90_predFrame .

:form_large_1_90 lemon:writtenRep "large"@en .

:form_large_1_90_comperative lemon:writtenRep "larger"@en .

:form_large_1_90_superlative lemon:writtenRep "largest"@en .

:large_1_90_predFrame a        lexinfo:AdjectiveSuperlativeFrame ;
  lexinfo:copulativeSubject :large_1_90_PredSynArg .

:large_1_90_sense_1 a  lemon:LexicalSense ;
  lemon:reference :large_1_90_res ;
  lemon:isA       :large_1_90_PredSynArg ;
  lemon:condition :large_1_90_sense_1_condition .

:large_1_90_res a   oils:CovariantScalar ;
  oils:boundTo  <http://dbpedia.org/ontology/areaTotal> ;
  oils:degree   <http://lemon-model.net/oils/high> .

:large_1_90_sense_1_condition a lemon:condition ;
  lemon:propertyDomain   <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange    <http://www.w3.org/2001/XMLSchema#double> .

## Prepositions ##
:form_large_1_90_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

