@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :president_of_295 ;
  lemon:entry    :president_of_295_form_preposition .

:president_of_295 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :president_of_295_form ;
  lemon:otherForm  :president_of_295_singular ;
  lemon:otherForm  :president_of_295_plural ;
  lemon:sense          :president_of_295_sense_1 ;
  lemon:synBehavior    :president_of_295_nounpp .

:president_of_295_form a lemon:Form ;

:president_of_295_singular a    lemon:Form ;
  lemon:writtenRep "president"@en ;
  lexinfo:number   lexinfo:singular .

:president_of_295_plural a   lemon:Form ;
  lemon:writtenRep "presidents"@en ;
  lexinfo:number    lexinfo:plural .

:president_of_295_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_president_of_295 ;
  lexinfo:prepositionalAdjunct :arg2_president_of_295 .

:president_of_295_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :president_of_295_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_president_of_295 ;
  lemon:objOfProp           :arg1_president_of_295 ;
  lemon:condition           :president_of_295_sense_1_condition .

:president_of_295_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/President> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/President> .

:arg2_president_of_295 lemon:marker :president_of_295_form_preposition .
## Prepositions ##
:president_of_295_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

