@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :danish_15_sense_1_res ;
  lemon:entry    :danish_15 .

:danish_15 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :danish_15_lemma ;
  lemon:sense          :danish_15_sense_1 ;
  lemon:synBehavior    :danish_15_attrFrame, :danish_15_predFrame .

:danish_15_lemma lemon:writtenRep "danish"@en .

:danish_15_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :danish_15_PredSynArg .

:danish_15_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :danish_15_AttrSynArg .

:danish_15_sense_1 a  lemon:LexicalSense ;
  lemon:reference :danish_15_sense_1_res ;
  lemon:isA       :danish_15_AttrSynArg, :danish_15_PredSynArg .

:danish_15_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Denmark> .
