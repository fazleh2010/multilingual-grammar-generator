:wine_region_of_305 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :wine_region_of_305_form ;
  lemon:otherForm  :wine_region_of_305_singular ;
  lemon:otherForm  :wine_region_of_305_plural ;
  lemon:sense          :wine_region_of_305_sense_1 ;
  lemon:synBehavior    :wine_region_of_305_nounpp .

:wine_region_of_305_form a lemon:Form ;
  lemon:writtenRep "wine region"@en .

:wine_region_of_305_singular a    lemon:Form ;
