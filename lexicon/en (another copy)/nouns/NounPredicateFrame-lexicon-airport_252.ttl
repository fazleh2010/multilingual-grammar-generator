@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :airport_252_sense_1_res ;
  lemon:entry    :airport_252 .

:airport_252 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :airport_252_form ;
  lemon:otherForm  :airport_252_singular ;
  lemon:otherForm  :airport_252_plural ;
  lemon:sense          :airport_252_sense_1 ;
  lemon:synBehavior    :airport_252_predFrame .

:airport_252_form
 lemon:writtenRep "airport"@en .

:airport_252_singular a    lemon:Form ;
  lemon:writtenRep "airport"@en ;
  lexinfo:number   lexinfo:singular .

:airport_252_plural a   lemon:Form ;
  lemon:writtenRep "airports"@en ;
  lexinfo:number    lexinfo:plural .

:airport_252_sense_1 a  lemon:LexicalSense ;
  lemon:reference :airport_252_sense_1_res ;
  lemon:isA       :airport_252_PredSynArg .

:airport_252_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Airport> .