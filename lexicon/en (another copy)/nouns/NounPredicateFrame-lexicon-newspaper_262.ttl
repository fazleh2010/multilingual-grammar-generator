@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :newspaper_262_sense_1_res ;
  lemon:entry    :newspaper_262 .

:newspaper_262 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :newspaper_262_form ;
  lemon:otherForm  :newspaper_262_singular ;
  lemon:otherForm  :newspaper_262_plural ;
  lemon:sense          :newspaper_262_sense_1 ;
  lemon:synBehavior    :newspaper_262_predFrame .

:newspaper_262_form
 lemon:writtenRep "newspaper"@en .

:newspaper_262_singular a    lemon:Form ;
  lemon:writtenRep "newspaper"@en ;
  lexinfo:number   lexinfo:singular .

:newspaper_262_plural a   lemon:Form ;
  lemon:writtenRep "newspapers"@en ;
  lexinfo:number    lexinfo:plural .

:newspaper_262_sense_1 a  lemon:LexicalSense ;
  lemon:reference :newspaper_262_sense_1_res ;
  lemon:isA       :newspaper_262_PredSynArg .

:newspaper_262_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Newspaper> .