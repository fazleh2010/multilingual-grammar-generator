@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-620 ;
  lemon:entry    :lexicalEntry-620ed ;
  lemon:entry    :form_lexicalEntry-620_preposition .

:to_lexicalEntry-620 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-620 ;
  lemon:otherForm      :form_lexicalEntry-620s ;
  lemon:otherForm      :form_lexicalEntry-620ed ;
  lemon:otherForm      :form_lexicalEntry-620perfect ;
  lemon:sense          :lexicalEntry-620_sense_1 ;
  lemon:synBehavior    :lexicalEntry-620_frame_transitive .

:form_lexicalEntry-620 a         lemon:Form ;
  lemon:writtenRep     "directed by"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-620s a    lemon:Form ;
  lemon:writtenRep "directed by"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-620ed a   lemon:Form ;
  lemon:writtenRep "directed by"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-620perfect a   lemon:Form ;
  lemon:writtenRep "directed by"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-620_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-620_subj ;
  lexinfo:directObject     :lexicalEntry-620_obj .

:lexicalEntry-620_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-620_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/director> ;
  lemon:subjOfProp  :lexicalEntry-620_obj ;
  lemon:objOfProp   :lexicalEntry-620_subj ;
  lemon:condition   :lexicalEntry-620_sense_1_condition .


:lexicalEntry-620_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Film> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .

:lexicalEntry-620ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-620ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-620ed_by ;
  lemon:synBehavior    :lexicalEntry-620ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-620ed_ontomap .

:form_lexicalEntry-620ed_canonical a lemon:Form ;
  lemon:writtenRep         "directed by"@en .

:form_lexicalEntry-620ed_by a    lemon:Form ;
  lemon:writtenRep     "directed by"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-620ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-620ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-620ed_obj .

:lexicalEntry-620ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-620ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/director> ;
  lemon:subjOfProp  :lexicalEntry-620ed_subj ;
  lemon:objOfProp   :lexicalEntry-620ed_obj ;
  lemon:condition   :lexicalEntry-620_sense_1_condition .
:lexicalEntry-620ed_obj lemon:marker :form_lexicalEntry-620_preposition .

## Prepositions ##
:form_lexicalEntry-620_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

