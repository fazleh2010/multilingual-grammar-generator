@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix oils:     <http://lemon-model.net/oils#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :long_17 ;
  lemon:entry    :form_long_17_preposition ;
  lemon:entry    :long_17_res .

:long_17 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_long_17 ;
  lemon:otherForm      :form_long_17_comperative ;
  lemon:otherForm      :form_long_17_superlative ;
  lemon:sense          :long_17_sense_1 ;
  lemon:sense          :long_17_sense_2 ;
  lemon:synBehavior    :long_17_predFrame .

:form_long_17 lemon:writtenRep "long"@en .

:form_long_17_comperative lemon:writtenRep "longer"@en .

:form_long_17_superlative lemon:writtenRep "longest"@en .

:long_17_predFrame a        lexinfo:AdjectiveSuperlativeFrame ;
  lexinfo:copulativeSubject :long_17_PredSynArg .

:long_17_sense_1 a  lemon:LexicalSense ;
  lemon:reference :long_17_res ;
  lemon:isA       :long_17_PredSynArg ;
  lemon:condition :long_17_sense_1_condition .

:long_17_res a   oils:CovariantScalar ;
  oils:boundTo  <http://dbpedia.org/ontology/length> ;
  oils:degree   <http://lemon-model.net/oils/high> .

:long_17_sense_1_condition a lemon:condition ;
  lemon:propertyDomain   <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange    <http://dbpedia.org/ontology/BodyOfWater> .
:long_17_sense_2 a  lemon:LexicalSense ;
  lemon:reference :long_17_res ;
  lemon:isA       :long_17_PredSynArg ;
  lemon:condition :long_17_sense_2_condition .

:long_17_res a   oils:CovariantScalar ;
  oils:boundTo  <http://dbpedia.org/ontology/length> ;
  oils:degree   <http://lemon-model.net/oils/high> .

:long_17_sense_2_condition a lemon:condition ;
  lemon:propertyDomain   <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange    <http://dbpedia.org/ontology/BodyOfWater> .

## Prepositions ##
:form_long_17_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

