@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :election_of_302 ;
  lemon:entry    :election_of_302_form_preposition .

:election_of_302 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :election_of_302_form ;
  lemon:otherForm  :election_of_302_singular ;
  lemon:otherForm  :election_of_302_plural ;
  lemon:sense          :election_of_302_sense_1 ;
  lemon:synBehavior    :election_of_302_nounpp .

:election_of_302_form a lemon:Form ;

:election_of_302_singular a    lemon:Form ;
  lemon:writtenRep "Election"@en ;
  lexinfo:number   lexinfo:singular .

:election_of_302_plural a   lemon:Form ;
  lemon:writtenRep "Elections"@en ;
  lexinfo:number    lexinfo:plural .

:election_of_302_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_election_of_302 ;
  lexinfo:prepositionalAdjunct :arg2_election_of_302 .

:election_of_302_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :election_of_302_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_election_of_302 ;
  lemon:objOfProp           :arg1_election_of_302 ;
  lemon:condition           :election_of_302_sense_1_condition .

:election_of_302_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Election> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Election> .

:arg2_election_of_302 lemon:marker :election_of_302_form_preposition .
## Prepositions ##
:election_of_302_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

