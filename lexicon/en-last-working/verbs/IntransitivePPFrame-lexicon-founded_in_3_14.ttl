@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :founded_in_3_14 ;
  lemon:entry    :form_founded_in_3_14_preposition .

:founded_in_3_14 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_founded_in_3_14 ;
  lemon:otherForm      :form_founded_in_3_14_present3rd ;
  lemon:otherForm      :form_founded_in_3_14_past ;
  lemon:otherForm      :form_founded_in_3_14_perfect ;
  lemon:sense          :founded_in_3_14_sense_1 ;
  lemon:synBehavior    :founded_in_3_14_frame .

:form_founded_in_3_14 a           lemon:Form ;
  lemon:writtenRep     "found"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_founded_in_3_14_present3rd a      lemon:Form ;
  lemon:writtenRep "founds"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_founded_in_3_14_past a lemon:Form ;
  lemon:writtenRep  "founded"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_founded_in_3_14_perfect a   lemon:Form ;
  lemon:writtenRep "founded"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:founded_in_3_14_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :founded_in_3_14_subj ;
  lexinfo:prepositionalAdjunct :founded_in_3_14_obj .

:founded_in_3_14_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :founded_in_3_14_ontomap ;
  lemon:reference   <http://dbpedia.org/property/founded> ;
  lemon:subjOfProp  :founded_in_3_14_obj ;
  lemon:objOfProp   :founded_in_3_14_subj ;
  lemon:condition   :founded_in_3_14_sense_1_condition .

:founded_in_3_14_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:founded_in_3_14_obj lemon:marker :form_founded_in_3_14_preposition .

## Prepositions ##
:form_founded_in_3_14_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

