@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :racecourse_of_316 ;
  lemon:entry    :racecourse_of_316_form_preposition .

:racecourse_of_316 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :racecourse_of_316_form ;
  lemon:otherForm  :racecourse_of_316_singular ;
  lemon:otherForm  :racecourse_of_316_plural ;
  lemon:sense          :racecourse_of_316_sense_1 ;
  lemon:synBehavior    :racecourse_of_316_nounpp .

:racecourse_of_316_form a lemon:Form ;
  lemon:writtenRep "Racecourse"@en .

:racecourse_of_316_singular a    lemon:Form ;
  lemon:writtenRep "Racecourse"@en ;
  lexinfo:number   lexinfo:singular .

:racecourse_of_316_plural a   lemon:Form ;
  lemon:writtenRep "Racecourses"@en ;
  lexinfo:number    lexinfo:plural .

:racecourse_of_316_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_racecourse_of_316 ;
  lexinfo:prepositionalAdjunct :arg2_racecourse_of_316 .

:racecourse_of_316_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :racecourse_of_316_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_racecourse_of_316 ;
  lemon:objOfProp           :arg1_racecourse_of_316 ;
  lemon:condition           :racecourse_of_316_sense_1_condition .

:racecourse_of_316_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Racecourse> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Racecourse> .

:arg2_racecourse_of_316 lemon:marker :racecourse_of_316_form_preposition .
## Prepositions ##
:racecourse_of_316_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

