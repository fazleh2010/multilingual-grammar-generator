:poem_523_sense_1 a  lemon:LexicalSense ;
  lemon:reference :poem_523_sense_1_res ;
  lemon:isA       :poem_523_PredSynArg .

:poem_523_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
