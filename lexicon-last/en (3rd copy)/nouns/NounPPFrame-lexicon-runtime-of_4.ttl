@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :runtime-of_4 ;
  lemon:entry    :runtime-of_4_form_preposition .

:runtime-of_4 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :runtime-of_4_form ;
  lemon:otherForm  :runtime-of_4_singular ;
  lemon:otherForm  :runtime-of_4_plural ;
  lemon:sense          :runtime-of_4_sense_1 ;
  lemon:synBehavior    :runtime-of_4_nounpp .

:runtime-of_4_form a lemon:Form ;
  lemon:writtenRep "runtime"@en .

:runtime-of_4_singular a    lemon:Form ;
  lemon:writtenRep "runtime"@en ;
  lexinfo:number   lexinfo:singular .

:runtime-of_4_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:runtime-of_4_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_runtime-of_4 ;
  lexinfo:prepositionalAdjunct :arg2_runtime-of_4 .

:runtime-of_4_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :runtime-of_4_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/borders> ;
  lemon:subjOfProp          :arg1_runtime-of_4 ;
  lemon:objOfProp           :arg2_runtime-of_4 ;
  lemon:condition           :runtime-of_4_sense_1_condition .

:runtime-of_4_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Software> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/PopulatedPlace> .

:arg2_runtime-of_4 lemon:marker :runtime-of_4_form_preposition .
## Prepositions ##
:runtime-of_4_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

