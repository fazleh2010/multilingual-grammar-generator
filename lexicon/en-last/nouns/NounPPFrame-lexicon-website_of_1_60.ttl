@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :website_of_1_60 ;
  lemon:entry    :website_of_1_60_form_preposition .

:website_of_1_60 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :website_of_1_60_form ;
  lemon:otherForm  :website_of_1_60_singular ;
  lemon:otherForm  :website_of_1_60_plural ;
  lemon:sense          :website_of_1_60_sense_1 ;
  lemon:synBehavior    :website_of_1_60_nounpp .

:website_of_1_60_form a lemon:Form ;
  lemon:writtenRep "website"@en .

:website_of_1_60_singular a    lemon:Form ;
  lemon:writtenRep "website"@en ;
  lexinfo:number   lexinfo:singular .

:website_of_1_60_plural a   lemon:Form ;
  lemon:writtenRep "websites"@en ;
  lexinfo:number    lexinfo:plural .

:website_of_1_60_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_website_of_1_60 ;
  lexinfo:prepositionalAdjunct :arg2_website_of_1_60 .

:website_of_1_60_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :website_of_1_60_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/website> ;
  lemon:subjOfProp          :arg2_website_of_1_60 ;
  lemon:objOfProp           :arg1_website_of_1_60 ;
  lemon:condition           :website_of_1_60_sense_1_condition .

:website_of_1_60_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Work> .

:arg2_website_of_1_60 lemon:marker :website_of_1_60_form_preposition .
## Prepositions ##
:website_of_1_60_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

