@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :released_on_2_35 ;
  lemon:entry    :form_released_on_2_35_preposition .

:released_on_2_35 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_released_on_2_35 ;
  lemon:otherForm      :form_released_on_2_35_present3rd ;
  lemon:otherForm      :form_released_on_2_35_past ;
  lemon:otherForm      :form_released_on_2_35_perfect ;
  lemon:sense          :released_on_2_35_sense_1 ;
  lemon:synBehavior    :released_on_2_35_frame .

:form_released_on_2_35 a           lemon:Form ;
  lemon:writtenRep     "release"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_released_on_2_35_present3rd a      lemon:Form ;
  lemon:writtenRep "releases"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_released_on_2_35_past a lemon:Form ;
  lemon:writtenRep  "released"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_released_on_2_35_perfect a   lemon:Form ;
  lemon:writtenRep "released"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:released_on_2_35_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :released_on_2_35_subj ;
  lexinfo:prepositionalAdjunct :released_on_2_35_obj .

:released_on_2_35_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :released_on_2_35_ontomap ;
  lemon:reference   <wd:P400> ;
  lemon:subjOfProp  :released_on_2_35_subj ;
  lemon:objOfProp   :released_on_2_35_obj ;
  lemon:condition   :released_on_2_35_sense_1_condition .

:released_on_2_35_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <wd:Q7397> ;
  lemon:propertyRange  <http://www.w3.org/2002/07/owl#Thing> .


:released_on_2_35_obj lemon:marker :form_released_on_2_35_preposition .

## Prepositions ##
:form_released_on_2_35_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "on"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

