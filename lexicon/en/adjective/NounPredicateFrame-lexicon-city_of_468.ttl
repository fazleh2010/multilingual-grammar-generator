:city_of_468_sense_1 a  lemon:LexicalSense ;
  lemon:reference :city_of_468_sense_1_res ;
  lemon:isA       :city_of_468_PredSynArg .

:city_of_468_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/City> .
:city_of_468_sense_2 a  lemon:LexicalSense ;
  lemon:reference :city_of_468_sense_2_res ;
  lemon:isA       :city_of_468_PredSynArg .

:city_of_468_sense_2_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Town> .
