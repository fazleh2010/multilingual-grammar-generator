@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :song_303_sense_1_res ;
  lemon:entry    :song_303 .

:song_303 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :song_303_form ;
  lemon:otherForm  :song_303_singular ;
  lemon:otherForm  :song_303_plural ;
  lemon:sense          :song_303_sense_1 ;
  lemon:synBehavior    :song_303_predFrame .

:song_303_form
 lemon:writtenRep "song"@en .

:song_303_singular a    lemon:Form ;
  lemon:writtenRep "song"@en ;
  lexinfo:number   lexinfo:singular .

:song_303_plural a   lemon:Form ;
  lemon:writtenRep "songs"@en ;
  lexinfo:number    lexinfo:plural .

:song_303_sense_1 a  lemon:LexicalSense ;
  lemon:reference :song_303_sense_1_res ;
  lemon:isA       :song_303_PredSynArg .

:song_303_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Song> .