@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-701 ;
  lemon:entry    :form_lexicalEntry-701_preposition .

:lexicalEntry-701 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-701 ;
  lemon:otherForm      :form_lexicalEntry-701_past ;
  lemon:sense          :lexicalEntry-701_sense_1 ;
  lemon:synBehavior    :lexicalEntry-701_frame .

:form_lexicalEntry-701 a           lemon:Form ;
  lemon:writtenRep     "expanded"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-701 a      lemon:Form ;
  lemon:writtenRep "expanded"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-701_past a lemon:Form ;
  lemon:writtenRep  "expanded"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-701perfect a   lemon:Form ;
  lemon:writtenRep "expanded"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-701_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-701_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-701_obj .

:lexicalEntry-701_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-701_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/dissolutionDate> ;
  lemon:subjOfProp  :lexicalEntry-701_obj ;
  lemon:objOfProp   :lexicalEntry-701_subj ;
  lemon:condition   :lexicalEntry-701_sense_1_condition .

:lexicalEntry-701_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:lexicalEntry-701_obj lemon:marker :form_lexicalEntry-701_preposition .

## Prepositions ##
:form_lexicalEntry-701_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

