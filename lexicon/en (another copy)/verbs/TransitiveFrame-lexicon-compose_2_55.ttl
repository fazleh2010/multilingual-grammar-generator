@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_compose_2_55 ;
  lemon:entry    :compose_2_55ed ;
  lemon:entry    :form_compose_2_55_preposition .

:to_compose_2_55 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_compose_2_55 ;
  lemon:otherForm      :form_compose_2_55s ;
  lemon:otherForm      :form_compose_2_55ed ;
  lemon:otherForm      :form_compose_2_55perfect ;
  lemon:sense          :compose_2_55_sense_1 ;
  lemon:synBehavior    :compose_2_55_frame_transitive .

:form_compose_2_55 a         lemon:Form ;
  lemon:writtenRep     "compose"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_compose_2_55s a    lemon:Form ;
  lemon:writtenRep "composes"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_compose_2_55ed a   lemon:Form ;
  lemon:writtenRep "composed"@en ;
  lexinfo:tense    lexinfo:past .

:form_compose_2_55perfect a   lemon:Form ;
  lemon:writtenRep "composed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:compose_2_55_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :compose_2_55_subj ;
  lexinfo:directObject     :compose_2_55_obj .

:compose_2_55_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :compose_2_55_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/musicComposer> ;
  lemon:subjOfProp  :compose_2_55_obj ;
  lemon:objOfProp   :compose_2_55_subj ;
  lemon:condition   :compose_2_55_sense_1_condition .


:compose_2_55_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:compose_2_55_obj lemon:marker :form_compose_2_55_preposition .

## Prepositions ##
:form_compose_2_55_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

