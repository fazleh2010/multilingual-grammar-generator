@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :part-of_485 ;
  lemon:entry    :part-of_485_form_preposition .

:part-of_485 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :part-of_485_form ;
  lemon:otherForm  :part-of_485_singular ;
  lemon:otherForm  :part-of_485_plural ;
  lemon:sense          :part-of_485_sense_1 ;
  lemon:synBehavior    :part-of_485_nounpp .

:part-of_485_form a lemon:Form ;

:part-of_485_singular a    lemon:Form ;
  lemon:writtenRep "part"@en ;
  lexinfo:number   lexinfo:singular .

:part-of_485_plural a   lemon:Form ;
  lemon:writtenRep "parts"@en ;
  lexinfo:number    lexinfo:plural .

:part-of_485_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_part-of_485 ;
  lexinfo:prepositionalAdjunct :arg2_part-of_485 .

:part-of_485_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :part-of_485_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/alliance> ;
  lemon:subjOfProp          :arg2_part-of_485 ;
  lemon:objOfProp           :arg1_part-of_485 ;
  lemon:condition           :part-of_485_sense_1_condition .

:part-of_485_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Agent> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Company> .

:arg2_part-of_485 lemon:marker :part-of_485_form_preposition .
## Prepositions ##
:part-of_485_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

