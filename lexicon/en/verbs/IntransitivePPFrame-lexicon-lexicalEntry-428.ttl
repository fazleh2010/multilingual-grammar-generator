@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-428 ;
  lemon:entry    :form_lexicalEntry-428_preposition .

:lexicalEntry-428 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-428 ;
  lemon:otherForm      :form_lexicalEntry-428_past ;
  lemon:sense          :lexicalEntry-428_sense_1 ;
  lemon:synBehavior    :lexicalEntry-428_frame .

:form_lexicalEntry-428 a           lemon:Form ;
  lemon:writtenRep     "die"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-428 a      lemon:Form ;
  lemon:writtenRep "dies"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-428_past a lemon:Form ;
  lemon:writtenRep  "died"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-428perfect a   lemon:Form ;
  lemon:writtenRep "died"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-428_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-428_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-428_obj .

:lexicalEntry-428_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-428_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/deathDate> ;
  lemon:subjOfProp  :lexicalEntry-428_obj ;
  lemon:objOfProp   :lexicalEntry-428_subj ;
  lemon:condition   :lexicalEntry-428_sense_1_condition .

:lexicalEntry-428_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:lexicalEntry-428_obj lemon:marker :form_lexicalEntry-428_preposition .

## Prepositions ##
:form_lexicalEntry-428_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

