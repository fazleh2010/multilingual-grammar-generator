@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :saint_283_sense_1_res ;
  lemon:entry    :saint_283 .

:saint_283 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :saint_283_form ;
  lemon:otherForm  :saint_283_singular ;
  lemon:otherForm  :saint_283_plural ;
  lemon:sense          :saint_283_sense_1 ;
  lemon:synBehavior    :saint_283_predFrame .

:saint_283_form
 lemon:writtenRep "saint"@en .

:saint_283_singular a    lemon:Form ;
  lemon:writtenRep "saint"@en ;
  lexinfo:number   lexinfo:singular .

:saint_283_plural a   lemon:Form ;
  lemon:writtenRep "saints"@en ;
  lexinfo:number    lexinfo:plural .

:saint_283_sense_1 a  lemon:LexicalSense ;
  lemon:reference :saint_283_sense_1_res ;
  lemon:isA       :saint_283_PredSynArg .

:saint_283_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Saint> .