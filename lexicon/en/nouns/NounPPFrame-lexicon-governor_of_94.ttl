  lemon:otherForm  :governor_of_94_singular ;
  lemon:otherForm  :governor_of_94_plural ;
  lemon:sense          :governor_of_94_sense_1 ;
  lemon:synBehavior    :governor_of_94_nounpp .

:governor_of_94_form a lemon:Form ;
  lemon:writtenRep "governor"@en .

:governor_of_94_singular a    lemon:Form ;
