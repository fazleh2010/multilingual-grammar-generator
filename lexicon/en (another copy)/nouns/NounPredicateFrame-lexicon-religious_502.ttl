@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :religious_502_sense_1_res ;
  lemon:entry    :religious_502 .

:religious_502 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :religious_502_form ;
  lemon:otherForm  :religious_502_singular ;
  lemon:otherForm  :religious_502_plural ;
  lemon:sense          :religious_502_sense_1 ;
  lemon:synBehavior    :religious_502_predFrame .

:religious_502_form
 lemon:writtenRep "religious"@en .

:religious_502_singular a    lemon:Form ;
  lemon:writtenRep "religious"@en ;
  lexinfo:number   lexinfo:singular .

:religious_502_plural a   lemon:Form ;
  lemon:writtenRep "religiouss"@en ;
  lexinfo:number    lexinfo:plural .

:religious_502_sense_1 a  lemon:LexicalSense ;
  lemon:reference :religious_502_sense_1_res ;
  lemon:isA       :religious_502_PredSynArg .

:religious_502_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Religious> .