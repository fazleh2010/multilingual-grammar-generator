@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :game_by_12 ;
  lemon:entry    :game_by_12_form_preposition .

:game_by_12 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :game_by_12_form ;
  lemon:otherForm  :game_by_12_singular ;
  lemon:otherForm  :game_by_12_plural ;
  lemon:sense          :game_by_12_sense_1 ;
  lemon:synBehavior    :game_by_12_nounpp .

:game_by_12_form a lemon:Form ;
  lemon:writtenRep "game"@en .

:game_by_12_singular a    lemon:Form ;
  lemon:writtenRep "game"@en ;
  lexinfo:number   lexinfo:singular .

:game_by_12_plural a   lemon:Form ;
  lemon:writtenRep "games"@en ;
  lexinfo:number    lexinfo:plural .

:game_by_12_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg2_game_by_12 ;
  lexinfo:prepositionalAdjunct :arg1_game_by_12 .

:game_by_12_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :game_by_12_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/publisher> ;
  lemon:subjOfProp          :arg2_game_by_12 ;
  lemon:objOfProp           :arg1_game_by_12 ;
  lemon:condition           :game_by_12_sense_1_condition .

:game_by_12_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/VideoGame> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Company> .

:arg1_game_by_12 lemon:marker :game_by_12_form_preposition .
## Prepositions ##
:game_by_12_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

