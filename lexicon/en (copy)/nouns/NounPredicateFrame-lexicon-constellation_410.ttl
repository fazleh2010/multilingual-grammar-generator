@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :constellation_410_sense_1_res ;
  lemon:entry    :constellation_410 .

:constellation_410 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :constellation_410_form ;
  lemon:otherForm  :constellation_410_singular ;
  lemon:otherForm  :constellation_410_plural ;
  lemon:sense          :constellation_410_sense_1 ;
  lemon:synBehavior    :constellation_410_predFrame .

:constellation_410_form
 lemon:writtenRep "constellation"@en .

:constellation_410_singular a    lemon:Form ;
  lemon:writtenRep "constellation"@en ;
  lexinfo:number   lexinfo:singular .

:constellation_410_plural a   lemon:Form ;
  lemon:writtenRep "constellations"@en ;
  lexinfo:number    lexinfo:plural .

:constellation_410_sense_1 a  lemon:LexicalSense ;
  lemon:reference :constellation_410_sense_1_res ;
  lemon:isA       :constellation_410_PredSynArg .

:constellation_410_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Constellation> .