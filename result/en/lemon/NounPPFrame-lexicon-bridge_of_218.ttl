@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :bridge_of_218 ;
  lemon:entry    :bridge_of_218_form_preposition .

:bridge_of_218 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :bridge_of_218_form ;
  lemon:otherForm  :bridge_of_218_singular ;
  lemon:otherForm  :bridge_of_218_plural ;
  lemon:sense          :bridge_of_218_sense_1 ;
  lemon:synBehavior    :bridge_of_218_nounpp .

:bridge_of_218_form a lemon:Form ;

:bridge_of_218_singular a    lemon:Form ;
  lemon:writtenRep "Bridge"@en ;
  lexinfo:number   lexinfo:singular .

:bridge_of_218_plural a   lemon:Form ;
  lemon:writtenRep "Bridges"@en ;
  lexinfo:number    lexinfo:plural .

:bridge_of_218_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_bridge_of_218 ;
  lexinfo:prepositionalAdjunct :arg2_bridge_of_218 .

:bridge_of_218_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :bridge_of_218_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_bridge_of_218 ;
  lemon:objOfProp           :arg1_bridge_of_218 ;
  lemon:condition           :bridge_of_218_sense_1_condition .

:bridge_of_218_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Bridge> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Bridge> .

:arg2_bridge_of_218 lemon:marker :bridge_of_218_form_preposition .
## Prepositions ##
:bridge_of_218_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

