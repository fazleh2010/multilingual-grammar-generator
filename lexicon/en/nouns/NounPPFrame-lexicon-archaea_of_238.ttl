@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :archaea_of_238 ;
  lemon:entry    :archaea_of_238_form_preposition .

:archaea_of_238 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :archaea_of_238_form ;
  lemon:otherForm  :archaea_of_238_singular ;
  lemon:otherForm  :archaea_of_238_plural ;
  lemon:sense          :archaea_of_238_sense_1 ;
  lemon:synBehavior    :archaea_of_238_nounpp .

:archaea_of_238_form a lemon:Form ;
  lemon:writtenRep "Archaea"@en .

:archaea_of_238_singular a    lemon:Form ;
  lemon:writtenRep "Archaea"@en ;
  lexinfo:number   lexinfo:singular .

:archaea_of_238_plural a   lemon:Form ;
  lemon:writtenRep "Archaeas"@en ;
  lexinfo:number    lexinfo:plural .

:archaea_of_238_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_archaea_of_238 ;
  lexinfo:prepositionalAdjunct :arg2_archaea_of_238 .

:archaea_of_238_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :archaea_of_238_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_archaea_of_238 ;
  lemon:objOfProp           :arg1_archaea_of_238 ;
  lemon:condition           :archaea_of_238_sense_1_condition .

:archaea_of_238_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Archaea> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Archaea> .

:arg2_archaea_of_238 lemon:marker :archaea_of_238_form_preposition .
## Prepositions ##
:archaea_of_238_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

