@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_call_57 ;
  lemon:entry    :call_57ed ;
  lemon:entry    :form_call_57_preposition .

:to_call_57 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_call_57 ;
  lemon:otherForm      :form_call_57s ;
  lemon:otherForm      :form_call_57ed ;
  lemon:otherForm      :form_call_57perfect ;
  lemon:sense          :call_57_sense_1 ;
  lemon:synBehavior    :call_57_frame_transitive .

:form_call_57 a         lemon:Form ;
  lemon:writtenRep     "call"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_call_57s a    lemon:Form ;
  lemon:writtenRep "calls"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_call_57ed a   lemon:Form ;
  lemon:writtenRep "called"@en ;
  lexinfo:tense    lexinfo:past .

:form_call_57perfect a   lemon:Form ;
  lemon:writtenRep "called"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:call_57_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :call_57_subj ;
  lexinfo:directObject     :call_57_obj .

:call_57_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :call_57_sense_1 ;
  lemon:reference   <wd:P742> ;
  lemon:subjOfProp  :call_57_obj ;
  lemon:objOfProp   :call_57_subj ;
  lemon:condition   :call_57_sense_1_condition .


:call_57_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <wd:Q19088> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#String> .


:call_57_obj lemon:marker :form_call_57_preposition .

## Prepositions ##
:form_call_57_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

