@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
   lemon:entry          :danish _sense_1_res ;
  lemon:entry    :danish  .

:danish  a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :danish _lemma ;
  lemon:sense          :danish _sense_1 ;
  lemon:synBehavior    :danish _attrFrame, :danish _predFrame .

:danish _lemma lemon:writtenRep "danish"@en .

:danish _predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :danish _PredSynArg .

:danish _attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :danish _AttrSynArg .

:danish _sense_1 a  lemon:LexicalSense ;
  lemon:reference :danish _sense_1_res ;
  lemon:isA       :danish _AttrSynArg, :danish _PredSynArg .

:danish _sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Denmark> .
