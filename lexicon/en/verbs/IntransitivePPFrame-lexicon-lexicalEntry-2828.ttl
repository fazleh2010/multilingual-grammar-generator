@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-2828 ;
  lemon:entry    :form_lexicalEntry-2828_preposition .

:lexicalEntry-2828 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-2828 ;
  lemon:otherForm      :form_lexicalEntry-2828_past ;
  lemon:sense          :lexicalEntry-2828_sense_1 ;
  lemon:synBehavior    :lexicalEntry-2828_frame .

:form_lexicalEntry-2828 a           lemon:Form ;
  lemon:writtenRep     "portrayed"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-2828 a      lemon:Form ;
  lemon:writtenRep "portrayed"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-2828_past a lemon:Form ;
  lemon:writtenRep  "portrayed"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-2828perfect a   lemon:Form ;
  lemon:writtenRep "portrayed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-2828_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-2828_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-2828_obj .

:lexicalEntry-2828_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2828_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/spouse> ;
  lemon:subjOfProp  :lexicalEntry-2828_obj ;
  lemon:objOfProp   :lexicalEntry-2828_subj ;
  lemon:condition   :lexicalEntry-2828_sense_1_condition .

:lexicalEntry-2828_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:lexicalEntry-2828_obj lemon:marker :form_lexicalEntry-2828_preposition .

## Prepositions ##
:form_lexicalEntry-2828_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

