@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :brewery_of_242 ;
  lemon:entry    :brewery_of_242_form_preposition .

:brewery_of_242 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :brewery_of_242_form ;
  lemon:otherForm  :brewery_of_242_singular ;
  lemon:otherForm  :brewery_of_242_plural ;
  lemon:sense          :brewery_of_242_sense_1 ;
  lemon:synBehavior    :brewery_of_242_nounpp .

:brewery_of_242_form a lemon:Form ;
  lemon:writtenRep "Brewery"@en .

:brewery_of_242_singular a    lemon:Form ;
  lemon:writtenRep "Brewery"@en ;
  lexinfo:number   lexinfo:singular .

:brewery_of_242_plural a   lemon:Form ;
  lemon:writtenRep "Brewerys"@en ;
  lexinfo:number    lexinfo:plural .

:brewery_of_242_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_brewery_of_242 ;
  lexinfo:prepositionalAdjunct :arg2_brewery_of_242 .

:brewery_of_242_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :brewery_of_242_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_brewery_of_242 ;
  lemon:objOfProp           :arg1_brewery_of_242 ;
  lemon:condition           :brewery_of_242_sense_1_condition .

:brewery_of_242_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Brewery> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Brewery> .

:arg2_brewery_of_242 lemon:marker :brewery_of_242_form_preposition .
## Prepositions ##
:brewery_of_242_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

