@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :ethnic_group_202_sense_1_res ;
  lemon:entry    :ethnic_group_202 .

:ethnic_group_202 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :ethnic_group_202_form ;
  lemon:otherForm  :ethnic_group_202_singular ;
  lemon:otherForm  :ethnic_group_202_plural ;
  lemon:sense          :ethnic_group_202_sense_1 ;
  lemon:synBehavior    :ethnic_group_202_predFrame .

:ethnic_group_202_form
 lemon:writtenRep "ethnic group"@en .

:ethnic_group_202_singular a    lemon:Form ;
  lemon:writtenRep "ethnic group"@en ;
  lexinfo:number   lexinfo:singular .

:ethnic_group_202_plural a   lemon:Form ;
  lemon:writtenRep "ethnic groups"@en ;
  lexinfo:number    lexinfo:plural .

:ethnic_group_202_sense_1 a  lemon:LexicalSense ;
  lemon:reference :ethnic_group_202_sense_1_res ;
  lemon:isA       :ethnic_group_202_PredSynArg .

:ethnic_group_202_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/EthnicGroup> .