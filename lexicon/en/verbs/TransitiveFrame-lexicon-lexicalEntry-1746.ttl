@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-1746 ;
  lemon:entry    :lexicalEntry-1746ed ;
  lemon:entry    :form_lexicalEntry-1746_preposition .

:to_lexicalEntry-1746 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-1746 ;
  lemon:otherForm      :form_lexicalEntry-1746s ;
  lemon:otherForm      :form_lexicalEntry-1746ed ;
  lemon:otherForm      :form_lexicalEntry-1746perfect ;
  lemon:sense          :lexicalEntry-1746_sense_1 ;
  lemon:synBehavior    :lexicalEntry-1746_frame_transitive .

:form_lexicalEntry-1746 a         lemon:Form ;
  lemon:writtenRep     "elected"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-1746s a    lemon:Form ;
  lemon:writtenRep "elected"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-1746ed a   lemon:Form ;
  lemon:writtenRep "elected"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-1746perfect a   lemon:Form ;
  lemon:writtenRep "elected"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-1746_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-1746_obj ;
  lexinfo:directObject     :lexicalEntry-1746_subj .

:lexicalEntry-1746_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1746_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/leader> ;
  lemon:subjOfProp  :lexicalEntry-1746_obj ;
  lemon:objOfProp   :lexicalEntry-1746_subj ;
  lemon:condition   :lexicalEntry-1746_sense_1_condition .


:lexicalEntry-1746_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Legislature/dbo> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .

:lexicalEntry-1746ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-1746ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-1746ed_by ;
  lemon:synBehavior    :lexicalEntry-1746ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-1746ed_ontomap .

:form_lexicalEntry-1746ed_canonical a lemon:Form ;
  lemon:writtenRep         "elected"@en .

:form_lexicalEntry-1746ed_by a    lemon:Form ;
  lemon:writtenRep     "elected"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-1746ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-1746ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-1746ed_obj .

:lexicalEntry-1746ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1746ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/leader> ;
  lemon:subjOfProp  :lexicalEntry-1746ed_subj ;
  lemon:objOfProp   :lexicalEntry-1746ed_obj ;
  lemon:condition   :lexicalEntry-1746_sense_1_condition .
:lexicalEntry-1746ed_obj lemon:marker :form_lexicalEntry-1746_preposition .

## Prepositions ##
:form_lexicalEntry-1746_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

