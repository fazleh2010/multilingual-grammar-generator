@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :sea_of_46 ;
  lemon:entry    :sea_of_46_form_preposition .

:sea_of_46 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :sea_of_46_form ;
  lemon:otherForm  :sea_of_46_singular ;
  lemon:otherForm  :sea_of_46_plural ;
  lemon:sense          :sea_of_46_sense_1 ;
  lemon:synBehavior    :sea_of_46_nounpp .

:sea_of_46_form a lemon:Form ;
  lemon:writtenRep "Sea"@en .

:sea_of_46_singular a    lemon:Form ;
  lemon:writtenRep "Sea"@en ;
  lexinfo:number   lexinfo:singular .

:sea_of_46_plural a   lemon:Form ;
  lemon:writtenRep "Seas"@en ;
  lexinfo:number    lexinfo:plural .

:sea_of_46_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_sea_of_46 ;
  lexinfo:prepositionalAdjunct :arg2_sea_of_46 .

:sea_of_46_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :sea_of_46_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_sea_of_46 ;
  lemon:objOfProp           :arg1_sea_of_46 ;
  lemon:condition           :sea_of_46_sense_1_condition .

:sea_of_46_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Sea> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Sea> .

:arg2_sea_of_46 lemon:marker :sea_of_46_form_preposition .
## Prepositions ##
:sea_of_46_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

