@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :rollercoaster_of_254 ;
  lemon:entry    :rollercoaster_of_254_form_preposition .

:rollercoaster_of_254 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :rollercoaster_of_254_form ;
  lemon:otherForm  :rollercoaster_of_254_singular ;
  lemon:otherForm  :rollercoaster_of_254_plural ;
  lemon:sense          :rollercoaster_of_254_sense_1 ;
  lemon:synBehavior    :rollercoaster_of_254_nounpp .

:rollercoaster_of_254_form a lemon:Form ;
  lemon:writtenRep "Roller Coaster"@en .

:rollercoaster_of_254_singular a    lemon:Form ;
  lemon:writtenRep "Roller Coaster"@en ;
  lexinfo:number   lexinfo:singular .

:rollercoaster_of_254_plural a   lemon:Form ;
  lemon:writtenRep "Roller Coasters"@en ;
  lexinfo:number    lexinfo:plural .

:rollercoaster_of_254_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_rollercoaster_of_254 ;
  lexinfo:prepositionalAdjunct :arg2_rollercoaster_of_254 .

:rollercoaster_of_254_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :rollercoaster_of_254_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_rollercoaster_of_254 ;
  lemon:objOfProp           :arg1_rollercoaster_of_254 ;
  lemon:condition           :rollercoaster_of_254_sense_1_condition .

:rollercoaster_of_254_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/RollerCoaster> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/RollerCoaster> .

:arg2_rollercoaster_of_254 lemon:marker :rollercoaster_of_254_form_preposition .
## Prepositions ##
:rollercoaster_of_254_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

