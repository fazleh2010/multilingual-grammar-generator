@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :park_of_170_sense_1_res ;
  lemon:entry    :park_of_170 .

:park_of_170 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :park_of_170_form ;
  lemon:otherForm  :park_of_170_singular ;
  lemon:otherForm  :park_of_170_plural ;
  lemon:sense          :park_of_170_sense_1 ;
  lemon:synBehavior    :park_of_170_predFrame .

:park_of_170_form
 lemon:writtenRep "Park"@en .

:park_of_170_singular a    lemon:Form ;
  lemon:writtenRep "Park"@en ;
  lexinfo:number   lexinfo:singular .

:park_of_170_plural a   lemon:Form ;
