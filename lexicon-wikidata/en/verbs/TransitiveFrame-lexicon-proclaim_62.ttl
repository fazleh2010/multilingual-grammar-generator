@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_proclaim_62 ;
  lemon:entry    :proclaim_62ed ;
  lemon:entry    :form_proclaim_62_preposition .

:to_proclaim_62 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_proclaim_62 ;
  lemon:otherForm      :form_proclaim_62s ;
  lemon:otherForm      :form_proclaim_62ed ;
  lemon:otherForm      :form_proclaim_62perfect ;
  lemon:sense          :proclaim_62_sense_1 ;
  lemon:synBehavior    :proclaim_62_frame_transitive .

:form_proclaim_62 a         lemon:Form ;
  lemon:writtenRep     "proclaim"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_proclaim_62s a    lemon:Form ;
  lemon:writtenRep "proclaims"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_proclaim_62ed a   lemon:Form ;
  lemon:writtenRep "proclaimed"@en ;
  lexinfo:tense    lexinfo:past .

:form_proclaim_62perfect a   lemon:Form ;
  lemon:writtenRep "proclaimed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:proclaim_62_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :proclaim_62_subj ;
  lexinfo:directObject     :proclaim_62_obj .

:proclaim_62_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :proclaim_62_sense_1 ;
  lemon:reference   <wd:P571> ;
  lemon:subjOfProp  :proclaim_62_obj ;
  lemon:objOfProp   :proclaim_62_subj ;
  lemon:condition   :proclaim_62_sense_1_condition .


:proclaim_62_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <wd:Q476028> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:proclaim_62_obj lemon:marker :form_proclaim_62_preposition .

## Prepositions ##
:form_proclaim_62_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

