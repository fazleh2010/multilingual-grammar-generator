@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :canoeist_286_sense_1_res ;
  lemon:entry    :canoeist_286 .

:canoeist_286 a  lemon:LexicalEntry ;
