@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-2540 ;
  lemon:entry    :form_lexicalEntry-2540_preposition .

:lexicalEntry-2540 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-2540 ;
  lemon:otherForm      :form_lexicalEntry-2540_past ;
  lemon:sense          :lexicalEntry-2540_sense_1 ;
  lemon:synBehavior    :lexicalEntry-2540_frame .

:form_lexicalEntry-2540 a           lemon:Form ;
  lemon:writtenRep     "released"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-2540 a      lemon:Form ;
  lemon:writtenRep "released"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-2540_past a lemon:Form ;
  lemon:writtenRep  "released"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-2540perfect a   lemon:Form ;
  lemon:writtenRep "released"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-2540_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-2540_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-2540_obj .

:lexicalEntry-2540_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2540_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/releaseDate> ;
  lemon:subjOfProp  :lexicalEntry-2540_obj ;
  lemon:objOfProp   :lexicalEntry-2540_subj ;
  lemon:condition   :lexicalEntry-2540_sense_1_condition .

:lexicalEntry-2540_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Software> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:lexicalEntry-2540_obj lemon:marker :form_lexicalEntry-2540_preposition .

## Prepositions ##
:form_lexicalEntry-2540_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

