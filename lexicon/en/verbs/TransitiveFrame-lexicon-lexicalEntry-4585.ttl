@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-4585 ;
  lemon:entry    :lexicalEntry-4585ed ;
  lemon:entry    :form_lexicalEntry-4585_preposition .

:to_lexicalEntry-4585 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-4585 ;
  lemon:otherForm      :form_lexicalEntry-4585s ;
  lemon:otherForm      :form_lexicalEntry-4585ed ;
  lemon:otherForm      :form_lexicalEntry-4585perfect ;
  lemon:sense          :lexicalEntry-4585_sense_1 ;
  lemon:synBehavior    :lexicalEntry-4585_frame_transitive .

:form_lexicalEntry-4585 a         lemon:Form ;
  lemon:writtenRep     "directed by"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-4585s a    lemon:Form ;
  lemon:writtenRep "directed by"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-4585ed a   lemon:Form ;
  lemon:writtenRep "directed by"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-4585perfect a   lemon:Form ;
  lemon:writtenRep "directed by"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-4585_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-4585_obj ;
  lexinfo:directObject     :lexicalEntry-4585_subj .

:lexicalEntry-4585_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-4585_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/writer> ;
  lemon:subjOfProp  :lexicalEntry-4585_obj ;
  lemon:objOfProp   :lexicalEntry-4585_subj ;
  lemon:condition   :lexicalEntry-4585_sense_1_condition .


:lexicalEntry-4585_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Song> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .

:lexicalEntry-4585ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-4585ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-4585ed_by ;
  lemon:synBehavior    :lexicalEntry-4585ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-4585ed_ontomap .

:form_lexicalEntry-4585ed_canonical a lemon:Form ;
  lemon:writtenRep         "directed by"@en .

:form_lexicalEntry-4585ed_by a    lemon:Form ;
  lemon:writtenRep     "directed by"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-4585ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-4585ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-4585ed_obj .

:lexicalEntry-4585ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-4585ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/writer> ;
  lemon:subjOfProp  :lexicalEntry-4585ed_subj ;
  lemon:objOfProp   :lexicalEntry-4585ed_obj ;
  lemon:condition   :lexicalEntry-4585_sense_1_condition .
:lexicalEntry-4585ed_obj lemon:marker :form_lexicalEntry-4585_preposition .

## Prepositions ##
:form_lexicalEntry-4585_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

