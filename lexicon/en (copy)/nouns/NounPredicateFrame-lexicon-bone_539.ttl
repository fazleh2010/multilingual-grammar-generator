@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :bone_539_sense_1_res ;
  lemon:entry    :bone_539 .

:bone_539 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :bone_539_form ;
  lemon:otherForm  :bone_539_singular ;
  lemon:otherForm  :bone_539_plural ;
  lemon:sense          :bone_539_sense_1 ;
