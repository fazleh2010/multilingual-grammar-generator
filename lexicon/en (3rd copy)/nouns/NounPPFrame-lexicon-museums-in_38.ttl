@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :museums-in_38 ;
  lemon:entry    :museums-in_38_form_preposition .

:museums-in_38 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :museums-in_38_form ;
  lemon:otherForm  :museums-in_38_singular ;
  lemon:otherForm  :museums-in_38_plural ;
  lemon:sense          :museums-in_38_sense_1 ;
  lemon:synBehavior    :museums-in_38_nounpp .

:museums-in_38_form a lemon:Form ;
  lemon:writtenRep "museum"@en .

:museums-in_38_singular a    lemon:Form ;
  lemon:writtenRep "museum"@en ;
  lexinfo:number   lexinfo:singular .

:museums-in_38_plural a   lemon:Form ;
  lemon:writtenRep "museums"@en ;
  lexinfo:number    lexinfo:plural .

:museums-in_38_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_museums-in_38 ;
  lexinfo:prepositionalAdjunct :arg2_museums-in_38 .

:museums-in_38_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :museums-in_38_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/museum> ;
  lemon:subjOfProp          :arg1_museums-in_38 ;
  lemon:objOfProp           :arg2_museums-in_38 ;
  lemon:condition           :museums-in_38_sense_1_condition .

:museums-in_38_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Settlement> .

:arg2_museums-in_38 lemon:marker :museums-in_38_form_preposition .
## Prepositions ##
:museums-in_38_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

