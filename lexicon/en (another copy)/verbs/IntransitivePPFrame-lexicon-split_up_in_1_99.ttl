@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :split_up_in_1_99 ;
  lemon:entry    :form_split_up_in_1_99_preposition .

:split_up_in_1_99 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_split_up_in_1_99 ;
  lemon:otherForm      :form_split_up_in_1_99_present3rd ;
  lemon:otherForm      :form_split_up_in_1_99_past ;
  lemon:otherForm      :form_split_up_in_1_99_perfect ;
  lemon:sense          :split_up_in_1_99_sense_1 ;
  lemon:synBehavior    :split_up_in_1_99_frame .

:form_split_up_in_1_99 a           lemon:Form ;
  lemon:writtenRep     "split "@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_split_up_in_1_99_present3rd a      lemon:Form ;
  lemon:writtenRep "splits "@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_split_up_in_1_99_past a lemon:Form ;
  lemon:writtenRep  "splited"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_split_up_in_1_99_perfect a   lemon:Form ;
  lemon:writtenRep "splited"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:split_up_in_1_99_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :split_up_in_1_99_subj ;
  lexinfo:prepositionalAdjunct :split_up_in_1_99_obj .

:split_up_in_1_99_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :split_up_in_1_99_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/activeYearsEndYear> ;
  lemon:subjOfProp  :split_up_in_1_99_obj ;
  lemon:objOfProp   :split_up_in_1_99_subj ;
  lemon:condition   :split_up_in_1_99_sense_1_condition .

:split_up_in_1_99_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Band> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:split_up_in_1_99_obj lemon:marker :form_split_up_in_1_99_preposition .

## Prepositions ##
:form_split_up_in_1_99_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "up"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

