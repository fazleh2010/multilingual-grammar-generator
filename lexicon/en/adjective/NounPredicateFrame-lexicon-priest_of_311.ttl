@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :priest_of_311_sense_1_res ;
  lemon:entry    :priest_of_311 .

:priest_of_311 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
