@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_create_2_378 ;
  lemon:entry    :create_2_378ed ;
  lemon:entry    :form_create_2_378_preposition .

:to_create_2_378 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_create_2_378 ;
  lemon:otherForm      :form_create_2_378s ;
  lemon:otherForm      :form_create_2_378ed ;
  lemon:otherForm      :form_create_2_378perfect ;
  lemon:sense          :create_2_378_sense_1 ;
  lemon:synBehavior    :create_2_378_frame_transitive .

:form_create_2_378 a         lemon:Form ;
  lemon:writtenRep     "create"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_create_2_378s a    lemon:Form ;
  lemon:writtenRep "creates"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_create_2_378ed a   lemon:Form ;
  lemon:writtenRep "created"@en ;
  lexinfo:tense    lexinfo:past .

:form_create_2_378perfect a   lemon:Form ;
  lemon:writtenRep "created"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:create_2_378_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :create_2_378_subj ;
  lexinfo:directObject     :create_2_378_obj .

:create_2_378_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :create_2_378_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/author> ;
  lemon:subjOfProp  :create_2_378_obj ;
  lemon:objOfProp   :create_2_378_subj ;
  lemon:condition   :create_2_378_sense_1_condition .


:create_2_378_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Website> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:create_2_378_obj lemon:marker :form_create_2_378_preposition .

## Prepositions ##
:form_create_2_378_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

