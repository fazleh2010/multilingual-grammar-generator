@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :mayor_of_2_889 ;
  lemon:entry    :mayor_of_2_889_form_preposition .

:mayor_of_2_889 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :mayor_of_2_889_form ;
  lemon:otherForm  :mayor_of_2_889_singular ;
  lemon:otherForm  :mayor_of_2_889_plural ;
  lemon:sense          :mayor_of_2_889_sense_1 ;
  lemon:synBehavior    :mayor_of_2_889_nounpp .

:mayor_of_2_889_form a lemon:Form ;
  lemon:writtenRep "mayor"@en .

:mayor_of_2_889_singular a    lemon:Form ;
  lemon:writtenRep "mayor"@en ;
  lexinfo:number   lexinfo:singular .

:mayor_of_2_889_plural a   lemon:Form ;
  lemon:writtenRep "mayors"@en ;
  lexinfo:number    lexinfo:plural .

:mayor_of_2_889_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_mayor_of_2_889 ;
  lexinfo:prepositionalAdjunct :arg2_mayor_of_2_889 .

:mayor_of_2_889_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :mayor_of_2_889_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/leaderName> ;
  lemon:subjOfProp          :arg2_mayor_of_2_889 ;
  lemon:objOfProp           :arg1_mayor_of_2_889 ;
  lemon:condition           :mayor_of_2_889_sense_1_condition .

:mayor_of_2_889_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/City> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2_mayor_of_2_889 lemon:marker :mayor_of_2_889_form_preposition .
## Prepositions ##
:mayor_of_2_889_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

