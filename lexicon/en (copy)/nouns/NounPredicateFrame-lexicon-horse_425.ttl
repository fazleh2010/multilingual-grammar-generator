@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :horse_425_sense_1_res ;
  lemon:entry    :horse_425 .

:horse_425 a  lemon:LexicalEntry ;
