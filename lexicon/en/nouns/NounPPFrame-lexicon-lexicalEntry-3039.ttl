@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-3039 ;
  lemon:entry    :lexicalEntry-3039_form_preposition .

:lexicalEntry-3039 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lexicalEntry-3039_form ;
  lemon:otherForm  :lexicalEntry-3039_singular ;
  lemon:otherForm  :lexicalEntry-3039_plural ;
  lemon:sense          :lexicalEntry-3039_sense_1 ;
  lemon:synBehavior    :lexicalEntry-3039_nounpp .

:lexicalEntry-3039_form a lemon:Form ;
  lemon:writtenRep "wine"@en .

:lexicalEntry-3039_singular a    lemon:Form ;
  lemon:writtenRep "wine"@en ;
  lexinfo:number   lexinfo:singular .

:lexicalEntry-3039_plural a   lemon:Form ;
  lemon:writtenRep "wine"@en ;
  lexinfo:number    lexinfo:plural .

:lexicalEntry-3039_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg2 ;
  lexinfo:prepositionalAdjunct :arg1 .

:lexicalEntry-3039_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :lexicalEntry-3039_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/wineProduced> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :lexicalEntry-3039_sense_1_condition .

:lexicalEntry-3039_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Location> .

:arg2 lemon:marker :lexicalEntry-3039_form_preposition .
## Prepositions ##
:lexicalEntry-3039_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

