@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :originate_in_1_13 ;
  lemon:entry    :form_originate_in_1_13_preposition .

:originate_in_1_13 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_originate_in_1_13 ;
  lemon:otherForm      :form_originate_in_1_13_present3rd ;
  lemon:otherForm      :form_originate_in_1_13_past ;
  lemon:otherForm      :form_originate_in_1_13_perfect ;
  lemon:sense          :originate_in_1_13_sense_1 ;
  lemon:synBehavior    :originate_in_1_13_frame .

:form_originate_in_1_13 a           lemon:Form ;
  lemon:writtenRep     "originate"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_originate_in_1_13_present3rd a      lemon:Form ;
  lemon:writtenRep "originates"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_originate_in_1_13_past a lemon:Form ;
  lemon:writtenRep  "originated"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_originate_in_1_13_perfect a   lemon:Form ;
  lemon:writtenRep "originated"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:originate_in_1_13_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :originate_in_1_13_subj ;
  lexinfo:prepositionalAdjunct :originate_in_1_13_obj .

:originate_in_1_13_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :originate_in_1_13_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/origin> ;
  lemon:subjOfProp  :originate_in_1_13_subj ;
  lemon:objOfProp   :originate_in_1_13_obj ;
  lemon:condition   :originate_in_1_13_sense_1_condition .

:originate_in_1_13_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Beverage> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Country> .


:originate_in_1_13_obj lemon:marker :form_originate_in_1_13_preposition .

## Prepositions ##
:form_originate_in_1_13_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

