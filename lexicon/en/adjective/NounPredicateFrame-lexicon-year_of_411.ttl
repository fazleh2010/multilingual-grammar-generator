@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :year_of_411_sense_1_res ;
  lemon:entry    :year_of_411 .

:year_of_411 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :year_of_411_form ;
  lemon:otherForm  :year_of_411_singular ;
  lemon:otherForm  :year_of_411_plural ;
  lemon:sense          :year_of_411_sense_1 ;
  lemon:synBehavior    :year_of_411_predFrame .

:year_of_411_form
 lemon:writtenRep "Year"@en .

:year_of_411_singular a    lemon:Form ;
  lemon:writtenRep "Year"@en ;
  lexinfo:number   lexinfo:singular .

:year_of_411_plural a   lemon:Form ;
  lemon:writtenRep "Years"@en ;
  lexinfo:number    lexinfo:plural .

:year_of_411_sense_1 a  lemon:LexicalSense ;
  lemon:reference :year_of_411_sense_1_res ;
  lemon:isA       :year_of_411_PredSynArg .

:year_of_411_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Year> .
