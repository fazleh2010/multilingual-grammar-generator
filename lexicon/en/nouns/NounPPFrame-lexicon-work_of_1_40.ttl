@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :work_of_1_40 ;
  lemon:entry    :work_of_1_40_form_preposition .

:work_of_1_40 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :work_of_1_40_form ;
  lemon:otherForm  :work_of_1_40_singular ;
  lemon:otherForm  :work_of_1_40_plural ;
  lemon:sense          :work_of_1_40_sense_1 ;
  lemon:synBehavior    :work_of_1_40_nounpp .

:work_of_1_40_form a lemon:Form ;
  lemon:writtenRep "work"@en .

:work_of_1_40_singular a    lemon:Form ;
  lemon:writtenRep "work"@en ;
  lexinfo:number   lexinfo:singular .

:work_of_1_40_plural a   lemon:Form ;
  lemon:writtenRep "works"@en ;
  lexinfo:number    lexinfo:plural .

:work_of_1_40_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_work_of_1_40 ;
  lexinfo:prepositionalAdjunct :arg2_work_of_1_40 .

:work_of_1_40_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :work_of_1_40_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/releaseDate> ;
  lemon:subjOfProp          :arg2_work_of_1_40 ;
  lemon:objOfProp           :arg1_work_of_1_40 ;
  lemon:condition           :work_of_1_40_sense_1_condition .

:work_of_1_40_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Book> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2_work_of_1_40 lemon:marker :work_of_1_40_form_preposition .
## Prepositions ##
:work_of_1_40_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

