@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_direct_1_58 ;
  lemon:entry    :direct_1_58ed ;
  lemon:entry    :form_direct_1_58_preposition .

:to_direct_1_58 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_direct_1_58 ;
  lemon:otherForm      :form_direct_1_58s ;
  lemon:otherForm      :form_direct_1_58ed ;
  lemon:otherForm      :form_direct_1_58perfect ;
  lemon:sense          :direct_1_58_sense_1 ;
  lemon:synBehavior    :direct_1_58_frame_transitive .

:form_direct_1_58 a         lemon:Form ;
  lemon:writtenRep     "direct"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_direct_1_58s a    lemon:Form ;
  lemon:writtenRep "directs"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_direct_1_58ed a   lemon:Form ;
  lemon:writtenRep "directed"@en ;
  lexinfo:tense    lexinfo:past .

:form_direct_1_58perfect a   lemon:Form ;
  lemon:writtenRep "directed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:direct_1_58_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :direct_1_58_subj ;
  lexinfo:directObject     :direct_1_58_obj .

:direct_1_58_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :direct_1_58_sense_1 ;
  lemon:reference   <wd:P57> ;
  lemon:subjOfProp  :direct_1_58_obj ;
  lemon:objOfProp   :direct_1_58_subj ;
  lemon:condition   :direct_1_58_sense_1_condition .


:direct_1_58_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <wd:Q386724> ;
  lemon:propertyRange  <wd:Q19088> .


:direct_1_58_obj lemon:marker :form_direct_1_58_preposition .

## Prepositions ##
:form_direct_1_58_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

