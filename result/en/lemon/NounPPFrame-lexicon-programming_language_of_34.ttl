@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :programming_language_of_34 ;
  lemon:entry    :programming_language_of_34_form_preposition .

:programming_language_of_34 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :programming_language_of_34_form ;
  lemon:otherForm  :programming_language_of_34_singular ;
  lemon:otherForm  :programming_language_of_34_plural ;
  lemon:sense          :programming_language_of_34_sense_1 ;
  lemon:synBehavior    :programming_language_of_34_nounpp .

:programming_language_of_34_form a lemon:Form ;

:programming_language_of_34_singular a    lemon:Form ;
  lemon:writtenRep "programming language"@en ;
  lexinfo:number   lexinfo:singular .

:programming_language_of_34_plural a   lemon:Form ;
  lemon:writtenRep "programming languages"@en ;
  lexinfo:number    lexinfo:plural .

:programming_language_of_34_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_programming_language_of_34 ;
  lexinfo:prepositionalAdjunct :arg2_programming_language_of_34 .

:programming_language_of_34_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :programming_language_of_34_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_programming_language_of_34 ;
  lemon:objOfProp           :arg1_programming_language_of_34 ;
  lemon:condition           :programming_language_of_34_sense_1_condition .

:programming_language_of_34_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ProgrammingLanguage> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/ProgrammingLanguage> .

:arg2_programming_language_of_34 lemon:marker :programming_language_of_34_form_preposition .
## Prepositions ##
:programming_language_of_34_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

