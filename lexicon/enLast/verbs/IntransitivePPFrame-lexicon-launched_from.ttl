@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :launched_from ;
  lemon:entry    :form_launched_from_preposition .

:launched_from a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_launched_from ;
  lemon:otherForm      :form_launched_from_past ;
  lemon:sense          :launched_from_sense_1 ;
  lemon:synBehavior    :launched_from_frame .

:form_launched_from a           lemon:Form ;
  lemon:writtenRep     "launch"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_launched_from a      lemon:Form ;
  lemon:writtenRep "launches"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_launched_from_past a lemon:Form ;
  lemon:writtenRep  "launched"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_launched_fromperfect a   lemon:Form ;
  lemon:writtenRep "launched"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:launched_from_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :launched_from_subj ;
  lexinfo:prepositionalAdjunct :launched_from_obj .

:launched_from_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :launched_from_ontomap ;
  lemon:reference   <http://dbpedia.org/property/launchPad> ;
  lemon:subjOfProp  :launched_from_obj ;
  lemon:objOfProp   :launched_from_subj ;
  lemon:condition   :launched_from_sense_1_condition .

:launched_from_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/SpaceStation> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Airport> .


:launched_from_obj lemon:marker :form_launched_from_preposition .

## Prepositions ##
:form_launched_from_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "from"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

