@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :sport_462_sense_1_res ;
  lemon:entry    :sport_462 .

:sport_462 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :sport_462_form ;
  lemon:otherForm  :sport_462_singular ;
  lemon:otherForm  :sport_462_plural ;
  lemon:sense          :sport_462_sense_1 ;
  lemon:synBehavior    :sport_462_predFrame .

:sport_462_form
 lemon:writtenRep "sport"@en .

:sport_462_singular a    lemon:Form ;
  lemon:writtenRep "sport"@en ;
  lexinfo:number   lexinfo:singular .

:sport_462_plural a   lemon:Form ;
  lemon:writtenRep "sports"@en ;
  lexinfo:number    lexinfo:plural .

:sport_462_sense_1 a  lemon:LexicalSense ;
  lemon:reference :sport_462_sense_1_res ;
  lemon:isA       :sport_462_PredSynArg .

:sport_462_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Sport> .