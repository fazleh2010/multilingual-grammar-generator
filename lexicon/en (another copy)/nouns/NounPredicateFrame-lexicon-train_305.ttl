@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :train_305_sense_1_res ;
  lemon:entry    :train_305 .

:train_305 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :train_305_form ;
  lemon:otherForm  :train_305_singular ;
  lemon:otherForm  :train_305_plural ;
  lemon:sense          :train_305_sense_1 ;
  lemon:synBehavior    :train_305_predFrame .

:train_305_form
 lemon:writtenRep "train"@en .

:train_305_singular a    lemon:Form ;
  lemon:writtenRep "train"@en ;
  lexinfo:number   lexinfo:singular .

:train_305_plural a   lemon:Form ;
  lemon:writtenRep "trains"@en ;
  lexinfo:number    lexinfo:plural .

:train_305_sense_1 a  lemon:LexicalSense ;
  lemon:reference :train_305_sense_1_res ;
  lemon:isA       :train_305_PredSynArg .

:train_305_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Train> .