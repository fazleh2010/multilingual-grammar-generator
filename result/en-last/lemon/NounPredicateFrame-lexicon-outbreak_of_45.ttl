@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :outbreak_of_45_sense_1_res ;
  lemon:entry    :outbreak_of_45 .

:outbreak_of_45 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :outbreak_of_45_form ;
  lemon:otherForm  :outbreak_of_45_singular ;
  lemon:otherForm  :outbreak_of_45_plural ;
  lemon:sense          :outbreak_of_45_sense_1 ;
  lemon:synBehavior    :outbreak_of_45_predFrame .

:outbreak_of_45_form
 lemon:writtenRep "Outbreak"@en .

:outbreak_of_45_singular a    lemon:Form ;
  lemon:writtenRep "Outbreak"@en ;
  lexinfo:number   lexinfo:singular .

:outbreak_of_45_plural a   lemon:Form ;
  lemon:writtenRep "Outbreaks"@en ;
  lexinfo:number    lexinfo:plural .

:outbreak_of_45_sense_1 a  lemon:LexicalSense ;
  lemon:reference :outbreak_of_45_sense_1_res ;
  lemon:isA       :outbreak_of_45_PredSynArg .

:outbreak_of_45_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
