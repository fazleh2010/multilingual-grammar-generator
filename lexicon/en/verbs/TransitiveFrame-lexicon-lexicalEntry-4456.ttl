@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-4456 ;
  lemon:entry    :lexicalEntry-4456ed ;
  lemon:entry    :form_lexicalEntry-4456_preposition .

:to_lexicalEntry-4456 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-4456 ;
  lemon:otherForm      :form_lexicalEntry-4456s ;
  lemon:otherForm      :form_lexicalEntry-4456ed ;
  lemon:otherForm      :form_lexicalEntry-4456perfect ;
  lemon:sense          :lexicalEntry-4456_sense_1 ;
  lemon:synBehavior    :lexicalEntry-4456_frame_transitive .

:form_lexicalEntry-4456 a         lemon:Form ;
  lemon:writtenRep     "held"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-4456s a    lemon:Form ;
  lemon:writtenRep "held"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-4456ed a   lemon:Form ;
  lemon:writtenRep "held"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-4456perfect a   lemon:Form ;
  lemon:writtenRep "held"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-4456_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-4456_obj ;
  lexinfo:directObject     :lexicalEntry-4456_subj .

:lexicalEntry-4456_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-4456_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/location> ;
  lemon:subjOfProp  :lexicalEntry-4456_obj ;
  lemon:objOfProp   :lexicalEntry-4456_subj ;
  lemon:condition   :lexicalEntry-4456_sense_1_condition .


:lexicalEntry-4456_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Building> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/City> .

:lexicalEntry-4456ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-4456ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-4456ed_by ;
  lemon:synBehavior    :lexicalEntry-4456ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-4456ed_ontomap .

:form_lexicalEntry-4456ed_canonical a lemon:Form ;
  lemon:writtenRep         "held"@en .

:form_lexicalEntry-4456ed_by a    lemon:Form ;
  lemon:writtenRep     "held"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-4456ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-4456ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-4456ed_obj .

:lexicalEntry-4456ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-4456ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/location> ;
  lemon:subjOfProp  :lexicalEntry-4456ed_subj ;
  lemon:objOfProp   :lexicalEntry-4456ed_obj ;
  lemon:condition   :lexicalEntry-4456_sense_1_condition .
:lexicalEntry-4456ed_obj lemon:marker :form_lexicalEntry-4456_preposition .

## Prepositions ##
:form_lexicalEntry-4456_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

