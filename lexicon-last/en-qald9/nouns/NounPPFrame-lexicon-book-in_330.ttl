@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :book-in_330 ;
  lemon:entry    :book-in_330_form_preposition .

:book-in_330 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :book-in_330_form ;
  lemon:otherForm  :book-in_330_singular ;
  lemon:otherForm  :book-in_330_plural ;
  lemon:sense          :book-in_330_sense_1 ;
  lemon:synBehavior    :book-in_330_nounpp .

:book-in_330_form a lemon:Form ;
  lemon:writtenRep "book"@en .

:book-in_330_singular a    lemon:Form ;
  lemon:writtenRep "book"@en ;
  lexinfo:number   lexinfo:singular .

:book-in_330_plural a   lemon:Form ;
  lemon:writtenRep "books"@en ;
  lexinfo:number    lexinfo:plural .

:book-in_330_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg2_book-in_330 ;
  lexinfo:prepositionalAdjunct :arg1_book-in_330 .

:book-in_330_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :book-in_330_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/series> ;
  lemon:subjOfProp          :arg2_book-in_330 ;
  lemon:objOfProp           :arg1_book-in_330 ;
  lemon:condition           :book-in_330_sense_1_condition .

:book-in_330_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Book> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/TelevisionShow> .

:arg1_book-in_330 lemon:marker :book-in_330_form_preposition .
## Prepositions ##
:book-in_330_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

