@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :skater_256_sense_1_res ;
  lemon:entry    :skater_256 .

:skater_256 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :skater_256_form ;
  lemon:otherForm  :skater_256_singular ;
  lemon:otherForm  :skater_256_plural ;
  lemon:sense          :skater_256_sense_1 ;
  lemon:synBehavior    :skater_256_predFrame .

:skater_256_form
 lemon:writtenRep "skater"@en .

:skater_256_singular a    lemon:Form ;
  lemon:writtenRep "skater"@en ;
  lexinfo:number   lexinfo:singular .

:skater_256_plural a   lemon:Form ;
  lemon:writtenRep "skaters"@en ;
  lexinfo:number    lexinfo:plural .

:skater_256_sense_1 a  lemon:LexicalSense ;
  lemon:reference :skater_256_sense_1_res ;
  lemon:isA       :skater_256_PredSynArg .

:skater_256_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Skater> .