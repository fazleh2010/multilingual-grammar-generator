@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :mayor_of_283 ;
  lemon:entry    :mayor_of_283_form_preposition .

:mayor_of_283 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :mayor_of_283_form ;
  lemon:otherForm  :mayor_of_283_singular ;
  lemon:otherForm  :mayor_of_283_plural ;
  lemon:sense          :mayor_of_283_sense_1 ;
  lemon:synBehavior    :mayor_of_283_nounpp .

:mayor_of_283_form a lemon:Form ;

:mayor_of_283_singular a    lemon:Form ;
  lemon:writtenRep "Mayor"@en ;
  lexinfo:number   lexinfo:singular .

:mayor_of_283_plural a   lemon:Form ;
  lemon:writtenRep "Mayors"@en ;
  lexinfo:number    lexinfo:plural .

:mayor_of_283_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_mayor_of_283 ;
  lexinfo:prepositionalAdjunct :arg2_mayor_of_283 .

:mayor_of_283_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :mayor_of_283_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_mayor_of_283 ;
  lemon:objOfProp           :arg1_mayor_of_283 ;
  lemon:condition           :mayor_of_283_sense_1_condition .

:mayor_of_283_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Mayor> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Mayor> .

:arg2_mayor_of_283 lemon:marker :mayor_of_283_form_preposition .
## Prepositions ##
:mayor_of_283_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

