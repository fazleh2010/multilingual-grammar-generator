@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-931 ;
  lemon:entry    :lexicalEntry-931_form_preposition .

:lexicalEntry-931 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lexicalEntry-931_form ;
  lemon:otherForm  :lexicalEntry-931_singular ;
  lemon:otherForm  :lexicalEntry-931_plural ;
  lemon:sense          :lexicalEntry-931_sense_1 ;
  lemon:synBehavior    :lexicalEntry-931_nounpp .

:lexicalEntry-931_form a lemon:Form ;
  lemon:writtenRep "music"@en .

:lexicalEntry-931_singular a    lemon:Form ;
  lemon:writtenRep "music"@en ;
  lexinfo:number   lexinfo:singular .

:lexicalEntry-931_plural a   lemon:Form ;
  lemon:writtenRep "music"@en ;
  lexinfo:number    lexinfo:plural .

:lexicalEntry-931_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:lexicalEntry-931_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :lexicalEntry-931_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/foundedBy> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :lexicalEntry-931_sense_1_condition .

:lexicalEntry-931_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Agent> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Pope> .

:arg2 lemon:marker :lexicalEntry-931_form_preposition .
## Prepositions ##
:lexicalEntry-931_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

