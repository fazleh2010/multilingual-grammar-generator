@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :country_of_9 ;
  lemon:entry    :country_of_9_form_preposition .

:country_of_9 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :country_of_9_form ;
  lemon:otherForm  :country_of_9_singular ;
  lemon:otherForm  :country_of_9_plural ;
  lemon:sense          :country_of_9_sense_1 ;
  lemon:synBehavior    :country_of_9_nounpp .

:country_of_9_form a lemon:Form ;
  lemon:writtenRep "country"@en .

:country_of_9_singular a    lemon:Form ;
  lemon:writtenRep "country"@en ;
  lexinfo:number   lexinfo:singular .

:country_of_9_plural a   lemon:Form ;
  lemon:writtenRep "countries"@en ;
  lexinfo:number    lexinfo:plural .

:country_of_9_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_country_of_9 ;
  lexinfo:prepositionalAdjunct :arg2_country_of_9 .

:country_of_9_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :country_of_9_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/country> ;
  lemon:subjOfProp          :arg2_country_of_9 ;
  lemon:objOfProp           :arg1_country_of_9 ;
  lemon:condition           :country_of_9_sense_1_condition .

:country_of_9_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Country> .

:arg2_country_of_9 lemon:marker :country_of_9_form_preposition .
## Prepositions ##
:country_of_9_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

