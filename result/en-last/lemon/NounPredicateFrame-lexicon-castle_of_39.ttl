@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :castle_of_39_sense_1_res ;
  lemon:entry    :castle_of_39 .

:castle_of_39 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :castle_of_39_form ;
  lemon:otherForm  :castle_of_39_singular ;
  lemon:otherForm  :castle_of_39_plural ;
  lemon:sense          :castle_of_39_sense_1 ;
  lemon:synBehavior    :castle_of_39_predFrame .

:castle_of_39_form
 lemon:writtenRep "Castle"@en .

:castle_of_39_singular a    lemon:Form ;
  lemon:writtenRep "Castle"@en ;
  lexinfo:number   lexinfo:singular .

:castle_of_39_plural a   lemon:Form ;
  lemon:writtenRep "Castles"@en ;
  lexinfo:number    lexinfo:plural .

:castle_of_39_sense_1 a  lemon:LexicalSense ;
  lemon:reference :castle_of_39_sense_1_res ;
  lemon:isA       :castle_of_39_PredSynArg .

:castle_of_39_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Castle> .
