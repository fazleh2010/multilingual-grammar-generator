@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-111 ;
  lemon:entry    :form_lexicalEntry-111_preposition .

:lexicalEntry-111 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-111 ;
  lemon:otherForm      :form_lexicalEntry-111_past ;
  lemon:sense          :lexicalEntry-111_sense_1 ;
  lemon:synBehavior    :lexicalEntry-111_frame .

:form_lexicalEntry-111 a           lemon:Form ;
  lemon:writtenRep     "portrayed by"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-111 a      lemon:Form ;
  lemon:writtenRep "portrayed by"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-111_past a lemon:Form ;
  lemon:writtenRep  "portrayed by"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-111perfect a   lemon:Form ;
  lemon:writtenRep "portrayed by"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-111_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-111_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-111_obj .

:lexicalEntry-111_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-111_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/child> ;
  lemon:subjOfProp  :lexicalEntry-111_obj ;
  lemon:objOfProp   :lexicalEntry-111_subj ;
  lemon:condition   :lexicalEntry-111_sense_1_condition .

:lexicalEntry-111_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:lexicalEntry-111_obj lemon:marker :form_lexicalEntry-111_preposition .

## Prepositions ##
:form_lexicalEntry-111_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

