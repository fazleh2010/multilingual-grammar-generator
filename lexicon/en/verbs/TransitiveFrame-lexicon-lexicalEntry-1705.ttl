@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-1705 ;
  lemon:entry    :lexicalEntry-1705ed ;
  lemon:entry    :form_lexicalEntry-1705_preposition .

:to_lexicalEntry-1705 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-1705 ;
  lemon:otherForm      :form_lexicalEntry-1705s ;
  lemon:otherForm      :form_lexicalEntry-1705ed ;
  lemon:otherForm      :form_lexicalEntry-1705perfect ;
  lemon:sense          :lexicalEntry-1705_sense_1 ;
  lemon:synBehavior    :lexicalEntry-1705_frame_transitive .

:form_lexicalEntry-1705 a         lemon:Form ;
  lemon:writtenRep     "elected"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-1705s a    lemon:Form ;
  lemon:writtenRep "elected"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-1705ed a   lemon:Form ;
  lemon:writtenRep "elected"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-1705perfect a   lemon:Form ;
  lemon:writtenRep "elected"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-1705_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-1705_subj ;
  lexinfo:directObject     :lexicalEntry-1705_obj .

:lexicalEntry-1705_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1705_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/lastElectionDate> ;
  lemon:subjOfProp  :lexicalEntry-1705_obj ;
  lemon:objOfProp   :lexicalEntry-1705_subj ;
  lemon:condition   :lexicalEntry-1705_sense_1_condition .


:lexicalEntry-1705_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .

:lexicalEntry-1705ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-1705ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-1705ed_by ;
  lemon:synBehavior    :lexicalEntry-1705ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-1705ed_ontomap .

:form_lexicalEntry-1705ed_canonical a lemon:Form ;
  lemon:writtenRep         "elected"@en .

:form_lexicalEntry-1705ed_by a    lemon:Form ;
  lemon:writtenRep     "elected"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-1705ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-1705ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-1705ed_obj .

:lexicalEntry-1705ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1705ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/lastElectionDate> ;
  lemon:subjOfProp  :lexicalEntry-1705ed_subj ;
  lemon:objOfProp   :lexicalEntry-1705ed_obj ;
  lemon:condition   :lexicalEntry-1705_sense_1_condition .
:lexicalEntry-1705ed_obj lemon:marker :form_lexicalEntry-1705_preposition .

## Prepositions ##
:form_lexicalEntry-1705_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

