@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :founding-date_of ;
  lemon:entry    :founding-date_of_form_preposition .

:founding-date_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :founding-date_of_form ;
  lemon:otherForm  :founding-date_of_singular ;
  lemon:otherForm  :founding-date_of_plural ;
  lemon:sense          :founding-date_of_sense_1 ;
  lemon:synBehavior    :founding-date_of_nounpp .

:founding-date_of_form a lemon:Form ;
  lemon:writtenRep "founding date "@en .

:founding-date_of_singular a    lemon:Form ;
  lemon:writtenRep "founding date "@en ;
  lexinfo:number   lexinfo:singular .

:founding-date_of_plural a   lemon:Form ;
  lemon:writtenRep "founding dates"@en ;
  lexinfo:number    lexinfo:plural .

:founding-date_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:founding-date_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :founding-date_of_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/foundingDate> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :founding-date_of_sense_1_condition .

:founding-date_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2 lemon:marker :founding-date_of_form_preposition .
## Prepositions ##
:founding-date_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

