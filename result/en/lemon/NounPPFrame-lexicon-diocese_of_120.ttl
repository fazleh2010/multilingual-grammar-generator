@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :diocese_of_120 ;
  lemon:entry    :diocese_of_120_form_preposition .

:diocese_of_120 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :diocese_of_120_form ;
  lemon:otherForm  :diocese_of_120_singular ;
  lemon:otherForm  :diocese_of_120_plural ;
  lemon:sense          :diocese_of_120_sense_1 ;
  lemon:synBehavior    :diocese_of_120_nounpp .

:diocese_of_120_form a lemon:Form ;
  lemon:writtenRep "Diocese"@en .

:diocese_of_120_singular a    lemon:Form ;
  lemon:writtenRep "Diocese"@en ;
  lexinfo:number   lexinfo:singular .

:diocese_of_120_plural a   lemon:Form ;
  lemon:writtenRep "Dioceses"@en ;
  lexinfo:number    lexinfo:plural .

:diocese_of_120_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_diocese_of_120 ;
  lexinfo:prepositionalAdjunct :arg2_diocese_of_120 .

:diocese_of_120_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :diocese_of_120_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_diocese_of_120 ;
  lemon:objOfProp           :arg1_diocese_of_120 ;
  lemon:condition           :diocese_of_120_sense_1_condition .

:diocese_of_120_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Diocese> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Diocese> .

:arg2_diocese_of_120 lemon:marker :diocese_of_120_form_preposition .
## Prepositions ##
:diocese_of_120_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

