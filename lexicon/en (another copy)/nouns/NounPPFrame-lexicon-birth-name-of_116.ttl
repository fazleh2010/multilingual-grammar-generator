  lemon:reference           <http://dbpedia.org/property/birthName> ;
  lemon:subjOfProp          :arg1_birth-name-of_116 ;
  lemon:objOfProp           :arg2_birth-name-of_116 ;
  lemon:condition           :birth-name-of_116_sense_1_condition .

:birth-name-of_116_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:birth-name-of_116_sense_2 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :birth-name-of_116_sense_2 ;
  lemon:reference           <http://dbpedia.org/ontology/birthName> ;
  lemon:subjOfProp          :arg1_birth-name-of_116 ;
  lemon:objOfProp           :arg2_birth-name-of_116 ;
