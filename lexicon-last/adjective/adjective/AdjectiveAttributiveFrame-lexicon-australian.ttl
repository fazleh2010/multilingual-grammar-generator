@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
	lemon:language "en" ;
	lemon:entry    :australian ;
	lemon:entry    :australian_res .

:australian a             lemon:LexicalEntry ;
	lexinfo:partOfSpeech lexinfo:adjective ;
	lemon:canonicalForm  :australian_lemma ;
	lemon:synBehavior    :australian_attrFrame, :australian_predFrame ;
	lemon:sense          :australian_sense .

:australian_lemma lemon:writtenRep "Australian"@en .

:australian_predFrame a        lexinfo:AdjectivePredicateFrame ;
	lexinfo:copulativeSubject :australian_PredSynArg .

:australian_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
	lexinfo:attributiveArg :australian_AttrSynArg .

:australian_sense a  lemon:LexicalSense ;
	lemon:reference :australian_res ;
	lemon:isA       :australian_AttrSynArg, :australian_PredSynArg .

:australian_res a   owl:Restriction ;
	owl:onProperty <http://dbpedia.org/ontology/hometown> ;
	owl:hasValue   <http://dbpedia.org/resource/Australia> .
