@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :form_in_1_159 ;
  lemon:entry    :form_form_in_1_159_preposition .

:form_in_1_159 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_form_in_1_159 ;
  lemon:otherForm      :form_form_in_1_159_present3rd ;
  lemon:otherForm      :form_form_in_1_159_past ;
  lemon:otherForm      :form_form_in_1_159_perfect ;
  lemon:sense          :form_in_1_159_sense_1 ;
  lemon:synBehavior    :form_in_1_159_frame .

:form_form_in_1_159 a           lemon:Form ;
  lemon:writtenRep     "form"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_form_in_1_159_present3rd a      lemon:Form ;
  lemon:writtenRep "forms"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_form_in_1_159_past a lemon:Form ;
  lemon:writtenRep  "formed"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_form_in_1_159_perfect a   lemon:Form ;
  lemon:writtenRep "formed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:form_in_1_159_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :form_in_1_159_subj ;
  lexinfo:prepositionalAdjunct :form_in_1_159_obj .

:form_in_1_159_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :form_in_1_159_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/activeYearsStartYear> ;
  lemon:subjOfProp  :form_in_1_159_obj ;
  lemon:objOfProp   :form_in_1_159_subj ;
  lemon:condition   :form_in_1_159_sense_1_condition .

:form_in_1_159_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Band> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:form_in_1_159_obj lemon:marker :form_form_in_1_159_preposition .

## Prepositions ##
:form_form_in_1_159_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

