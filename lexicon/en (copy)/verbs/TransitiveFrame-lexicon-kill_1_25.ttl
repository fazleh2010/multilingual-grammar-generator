@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_kill_1_25 ;
  lemon:entry    :kill_1_25ed ;
  lemon:entry    :form_kill_1_25_preposition .

:to_kill_1_25 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_kill_1_25 ;
  lemon:otherForm      :form_kill_1_25s ;
  lemon:otherForm      :form_kill_1_25ed ;
  lemon:otherForm      :form_kill_1_25perfect ;
  lemon:sense          :kill_1_25_sense_1 ;
  lemon:synBehavior    :kill_1_25_frame_transitive .

:form_kill_1_25 a         lemon:Form ;
  lemon:writtenRep     "kill"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_kill_1_25s a    lemon:Form ;
  lemon:writtenRep "kills"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_kill_1_25ed a   lemon:Form ;
  lemon:writtenRep "killed"@en ;
  lexinfo:tense    lexinfo:past .

:form_kill_1_25perfect a   lemon:Form ;
  lemon:writtenRep "killed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:kill_1_25_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :kill_1_25_obj ;
  lexinfo:directObject     :kill_1_25_subj .

:kill_1_25_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :kill_1_25_sense_1 ;
  lemon:reference   <http://dbpedia.org/property/conviction> ;
  lemon:subjOfProp  :kill_1_25_obj ;
  lemon:objOfProp   :kill_1_25_subj ;
  lemon:condition   :kill_1_25_sense_1_condition .


:kill_1_25_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/MilitaryConflict> .


:kill_1_25_obj lemon:marker :form_kill_1_25_preposition .

## Prepositions ##
:form_kill_1_25_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

