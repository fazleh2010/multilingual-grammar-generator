@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :museum_in ;
  lemon:entry    :museum_in_form_preposition .

:museum_in a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :museum_in_form ;
  lemon:otherForm  :museum_in_singular ;
  lemon:otherForm  :museum_in_plural ;
  lemon:sense          :museum_in_sense_1 ;
  lemon:synBehavior    :museum_in_nounpp .

:museum_in_form a lemon:Form ;
  lemon:writtenRep "museum"@en .

:museum_in_singular a    lemon:Form ;
  lemon:writtenRep "museum"@en ;
  lexinfo:number   lexinfo:singular .

:museum_in_plural a   lemon:Form ;
  lemon:writtenRep "museums"@en ;
  lexinfo:number    lexinfo:plural .

:museum_in_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg2 ;
  lexinfo:prepositionalAdjunct :arg1 .

:museum_in_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :museum_in_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/location> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :museum_in_sense_1_condition .

:museum_in_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Building> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2 lemon:marker :museum_in_form_preposition .
## Prepositions ##
:museum_in_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

