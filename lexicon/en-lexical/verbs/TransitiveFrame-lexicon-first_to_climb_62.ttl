@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_first_to_climb_62 ;
  lemon:entry    :first_to_climb_62ed ;
  lemon:entry    :form_first_to_climb_62_preposition .

:to_first_to_climb_62 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_first_to_climb_62 ;
  lemon:otherForm      :form_first_to_climb_62s ;
  lemon:otherForm      :form_first_to_climb_62ed ;
  lemon:otherForm      :form_first_to_climb_62perfect ;
  lemon:sense          :first_to_climb_62_sense_1 ;
  lemon:synBehavior    :first_to_climb_62_frame_transitive .

:form_first_to_climb_62 a         lemon:Form ;
  lemon:writtenRep     "first to climb"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_first_to_climb_62s a    lemon:Form ;
  lemon:writtenRep "-"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_first_to_climb_62ed a   lemon:Form ;
  lemon:writtenRep "-"@en ;
  lexinfo:tense    lexinfo:past .

:form_first_to_climb_62perfect a   lemon:Form ;
  lemon:writtenRep "-"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:first_to_climb_62_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :first_to_climb_62_obj ;
  lexinfo:directObject     :first_to_climb_62_subj .

:first_to_climb_62_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :first_to_climb_62_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/firstAscentPerson> ;
  lemon:subjOfProp  :first_to_climb_62_obj ;
  lemon:objOfProp   :first_to_climb_62_subj ;
  lemon:condition   :first_to_climb_62_sense_1_condition .


:first_to_climb_62_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:first_to_climb_62_obj lemon:marker :form_first_to_climb_62_preposition .

## Prepositions ##
:form_first_to_climb_62_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

