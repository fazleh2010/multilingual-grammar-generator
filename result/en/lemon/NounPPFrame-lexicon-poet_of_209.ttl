@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :poet_of_209 ;
  lemon:entry    :poet_of_209_form_preposition .

:poet_of_209 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :poet_of_209_form ;
  lemon:otherForm  :poet_of_209_singular ;
  lemon:otherForm  :poet_of_209_plural ;
  lemon:sense          :poet_of_209_sense_1 ;
  lemon:synBehavior    :poet_of_209_nounpp .

:poet_of_209_form a lemon:Form ;

:poet_of_209_singular a    lemon:Form ;
  lemon:writtenRep "Poet"@en ;
  lexinfo:number   lexinfo:singular .

:poet_of_209_plural a   lemon:Form ;
  lemon:writtenRep "Poets"@en ;
  lexinfo:number    lexinfo:plural .

:poet_of_209_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_poet_of_209 ;
  lexinfo:prepositionalAdjunct :arg2_poet_of_209 .

:poet_of_209_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :poet_of_209_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_poet_of_209 ;
  lemon:objOfProp           :arg1_poet_of_209 ;
  lemon:condition           :poet_of_209_sense_1_condition .

:poet_of_209_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Poet> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Poet> .

:arg2_poet_of_209 lemon:marker :poet_of_209_form_preposition .
## Prepositions ##
:poet_of_209_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

