@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :film_of ;
  lemon:entry    :film_of_form_preposition .

:film_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :film_of_form ;
  lemon:otherForm  :film_of_singular ;
  lemon:otherForm  :film_of_plural ;
  lemon:sense          :film_of_sense_1 ;
  lemon:synBehavior    :film_of_nounpp .

:film_of_form a lemon:Form ;
  lemon:writtenRep "film"@en .

:film_of_singular a    lemon:Form ;
  lemon:writtenRep "film"@en ;
  lexinfo:number   lexinfo:singular .

:film_of_plural a   lemon:Form ;
  lemon:writtenRep "films"@en ;
  lexinfo:number    lexinfo:plural .

:film_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:film_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :film_of_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :film_of_sense_1_condition .

:film_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Film> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Film> .

:arg2 lemon:marker :film_of_form_preposition .
## Prepositions ##
:film_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

