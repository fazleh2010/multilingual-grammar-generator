@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :ideology_of_271 ;
  lemon:entry    :ideology_of_271_form_preposition .

:ideology_of_271 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :ideology_of_271_form ;
  lemon:otherForm  :ideology_of_271_singular ;
  lemon:otherForm  :ideology_of_271_plural ;
  lemon:sense          :ideology_of_271_sense_1 ;
  lemon:synBehavior    :ideology_of_271_nounpp .

:ideology_of_271_form a lemon:Form ;

:ideology_of_271_singular a    lemon:Form ;
  lemon:writtenRep "ideology"@en ;
  lexinfo:number   lexinfo:singular .

:ideology_of_271_plural a   lemon:Form ;
  lemon:writtenRep "ideologies"@en ;
  lexinfo:number    lexinfo:plural .

:ideology_of_271_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_ideology_of_271 ;
  lexinfo:prepositionalAdjunct :arg2_ideology_of_271 .

:ideology_of_271_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :ideology_of_271_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_ideology_of_271 ;
  lemon:objOfProp           :arg1_ideology_of_271 ;
  lemon:condition           :ideology_of_271_sense_1_condition .

:ideology_of_271_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Ideology> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Ideology> .

:arg2_ideology_of_271 lemon:marker :ideology_of_271_form_preposition .
## Prepositions ##
:ideology_of_271_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

