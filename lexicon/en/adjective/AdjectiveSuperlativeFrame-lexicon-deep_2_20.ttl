@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix oils:     <http://lemon-model.net/oils#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :deep_2_20 ;
  lemon:entry    :form_deep_2_20_preposition ;
  lemon:entry    :deep_2_20_res .

:deep_2_20 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_deep_2_20 ;
  lemon:otherForm      :form_deep_2_20_comperative ;
  lemon:otherForm      :form_deep_2_20_superlative ;
  lemon:sense          :deep_2_20_sense_1 ;
  lemon:synBehavior    :deep_2_20_predFrame .

:form_deep_2_20 lemon:writtenRep "deep"@en .

:form_deep_2_20_comperative lemon:writtenRep "deeper"@en .

:form_deep_2_20_superlative lemon:writtenRep "deepest"@en .

:deep_2_20_predFrame a        lexinfo:AdjectiveSuperlativeFrame ;
  lexinfo:copulativeSubject :deep_2_20_PredSynArg .

:deep_2_20_sense_1 a  lemon:LexicalSense ;
  lemon:reference :deep_2_20_res ;
  lemon:isA       :deep_2_20_PredSynArg ;
  lemon:condition :deep_2_20_sense_1_condition .

:deep_2_20_res a   oils:CovariantScalar ;
  oils:boundTo  <http://dbpedia.org/ontology/maximumDepth> ;
  oils:degree   <http://lemon-model.net/oils/strong> .

:deep_2_20_sense_1_condition a lemon:condition ;
  lemon:propertyDomain   <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange    <http://dbpedia.org/ontology/Lake> .

## Prepositions ##
:form_deep_2_20_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

