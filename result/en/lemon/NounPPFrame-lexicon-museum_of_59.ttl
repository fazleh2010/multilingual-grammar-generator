@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :museum_of_59 ;
  lemon:entry    :museum_of_59_form_preposition .

:museum_of_59 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :museum_of_59_form ;
  lemon:otherForm  :museum_of_59_singular ;
  lemon:otherForm  :museum_of_59_plural ;
  lemon:sense          :museum_of_59_sense_1 ;
  lemon:synBehavior    :museum_of_59_nounpp .

:museum_of_59_form a lemon:Form ;

:museum_of_59_singular a    lemon:Form ;
  lemon:writtenRep "museum"@en ;
  lexinfo:number   lexinfo:singular .

:museum_of_59_plural a   lemon:Form ;
  lemon:writtenRep "museums"@en ;
  lexinfo:number    lexinfo:plural .

:museum_of_59_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_museum_of_59 ;
  lexinfo:prepositionalAdjunct :arg2_museum_of_59 .

:museum_of_59_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :museum_of_59_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_museum_of_59 ;
  lemon:objOfProp           :arg1_museum_of_59 ;
  lemon:condition           :museum_of_59_sense_1_condition .

:museum_of_59_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Museum> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Museum> .

:arg2_museum_of_59 lemon:marker :museum_of_59_form_preposition .
## Prepositions ##
:museum_of_59_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

