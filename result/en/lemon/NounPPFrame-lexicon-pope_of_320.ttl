@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :pope_of_320 ;
  lemon:entry    :pope_of_320_form_preposition .

:pope_of_320 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :pope_of_320_form ;
  lemon:otherForm  :pope_of_320_singular ;
  lemon:otherForm  :pope_of_320_plural ;
  lemon:sense          :pope_of_320_sense_1 ;
  lemon:synBehavior    :pope_of_320_nounpp .

:pope_of_320_form a lemon:Form ;
  lemon:writtenRep "pope"@en .

:pope_of_320_singular a    lemon:Form ;
  lemon:writtenRep "pope"@en ;
  lexinfo:number   lexinfo:singular .

:pope_of_320_plural a   lemon:Form ;
  lemon:writtenRep "popes"@en ;
  lexinfo:number    lexinfo:plural .

:pope_of_320_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_pope_of_320 ;
  lexinfo:prepositionalAdjunct :arg2_pope_of_320 .

:pope_of_320_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :pope_of_320_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_pope_of_320 ;
  lemon:objOfProp           :arg1_pope_of_320 ;
  lemon:condition           :pope_of_320_sense_1_condition .

:pope_of_320_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Pope> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Pope> .

:arg2_pope_of_320 lemon:marker :pope_of_320_form_preposition .
## Prepositions ##
:pope_of_320_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

