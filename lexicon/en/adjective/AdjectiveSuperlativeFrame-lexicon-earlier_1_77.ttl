@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix oils:     <http://lemon-model.net/oils#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :earlier_1_77 ;
  lemon:entry    :form_earlier_1_77_preposition ;
  lemon:entry    :earlier_1_77_res .

:earlier_1_77 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_earlier_1_77 ;
  lemon:otherForm      :form_earlier_1_77_comperative ;
  lemon:otherForm      :form_earlier_1_77_superlative ;
  lemon:sense          :earlier_1_77_sense_1 ;
  lemon:synBehavior    :earlier_1_77_predFrame .

:form_earlier_1_77 lemon:writtenRep "early"@en .

:form_earlier_1_77_comperative lemon:writtenRep "earlier"@en .

:form_earlier_1_77_superlative lemon:writtenRep "earliest"@en .

:earlier_1_77_predFrame a        lexinfo:AdjectiveSuperlativeFrame ;
  lexinfo:copulativeSubject :earlier_1_77_PredSynArg .

:earlier_1_77_sense_1 a  lemon:LexicalSense ;
