@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_write_9_53 ;
  lemon:entry    :write_9_53ed ;
  lemon:entry    :form_write_9_53_preposition .

:to_write_9_53 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_write_9_53 ;
  lemon:otherForm      :form_write_9_53s ;
  lemon:otherForm      :form_write_9_53ed ;
  lemon:otherForm      :form_write_9_53perfect ;
  lemon:sense          :write_9_53_sense_1 ;
  lemon:synBehavior    :write_9_53_frame_transitive .

:form_write_9_53 a         lemon:Form ;
  lemon:writtenRep     "write"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_write_9_53s a    lemon:Form ;
  lemon:writtenRep "writes"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_write_9_53ed a   lemon:Form ;
  lemon:writtenRep "wrote"@en ;
  lexinfo:tense    lexinfo:past .

:form_write_9_53perfect a   lemon:Form ;
  lemon:writtenRep "wrote"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:write_9_53_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :write_9_53_subj ;
  lexinfo:directObject     :write_9_53_obj .

:write_9_53_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :write_9_53_sense_1 ;
  lemon:reference   <http://dbpedia.org/property/editor> ;
  lemon:subjOfProp  :write_9_53_obj ;
  lemon:objOfProp   :write_9_53_subj ;
  lemon:condition   :write_9_53_sense_1_condition .


:write_9_53_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:write_9_53_obj lemon:marker :form_write_9_53_preposition .

## Prepositions ##
:form_write_9_53_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

