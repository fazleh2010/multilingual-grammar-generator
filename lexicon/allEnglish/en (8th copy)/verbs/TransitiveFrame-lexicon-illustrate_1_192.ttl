@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_illustrate_1_192 ;
  lemon:entry    :illustrate_1_192ed ;
  lemon:entry    :form_illustrate_1_192_preposition .

:to_illustrate_1_192 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_illustrate_1_192 ;
  lemon:otherForm      :form_illustrate_1_192s ;
  lemon:otherForm      :form_illustrate_1_192ed ;
  lemon:otherForm      :form_illustrate_1_192perfect ;
  lemon:sense          :illustrate_1_192_sense_1 ;
  lemon:synBehavior    :illustrate_1_192_frame_transitive .

:form_illustrate_1_192 a         lemon:Form ;
  lemon:writtenRep     "illustrate"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_illustrate_1_192s a    lemon:Form ;
  lemon:writtenRep "illustrates"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_illustrate_1_192ed a   lemon:Form ;
  lemon:writtenRep "illustrated"@en ;
  lexinfo:tense    lexinfo:past .

:form_illustrate_1_192perfect a   lemon:Form ;
  lemon:writtenRep "illustrated"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:illustrate_1_192_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :illustrate_1_192_subj ;
  lexinfo:directObject     :illustrate_1_192_obj .

:illustrate_1_192_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :illustrate_1_192_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/illustrator> ;
  lemon:subjOfProp  :illustrate_1_192_obj ;
  lemon:objOfProp   :illustrate_1_192_subj ;
  lemon:condition   :illustrate_1_192_sense_1_condition .


:illustrate_1_192_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/WrittenWork> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:illustrate_1_192_obj lemon:marker :form_illustrate_1_192_preposition .

## Prepositions ##
:form_illustrate_1_192_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

