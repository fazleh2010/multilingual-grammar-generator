@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
	lemon:language "en" ;
	lemon:entry    :danish ;
	lemon:entry    :danish_res .

:danish a             lemon:LexicalEntry ;
	lexinfo:partOfSpeech lexinfo:adjective ;
	lemon:canonicalForm  :danish_lemma ;
	lemon:synBehavior    :danish_attrFrame, :danish_predFrame ;
	lemon:sense          :danish_sense .

:danish_lemma lemon:writtenRep "danish"@en .

:danish_predFrame a        lexinfo:AdjectivePredicateFrame ;
	lexinfo:copulativeSubject :danish_PredSynArg .

:danish_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
	lexinfo:attributiveArg :danish_AttrSynArg .

:danish_sense a  lemon:LexicalSense ;
	lemon:reference :danish_res ;
	lemon:isA       :danish_AttrSynArg, :danish_PredSynArg .

:danish_res a   owl:Restriction ;
	owl:onProperty <http://dbpedia.org/ontology/country> ;
	owl:hasValue   <http://dbpedia.org/resource/Denmark> .
