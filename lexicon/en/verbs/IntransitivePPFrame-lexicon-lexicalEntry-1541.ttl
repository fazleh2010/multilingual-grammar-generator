@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-1541 ;
  lemon:entry    :form_lexicalEntry-1541_preposition .

:lexicalEntry-1541 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-1541 ;
  lemon:otherForm      :form_lexicalEntry-1541_past ;
  lemon:sense          :lexicalEntry-1541_sense_1 ;
  lemon:synBehavior    :lexicalEntry-1541_frame .

:form_lexicalEntry-1541 a           lemon:Form ;
  lemon:writtenRep     "write"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-1541 a      lemon:Form ;
  lemon:writtenRep "writes"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-1541_past a lemon:Form ;
  lemon:writtenRep  "wrote"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-1541perfect a   lemon:Form ;
  lemon:writtenRep "written"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-1541_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-1541_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-1541_obj .

:lexicalEntry-1541_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1541_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/isbn> ;
  lemon:subjOfProp  :lexicalEntry-1541_obj ;
  lemon:objOfProp   :lexicalEntry-1541_subj ;
  lemon:condition   :lexicalEntry-1541_sense_1_condition .

:lexicalEntry-1541_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Book> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#string> .


:lexicalEntry-1541_obj lemon:marker :form_lexicalEntry-1541_preposition .

## Prepositions ##
:form_lexicalEntry-1541_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

