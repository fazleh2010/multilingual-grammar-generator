@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :mountain_of_451_sense_1_res ;
  lemon:entry    :mountain_of_451 .

:mountain_of_451 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :mountain_of_451_form ;
  lemon:otherForm  :mountain_of_451_singular ;
  lemon:otherForm  :mountain_of_451_plural ;
  lemon:sense          :mountain_of_451_sense_1 ;
  lemon:synBehavior    :mountain_of_451_predFrame .

:mountain_of_451_form
 lemon:writtenRep "mountain"@en .

:mountain_of_451_singular a    lemon:Form ;
  lemon:writtenRep "mountain"@en ;
  lexinfo:number   lexinfo:singular .

:mountain_of_451_plural a   lemon:Form ;
  lemon:writtenRep "mountains"@en ;
  lexinfo:number    lexinfo:plural .

:mountain_of_451_sense_1 a  lemon:LexicalSense ;
  lemon:reference :mountain_of_451_sense_1_res ;
  lemon:isA       :mountain_of_451_PredSynArg .

:mountain_of_451_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Mountain> .
