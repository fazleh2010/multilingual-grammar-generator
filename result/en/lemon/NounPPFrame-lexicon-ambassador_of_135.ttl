@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :ambassador_of_135 ;
  lemon:entry    :ambassador_of_135_form_preposition .

:ambassador_of_135 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :ambassador_of_135_form ;
  lemon:otherForm  :ambassador_of_135_singular ;
  lemon:otherForm  :ambassador_of_135_plural ;
  lemon:sense          :ambassador_of_135_sense_1 ;
  lemon:synBehavior    :ambassador_of_135_nounpp .

:ambassador_of_135_form a lemon:Form ;
  lemon:writtenRep "Ambassador"@en .

:ambassador_of_135_singular a    lemon:Form ;
  lemon:writtenRep "Ambassador"@en ;
  lexinfo:number   lexinfo:singular .

:ambassador_of_135_plural a   lemon:Form ;
  lemon:writtenRep "Ambassadors"@en ;
  lexinfo:number    lexinfo:plural .

:ambassador_of_135_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_ambassador_of_135 ;
  lexinfo:prepositionalAdjunct :arg2_ambassador_of_135 .

:ambassador_of_135_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :ambassador_of_135_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_ambassador_of_135 ;
  lemon:objOfProp           :arg1_ambassador_of_135 ;
  lemon:condition           :ambassador_of_135_sense_1_condition .

:ambassador_of_135_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Ambassador> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Ambassador> .

:arg2_ambassador_of_135 lemon:marker :ambassador_of_135_form_preposition .
## Prepositions ##
:ambassador_of_135_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

