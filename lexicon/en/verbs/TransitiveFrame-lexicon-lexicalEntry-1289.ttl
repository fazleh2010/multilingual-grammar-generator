@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-1289 ;
  lemon:entry    :lexicalEntry-1289ed ;
  lemon:entry    :form_lexicalEntry-1289_preposition .

:to_lexicalEntry-1289 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-1289 ;
  lemon:otherForm      :form_lexicalEntry-1289s ;
  lemon:otherForm      :form_lexicalEntry-1289ed ;
  lemon:otherForm      :form_lexicalEntry-1289perfect ;
  lemon:sense          :lexicalEntry-1289_sense_1 ;
  lemon:synBehavior    :lexicalEntry-1289_frame_transitive .

:form_lexicalEntry-1289 a         lemon:Form ;
  lemon:writtenRep     "based in"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-1289s a    lemon:Form ;
  lemon:writtenRep "based in"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-1289ed a   lemon:Form ;
  lemon:writtenRep "based in"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-1289perfect a   lemon:Form ;
  lemon:writtenRep "based in"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-1289_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-1289_subj ;
  lexinfo:directObject     :lexicalEntry-1289_obj .

:lexicalEntry-1289_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1289_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/ground> ;
  lemon:subjOfProp  :lexicalEntry-1289_obj ;
  lemon:objOfProp   :lexicalEntry-1289_subj ;
  lemon:condition   :lexicalEntry-1289_sense_1_condition .


:lexicalEntry-1289_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/SportsClub> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Stadium> .

:lexicalEntry-1289ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-1289ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-1289ed_by ;
  lemon:synBehavior    :lexicalEntry-1289ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-1289ed_ontomap .

:form_lexicalEntry-1289ed_canonical a lemon:Form ;
  lemon:writtenRep         "based in"@en .

:form_lexicalEntry-1289ed_by a    lemon:Form ;
  lemon:writtenRep     "based in"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-1289ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-1289ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-1289ed_obj .

:lexicalEntry-1289ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1289ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/ground> ;
  lemon:subjOfProp  :lexicalEntry-1289ed_subj ;
  lemon:objOfProp   :lexicalEntry-1289ed_obj ;
  lemon:condition   :lexicalEntry-1289_sense_1_condition .
:lexicalEntry-1289ed_obj lemon:marker :form_lexicalEntry-1289_preposition .

## Prepositions ##
:form_lexicalEntry-1289_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

