@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :judge_300_sense_1_res ;
  lemon:entry    :judge_300 .

:judge_300 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :judge_300_form ;
  lemon:otherForm  :judge_300_singular ;
  lemon:otherForm  :judge_300_plural ;
  lemon:sense          :judge_300_sense_1 ;
  lemon:synBehavior    :judge_300_predFrame .

:judge_300_form
 lemon:writtenRep "judge"@en .

:judge_300_singular a    lemon:Form ;
  lemon:writtenRep "judge"@en ;
  lexinfo:number   lexinfo:singular .

:judge_300_plural a   lemon:Form ;
  lemon:writtenRep "judges"@en ;
  lexinfo:number    lexinfo:plural .

:judge_300_sense_1 a  lemon:LexicalSense ;
  lemon:reference :judge_300_sense_1_res ;
  lemon:isA       :judge_300_PredSynArg .

:judge_300_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Judge> .