@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :writer_of_205 ;
  lemon:entry    :writer_of_205_form_preposition .

:writer_of_205 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :writer_of_205_form ;
  lemon:otherForm  :writer_of_205_singular ;
  lemon:otherForm  :writer_of_205_plural ;
  lemon:sense          :writer_of_205_sense_1 ;
  lemon:synBehavior    :writer_of_205_nounpp .

:writer_of_205_form a lemon:Form ;

:writer_of_205_singular a    lemon:Form ;
  lemon:writtenRep "writer"@en ;
  lexinfo:number   lexinfo:singular .

:writer_of_205_plural a   lemon:Form ;
  lemon:writtenRep "writers"@en ;
  lexinfo:number    lexinfo:plural .

:writer_of_205_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_writer_of_205 ;
  lexinfo:prepositionalAdjunct :arg2_writer_of_205 .

:writer_of_205_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :writer_of_205_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_writer_of_205 ;
  lemon:objOfProp           :arg1_writer_of_205 ;
  lemon:condition           :writer_of_205_sense_1_condition .

:writer_of_205_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Writer> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Writer> .

:arg2_writer_of_205 lemon:marker :writer_of_205_form_preposition .
## Prepositions ##
:writer_of_205_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

