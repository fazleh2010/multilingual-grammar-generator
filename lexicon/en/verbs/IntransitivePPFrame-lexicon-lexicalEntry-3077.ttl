@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-3077 ;
  lemon:entry    :form_lexicalEntry-3077_preposition .

:lexicalEntry-3077 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-3077 ;
  lemon:otherForm      :form_lexicalEntry-3077_past ;
  lemon:sense          :lexicalEntry-3077_sense_1 ;
  lemon:synBehavior    :lexicalEntry-3077_frame .

:form_lexicalEntry-3077 a           lemon:Form ;
  lemon:writtenRep     "grow"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-3077 a      lemon:Form ;
  lemon:writtenRep "grows"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-3077_past a lemon:Form ;
  lemon:writtenRep  "grew"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-3077perfect a   lemon:Form ;
  lemon:writtenRep "grown"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-3077_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-3077_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-3077_obj .

:lexicalEntry-3077_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3077_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/wineRegion> ;
  lemon:subjOfProp  :lexicalEntry-3077_obj ;
  lemon:objOfProp   :lexicalEntry-3077_subj ;
  lemon:condition   :lexicalEntry-3077_sense_1_condition .

:lexicalEntry-3077_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Grape> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Place> .


:lexicalEntry-3077_obj lemon:marker :form_lexicalEntry-3077_preposition .

## Prepositions ##
:form_lexicalEntry-3077_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

