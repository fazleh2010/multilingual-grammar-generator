  lemon:canonicalForm  :first-person-to-climb_of_550_form ;
  lemon:otherForm  :first-person-to-climb_of_550_singular ;
  lemon:otherForm  :first-person-to-climb_of_550_plural ;
  lemon:sense          :first-person-to-climb_of_550_sense_1 ;
  lemon:synBehavior    :first-person-to-climb_of_550_nounpp .

:first-person-to-climb_of_550_form a lemon:Form ;
  lemon:writtenRep "first person to climb "@en .

:first-person-to-climb_of_550_singular a    lemon:Form ;
