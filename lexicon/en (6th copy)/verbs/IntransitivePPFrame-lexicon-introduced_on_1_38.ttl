@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :introduced_on_1_38 ;
  lemon:entry    :form_introduced_on_1_38_preposition .

:introduced_on_1_38 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_introduced_on_1_38 ;
  lemon:otherForm      :form_introduced_on_1_38_present3rd ;
  lemon:otherForm      :form_introduced_on_1_38_past ;
  lemon:otherForm      :form_introduced_on_1_38_perfect ;
  lemon:sense          :introduced_on_1_38_sense_1 ;
  lemon:synBehavior    :introduced_on_1_38_frame .

:form_introduced_on_1_38 a           lemon:Form ;
  lemon:writtenRep     "introduce"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_introduced_on_1_38_present3rd a      lemon:Form ;
  lemon:writtenRep "introduces"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_introduced_on_1_38_past a lemon:Form ;
  lemon:writtenRep  "introduced"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_introduced_on_1_38_perfect a   lemon:Form ;
  lemon:writtenRep "introduced"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:introduced_on_1_38_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :introduced_on_1_38_subj ;
  lexinfo:prepositionalAdjunct :introduced_on_1_38_obj .

:introduced_on_1_38_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :introduced_on_1_38_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/introduced> ;
  lemon:subjOfProp  :introduced_on_1_38_subj ;
  lemon:objOfProp   :introduced_on_1_38_obj ;
  lemon:condition   :introduced_on_1_38_sense_1_condition .

:introduced_on_1_38_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Beverage> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:introduced_on_1_38_obj lemon:marker :form_introduced_on_1_38_preposition .

## Prepositions ##
:form_introduced_on_1_38_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "on"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

