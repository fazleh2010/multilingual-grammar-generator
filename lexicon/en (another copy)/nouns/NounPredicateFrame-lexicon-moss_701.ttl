:moss_701_sense_1 a  lemon:LexicalSense ;
  lemon:reference :moss_701_sense_1_res ;
  lemon:isA       :moss_701_PredSynArg .

:moss_701_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
