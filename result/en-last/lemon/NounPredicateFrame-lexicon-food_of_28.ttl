@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :food_of_28_sense_1_res ;
  lemon:entry    :food_of_28 .

:food_of_28 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :food_of_28_form ;
  lemon:otherForm  :food_of_28_singular ;
  lemon:otherForm  :food_of_28_plural ;
  lemon:sense          :food_of_28_sense_1 ;
  lemon:synBehavior    :food_of_28_predFrame .

:food_of_28_form
 lemon:writtenRep "food"@en .

:food_of_28_singular a    lemon:Form ;
  lemon:writtenRep "food"@en ;
  lexinfo:number   lexinfo:singular .

:food_of_28_plural a   lemon:Form ;
  lemon:writtenRep "foods"@en ;
  lexinfo:number    lexinfo:plural .

:food_of_28_sense_1 a  lemon:LexicalSense ;
  lemon:reference :food_of_28_sense_1_res ;
  lemon:isA       :food_of_28_PredSynArg .

:food_of_28_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Food> .
