@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :restaurant_of_282 ;
  lemon:entry    :restaurant_of_282_form_preposition .

:restaurant_of_282 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :restaurant_of_282_form ;
  lemon:otherForm  :restaurant_of_282_singular ;
  lemon:otherForm  :restaurant_of_282_plural ;
  lemon:sense          :restaurant_of_282_sense_1 ;
  lemon:synBehavior    :restaurant_of_282_nounpp .

:restaurant_of_282_form a lemon:Form ;

:restaurant_of_282_singular a    lemon:Form ;
  lemon:writtenRep "Restaurant"@en ;
