@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_distribute_1_360 ;
  lemon:entry    :distribute_1_360ed ;
  lemon:entry    :form_distribute_1_360_preposition .

:to_distribute_1_360 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_distribute_1_360 ;
  lemon:otherForm      :form_distribute_1_360s ;
  lemon:otherForm      :form_distribute_1_360ed ;
  lemon:otherForm      :form_distribute_1_360perfect ;
  lemon:sense          :distribute_1_360_sense_1 ;
  lemon:synBehavior    :distribute_1_360_frame_transitive .

:form_distribute_1_360 a         lemon:Form ;
  lemon:writtenRep     "distribute"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_distribute_1_360s a    lemon:Form ;
  lemon:writtenRep "distributes"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_distribute_1_360ed a   lemon:Form ;
  lemon:writtenRep "distributed"@en ;
  lexinfo:tense    lexinfo:past .

:form_distribute_1_360perfect a   lemon:Form ;
  lemon:writtenRep "distributed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:distribute_1_360_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :distribute_1_360_subj ;
  lexinfo:directObject     :distribute_1_360_obj .

:distribute_1_360_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :distribute_1_360_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/distributor> ;
  lemon:subjOfProp  :distribute_1_360_obj ;
  lemon:objOfProp   :distribute_1_360_subj ;
  lemon:condition   :distribute_1_360_sense_1_condition .


:distribute_1_360_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Organisation> .


:distribute_1_360_obj lemon:marker :form_distribute_1_360_preposition .

## Prepositions ##
:form_distribute_1_360_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

