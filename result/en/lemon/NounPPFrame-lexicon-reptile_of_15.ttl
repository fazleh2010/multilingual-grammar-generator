@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :reptile_of_15 ;
  lemon:entry    :reptile_of_15_form_preposition .

:reptile_of_15 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :reptile_of_15_form ;
  lemon:otherForm  :reptile_of_15_singular ;
  lemon:otherForm  :reptile_of_15_plural ;
  lemon:sense          :reptile_of_15_sense_1 ;
  lemon:synBehavior    :reptile_of_15_nounpp .

:reptile_of_15_form a lemon:Form ;

:reptile_of_15_singular a    lemon:Form ;
  lemon:writtenRep "Reptile"@en ;
  lexinfo:number   lexinfo:singular .

:reptile_of_15_plural a   lemon:Form ;
  lemon:writtenRep "Reptiles"@en ;
  lexinfo:number    lexinfo:plural .

:reptile_of_15_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_reptile_of_15 ;
  lexinfo:prepositionalAdjunct :arg2_reptile_of_15 .

:reptile_of_15_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :reptile_of_15_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_reptile_of_15 ;
  lemon:objOfProp           :arg1_reptile_of_15 ;
  lemon:condition           :reptile_of_15_sense_1_condition .

:reptile_of_15_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Reptile> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Reptile> .

:arg2_reptile_of_15 lemon:marker :reptile_of_15_form_preposition .
## Prepositions ##
:reptile_of_15_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

