@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :wrestler_of ;
  lemon:entry    :wrestler_of_form_preposition .

:wrestler_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :wrestler_of_form ;
  lemon:otherForm  :wrestler_of_singular ;
  lemon:otherForm  :wrestler_of_plural ;
  lemon:sense          :wrestler_of_sense_1 ;
  lemon:synBehavior    :wrestler_of_nounpp .

:wrestler_of_form a lemon:Form ;
  lemon:writtenRep "Wrestler"@en .

:wrestler_of_singular a    lemon:Form ;
  lemon:writtenRep "Wrestler"@en ;
  lexinfo:number   lexinfo:singular .

:wrestler_of_plural a   lemon:Form ;
  lemon:writtenRep "Wrestlers"@en ;
  lexinfo:number    lexinfo:plural .

:wrestler_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:wrestler_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :wrestler_of_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :wrestler_of_sense_1_condition .

:wrestler_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Wrestler> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Wrestler> .

:arg2 lemon:marker :wrestler_of_form_preposition .
## Prepositions ##
:wrestler_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

