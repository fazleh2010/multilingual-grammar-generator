@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-960 ;
  lemon:entry    :form_lexicalEntry-960_preposition .

:lexicalEntry-960 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-960 ;
  lemon:otherForm      :form_lexicalEntry-960_past ;
  lemon:sense          :lexicalEntry-960_sense_1 ;
  lemon:synBehavior    :lexicalEntry-960_frame .

:form_lexicalEntry-960 a           lemon:Form ;
  lemon:writtenRep     "found"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-960 a      lemon:Form ;
  lemon:writtenRep "found"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-960_past a lemon:Form ;
  lemon:writtenRep  "founded"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-960perfect a   lemon:Form ;
  lemon:writtenRep "founded"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-960_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-960_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-960_obj .

:lexicalEntry-960_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-960_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/founder> ;
  lemon:subjOfProp  :lexicalEntry-960_obj ;
  lemon:objOfProp   :lexicalEntry-960_subj ;
  lemon:condition   :lexicalEntry-960_sense_1_condition .

:lexicalEntry-960_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Publisher> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Agent> .


:lexicalEntry-960_obj lemon:marker :form_lexicalEntry-960_preposition .

## Prepositions ##
:form_lexicalEntry-960_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

