@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :publisher_of_324 ;
  lemon:entry    :publisher_of_324_form_preposition .

:publisher_of_324 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :publisher_of_324_form ;
  lemon:otherForm  :publisher_of_324_singular ;
  lemon:otherForm  :publisher_of_324_plural ;
  lemon:sense          :publisher_of_324_sense_1 ;
  lemon:synBehavior    :publisher_of_324_nounpp .

:publisher_of_324_form a lemon:Form ;

:publisher_of_324_singular a    lemon:Form ;
  lemon:writtenRep "publisher"@en ;
  lexinfo:number   lexinfo:singular .

:publisher_of_324_plural a   lemon:Form ;
  lemon:writtenRep "publishers"@en ;
  lexinfo:number    lexinfo:plural .

:publisher_of_324_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_publisher_of_324 ;
  lexinfo:prepositionalAdjunct :arg2_publisher_of_324 .

:publisher_of_324_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :publisher_of_324_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_publisher_of_324 ;
  lemon:objOfProp           :arg1_publisher_of_324 ;
  lemon:condition           :publisher_of_324_sense_1_condition .

:publisher_of_324_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Publisher> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Publisher> .

:arg2_publisher_of_324 lemon:marker :publisher_of_324_form_preposition .
## Prepositions ##
:publisher_of_324_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

