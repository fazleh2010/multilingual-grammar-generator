@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :archaea_651_sense_1_res ;
  lemon:entry    :archaea_651 .

:archaea_651 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :archaea_651_form ;
  lemon:otherForm  :archaea_651_singular ;
  lemon:otherForm  :archaea_651_plural ;
  lemon:sense          :archaea_651_sense_1 ;
  lemon:synBehavior    :archaea_651_predFrame .

:archaea_651_form
 lemon:writtenRep "archaea"@en .

:archaea_651_singular a    lemon:Form ;
  lemon:writtenRep "archaea"@en ;
  lexinfo:number   lexinfo:singular .

:archaea_651_plural a   lemon:Form ;
  lemon:writtenRep "archaeas"@en ;
  lexinfo:number    lexinfo:plural .

:archaea_651_sense_1 a  lemon:LexicalSense ;
  lemon:reference :archaea_651_sense_1_res ;
  lemon:isA       :archaea_651_PredSynArg .

:archaea_651_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Archaea> .