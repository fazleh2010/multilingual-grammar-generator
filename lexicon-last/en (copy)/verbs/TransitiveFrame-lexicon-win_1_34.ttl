@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_win_1_34 ;
  lemon:entry    :win_1_34ed ;
  lemon:entry    :form_win_1_34_preposition .

:to_win_1_34 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_win_1_34 ;
  lemon:otherForm      :form_win_1_34s ;
  lemon:otherForm      :form_win_1_34ed ;
  lemon:otherForm      :form_win_1_34perfect ;
  lemon:sense          :win_1_34_sense_1 ;
  lemon:synBehavior    :win_1_34_frame_transitive .

:form_win_1_34 a         lemon:Form ;
  lemon:writtenRep     "win"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_win_1_34s a    lemon:Form ;
  lemon:writtenRep "wins"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_win_1_34ed a   lemon:Form ;
  lemon:writtenRep "won"@en ;
  lexinfo:tense    lexinfo:past .

:form_win_1_34perfect a   lemon:Form ;
  lemon:writtenRep "won"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:win_1_34_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :win_1_34_obj ;
  lexinfo:directObject     :win_1_34_subj .

:win_1_34_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :win_1_34_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/award> ;
  lemon:subjOfProp  :win_1_34_obj ;
  lemon:objOfProp   :win_1_34_subj ;
  lemon:condition   :win_1_34_sense_1_condition .


:win_1_34_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Scientist> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Award> .


:win_1_34_obj lemon:marker :form_win_1_34_preposition .

## Prepositions ##
:form_win_1_34_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

