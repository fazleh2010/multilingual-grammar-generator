@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_win_14 ;
  lemon:entry    :win_14ed ;
  lemon:entry    :form_win_14_preposition .

:to_win_14 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_win_14 ;
  lemon:otherForm      :form_win_14s ;
  lemon:otherForm      :form_win_14ed ;
  lemon:otherForm      :form_win_14perfect ;
  lemon:sense          :win_14_sense_1 ;
  lemon:synBehavior    :win_14_frame_transitive .

:form_win_14 a         lemon:Form ;
  lemon:writtenRep     "win"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_win_14s a    lemon:Form ;
  lemon:writtenRep "won"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_win_14ed a   lemon:Form ;
  lemon:writtenRep "won"@en ;
  lexinfo:tense    lexinfo:past .

:form_win_14perfect a   lemon:Form ;
  lemon:writtenRep "won"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:win_14_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :win_14_subj ;
  lexinfo:directObject     :win_14_obj .

:win_14_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :win_14_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/goldMedalist> ;
  lemon:subjOfProp  :win_14_obj ;
  lemon:objOfProp   :win_14_subj ;
  lemon:condition   :win_14_sense_1_condition .


:win_14_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Event> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:win_14_obj lemon:marker :form_win_14_preposition .

## Prepositions ##
:form_win_14_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

