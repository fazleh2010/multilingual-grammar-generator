@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :knownfor_2_85 ;
  lemon:entry    :form_knownfor_2_85_preposition .

:knownfor_2_85 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_knownfor_2_85 ;
  lemon:otherForm      :form_knownfor_2_85_present3rd ;
  lemon:otherForm      :form_knownfor_2_85_past ;
  lemon:otherForm      :form_knownfor_2_85_perfect ;
  lemon:sense          :knownfor_2_85_sense_1 ;
  lemon:synBehavior    :knownfor_2_85_frame .

:form_knownfor_2_85 a           lemon:Form ;
  lemon:writtenRep     "known"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_knownfor_2_85_present3rd a      lemon:Form ;
  lemon:writtenRep "known"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_knownfor_2_85_past a lemon:Form ;
  lemon:writtenRep  "known"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_knownfor_2_85_perfect a   lemon:Form ;
  lemon:writtenRep "known"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:knownfor_2_85_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :knownfor_2_85_subj ;
  lexinfo:prepositionalAdjunct :knownfor_2_85_obj .

:knownfor_2_85_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :knownfor_2_85_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/knownFor> ;
  lemon:subjOfProp  :knownfor_2_85_subj ;
  lemon:objOfProp   :knownfor_2_85_obj ;
  lemon:condition   :knownfor_2_85_sense_1_condition .

:knownfor_2_85_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Award> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:knownfor_2_85_obj lemon:marker :form_knownfor_2_85_preposition .

## Prepositions ##
:form_knownfor_2_85_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "for"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

