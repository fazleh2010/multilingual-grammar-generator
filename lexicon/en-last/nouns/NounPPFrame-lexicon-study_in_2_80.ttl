@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :study_in_2_80 ;
  lemon:entry    :study_in_2_80_form_preposition .

:study_in_2_80 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :study_in_2_80_form ;
  lemon:otherForm  :study_in_2_80_singular ;
  lemon:otherForm  :study_in_2_80_plural ;
  lemon:sense          :study_in_2_80_sense_1 ;
  lemon:synBehavior    :study_in_2_80_nounpp .

:study_in_2_80_form a lemon:Form ;
  lemon:writtenRep "study"@en .

:study_in_2_80_singular a    lemon:Form ;
