@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :royalty_of_150 ;
  lemon:entry    :royalty_of_150_form_preposition .

:royalty_of_150 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :royalty_of_150_form ;
  lemon:otherForm  :royalty_of_150_singular ;
  lemon:otherForm  :royalty_of_150_plural ;
  lemon:sense          :royalty_of_150_sense_1 ;
  lemon:synBehavior    :royalty_of_150_nounpp .

:royalty_of_150_form a lemon:Form ;
  lemon:writtenRep "Royalty"@en .

:royalty_of_150_singular a    lemon:Form ;
  lemon:writtenRep "Royalty"@en ;
  lexinfo:number   lexinfo:singular .

:royalty_of_150_plural a   lemon:Form ;
  lemon:writtenRep "Royalties"@en ;
  lexinfo:number    lexinfo:plural .

:royalty_of_150_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_royalty_of_150 ;
  lexinfo:prepositionalAdjunct :arg2_royalty_of_150 .

:royalty_of_150_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :royalty_of_150_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_royalty_of_150 ;
  lemon:objOfProp           :arg1_royalty_of_150 ;
  lemon:condition           :royalty_of_150_sense_1_condition .

:royalty_of_150_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Royalty> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Royalty> .

:arg2_royalty_of_150 lemon:marker :royalty_of_150_form_preposition .
## Prepositions ##
:royalty_of_150_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

