@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix oils:     <http://lemon-model.net/oils#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :populated_1_581 ;
  lemon:entry    :form_populated_1_581_preposition ;
  lemon:entry    :populated_1_581_res .

:populated_1_581 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_populated_1_581 ;
  lemon:otherForm      :form_populated_1_581_comperative ;
  lemon:otherForm      :form_populated_1_581_superlative ;
  lemon:sense          :populated_1_581_sense_1 ;
  lemon:synBehavior    :populated_1_581_predFrame .

:form_populated_1_581 lemon:writtenRep "populated"@en .

:form_populated_1_581_comperative lemon:writtenRep "more populated"@en .

:form_populated_1_581_superlative lemon:writtenRep "most populated"@en .

:populated_1_581_predFrame a        lexinfo:AdjectiveSuperlativeFrame ;
  lexinfo:copulativeSubject :populated_1_581_PredSynArg .

:populated_1_581_sense_1 a  lemon:LexicalSense ;
  lemon:reference :populated_1_581_res ;
  lemon:isA       :populated_1_581_PredSynArg ;
  lemon:condition :populated_1_581_sense_1_condition .

:populated_1_581_res a   oils:CovariantScalar ;
