@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_succeed_1_12 ;
  lemon:entry    :succeed_1_12ed ;
  lemon:entry    :form_succeed_1_12_preposition .

:to_succeed_1_12 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_succeed_1_12 ;
  lemon:otherForm      :form_succeed_1_12s ;
  lemon:otherForm      :form_succeed_1_12ed ;
  lemon:otherForm      :form_succeed_1_12perfect ;
  lemon:sense          :succeed_1_12_sense_1 ;
  lemon:sense          :succeed_1_12_sense_2 ;
  lemon:synBehavior    :succeed_1_12_frame_transitive .

:form_succeed_1_12 a         lemon:Form ;
  lemon:writtenRep     "succeed"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_succeed_1_12s a    lemon:Form ;
  lemon:writtenRep "succeeds"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_succeed_1_12ed a   lemon:Form ;
  lemon:writtenRep "succeeded"@en ;
  lexinfo:tense    lexinfo:past .

:form_succeed_1_12perfect a   lemon:Form ;
  lemon:writtenRep "succeeded"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:succeed_1_12_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :succeed_1_12_subj ;
  lexinfo:directObject     :succeed_1_12_obj .

:succeed_1_12_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :succeed_1_12_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/successor> ;
  lemon:subjOfProp  :succeed_1_12_obj ;
  lemon:objOfProp   :succeed_1_12_subj ;
  lemon:condition   :succeed_1_12_sense_1_condition .


:succeed_1_12_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .

:succeed_1_12_sense_2 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :succeed_1_12_sense_2 ;
  lemon:reference   <http://dbpedia.org/property/successor> ;
  lemon:subjOfProp  :succeed_1_12_obj ;
  lemon:objOfProp   :succeed_1_12_subj ;
  lemon:condition   :succeed_1_12_sense_2_condition .


:succeed_1_12_sense_2_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:succeed_1_12_obj lemon:marker :form_succeed_1_12_preposition .

## Prepositions ##
:form_succeed_1_12_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

