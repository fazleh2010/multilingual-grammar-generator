@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :composer-of_280 ;
  lemon:entry    :composer-of_280_form_preposition .

:composer-of_280 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :composer-of_280_form ;
  lemon:otherForm  :composer-of_280_singular ;
  lemon:otherForm  :composer-of_280_plural ;
  lemon:sense          :composer-of_280_sense_1 ;
  lemon:synBehavior    :composer-of_280_nounpp .

:composer-of_280_form a lemon:Form ;
  lemon:writtenRep "music composer"@en .

:composer-of_280_singular a    lemon:Form ;
  lemon:writtenRep "music composer"@en ;
  lexinfo:number   lexinfo:singular .

:composer-of_280_plural a   lemon:Form ;
  lemon:writtenRep "music composers"@en ;
  lexinfo:number    lexinfo:plural .

:composer-of_280_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_composer-of_280 ;
  lexinfo:prepositionalAdjunct :arg2_composer-of_280 .

:composer-of_280_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :composer-of_280_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/composer> ;
  lemon:subjOfProp          :arg2_composer-of_280 ;
  lemon:objOfProp           :arg1_composer-of_280 ;
  lemon:condition           :composer-of_280_sense_1_condition .

:composer-of_280_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2_composer-of_280 lemon:marker :composer-of_280_form_preposition .
## Prepositions ##
:composer-of_280_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

