@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_write_4_56 ;
  lemon:entry    :write_4_56ed ;
  lemon:entry    :form_write_4_56_preposition .

:to_write_4_56 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_write_4_56 ;
  lemon:otherForm      :form_write_4_56s ;
  lemon:otherForm      :form_write_4_56ed ;
  lemon:otherForm      :form_write_4_56perfect ;
  lemon:sense          :write_4_56_sense_1 ;
  lemon:synBehavior    :write_4_56_frame_transitive .

:form_write_4_56 a         lemon:Form ;
  lemon:writtenRep     "write"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_write_4_56s a    lemon:Form ;
  lemon:writtenRep "writes"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_write_4_56ed a   lemon:Form ;
  lemon:writtenRep "wrote"@en ;
  lexinfo:tense    lexinfo:past .

:form_write_4_56perfect a   lemon:Form ;
  lemon:writtenRep "written"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:write_4_56_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :write_4_56_subj ;
  lexinfo:directObject     :write_4_56_obj .

:write_4_56_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :write_4_56_sense_1 ;
  lemon:reference   <wd:P50> ;
  lemon:subjOfProp  :write_4_56_obj ;
  lemon:objOfProp   :write_4_56_subj ;
  lemon:condition   :write_4_56_sense_1_condition .


:write_4_56_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <wd:Q7397> ;
  lemon:propertyRange  <wd:Q19088> .


:write_4_56_obj lemon:marker :form_write_4_56_preposition .

## Prepositions ##
:form_write_4_56_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

