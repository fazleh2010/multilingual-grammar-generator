@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix oils:     <http://lemon-model.net/oils#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :heavy_2_90 ;
  lemon:entry    :form_heavy_2_90_preposition ;
  lemon:entry    :heavy_2_90_res .

:heavy_2_90 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_heavy_2_90 ;
  lemon:otherForm      :form_heavy_2_90_comperative ;
  lemon:otherForm      :form_heavy_2_90_superlative ;
  lemon:sense          :heavy_2_90_sense_1 ;
  lemon:synBehavior    :heavy_2_90_predFrame .

:form_heavy_2_90 lemon:writtenRep "heavy"@en .

:form_heavy_2_90_comperative lemon:writtenRep "heavier"@en .

:form_heavy_2_90_superlative lemon:writtenRep "heaviest"@en .

:heavy_2_90_predFrame a        lexinfo:AdjectiveSuperlativeFrame ;
  lexinfo:copulativeSubject :heavy_2_90_PredSynArg .

:heavy_2_90_sense_1 a  lemon:LexicalSense ;
  lemon:reference :heavy_2_90_res ;
  lemon:isA       :heavy_2_90_PredSynArg ;
  lemon:condition :heavy_2_90_sense_1_condition .

:heavy_2_90_res a   oils:CovariantScalar ;
  oils:boundTo  <http://dbpedia.org/ontology/weight> ;
  oils:degree   <http://lemon-model.net/oils/high> .

:heavy_2_90_sense_1_condition a lemon:condition ;
  lemon:propertyDomain   <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange    <http://www.w3.org/2001/XMLSchema#double> .

## Prepositions ##
:form_heavy_2_90_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

