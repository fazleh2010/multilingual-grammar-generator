@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :cleric_337_sense_1_res ;
  lemon:entry    :cleric_337 .

:cleric_337 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :cleric_337_form ;
  lemon:otherForm  :cleric_337_singular ;
  lemon:otherForm  :cleric_337_plural ;
  lemon:sense          :cleric_337_sense_1 ;
  lemon:synBehavior    :cleric_337_predFrame .

:cleric_337_form
 lemon:writtenRep "cleric"@en .

:cleric_337_singular a    lemon:Form ;
  lemon:writtenRep "cleric"@en ;
  lexinfo:number   lexinfo:singular .

:cleric_337_plural a   lemon:Form ;
  lemon:writtenRep "clerics"@en ;
  lexinfo:number    lexinfo:plural .

:cleric_337_sense_1 a  lemon:LexicalSense ;
  lemon:reference :cleric_337_sense_1_res ;
  lemon:isA       :cleric_337_PredSynArg .

:cleric_337_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Cleric> .