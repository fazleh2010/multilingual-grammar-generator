@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :brewery_of_242_sense_1_res ;
  lemon:entry    :brewery_of_242 .

:brewery_of_242 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :brewery_of_242_form ;
  lemon:otherForm  :brewery_of_242_singular ;
  lemon:otherForm  :brewery_of_242_plural ;
  lemon:sense          :brewery_of_242_sense_1 ;
  lemon:synBehavior    :brewery_of_242_predFrame .

:brewery_of_242_form
 lemon:writtenRep "Brewery"@en .

:brewery_of_242_singular a    lemon:Form ;
  lemon:writtenRep "Brewery"@en ;
  lexinfo:number   lexinfo:singular .

:brewery_of_242_plural a   lemon:Form ;
  lemon:writtenRep "Brewerys"@en ;
  lexinfo:number    lexinfo:plural .

:brewery_of_242_sense_1 a  lemon:LexicalSense ;
  lemon:reference :brewery_of_242_sense_1_res ;
  lemon:isA       :brewery_of_242_PredSynArg .

:brewery_of_242_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Brewery> .
