@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :residence-of_406 ;
  lemon:entry    :residence-of_406_form_preposition .

:residence-of_406 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :residence-of_406_form ;
  lemon:otherForm  :residence-of_406_singular ;
  lemon:otherForm  :residence-of_406_plural ;
  lemon:sense          :residence-of_406_sense_1 ;
  lemon:synBehavior    :residence-of_406_nounpp .

:residence-of_406_form a lemon:Form ;
  lemon:writtenRep "residence"@en .

:residence-of_406_singular a    lemon:Form ;
