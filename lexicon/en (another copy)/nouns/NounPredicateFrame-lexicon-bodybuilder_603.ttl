@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :bodybuilder_603_sense_1_res ;
  lemon:entry    :bodybuilder_603 .

:bodybuilder_603 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :bodybuilder_603_form ;
  lemon:otherForm  :bodybuilder_603_singular ;
  lemon:otherForm  :bodybuilder_603_plural ;
  lemon:sense          :bodybuilder_603_sense_1 ;
  lemon:synBehavior    :bodybuilder_603_predFrame .

:bodybuilder_603_form
 lemon:writtenRep "bodybuilder"@en .

:bodybuilder_603_singular a    lemon:Form ;
  lemon:writtenRep "bodybuilder"@en ;
  lexinfo:number   lexinfo:singular .

:bodybuilder_603_plural a   lemon:Form ;
  lemon:writtenRep "bodybuilders"@en ;
  lexinfo:number    lexinfo:plural .

:bodybuilder_603_sense_1 a  lemon:LexicalSense ;
  lemon:reference :bodybuilder_603_sense_1_res ;
  lemon:isA       :bodybuilder_603_PredSynArg .

:bodybuilder_603_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Bodybuilder> .