@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :garden_672_sense_1_res ;
  lemon:entry    :garden_672 .

:garden_672 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :garden_672_form ;
  lemon:otherForm  :garden_672_singular ;
  lemon:otherForm  :garden_672_plural ;
  lemon:sense          :garden_672_sense_1 ;
  lemon:synBehavior    :garden_672_predFrame .

:garden_672_form
 lemon:writtenRep "garden"@en .

:garden_672_singular a    lemon:Form ;
  lemon:writtenRep "garden"@en ;
  lexinfo:number   lexinfo:singular .

:garden_672_plural a   lemon:Form ;
  lemon:writtenRep "gardens"@en ;
  lexinfo:number    lexinfo:plural .

:garden_672_sense_1 a  lemon:LexicalSense ;
  lemon:reference :garden_672_sense_1_res ;
  lemon:isA       :garden_672_PredSynArg .

:garden_672_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Garden> .