@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :daughter_of_78 ;
  lemon:entry    :daughter_of_78_form_preposition .

:daughter_of_78 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :daughter_of_78_form ;
  lemon:otherForm  :daughter_of_78_singular ;
  lemon:otherForm  :daughter_of_78_plural ;
  lemon:sense          :daughter_of_78_sense_1 ;
  lemon:sense          :daughter_of_78_sense_2 ;
  lemon:sense          :daughter_of_78_sense_3 ;
  lemon:synBehavior    :daughter_of_78_nounpp .

:daughter_of_78_form a lemon:Form ;
  lemon:writtenRep "daughter"@en .

:daughter_of_78_singular a    lemon:Form ;
  lemon:writtenRep "daughter"@en ;
  lexinfo:number   lexinfo:singular .

:daughter_of_78_plural a   lemon:Form ;
  lemon:writtenRep "daughters"@en ;
  lexinfo:number    lexinfo:plural .

:daughter_of_78_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_daughter_of_78 ;
  lexinfo:prepositionalAdjunct :arg2_daughter_of_78 .

:daughter_of_78_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :daughter_of_78_sense_1 ;
  lemon:reference           <wd:P40> ;
  lemon:subjOfProp          :arg2_daughter_of_78 ;
  lemon:objOfProp           :arg1_daughter_of_78 ;
  lemon:condition           :daughter_of_78_sense_1_condition .

:daughter_of_78_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <wd:Q19088> ;
  lemon:propertyRange   <wd:Q19088> .

:daughter_of_78_sense_2 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :daughter_of_78_sense_2 ;
  lemon:reference           <wd:P40> ;
  lemon:subjOfProp          :arg2_daughter_of_78 ;
  lemon:objOfProp           :arg1_daughter_of_78 ;
  lemon:condition           :daughter_of_78_sense_2_condition .

:daughter_of_78_sense_2_condition a lemon:condition ;
  lemon:propertyDomain  <wd:Q19088> ;
  lemon:propertyRange   <wd:Q19088> .

:daughter_of_78_sense_3 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :daughter_of_78_sense_3 ;
  lemon:reference           <wd:P26> ;
  lemon:subjOfProp          :arg2_daughter_of_78 ;
  lemon:objOfProp           :arg1_daughter_of_78 ;
  lemon:condition           :daughter_of_78_sense_3_condition .

:daughter_of_78_sense_3_condition a lemon:condition ;
  lemon:propertyDomain  <wd:Q19088> ;
  lemon:propertyRange   <wd:Q19088> .

:arg2_daughter_of_78 lemon:marker :daughter_of_78_form_preposition .
## Prepositions ##
:daughter_of_78_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

