@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :bridge_of_218_sense_1_res ;
  lemon:entry    :bridge_of_218 .

:bridge_of_218 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :bridge_of_218_form ;
  lemon:otherForm  :bridge_of_218_singular ;
  lemon:otherForm  :bridge_of_218_plural ;
  lemon:sense          :bridge_of_218_sense_1 ;
  lemon:synBehavior    :bridge_of_218_predFrame .

:bridge_of_218_form
 lemon:writtenRep "Bridge"@en .

:bridge_of_218_singular a    lemon:Form ;
  lemon:writtenRep "Bridge"@en ;
  lexinfo:number   lexinfo:singular .

:bridge_of_218_plural a   lemon:Form ;
  lemon:writtenRep "Bridges"@en ;
  lexinfo:number    lexinfo:plural .

:bridge_of_218_sense_1 a  lemon:LexicalSense ;
  lemon:reference :bridge_of_218_sense_1_res ;
  lemon:isA       :bridge_of_218_PredSynArg .

:bridge_of_218_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Bridge> .
