@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :bengali_310_sense_1_res ;
  lemon:entry    :bengali_310 .

:bengali_310 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :bengali_310_lemma ;
  lemon:sense          :bengali_310_sense_1 ;
  lemon:synBehavior    :bengali_310_attrFrame, :bengali_310_predFrame .

:bengali_310_lemma lemon:writtenRep "bengali"@en .

:bengali_310_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :bengali_310_PredSynArg .

:bengali_310_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :bengali_310_AttrSynArg .

:bengali_310_sense_1 a  lemon:LexicalSense ;
  lemon:reference :bengali_310_sense_1_res ;
  lemon:isA       :bengali_310_AttrSynArg, :bengali_310_PredSynArg .

:bengali_310_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Bangladesh> .
