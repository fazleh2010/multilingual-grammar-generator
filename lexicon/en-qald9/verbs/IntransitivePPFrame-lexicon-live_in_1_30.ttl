@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :live_in_1_30 ;
  lemon:entry    :form_live_in_1_30_preposition .

:live_in_1_30 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_live_in_1_30 ;
  lemon:otherForm      :form_live_in_1_30_present3rd ;
  lemon:otherForm      :form_live_in_1_30_past ;
  lemon:otherForm      :form_live_in_1_30_perfect ;
  lemon:sense          :live_in_1_30_sense_1 ;
  lemon:synBehavior    :live_in_1_30_frame .

:form_live_in_1_30 a           lemon:Form ;
  lemon:writtenRep     "live"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_live_in_1_30_present3rd a      lemon:Form ;
  lemon:writtenRep "lives"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_live_in_1_30_past a lemon:Form ;
  lemon:writtenRep  "lived"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_live_in_1_30_perfect a   lemon:Form ;
  lemon:writtenRep "lived"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:live_in_1_30_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :live_in_1_30_subj ;
  lexinfo:prepositionalAdjunct :live_in_1_30_obj .

:live_in_1_30_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :live_in_1_30_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/residence> ;
  lemon:subjOfProp  :live_in_1_30_subj ;
  lemon:objOfProp   :live_in_1_30_obj ;
  lemon:condition   :live_in_1_30_sense_1_condition .

:live_in_1_30_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/City> .


:live_in_1_30_obj lemon:marker :form_live_in_1_30_preposition .

## Prepositions ##
:form_live_in_1_30_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

