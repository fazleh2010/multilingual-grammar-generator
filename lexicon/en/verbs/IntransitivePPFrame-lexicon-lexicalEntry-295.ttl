@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-295 ;
  lemon:entry    :form_lexicalEntry-295_preposition .

:lexicalEntry-295 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-295 ;
  lemon:otherForm      :form_lexicalEntry-295_past ;
  lemon:sense          :lexicalEntry-295_sense_1 ;
  lemon:synBehavior    :lexicalEntry-295_frame .

:form_lexicalEntry-295 a           lemon:Form ;
  lemon:writtenRep     "built"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-295 a      lemon:Form ;
  lemon:writtenRep "built"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-295_past a lemon:Form ;
  lemon:writtenRep  "built"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-295perfect a   lemon:Form ;
  lemon:writtenRep "built"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-295_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-295_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-295_obj .

:lexicalEntry-295_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-295_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/crosses> ;
  lemon:subjOfProp  :lexicalEntry-295_obj ;
  lemon:objOfProp   :lexicalEntry-295_subj ;
  lemon:condition   :lexicalEntry-295_sense_1_condition .

:lexicalEntry-295_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Bridge> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/River> .


:lexicalEntry-295_obj lemon:marker :form_lexicalEntry-295_preposition .

## Prepositions ##
:form_lexicalEntry-295_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

