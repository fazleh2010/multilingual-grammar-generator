@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :greek_14_sense_1_res ;
  lemon:entry    :greek_14 .

:greek_14 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :greek_14_lemma ;
  lemon:sense          :greek_14_sense_1 ;
  lemon:synBehavior    :greek_14_attrFrame, :greek_14_predFrame .

:greek_14_lemma lemon:writtenRep "greek"@en .

:greek_14_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :greek_14_PredSynArg .

:greek_14_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :greek_14_AttrSynArg .

:greek_14_sense_1 a  lemon:LexicalSense ;
  lemon:reference :greek_14_sense_1_res ;
  lemon:isA       :greek_14_AttrSynArg, :greek_14_PredSynArg .

:greek_14_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Greece> .
