@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :afghan_950_sense_1_res ;
  lemon:entry    :afghan_950 .

:afghan_950 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :afghan_950_lemma ;
  lemon:sense          :afghan_950_sense_1 ;
  lemon:synBehavior    :afghan_950_attrFrame, :afghan_950_predFrame .

:afghan_950_lemma lemon:writtenRep "afghan"@en .

:afghan_950_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :afghan_950_PredSynArg .

:afghan_950_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :afghan_950_AttrSynArg .

:afghan_950_sense_1 a  lemon:LexicalSense ;
  lemon:reference :afghan_950_sense_1_res ;
  lemon:isA       :afghan_950_AttrSynArg, :afghan_950_PredSynArg .

:afghan_950_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Afghanistan> .
