@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :grape_of_124 ;
  lemon:entry    :grape_of_124_form_preposition .

:grape_of_124 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :grape_of_124_form ;
  lemon:otherForm  :grape_of_124_singular ;
  lemon:otherForm  :grape_of_124_plural ;
  lemon:sense          :grape_of_124_sense_1 ;
  lemon:synBehavior    :grape_of_124_nounpp .

:grape_of_124_form a lemon:Form ;
  lemon:writtenRep "grape"@en .

:grape_of_124_singular a    lemon:Form ;
  lemon:writtenRep "grape"@en ;
  lexinfo:number   lexinfo:singular .

:grape_of_124_plural a   lemon:Form ;
  lemon:writtenRep "grapes"@en ;
  lexinfo:number    lexinfo:plural .

:grape_of_124_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_grape_of_124 ;
  lexinfo:prepositionalAdjunct :arg2_grape_of_124 .

:grape_of_124_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :grape_of_124_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_grape_of_124 ;
  lemon:objOfProp           :arg1_grape_of_124 ;
  lemon:condition           :grape_of_124_sense_1_condition .

:grape_of_124_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Grape> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Grape> .

:arg2_grape_of_124 lemon:marker :grape_of_124_form_preposition .
## Prepositions ##
:grape_of_124_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

