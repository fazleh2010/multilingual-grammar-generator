@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-4587 ;
  lemon:entry    :form_lexicalEntry-4587_preposition .

:lexicalEntry-4587 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-4587 ;
  lemon:otherForm      :form_lexicalEntry-4587_past ;
  lemon:sense          :lexicalEntry-4587_sense_1 ;
  lemon:synBehavior    :lexicalEntry-4587_frame .

:form_lexicalEntry-4587 a           lemon:Form ;
  lemon:writtenRep     "released"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-4587 a      lemon:Form ;
  lemon:writtenRep "released"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-4587_past a lemon:Form ;
  lemon:writtenRep  "released"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-4587perfect a   lemon:Form ;
  lemon:writtenRep "released"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-4587_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-4587_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-4587_obj .

:lexicalEntry-4587_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-4587_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/writer> ;
  lemon:subjOfProp  :lexicalEntry-4587_obj ;
  lemon:objOfProp   :lexicalEntry-4587_subj ;
  lemon:condition   :lexicalEntry-4587_sense_1_condition .

:lexicalEntry-4587_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Song> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:lexicalEntry-4587_obj lemon:marker :form_lexicalEntry-4587_preposition .

## Prepositions ##
:form_lexicalEntry-4587_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

