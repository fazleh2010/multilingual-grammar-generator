@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :person_of_447_sense_1_res ;
  lemon:entry    :person_of_447_sense_2_res ;
  lemon:entry    :person_of_447_sense_3_res ;
  lemon:entry    :person_of_447 .

:person_of_447 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :person_of_447_form ;
  lemon:otherForm  :person_of_447_singular ;
  lemon:otherForm  :person_of_447_plural ;
  lemon:sense          :person_of_447_sense_1 ;
  lemon:sense          :person_of_447_sense_2 ;
  lemon:sense          :person_of_447_sense_3 ;
  lemon:synBehavior    :person_of_447_predFrame .

:person_of_447_form
 lemon:writtenRep "person"@en .

:person_of_447_singular a    lemon:Form ;
  lemon:writtenRep "person"@en ;
  lexinfo:number   lexinfo:singular .

:person_of_447_plural a   lemon:Form ;
  lemon:writtenRep "persons"@en ;
  lexinfo:number    lexinfo:plural .

:person_of_447_sense_1 a  lemon:LexicalSense ;
  lemon:reference :person_of_447_sense_1_res ;
  lemon:isA       :person_of_447_PredSynArg .

:person_of_447_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/MilitaryPerson> .
:person_of_447_sense_2 a  lemon:LexicalSense ;
  lemon:reference :person_of_447_sense_2_res ;
  lemon:isA       :person_of_447_PredSynArg .

:person_of_447_sense_2_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Person> .
:person_of_447_sense_3 a  lemon:LexicalSense ;
  lemon:reference :person_of_447_sense_3_res ;
  lemon:isA       :person_of_447_PredSynArg .

:person_of_447_sense_3_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Single> .
