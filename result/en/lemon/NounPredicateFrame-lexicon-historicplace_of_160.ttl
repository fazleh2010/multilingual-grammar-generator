@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :historicplace_of_160_sense_1_res ;
  lemon:entry    :historicplace_of_160 .

:historicplace_of_160 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :historicplace_of_160_form ;
  lemon:otherForm  :historicplace_of_160_singular ;
  lemon:otherForm  :historicplace_of_160_plural ;
  lemon:sense          :historicplace_of_160_sense_1 ;
  lemon:synBehavior    :historicplace_of_160_predFrame .

:historicplace_of_160_form
 lemon:writtenRep "Historic Place"@en .

:historicplace_of_160_singular a    lemon:Form ;
  lemon:writtenRep "Historic Place"@en ;
  lexinfo:number   lexinfo:singular .

:historicplace_of_160_plural a   lemon:Form ;
  lemon:writtenRep "Historic Places"@en ;
  lexinfo:number    lexinfo:plural .

:historicplace_of_160_sense_1 a  lemon:LexicalSense ;
  lemon:reference :historicplace_of_160_sense_1_res ;
  lemon:isA       :historicplace_of_160_PredSynArg .

:historicplace_of_160_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/HistoricPlace> .
