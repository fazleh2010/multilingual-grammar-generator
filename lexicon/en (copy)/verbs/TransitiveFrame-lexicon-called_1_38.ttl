@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_called_1_38 ;
  lemon:entry    :called_1_38ed ;
  lemon:entry    :form_called_1_38_preposition .

:to_called_1_38 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_called_1_38 ;
  lemon:otherForm      :form_called_1_38s ;
  lemon:otherForm      :form_called_1_38ed ;
  lemon:otherForm      :form_called_1_38perfect ;
  lemon:sense          :called_1_38_sense_1 ;
  lemon:synBehavior    :called_1_38_frame_transitive .

:form_called_1_38 a         lemon:Form ;
  lemon:writtenRep     "call"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_called_1_38s a    lemon:Form ;
  lemon:writtenRep "calls"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_called_1_38ed a   lemon:Form ;
  lemon:writtenRep "called"@en ;
  lexinfo:tense    lexinfo:past .

:form_called_1_38perfect a   lemon:Form ;
  lemon:writtenRep "called"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:called_1_38_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :called_1_38_obj ;
  lexinfo:directObject     :called_1_38_subj .

:called_1_38_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :called_1_38_sense_1 ;
  lemon:reference   <http://www.w3.org/2000/01/rdf-schema#label> ;
  lemon:subjOfProp  :called_1_38_obj ;
  lemon:objOfProp   :called_1_38_subj ;
  lemon:condition   :called_1_38_sense_1_condition .


:called_1_38_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/VideoGame> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#string> .


:called_1_38_obj lemon:marker :form_called_1_38_preposition .

## Prepositions ##
:form_called_1_38_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

