@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :topleveldomain_of_126 ;
  lemon:entry    :topleveldomain_of_126_form_preposition .

:topleveldomain_of_126 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :topleveldomain_of_126_form ;
  lemon:otherForm  :topleveldomain_of_126_singular ;
  lemon:otherForm  :topleveldomain_of_126_plural ;
  lemon:sense          :topleveldomain_of_126_sense_1 ;
  lemon:synBehavior    :topleveldomain_of_126_nounpp .

:topleveldomain_of_126_form a lemon:Form ;

:topleveldomain_of_126_singular a    lemon:Form ;
  lemon:writtenRep "Top Level Domain"@en ;
  lexinfo:number   lexinfo:singular .

:topleveldomain_of_126_plural a   lemon:Form ;
  lemon:writtenRep "Top Level Domains"@en ;
  lexinfo:number    lexinfo:plural .

:topleveldomain_of_126_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_topleveldomain_of_126 ;
  lexinfo:prepositionalAdjunct :arg2_topleveldomain_of_126 .

:topleveldomain_of_126_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :topleveldomain_of_126_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_topleveldomain_of_126 ;
  lemon:objOfProp           :arg1_topleveldomain_of_126 ;
  lemon:condition           :topleveldomain_of_126_sense_1_condition .

:topleveldomain_of_126_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/TopLevelDomain> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/TopLevelDomain> .

:arg2_topleveldomain_of_126 lemon:marker :topleveldomain_of_126_form_preposition .
## Prepositions ##
:topleveldomain_of_126_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

