@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :novelist_of_67 ;
  lemon:entry    :novelist_of_67_form_preposition .

:novelist_of_67 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :novelist_of_67_form ;
  lemon:otherForm  :novelist_of_67_singular ;
  lemon:otherForm  :novelist_of_67_plural ;
  lemon:sense          :novelist_of_67_sense_1 ;
  lemon:synBehavior    :novelist_of_67_nounpp .

:novelist_of_67_form a lemon:Form ;
  lemon:writtenRep "novelist"@en .

:novelist_of_67_singular a    lemon:Form ;
  lemon:writtenRep "novelist"@en ;
  lexinfo:number   lexinfo:singular .

:novelist_of_67_plural a   lemon:Form ;
  lemon:writtenRep "novelists"@en ;
  lexinfo:number    lexinfo:plural .

:novelist_of_67_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_novelist_of_67 ;
  lexinfo:prepositionalAdjunct :arg2_novelist_of_67 .

:novelist_of_67_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :novelist_of_67_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/author> ;
  lemon:subjOfProp          :arg2_novelist_of_67 ;
  lemon:objOfProp           :arg1_novelist_of_67 ;
  lemon:condition           :novelist_of_67_sense_1_condition .

:novelist_of_67_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Book> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2_novelist_of_67 lemon:marker :novelist_of_67_form_preposition .
## Prepositions ##
:novelist_of_67_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

