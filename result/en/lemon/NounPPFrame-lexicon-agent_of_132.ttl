@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :agent_of_132 ;
  lemon:entry    :agent_of_132_form_preposition .

:agent_of_132 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :agent_of_132_form ;
  lemon:otherForm  :agent_of_132_singular ;
  lemon:otherForm  :agent_of_132_plural ;
  lemon:sense          :agent_of_132_sense_1 ;
  lemon:synBehavior    :agent_of_132_nounpp .

:agent_of_132_form a lemon:Form ;

:agent_of_132_singular a    lemon:Form ;
  lemon:writtenRep "agent"@en ;
  lexinfo:number   lexinfo:singular .

:agent_of_132_plural a   lemon:Form ;
  lemon:writtenRep "agents"@en ;
  lexinfo:number    lexinfo:plural .

:agent_of_132_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_agent_of_132 ;
  lexinfo:prepositionalAdjunct :arg2_agent_of_132 .

:agent_of_132_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :agent_of_132_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_agent_of_132 ;
  lemon:objOfProp           :arg1_agent_of_132 ;
  lemon:condition           :agent_of_132_sense_1_condition .

:agent_of_132_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Agent> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Agent> .

:arg2_agent_of_132 lemon:marker :agent_of_132_form_preposition .
## Prepositions ##
:agent_of_132_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

