@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-1065 ;
  lemon:entry    :lexicalEntry-1065ed ;
  lemon:entry    :form_lexicalEntry-1065_preposition .

:to_lexicalEntry-1065 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-1065 ;
  lemon:otherForm      :form_lexicalEntry-1065s ;
  lemon:otherForm      :form_lexicalEntry-1065ed ;
  lemon:otherForm      :form_lexicalEntry-1065perfect ;
  lemon:sense          :lexicalEntry-1065_sense_1 ;
  lemon:synBehavior    :lexicalEntry-1065_frame_transitive .

:form_lexicalEntry-1065 a         lemon:Form ;
  lemon:writtenRep     "reflecting"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-1065s a    lemon:Form ;
  lemon:writtenRep "reflecting"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-1065ed a   lemon:Form ;
  lemon:writtenRep "reflecting"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-1065perfect a   lemon:Form ;
  lemon:writtenRep "reflecting"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-1065_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-1065_subj ;
  lexinfo:directObject     :lexicalEntry-1065_obj .

:lexicalEntry-1065_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1065_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/governmentType> ;
  lemon:subjOfProp  :lexicalEntry-1065_obj ;
  lemon:objOfProp   :lexicalEntry-1065_subj ;
  lemon:condition   :lexicalEntry-1065_sense_1_condition .


:lexicalEntry-1065_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Town> ;
  lemon:propertyRange  <http://www.w3.org/2002/07/owl#Thing> .

:lexicalEntry-1065ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-1065ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-1065ed_by ;
  lemon:synBehavior    :lexicalEntry-1065ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-1065ed_ontomap .

:form_lexicalEntry-1065ed_canonical a lemon:Form ;
  lemon:writtenRep         "reflecting"@en .

:form_lexicalEntry-1065ed_by a    lemon:Form ;
  lemon:writtenRep     "reflecting"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-1065ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-1065ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-1065ed_obj .

:lexicalEntry-1065ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1065ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/governmentType> ;
  lemon:subjOfProp  :lexicalEntry-1065ed_subj ;
  lemon:objOfProp   :lexicalEntry-1065ed_obj ;
  lemon:condition   :lexicalEntry-1065_sense_1_condition .
:lexicalEntry-1065ed_obj lemon:marker :form_lexicalEntry-1065_preposition .

## Prepositions ##
:form_lexicalEntry-1065_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

