:racecourse_600_sense_1 a  lemon:LexicalSense ;
  lemon:reference :racecourse_600_sense_1_res ;
  lemon:isA       :racecourse_600_PredSynArg .

:racecourse_600_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
