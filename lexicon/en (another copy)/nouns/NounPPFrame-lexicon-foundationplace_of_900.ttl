@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :foundationplace_of_900 ;
  lemon:entry    :foundationplace_of_900_form_preposition .

:foundationplace_of_900 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :foundationplace_of_900_form ;
  lemon:otherForm  :foundationplace_of_900_singular ;
  lemon:otherForm  :foundationplace_of_900_plural ;
  lemon:sense          :foundationplace_of_900_sense_1 ;
  lemon:synBehavior    :foundationplace_of_900_nounpp .

:foundationplace_of_900_form a lemon:Form ;
  lemon:writtenRep "foundation place"@en .

:foundationplace_of_900_singular a    lemon:Form ;
  lemon:writtenRep "foundation place"@en ;
  lexinfo:number   lexinfo:singular .

:foundationplace_of_900_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:foundationplace_of_900_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_foundationplace_of_900 ;
  lexinfo:prepositionalAdjunct :arg2_foundationplace_of_900 .

:foundationplace_of_900_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :foundationplace_of_900_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/foundationPlace> ;
  lemon:subjOfProp          :arg2_foundationplace_of_900 ;
  lemon:objOfProp           :arg1_foundationplace_of_900 ;
  lemon:condition           :foundationplace_of_900_sense_1_condition .

:foundationplace_of_900_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/City> .

:arg2_foundationplace_of_900 lemon:marker :foundationplace_of_900_form_preposition .
## Prepositions ##
:foundationplace_of_900_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

