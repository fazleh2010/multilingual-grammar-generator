@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :garden_of_156 ;
  lemon:entry    :garden_of_156_form_preposition .

:garden_of_156 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :garden_of_156_form ;
  lemon:otherForm  :garden_of_156_singular ;
  lemon:otherForm  :garden_of_156_plural ;
  lemon:sense          :garden_of_156_sense_1 ;
  lemon:synBehavior    :garden_of_156_nounpp .

:garden_of_156_form a lemon:Form ;
  lemon:writtenRep "Garden"@en .

:garden_of_156_singular a    lemon:Form ;
  lemon:writtenRep "Garden"@en ;
  lexinfo:number   lexinfo:singular .

:garden_of_156_plural a   lemon:Form ;
  lemon:writtenRep "Gardens"@en ;
  lexinfo:number    lexinfo:plural .

:garden_of_156_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_garden_of_156 ;
  lexinfo:prepositionalAdjunct :arg2_garden_of_156 .

:garden_of_156_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :garden_of_156_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_garden_of_156 ;
  lemon:objOfProp           :arg1_garden_of_156 ;
  lemon:condition           :garden_of_156_sense_1_condition .

:garden_of_156_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Garden> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Garden> .

:arg2_garden_of_156 lemon:marker :garden_of_156_form_preposition .
## Prepositions ##
:garden_of_156_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

