@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :rower_of_350_sense_1_res ;
  lemon:entry    :rower_of_350 .

:rower_of_350 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :rower_of_350_form ;
  lemon:otherForm  :rower_of_350_singular ;
  lemon:otherForm  :rower_of_350_plural ;
  lemon:sense          :rower_of_350_sense_1 ;
