@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :deliverydate-of_300 ;
  lemon:entry    :deliverydate-of_300_form_preposition .

:deliverydate-of_300 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :deliverydate-of_300_form ;
  lemon:otherForm  :deliverydate-of_300_singular ;
  lemon:otherForm  :deliverydate-of_300_plural ;
  lemon:sense          :deliverydate-of_300_sense_1 ;
  lemon:synBehavior    :deliverydate-of_300_nounpp .

:deliverydate-of_300_form a lemon:Form ;
  lemon:writtenRep "delivery Date"@en .

:deliverydate-of_300_singular a    lemon:Form ;
  lemon:writtenRep "delivery Date"@en ;
  lexinfo:number   lexinfo:singular .

:deliverydate-of_300_plural a   lemon:Form ;
  lemon:writtenRep "delivery Dates"@en ;
  lexinfo:number    lexinfo:plural .

:deliverydate-of_300_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :null ;
  lexinfo:prepositionalAdjunct :null .

:deliverydate-of_300_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :deliverydate-of_300_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/deliveryDate> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :deliverydate-of_300_sense_1_condition .

:deliverydate-of_300_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/MeanOfTransportation> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:null lemon:marker :deliverydate-of_300_form_preposition .
## Prepositions ##
:deliverydate-of_300_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

