@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-2420 ;
  lemon:entry    :lexicalEntry-2420ed ;
  lemon:entry    :form_lexicalEntry-2420_preposition .

:to_lexicalEntry-2420 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-2420 ;
  lemon:otherForm      :form_lexicalEntry-2420s ;
  lemon:otherForm      :form_lexicalEntry-2420ed ;
  lemon:otherForm      :form_lexicalEntry-2420perfect ;
  lemon:sense          :lexicalEntry-2420_sense_1 ;
  lemon:synBehavior    :lexicalEntry-2420_frame_transitive .

:form_lexicalEntry-2420 a         lemon:Form ;
  lemon:writtenRep     "launch"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-2420s a    lemon:Form ;
  lemon:writtenRep "launch"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-2420ed a   lemon:Form ;
  lemon:writtenRep "launch"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-2420perfect a   lemon:Form ;
  lemon:writtenRep "launch"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-2420_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-2420_subj ;
  lexinfo:directObject     :lexicalEntry-2420_obj .

:lexicalEntry-2420_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2420_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/operator> ;
  lemon:subjOfProp  :lexicalEntry-2420_obj ;
  lemon:objOfProp   :lexicalEntry-2420_subj ;
  lemon:condition   :lexicalEntry-2420_sense_1_condition .


:lexicalEntry-2420_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/LaunchPad> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Organisation> .

:lexicalEntry-2420ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-2420ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-2420ed_by ;
  lemon:synBehavior    :lexicalEntry-2420ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-2420ed_ontomap .

:form_lexicalEntry-2420ed_canonical a lemon:Form ;
  lemon:writtenRep         "launch"@en .

:form_lexicalEntry-2420ed_by a    lemon:Form ;
  lemon:writtenRep     "launch"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-2420ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-2420ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-2420ed_obj .

:lexicalEntry-2420ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2420ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/operator> ;
  lemon:subjOfProp  :lexicalEntry-2420ed_subj ;
  lemon:objOfProp   :lexicalEntry-2420ed_obj ;
  lemon:condition   :lexicalEntry-2420_sense_1_condition .
:lexicalEntry-2420ed_obj lemon:marker :form_lexicalEntry-2420_preposition .

## Prepositions ##
:form_lexicalEntry-2420_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

