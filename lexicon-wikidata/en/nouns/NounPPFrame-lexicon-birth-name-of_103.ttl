@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :birth-name-of_103 ;
  lemon:entry    :birth-name-of_103_form_preposition .

:birth-name-of_103 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :birth-name-of_103_form ;
  lemon:otherForm  :birth-name-of_103_singular ;
  lemon:otherForm  :birth-name-of_103_plural ;
  lemon:sense          :birth-name-of_103_sense_1 ;
  lemon:synBehavior    :birth-name-of_103_nounpp .

:birth-name-of_103_form a lemon:Form ;
  lemon:writtenRep "birth name"@en .

:birth-name-of_103_singular a    lemon:Form ;
  lemon:writtenRep "birth name"@en ;
  lexinfo:number   lexinfo:singular .

:birth-name-of_103_plural a   lemon:Form ;
  lemon:writtenRep "birth names"@en ;
  lexinfo:number    lexinfo:plural .

:birth-name-of_103_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_birth-name-of_103 ;
  lexinfo:prepositionalAdjunct :arg2_birth-name-of_103 .

:birth-name-of_103_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :birth-name-of_103_sense_1 ;
  lemon:reference           <wd:P1477> ;
  lemon:subjOfProp          :arg2_birth-name-of_103 ;
  lemon:objOfProp           :arg1_birth-name-of_103 ;
  lemon:condition           :birth-name-of_103_sense_1_condition .

:birth-name-of_103_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <wd:Q19088> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2_birth-name-of_103 lemon:marker :birth-name-of_103_form_preposition .
## Prepositions ##
:birth-name-of_103_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

