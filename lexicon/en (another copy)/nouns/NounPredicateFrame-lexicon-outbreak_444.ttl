@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :outbreak_444_sense_1_res ;
  lemon:entry    :outbreak_444 .

:outbreak_444 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :outbreak_444_form ;
  lemon:otherForm  :outbreak_444_singular ;
  lemon:otherForm  :outbreak_444_plural ;
  lemon:sense          :outbreak_444_sense_1 ;
  lemon:synBehavior    :outbreak_444_predFrame .

:outbreak_444_form
 lemon:writtenRep "outbreak"@en .

:outbreak_444_singular a    lemon:Form ;
  lemon:writtenRep "outbreak"@en ;
  lexinfo:number   lexinfo:singular .

:outbreak_444_plural a   lemon:Form ;
  lemon:writtenRep "outbreaks"@en ;
  lexinfo:number    lexinfo:plural .

:outbreak_444_sense_1 a  lemon:LexicalSense ;
  lemon:reference :outbreak_444_sense_1_res ;
  lemon:isA       :outbreak_444_PredSynArg .

:outbreak_444_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Outbreak> .