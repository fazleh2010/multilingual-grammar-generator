@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :organization-of_400 ;
  lemon:entry    :organization-of_400_form_preposition .

:organization-of_400 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :organization-of_400_form ;
  lemon:otherForm  :organization-of_400_singular ;
  lemon:otherForm  :organization-of_400_plural ;
  lemon:sense          :organization-of_400_sense_1 ;
  lemon:synBehavior    :organization-of_400_nounpp .

:organization-of_400_form a lemon:Form ;
  lemon:writtenRep "organization"@en .

:organization-of_400_singular a    lemon:Form ;
  lemon:writtenRep "organization"@en ;
  lexinfo:number   lexinfo:singular .

:organization-of_400_plural a   lemon:Form ;
  lemon:writtenRep "organizations"@en ;
  lexinfo:number    lexinfo:plural .

:organization-of_400_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_organization-of_400 ;
  lexinfo:prepositionalAdjunct :arg2_organization-of_400 .

:organization-of_400_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :organization-of_400_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/industry> ;
  lemon:subjOfProp          :arg2_organization-of_400 ;
  lemon:objOfProp           :arg1_organization-of_400 ;
  lemon:condition           :organization-of_400_sense_1_condition .

:organization-of_400_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Agent> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Food> .

:arg2_organization-of_400 lemon:marker :organization-of_400_form_preposition .
## Prepositions ##
:organization-of_400_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

