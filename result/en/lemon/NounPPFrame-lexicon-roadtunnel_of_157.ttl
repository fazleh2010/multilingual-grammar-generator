@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :roadtunnel_of_157 ;
  lemon:entry    :roadtunnel_of_157_form_preposition .

:roadtunnel_of_157 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :roadtunnel_of_157_form ;
  lemon:otherForm  :roadtunnel_of_157_singular ;
  lemon:otherForm  :roadtunnel_of_157_plural ;
  lemon:sense          :roadtunnel_of_157_sense_1 ;
  lemon:synBehavior    :roadtunnel_of_157_nounpp .

:roadtunnel_of_157_form a lemon:Form ;
  lemon:writtenRep "Road Tunnel"@en .

:roadtunnel_of_157_singular a    lemon:Form ;
  lemon:writtenRep "Road Tunnel"@en ;
  lexinfo:number   lexinfo:singular .

:roadtunnel_of_157_plural a   lemon:Form ;
  lemon:writtenRep "Road Tunnels"@en ;
  lexinfo:number    lexinfo:plural .

:roadtunnel_of_157_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_roadtunnel_of_157 ;
  lexinfo:prepositionalAdjunct :arg2_roadtunnel_of_157 .

:roadtunnel_of_157_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :roadtunnel_of_157_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_roadtunnel_of_157 ;
  lemon:objOfProp           :arg1_roadtunnel_of_157 ;
  lemon:condition           :roadtunnel_of_157_sense_1_condition .

:roadtunnel_of_157_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/RoadTunnel> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/RoadTunnel> .

:arg2_roadtunnel_of_157 lemon:marker :roadtunnel_of_157_form_preposition .
## Prepositions ##
:roadtunnel_of_157_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

