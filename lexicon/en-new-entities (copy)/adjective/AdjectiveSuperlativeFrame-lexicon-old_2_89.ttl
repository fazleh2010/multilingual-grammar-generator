@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix oils:     <http://lemon-model.net/oils#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :old_2_89 ;
  lemon:entry    :form_old_2_89_preposition ;
  lemon:entry    :old_2_89_res .

:old_2_89 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_old_2_89 ;
  lemon:otherForm      :form_old_2_89_comperative ;
  lemon:otherForm      :form_old_2_89_superlative ;
  lemon:sense          :old_2_89_sense_1 ;
  lemon:synBehavior    :old_2_89_predFrame .

:form_old_2_89 lemon:writtenRep "old"@en .

:form_old_2_89_comperative lemon:writtenRep "older"@en .

:form_old_2_89_superlative lemon:writtenRep "oldest"@en .

:old_2_89_predFrame a        lexinfo:AdjectiveSuperlativeFrame ;
  lexinfo:copulativeSubject :old_2_89_PredSynArg .

:old_2_89_sense_1 a  lemon:LexicalSense ;
  lemon:reference :old_2_89_res ;
  lemon:isA       :old_2_89_PredSynArg ;
  lemon:condition :old_2_89_sense_1_condition .

:old_2_89_res a   oils:CovariantScalar ;
  oils:boundTo  <http://dbpedia.org/ontology/birthDate> ;
  oils:degree   <http://lemon-model.net/oils/high> .

:old_2_89_sense_1_condition a lemon:condition ;
  lemon:propertyDomain   <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange    <http://www.w3.org/2001/XMLSchema#date> .

## Prepositions ##
:form_old_2_89_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

