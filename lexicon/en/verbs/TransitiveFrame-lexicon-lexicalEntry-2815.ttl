@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-2815 ;
  lemon:entry    :lexicalEntry-2815ed ;
  lemon:entry    :form_lexicalEntry-2815_preposition .

:to_lexicalEntry-2815 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-2815 ;
  lemon:otherForm      :form_lexicalEntry-2815s ;
  lemon:otherForm      :form_lexicalEntry-2815ed ;
  lemon:otherForm      :form_lexicalEntry-2815perfect ;
  lemon:sense          :lexicalEntry-2815_sense_1 ;
  lemon:synBehavior    :lexicalEntry-2815_frame_transitive .

:form_lexicalEntry-2815 a         lemon:Form ;
  lemon:writtenRep     "marry"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-2815s a    lemon:Form ;
  lemon:writtenRep "marries"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-2815ed a   lemon:Form ;
  lemon:writtenRep "married"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-2815perfect a   lemon:Form ;
  lemon:writtenRep "married"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-2815_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-2815_subj ;
  lexinfo:directObject     :lexicalEntry-2815_obj .

:lexicalEntry-2815_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2815_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/spouse> ;
  lemon:subjOfProp  :lexicalEntry-2815_obj ;
  lemon:objOfProp   :lexicalEntry-2815_subj ;
  lemon:condition   :lexicalEntry-2815_sense_1_condition .


:lexicalEntry-2815_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .

:lexicalEntry-2815ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-2815ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-2815ed_by ;
  lemon:synBehavior    :lexicalEntry-2815ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-2815ed_ontomap .

:form_lexicalEntry-2815ed_canonical a lemon:Form ;
  lemon:writtenRep         "marry"@en .

:form_lexicalEntry-2815ed_by a    lemon:Form ;
  lemon:writtenRep     "marry"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-2815ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-2815ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-2815ed_obj .

:lexicalEntry-2815ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2815ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/spouse> ;
  lemon:subjOfProp  :lexicalEntry-2815ed_subj ;
  lemon:objOfProp   :lexicalEntry-2815ed_obj ;
  lemon:condition   :lexicalEntry-2815_sense_1_condition .
:lexicalEntry-2815ed_obj lemon:marker :form_lexicalEntry-2815_preposition .

## Prepositions ##
:form_lexicalEntry-2815_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

