@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :song_of_62_sense_1_res ;
  lemon:entry    :song_of_62 .

:song_of_62 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :song_of_62_form ;
  lemon:otherForm  :song_of_62_singular ;
  lemon:otherForm  :song_of_62_plural ;
  lemon:sense          :song_of_62_sense_1 ;
  lemon:synBehavior    :song_of_62_predFrame .

:song_of_62_form
 lemon:writtenRep "song"@en .

:song_of_62_singular a    lemon:Form ;
  lemon:writtenRep "song"@en ;
  lexinfo:number   lexinfo:singular .

:song_of_62_plural a   lemon:Form ;
  lemon:writtenRep "songs"@en ;
  lexinfo:number    lexinfo:plural .

:song_of_62_sense_1 a  lemon:LexicalSense ;
  lemon:reference :song_of_62_sense_1_res ;
  lemon:isA       :song_of_62_PredSynArg .

:song_of_62_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Song> .
