@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :author_of_173_sense_1_res ;
  lemon:entry    :author_of_173 .

:author_of_173 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :author_of_173_form ;
  lemon:otherForm  :author_of_173_singular ;
  lemon:otherForm  :author_of_173_plural ;
  lemon:sense          :author_of_173_sense_1 ;
  lemon:synBehavior    :author_of_173_predFrame .

:author_of_173_form
 lemon:writtenRep "author"@en .

:author_of_173_singular a    lemon:Form ;
  lemon:writtenRep "author"@en ;
  lexinfo:number   lexinfo:singular .

:author_of_173_plural a   lemon:Form ;
  lemon:writtenRep "authors"@en ;
  lexinfo:number    lexinfo:plural .

:author_of_173_sense_1 a  lemon:LexicalSense ;
  lemon:reference :author_of_173_sense_1_res ;
  lemon:isA       :author_of_173_PredSynArg .

:author_of_173_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Author> .
