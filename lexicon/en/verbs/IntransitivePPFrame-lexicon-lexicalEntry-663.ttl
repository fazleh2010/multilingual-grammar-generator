@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-663 ;
  lemon:entry    :form_lexicalEntry-663_preposition .

:lexicalEntry-663 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-663 ;
  lemon:otherForm      :form_lexicalEntry-663_past ;
  lemon:sense          :lexicalEntry-663_sense_1 ;
  lemon:synBehavior    :lexicalEntry-663_frame .

:form_lexicalEntry-663 a           lemon:Form ;
  lemon:writtenRep     "discover"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-663 a      lemon:Form ;
  lemon:writtenRep "discovers"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-663_past a lemon:Form ;
  lemon:writtenRep  "discovered"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-663perfect a   lemon:Form ;
  lemon:writtenRep "discovered"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-663_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-663_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-663_obj .

:lexicalEntry-663_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-663_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/discoverer> ;
  lemon:subjOfProp  :lexicalEntry-663_obj ;
  lemon:objOfProp   :lexicalEntry-663_subj ;
  lemon:condition   :lexicalEntry-663_sense_1_condition .

:lexicalEntry-663_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Planet> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:lexicalEntry-663_obj lemon:marker :form_lexicalEntry-663_preposition .

## Prepositions ##
:form_lexicalEntry-663_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

