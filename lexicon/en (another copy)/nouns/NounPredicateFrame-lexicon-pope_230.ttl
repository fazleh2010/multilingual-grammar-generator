@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :pope_230_sense_1_res ;
  lemon:entry    :pope_230 .

:pope_230 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :pope_230_form ;
  lemon:otherForm  :pope_230_singular ;
  lemon:otherForm  :pope_230_plural ;
  lemon:sense          :pope_230_sense_1 ;
  lemon:synBehavior    :pope_230_predFrame .

:pope_230_form
 lemon:writtenRep "pope"@en .

:pope_230_singular a    lemon:Form ;
  lemon:writtenRep "pope"@en ;
  lexinfo:number   lexinfo:singular .

:pope_230_plural a   lemon:Form ;
  lemon:writtenRep "popes"@en ;
  lexinfo:number    lexinfo:plural .

:pope_230_sense_1 a  lemon:LexicalSense ;
  lemon:reference :pope_230_sense_1_res ;
  lemon:isA       :pope_230_PredSynArg .

:pope_230_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Pope> .