@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_succeed_1_201 ;
  lemon:entry    :succeed_1_201ed ;
  lemon:entry    :form_succeed_1_201_preposition .

:to_succeed_1_201 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_succeed_1_201 ;
  lemon:otherForm      :form_succeed_1_201s ;
  lemon:otherForm      :form_succeed_1_201ed ;
  lemon:otherForm      :form_succeed_1_201perfect ;
  lemon:sense          :succeed_1_201_sense_1 ;
  lemon:synBehavior    :succeed_1_201_frame_transitive .

:form_succeed_1_201 a         lemon:Form ;
  lemon:writtenRep     "succeed"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_succeed_1_201s a    lemon:Form ;
  lemon:writtenRep "succeeds"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_succeed_1_201ed a   lemon:Form ;
  lemon:writtenRep "succeeded"@en ;
  lexinfo:tense    lexinfo:past .

:form_succeed_1_201perfect a   lemon:Form ;
  lemon:writtenRep "succeeded"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:succeed_1_201_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :succeed_1_201_subj ;
  lexinfo:directObject     :succeed_1_201_obj .

:succeed_1_201_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :succeed_1_201_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/successor> ;
  lemon:subjOfProp  :succeed_1_201_obj ;
  lemon:objOfProp   :succeed_1_201_subj ;
  lemon:condition   :succeed_1_201_sense_1_condition .


:succeed_1_201_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:succeed_1_201_obj lemon:marker :form_succeed_1_201_preposition .

## Prepositions ##
:form_succeed_1_201_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

