@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :artist_653_sense_1_res ;
  lemon:entry    :artist_653 .

:artist_653 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :artist_653_form ;
  lemon:otherForm  :artist_653_singular ;
  lemon:otherForm  :artist_653_plural ;
  lemon:sense          :artist_653_sense_1 ;
  lemon:synBehavior    :artist_653_predFrame .

:artist_653_form
 lemon:writtenRep "artist"@en .

:artist_653_singular a    lemon:Form ;
  lemon:writtenRep "artist"@en ;
  lexinfo:number   lexinfo:singular .

:artist_653_plural a   lemon:Form ;
  lemon:writtenRep "artists"@en ;
  lexinfo:number    lexinfo:plural .

:artist_653_sense_1 a  lemon:LexicalSense ;
  lemon:reference :artist_653_sense_1_res ;
  lemon:isA       :artist_653_PredSynArg .

:artist_653_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Artist> .