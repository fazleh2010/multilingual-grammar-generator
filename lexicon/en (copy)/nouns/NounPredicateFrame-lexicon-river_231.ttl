@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :river_231_sense_1_res ;
  lemon:entry    :river_231 .

:river_231 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :river_231_form ;
  lemon:otherForm  :river_231_singular ;
  lemon:otherForm  :river_231_plural ;
  lemon:sense          :river_231_sense_1 ;
  lemon:synBehavior    :river_231_predFrame .

:river_231_form
 lemon:writtenRep "river"@en .

:river_231_singular a    lemon:Form ;
  lemon:writtenRep "river"@en ;
  lexinfo:number   lexinfo:singular .

:river_231_plural a   lemon:Form ;
  lemon:writtenRep "rivers"@en ;
  lexinfo:number    lexinfo:plural .

:river_231_sense_1 a  lemon:LexicalSense ;
  lemon:reference :river_231_sense_1_res ;
  lemon:isA       :river_231_PredSynArg .

:river_231_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/River> .