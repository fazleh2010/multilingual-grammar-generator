@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :railwayline_of_387_sense_1_res ;
  lemon:entry    :railwayline_of_387 .

:railwayline_of_387 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :railwayline_of_387_form ;
  lemon:otherForm  :railwayline_of_387_singular ;
  lemon:otherForm  :railwayline_of_387_plural ;
  lemon:sense          :railwayline_of_387_sense_1 ;
  lemon:synBehavior    :railwayline_of_387_predFrame .

:railwayline_of_387_form
 lemon:writtenRep "Railway Line"@en .

:railwayline_of_387_singular a    lemon:Form ;
  lemon:writtenRep "Railway Line"@en ;
  lexinfo:number   lexinfo:singular .

:railwayline_of_387_plural a   lemon:Form ;
  lemon:writtenRep "Railway Lines"@en ;
  lexinfo:number    lexinfo:plural .

:railwayline_of_387_sense_1 a  lemon:LexicalSense ;
  lemon:reference :railwayline_of_387_sense_1_res ;
  lemon:isA       :railwayline_of_387_PredSynArg .

:railwayline_of_387_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/RailwayLine> .
