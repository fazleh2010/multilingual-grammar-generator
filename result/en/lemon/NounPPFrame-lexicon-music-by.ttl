@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :music-by ;
  lemon:entry    :music-by_form_preposition .

:music-by a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :music-by_form ;
  lemon:otherForm  :music-by_singular ;
  lemon:otherForm  :music-by_plural ;
  lemon:sense          :music-by_sense_1 ;
  lemon:synBehavior    :music-by_nounpp .

:music-by_form a lemon:Form ;
  lemon:writtenRep "music"@en .

:music-by_singular a    lemon:Form ;
  lemon:writtenRep "music"@en ;
  lexinfo:number   lexinfo:singular .

:music-by_plural a   lemon:Form ;
  lemon:writtenRep "musics"@en ;
  lexinfo:number    lexinfo:plural .

:music-by_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:music-by_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :music-by_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/musicBy> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :music-by_sense_1_condition .

:music-by_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :music-by_form_preposition .
## Prepositions ##
:music-by_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

