@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_paint_2_200 ;
  lemon:entry    :paint_2_200ed ;
  lemon:entry    :form_paint_2_200_preposition .

:to_paint_2_200 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_paint_2_200 ;
  lemon:otherForm      :form_paint_2_200s ;
  lemon:otherForm      :form_paint_2_200ed ;
  lemon:otherForm      :form_paint_2_200perfect ;
  lemon:sense          :paint_2_200_sense_1 ;
  lemon:synBehavior    :paint_2_200_frame_transitive .

:form_paint_2_200 a         lemon:Form ;
  lemon:writtenRep     "paint"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_paint_2_200s a    lemon:Form ;
  lemon:writtenRep "paints"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_paint_2_200ed a   lemon:Form ;
  lemon:writtenRep "painted"@en ;
  lexinfo:tense    lexinfo:past .

:form_paint_2_200perfect a   lemon:Form ;
  lemon:writtenRep "painted"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:paint_2_200_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :paint_2_200_subj ;
  lexinfo:directObject     :paint_2_200_obj .

:paint_2_200_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :paint_2_200_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/artist> ;
  lemon:subjOfProp  :paint_2_200_obj ;
  lemon:objOfProp   :paint_2_200_subj ;
  lemon:condition   :paint_2_200_sense_1_condition .


:paint_2_200_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:paint_2_200_obj lemon:marker :form_paint_2_200_preposition .

## Prepositions ##
:form_paint_2_200_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

