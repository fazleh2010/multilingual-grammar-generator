@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_create_2_55 ;
  lemon:entry    :create_2_55ed ;
  lemon:entry    :form_create_2_55_preposition .

:to_create_2_55 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_create_2_55 ;
  lemon:otherForm      :form_create_2_55s ;
  lemon:otherForm      :form_create_2_55ed ;
  lemon:otherForm      :form_create_2_55perfect ;
  lemon:sense          :create_2_55_sense_1 ;
  lemon:synBehavior    :create_2_55_frame_transitive .

:form_create_2_55 a         lemon:Form ;
  lemon:writtenRep     "create"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_create_2_55s a    lemon:Form ;
  lemon:writtenRep "creates"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_create_2_55ed a   lemon:Form ;
  lemon:writtenRep "created"@en ;
  lexinfo:tense    lexinfo:past .

:form_create_2_55perfect a   lemon:Form ;
  lemon:writtenRep "created"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:create_2_55_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :create_2_55_subj ;
  lexinfo:directObject     :create_2_55_obj .

:create_2_55_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :create_2_55_sense_1 ;
  lemon:reference   <wd:P50> ;
  lemon:subjOfProp  :create_2_55_obj ;
  lemon:objOfProp   :create_2_55_subj ;
  lemon:condition   :create_2_55_sense_1_condition .


:create_2_55_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <wd:Q386724> ;
  lemon:propertyRange  <wd:Q19088> .


:create_2_55_obj lemon:marker :form_create_2_55_preposition .

## Prepositions ##
:form_create_2_55_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

