@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-1369 ;
  lemon:entry    :lexicalEntry-1369ed ;
  lemon:entry    :form_lexicalEntry-1369_preposition .

:to_lexicalEntry-1369 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-1369 ;
  lemon:otherForm      :form_lexicalEntry-1369s ;
  lemon:otherForm      :form_lexicalEntry-1369ed ;
  lemon:otherForm      :form_lexicalEntry-1369perfect ;
  lemon:sense          :lexicalEntry-1369_sense_1 ;
  lemon:synBehavior    :lexicalEntry-1369_frame_transitive .

:form_lexicalEntry-1369 a         lemon:Form ;
  lemon:writtenRep     "based in"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-1369s a    lemon:Form ;
  lemon:writtenRep "based in"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-1369ed a   lemon:Form ;
  lemon:writtenRep "based in"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-1369perfect a   lemon:Form ;
  lemon:writtenRep "based in"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-1369_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-1369_obj ;
  lexinfo:directObject     :lexicalEntry-1369_subj .

:lexicalEntry-1369_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1369_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/headquarter> ;
  lemon:subjOfProp  :lexicalEntry-1369_obj ;
  lemon:objOfProp   :lexicalEntry-1369_subj ;
  lemon:condition   :lexicalEntry-1369_sense_1_condition .


:lexicalEntry-1369_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/PopulatedPlace> .

:lexicalEntry-1369ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-1369ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-1369ed_by ;
  lemon:synBehavior    :lexicalEntry-1369ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-1369ed_ontomap .

:form_lexicalEntry-1369ed_canonical a lemon:Form ;
  lemon:writtenRep         "based in"@en .

:form_lexicalEntry-1369ed_by a    lemon:Form ;
  lemon:writtenRep     "based in"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-1369ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-1369ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-1369ed_obj .

:lexicalEntry-1369ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1369ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/headquarter> ;
  lemon:subjOfProp  :lexicalEntry-1369ed_subj ;
  lemon:objOfProp   :lexicalEntry-1369ed_obj ;
  lemon:condition   :lexicalEntry-1369_sense_1_condition .
:lexicalEntry-1369ed_obj lemon:marker :form_lexicalEntry-1369_preposition .

## Prepositions ##
:form_lexicalEntry-1369_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

