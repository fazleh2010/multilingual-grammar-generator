@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :start_in_1_63 ;
  lemon:entry    :form_start_in_1_63_preposition .

:start_in_1_63 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_start_in_1_63 ;
  lemon:otherForm      :form_start_in_1_63_present3rd ;
  lemon:otherForm      :form_start_in_1_63_past ;
  lemon:otherForm      :form_start_in_1_63_perfect ;
  lemon:sense          :start_in_1_63_sense_1 ;
  lemon:synBehavior    :start_in_1_63_frame .

:form_start_in_1_63 a           lemon:Form ;
  lemon:writtenRep     "start"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_start_in_1_63_present3rd a      lemon:Form ;
  lemon:writtenRep "starts"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_start_in_1_63_past a lemon:Form ;
  lemon:writtenRep  "started"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_start_in_1_63_perfect a   lemon:Form ;
  lemon:writtenRep "started"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:start_in_1_63_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :start_in_1_63_subj ;
  lexinfo:prepositionalAdjunct :start_in_1_63_obj .

:start_in_1_63_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :start_in_1_63_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/routeStart> ;
  lemon:subjOfProp  :start_in_1_63_subj ;
  lemon:objOfProp   :start_in_1_63_obj ;
  lemon:condition   :start_in_1_63_sense_1_condition .

:start_in_1_63_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Road> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Road> .


:start_in_1_63_obj lemon:marker :form_start_in_1_63_preposition .

## Prepositions ##
:form_start_in_1_63_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

