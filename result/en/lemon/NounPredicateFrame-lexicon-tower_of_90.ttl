@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :tower_of_90_sense_1_res ;
  lemon:entry    :tower_of_90 .

:tower_of_90 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :tower_of_90_form ;
  lemon:otherForm  :tower_of_90_singular ;
  lemon:otherForm  :tower_of_90_plural ;
  lemon:sense          :tower_of_90_sense_1 ;
  lemon:synBehavior    :tower_of_90_predFrame .

:tower_of_90_form
 lemon:writtenRep "tower"@en .

:tower_of_90_singular a    lemon:Form ;
  lemon:writtenRep "tower"@en ;
  lexinfo:number   lexinfo:singular .

:tower_of_90_plural a   lemon:Form ;
  lemon:writtenRep "towers"@en ;
  lexinfo:number    lexinfo:plural .

:tower_of_90_sense_1 a  lemon:LexicalSense ;
  lemon:reference :tower_of_90_sense_1_res ;
  lemon:isA       :tower_of_90_PredSynArg .

:tower_of_90_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Tower> .
