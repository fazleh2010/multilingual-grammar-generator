@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :broadcaster_of_1 ;
  lemon:entry    :broadcaster_of_1_form_preposition .

:broadcaster_of_1 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :broadcaster_of_1_form ;
  lemon:otherForm  :broadcaster_of_1_singular ;
  lemon:otherForm  :broadcaster_of_1_plural ;
  lemon:sense          :broadcaster_of_1_sense_1 ;
  lemon:synBehavior    :broadcaster_of_1_nounpp .

:broadcaster_of_1_form a lemon:Form ;
  lemon:writtenRep "broadcaster"@en .

:broadcaster_of_1_singular a    lemon:Form ;
  lemon:writtenRep "broadcaster"@en ;
  lexinfo:number   lexinfo:singular .

:broadcaster_of_1_plural a   lemon:Form ;
  lemon:writtenRep "broadcasters"@en ;
  lexinfo:number    lexinfo:plural .

:broadcaster_of_1_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_broadcaster_of_1 ;
  lexinfo:prepositionalAdjunct :arg2_broadcaster_of_1 .

:broadcaster_of_1_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :broadcaster_of_1_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_broadcaster_of_1 ;
  lemon:objOfProp           :arg1_broadcaster_of_1 ;
  lemon:condition           :broadcaster_of_1_sense_1_condition .

:broadcaster_of_1_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Broadcaster> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Broadcaster> .

:arg2_broadcaster_of_1 lemon:marker :broadcaster_of_1_form_preposition .
## Prepositions ##
:broadcaster_of_1_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

