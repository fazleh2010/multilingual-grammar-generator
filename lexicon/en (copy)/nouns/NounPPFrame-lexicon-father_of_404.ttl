@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :father_of_404 ;
  lemon:entry    :father_of_404_form_preposition .

:father_of_404 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :father_of_404_form ;
  lemon:otherForm  :father_of_404_singular ;
  lemon:otherForm  :father_of_404_plural ;
  lemon:sense          :father_of_404_sense_1 ;
  lemon:sense          :father_of_404_sense_2 ;
  lemon:sense          :father_of_404_sense_3 ;
  lemon:synBehavior    :father_of_404_nounpp .

:father_of_404_form a lemon:Form ;
  lemon:writtenRep "father"@en .

:father_of_404_singular a    lemon:Form ;
  lemon:writtenRep "father"@en ;
  lexinfo:number   lexinfo:singular .

:father_of_404_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:father_of_404_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :null ;
  lexinfo:prepositionalAdjunct :null .

:father_of_404_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :father_of_404_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/father> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :father_of_404_sense_1_condition .

:father_of_404_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Royalty> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:father_of_404_sense_2 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :father_of_404_sense_2 ;
  lemon:reference           <http://dbpedia.org/property/father> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :father_of_404_sense_2_condition .

:father_of_404_sense_2_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Royalty> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:father_of_404_sense_3 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :father_of_404_sense_3 ;
  lemon:reference           <http://dbpedia.org/property/father> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :father_of_404_sense_3_condition .

:father_of_404_sense_3_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Royalty> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:null lemon:marker :father_of_404_form_preposition .
## Prepositions ##
:father_of_404_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

