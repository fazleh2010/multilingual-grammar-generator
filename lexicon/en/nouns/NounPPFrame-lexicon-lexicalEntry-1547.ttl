@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-1547 ;
  lemon:entry    :lexicalEntry-1547_form_preposition .

:lexicalEntry-1547 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lexicalEntry-1547_form ;
  lemon:otherForm  :lexicalEntry-1547_singular ;
  lemon:otherForm  :lexicalEntry-1547_plural ;
  lemon:sense          :lexicalEntry-1547_sense_1 ;
  lemon:synBehavior    :lexicalEntry-1547_nounpp .

:lexicalEntry-1547_form a lemon:Form ;
  lemon:writtenRep "the book"@en .

:lexicalEntry-1547_singular a    lemon:Form ;
  lemon:writtenRep "the book"@en ;
  lexinfo:number   lexinfo:singular .

:lexicalEntry-1547_plural a   lemon:Form ;
  lemon:writtenRep "the book"@en ;
  lexinfo:number    lexinfo:plural .

:lexicalEntry-1547_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:lexicalEntry-1547_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :lexicalEntry-1547_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/isbn> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :lexicalEntry-1547_sense_1_condition .

:lexicalEntry-1547_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Book> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2 lemon:marker :lexicalEntry-1547_form_preposition .
## Prepositions ##
:lexicalEntry-1547_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

