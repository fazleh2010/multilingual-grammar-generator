@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :horse_of_212 ;
  lemon:entry    :horse_of_212_form_preposition .

:horse_of_212 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :horse_of_212_form ;
  lemon:otherForm  :horse_of_212_singular ;
  lemon:otherForm  :horse_of_212_plural ;
  lemon:sense          :horse_of_212_sense_1 ;
  lemon:synBehavior    :horse_of_212_nounpp .

:horse_of_212_form a lemon:Form ;
  lemon:writtenRep "Horse"@en .

:horse_of_212_singular a    lemon:Form ;
  lemon:writtenRep "Horse"@en ;
  lexinfo:number   lexinfo:singular .

:horse_of_212_plural a   lemon:Form ;
  lemon:writtenRep "Horses"@en ;
  lexinfo:number    lexinfo:plural .

:horse_of_212_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_horse_of_212 ;
  lexinfo:prepositionalAdjunct :arg2_horse_of_212 .

:horse_of_212_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :horse_of_212_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_horse_of_212 ;
  lemon:objOfProp           :arg1_horse_of_212 ;
  lemon:condition           :horse_of_212_sense_1_condition .

:horse_of_212_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Horse> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Horse> .

:arg2_horse_of_212 lemon:marker :horse_of_212_form_preposition .
## Prepositions ##
:horse_of_212_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

