@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :article_of_17 ;
  lemon:entry    :article_of_17_form_preposition .

:article_of_17 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :article_of_17_form ;
  lemon:otherForm  :article_of_17_singular ;
  lemon:otherForm  :article_of_17_plural ;
  lemon:sense          :article_of_17_sense_1 ;
  lemon:synBehavior    :article_of_17_nounpp .

:article_of_17_form a lemon:Form ;

:article_of_17_singular a    lemon:Form ;
  lemon:writtenRep "article"@en ;
  lexinfo:number   lexinfo:singular .

:article_of_17_plural a   lemon:Form ;
  lemon:writtenRep "articles"@en ;
  lexinfo:number    lexinfo:plural .

:article_of_17_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_article_of_17 ;
  lexinfo:prepositionalAdjunct :arg2_article_of_17 .

:article_of_17_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :article_of_17_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_article_of_17 ;
  lemon:objOfProp           :arg1_article_of_17 ;
  lemon:condition           :article_of_17_sense_1_condition .

:article_of_17_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Article> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Article> .

:arg2_article_of_17 lemon:marker :article_of_17_form_preposition .
## Prepositions ##
:article_of_17_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

