@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :record_in_1_25 ;
  lemon:entry    :form_record_in_1_25_preposition .

:record_in_1_25 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_record_in_1_25 ;
  lemon:otherForm      :form_record_in_1_25_present3rd ;
  lemon:otherForm      :form_record_in_1_25_past ;
  lemon:otherForm      :form_record_in_1_25_perfect ;
  lemon:sense          :record_in_1_25_sense_1 ;
  lemon:synBehavior    :record_in_1_25_frame .

:form_record_in_1_25 a           lemon:Form ;
  lemon:writtenRep     "record"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_record_in_1_25_present3rd a      lemon:Form ;
  lemon:writtenRep "records"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_record_in_1_25_past a lemon:Form ;
  lemon:writtenRep  "recored"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_record_in_1_25_perfect a   lemon:Form ;
  lemon:writtenRep "recored"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:record_in_1_25_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :record_in_1_25_subj ;
  lexinfo:prepositionalAdjunct :record_in_1_25_obj .

:record_in_1_25_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :record_in_1_25_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/recordedIn> ;
  lemon:subjOfProp  :record_in_1_25_subj ;
  lemon:objOfProp   :record_in_1_25_obj ;
  lemon:condition   :record_in_1_25_sense_1_condition .

:record_in_1_25_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/MusicalWork> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/PopulatedPlace> .


:record_in_1_25_obj lemon:marker :form_record_in_1_25_preposition .

## Prepositions ##
:form_record_in_1_25_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

