@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :record_for_1_234 ;
  lemon:entry    :form_record_for_1_234_preposition .

:record_for_1_234 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_record_for_1_234 ;
  lemon:otherForm      :form_record_for_1_234_present3rd ;
  lemon:otherForm      :form_record_for_1_234_past ;
  lemon:otherForm      :form_record_for_1_234_perfect ;
  lemon:sense          :record_for_1_234_sense_1 ;
  lemon:synBehavior    :record_for_1_234_frame .

:form_record_for_1_234 a           lemon:Form ;
  lemon:writtenRep     "record"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_record_for_1_234_present3rd a      lemon:Form ;
  lemon:writtenRep "records"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_record_for_1_234_past a lemon:Form ;
  lemon:writtenRep  "recorded"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_record_for_1_234_perfect a   lemon:Form ;
  lemon:writtenRep "recorded"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:record_for_1_234_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :record_for_1_234_subj ;
  lexinfo:prepositionalAdjunct :record_for_1_234_obj .

:record_for_1_234_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :record_for_1_234_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/releaseDate> ;
  lemon:subjOfProp  :record_for_1_234_obj ;
  lemon:objOfProp   :record_for_1_234_subj ;
  lemon:condition   :record_for_1_234_sense_1_condition .

:record_for_1_234_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:record_for_1_234_obj lemon:marker :form_record_for_1_234_preposition .

## Prepositions ##
:form_record_for_1_234_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "for"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

