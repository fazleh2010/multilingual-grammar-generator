@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_call_1_21 ;
  lemon:entry    :call_1_21ed ;
  lemon:entry    :form_call_1_21_preposition .

:to_call_1_21 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_call_1_21 ;
  lemon:otherForm      :form_call_1_21s ;
  lemon:otherForm      :form_call_1_21ed ;
  lemon:otherForm      :form_call_1_21perfect ;
  lemon:sense          :call_1_21_sense_1 ;
  lemon:synBehavior    :call_1_21_frame_transitive .

:form_call_1_21 a         lemon:Form ;
  lemon:writtenRep     "call"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_call_1_21s a    lemon:Form ;
  lemon:writtenRep "calls"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_call_1_21ed a   lemon:Form ;
  lemon:writtenRep "called"@en ;
  lexinfo:tense    lexinfo:past .

:form_call_1_21perfect a   lemon:Form ;
  lemon:writtenRep "called"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:call_1_21_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :call_1_21_obj ;
  lexinfo:directObject     :call_1_21_subj .

:call_1_21_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :call_1_21_sense_1 ;
  lemon:reference   <http://dbpedia.org/property/shipNamesake> ;
  lemon:subjOfProp  :call_1_21_obj ;
  lemon:objOfProp   :call_1_21_subj ;
  lemon:condition   :call_1_21_sense_1_condition .


:call_1_21_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Ship> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:call_1_21_obj lemon:marker :form_call_1_21_preposition .

## Prepositions ##
:form_call_1_21_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

