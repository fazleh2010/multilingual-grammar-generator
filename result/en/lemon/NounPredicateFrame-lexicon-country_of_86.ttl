@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :country_of_86_sense_1_res ;
  lemon:entry    :country_of_86 .

:country_of_86 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :country_of_86_form ;
  lemon:otherForm  :country_of_86_singular ;
  lemon:otherForm  :country_of_86_plural ;
  lemon:sense          :country_of_86_sense_1 ;
  lemon:synBehavior    :country_of_86_predFrame .

:country_of_86_form
 lemon:writtenRep "country"@en .

:country_of_86_singular a    lemon:Form ;
  lemon:writtenRep "country"@en ;
  lexinfo:number   lexinfo:singular .

:country_of_86_plural a   lemon:Form ;
  lemon:writtenRep "countries"@en ;
  lexinfo:number    lexinfo:plural .

:country_of_86_sense_1 a  lemon:LexicalSense ;
  lemon:reference :country_of_86_sense_1_res ;
  lemon:isA       :country_of_86_PredSynArg .

:country_of_86_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Country> .
