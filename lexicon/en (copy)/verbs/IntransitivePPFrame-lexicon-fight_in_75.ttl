@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :fight_in_75 ;
  lemon:entry    :form_fight_in_75_preposition .

:fight_in_75 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_fight_in_75 ;
  lemon:otherForm      :form_fight_in_75_present3rd ;
  lemon:otherForm      :form_fight_in_75_past ;
  lemon:otherForm      :form_fight_in_75_perfect ;
  lemon:sense          :fight_in_75_sense_1 ;
  lemon:synBehavior    :fight_in_75_frame .

:form_fight_in_75 a           lemon:Form ;
  lemon:writtenRep     "fight"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_fight_in_75_present3rd a      lemon:Form ;
  lemon:writtenRep "fights"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_fight_in_75_past a lemon:Form ;
  lemon:writtenRep  "fought"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_fight_in_75_perfect a   lemon:Form ;
  lemon:writtenRep "fought"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:fight_in_75_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :fight_in_75_subj ;
  lexinfo:prepositionalAdjunct :fight_in_75_obj .

:fight_in_75_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :fight_in_75_ontomap ;
  lemon:reference   <http://dbpedia.org/property/date> ;
  lemon:subjOfProp  :fight_in_75_obj ;
  lemon:objOfProp   :fight_in_75_subj ;
  lemon:condition   :fight_in_75_sense_1_condition .

:fight_in_75_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/MilitaryConflict> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:fight_in_75_obj lemon:marker :form_fight_in_75_preposition .

## Prepositions ##
:form_fight_in_75_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

