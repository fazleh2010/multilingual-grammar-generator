@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :ship_404_sense_1_res ;
  lemon:entry    :ship_404 .

:ship_404 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :ship_404_form ;
  lemon:otherForm  :ship_404_singular ;
  lemon:otherForm  :ship_404_plural ;
  lemon:sense          :ship_404_sense_1 ;
  lemon:synBehavior    :ship_404_predFrame .

:ship_404_form
 lemon:writtenRep "ship"@en .

:ship_404_singular a    lemon:Form ;
  lemon:writtenRep "ship"@en ;
  lexinfo:number   lexinfo:singular .

:ship_404_plural a   lemon:Form ;
  lemon:writtenRep "ships"@en ;
  lexinfo:number    lexinfo:plural .

:ship_404_sense_1 a  lemon:LexicalSense ;
  lemon:reference :ship_404_sense_1_res ;
  lemon:isA       :ship_404_PredSynArg .

:ship_404_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Ship> .