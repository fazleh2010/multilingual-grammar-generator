@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :creator_of_930 ;
  lemon:entry    :creator_of_930_form_preposition .

:creator_of_930 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :creator_of_930_form ;
  lemon:otherForm  :creator_of_930_singular ;
  lemon:otherForm  :creator_of_930_plural ;
  lemon:sense          :creator_of_930_sense_1 ;
  lemon:sense          :creator_of_930_sense_2 ;
  lemon:synBehavior    :creator_of_930_nounpp .

:creator_of_930_form a lemon:Form ;
  lemon:writtenRep "creator"@en .

:creator_of_930_singular a    lemon:Form ;
  lemon:writtenRep "creator"@en ;
  lexinfo:number   lexinfo:singular .

:creator_of_930_plural a   lemon:Form ;
  lemon:writtenRep "creators"@en ;
  lexinfo:number    lexinfo:plural .

:creator_of_930_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_creator_of_930 ;
  lexinfo:prepositionalAdjunct :arg2_creator_of_930 .

:creator_of_930_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :creator_of_930_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/creator> ;
  lemon:subjOfProp          :arg2_creator_of_930 ;
  lemon:objOfProp           :arg1_creator_of_930 ;
  lemon:condition           :creator_of_930_sense_1_condition .

:creator_of_930_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Organisation> .

:creator_of_930_sense_2 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :creator_of_930_sense_2 ;
  lemon:reference           <http://dbpedia.org/ontology/creator> ;
  lemon:subjOfProp          :arg2_creator_of_930 ;
  lemon:objOfProp           :arg1_creator_of_930 ;
  lemon:condition           :creator_of_930_sense_2_condition .

:creator_of_930_sense_2_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Organisation> .

:arg2_creator_of_930 lemon:marker :creator_of_930_form_preposition .
## Prepositions ##
:creator_of_930_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

