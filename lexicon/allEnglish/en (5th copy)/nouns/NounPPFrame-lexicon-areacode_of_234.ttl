  lemon:subjOfProp          :arg2_areacode_of_234 ;
  lemon:objOfProp           :arg1_areacode_of_234 ;
  lemon:condition           :areacode_of_234_sense_1_condition .

:areacode_of_234_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/City> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#string> .

:arg2_areacode_of_234 lemon:marker :areacode_of_234_form_preposition .
## Prepositions ##
:areacode_of_234_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

