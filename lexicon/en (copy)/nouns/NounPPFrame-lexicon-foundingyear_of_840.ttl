@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :foundingyear_of_840 ;
  lemon:entry    :foundingyear_of_840_form_preposition .

:foundingyear_of_840 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :foundingyear_of_840_form ;
  lemon:otherForm  :foundingyear_of_840_singular ;
  lemon:otherForm  :foundingyear_of_840_plural ;
  lemon:sense          :foundingyear_of_840_sense_1 ;
  lemon:synBehavior    :foundingyear_of_840_nounpp .

:foundingyear_of_840_form a lemon:Form ;
  lemon:writtenRep "founding year"@en .

:foundingyear_of_840_singular a    lemon:Form ;
