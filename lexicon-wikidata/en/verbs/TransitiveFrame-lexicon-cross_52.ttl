@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_cross_52 ;
  lemon:entry    :cross_52ed ;
  lemon:entry    :form_cross_52_preposition .

:to_cross_52 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_cross_52 ;
  lemon:otherForm      :form_cross_52s ;
  lemon:otherForm      :form_cross_52ed ;
  lemon:otherForm      :form_cross_52perfect ;
  lemon:sense          :cross_52_sense_1 ;
  lemon:synBehavior    :cross_52_frame_transitive .

:form_cross_52 a         lemon:Form ;
  lemon:writtenRep     "cross"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_cross_52s a    lemon:Form ;
  lemon:writtenRep "crosses"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_cross_52ed a   lemon:Form ;
  lemon:writtenRep "crossed"@en ;
  lexinfo:tense    lexinfo:past .

:form_cross_52perfect a   lemon:Form ;
  lemon:writtenRep "crossed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:cross_52_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :cross_52_obj ;
  lexinfo:directObject     :cross_52_subj .

:cross_52_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :cross_52_sense_1 ;
  lemon:reference   <wd:P177> ;
  lemon:subjOfProp  :cross_52_obj ;
  lemon:objOfProp   :cross_52_subj ;
  lemon:condition   :cross_52_sense_1_condition .


:cross_52_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <wd:Q12280> ;
  lemon:propertyRange  <wd:Q47521> .


:cross_52_obj lemon:marker :form_cross_52_preposition .

## Prepositions ##
:form_cross_52_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

