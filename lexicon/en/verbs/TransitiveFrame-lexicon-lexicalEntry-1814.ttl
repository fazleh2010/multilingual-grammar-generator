@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-1814 ;
  lemon:entry    :lexicalEntry-1814ed ;
  lemon:entry    :form_lexicalEntry-1814_preposition .

:to_lexicalEntry-1814 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-1814 ;
  lemon:otherForm      :form_lexicalEntry-1814s ;
  lemon:otherForm      :form_lexicalEntry-1814ed ;
  lemon:otherForm      :form_lexicalEntry-1814perfect ;
  lemon:sense          :lexicalEntry-1814_sense_1 ;
  lemon:synBehavior    :lexicalEntry-1814_frame_transitive .

:form_lexicalEntry-1814 a         lemon:Form ;
  lemon:writtenRep     "based in"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-1814s a    lemon:Form ;
  lemon:writtenRep "based in"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-1814ed a   lemon:Form ;
  lemon:writtenRep "based in"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-1814perfect a   lemon:Form ;
  lemon:writtenRep "based in"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-1814_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-1814_subj ;
  lexinfo:directObject     :lexicalEntry-1814_obj .

:lexicalEntry-1814_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1814_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/league> ;
  lemon:subjOfProp  :lexicalEntry-1814_obj ;
  lemon:objOfProp   :lexicalEntry-1814_subj ;
  lemon:condition   :lexicalEntry-1814_sense_1_condition .


:lexicalEntry-1814_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/HockeyTeam> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/IceHockeyLeague> .

:lexicalEntry-1814ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-1814ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-1814ed_by ;
  lemon:synBehavior    :lexicalEntry-1814ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-1814ed_ontomap .

:form_lexicalEntry-1814ed_canonical a lemon:Form ;
  lemon:writtenRep         "based in"@en .

:form_lexicalEntry-1814ed_by a    lemon:Form ;
  lemon:writtenRep     "based in"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-1814ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-1814ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-1814ed_obj .

:lexicalEntry-1814ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1814ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/league> ;
  lemon:subjOfProp  :lexicalEntry-1814ed_subj ;
  lemon:objOfProp   :lexicalEntry-1814ed_obj ;
  lemon:condition   :lexicalEntry-1814_sense_1_condition .
:lexicalEntry-1814ed_obj lemon:marker :form_lexicalEntry-1814_preposition .

## Prepositions ##
:form_lexicalEntry-1814_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

