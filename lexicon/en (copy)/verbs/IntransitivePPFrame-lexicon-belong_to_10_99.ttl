@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :belong_to_10_99 ;
  lemon:entry    :form_belong_to_10_99_preposition .

:belong_to_10_99 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_belong_to_10_99 ;
  lemon:otherForm      :form_belong_to_10_99_present3rd ;
  lemon:otherForm      :form_belong_to_10_99_past ;
  lemon:otherForm      :form_belong_to_10_99_perfect ;
  lemon:sense          :belong_to_10_99_sense_1 ;
  lemon:synBehavior    :belong_to_10_99_frame .

:form_belong_to_10_99 a           lemon:Form ;
  lemon:writtenRep     "belong"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_belong_to_10_99_present3rd a      lemon:Form ;
  lemon:writtenRep "belongs"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_belong_to_10_99_past a lemon:Form ;
  lemon:writtenRep  "belonged"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_belong_to_10_99_perfect a   lemon:Form ;
  lemon:writtenRep "belonged"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:belong_to_10_99_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :belong_to_10_99_subj ;
  lexinfo:prepositionalAdjunct :belong_to_10_99_obj .

:belong_to_10_99_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :belong_to_10_99_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/bandMember> ;
  lemon:subjOfProp  :belong_to_10_99_subj ;
  lemon:objOfProp   :belong_to_10_99_obj ;
  lemon:condition   :belong_to_10_99_sense_1_condition .

:belong_to_10_99_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Band> .


:belong_to_10_99_obj lemon:marker :form_belong_to_10_99_preposition .

## Prepositions ##
:form_belong_to_10_99_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "to"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

