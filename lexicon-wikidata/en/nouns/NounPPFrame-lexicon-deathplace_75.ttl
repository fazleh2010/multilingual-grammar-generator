@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :deathplace_75 ;
  lemon:entry    :deathplace_75_form_preposition .

:deathplace_75 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :deathplace_75_form ;
  lemon:otherForm  :deathplace_75_singular ;
  lemon:otherForm  :deathplace_75_plural ;
  lemon:sense          :deathplace_75_sense_1 ;
  lemon:synBehavior    :deathplace_75_nounpp .

:deathplace_75_form a lemon:Form ;
  lemon:writtenRep "death place"@en .

:deathplace_75_singular a    lemon:Form ;
  lemon:writtenRep "death place"@en ;
  lexinfo:number   lexinfo:singular .

:deathplace_75_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:deathplace_75_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_deathplace_75 ;
  lexinfo:prepositionalAdjunct :arg2_deathplace_75 .

:deathplace_75_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :deathplace_75_sense_1 ;
  lemon:reference           <wd:P20> ;
  lemon:subjOfProp          :arg2_deathplace_75 ;
  lemon:objOfProp           :arg1_deathplace_75 ;
  lemon:condition           :deathplace_75_sense_1_condition .

:deathplace_75_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <wd:Q19088> ;
  lemon:propertyRange   <wd:Q3957> .

:arg2_deathplace_75 lemon:marker :deathplace_75_form_preposition .
## Prepositions ##
:deathplace_75_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

