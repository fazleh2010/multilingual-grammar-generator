@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :place_of_133 ;
  lemon:entry    :place_of_133_form_preposition .

:place_of_133 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :place_of_133_form ;
  lemon:otherForm  :place_of_133_singular ;
  lemon:otherForm  :place_of_133_plural ;
  lemon:sense          :place_of_133_sense_1 ;
  lemon:synBehavior    :place_of_133_nounpp .

:place_of_133_form a lemon:Form ;

:place_of_133_singular a    lemon:Form ;
  lemon:writtenRep "place"@en ;
  lexinfo:number   lexinfo:singular .

:place_of_133_plural a   lemon:Form ;
  lemon:writtenRep "places"@en ;
  lexinfo:number    lexinfo:plural .

:place_of_133_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_place_of_133 ;
  lexinfo:prepositionalAdjunct :arg2_place_of_133 .

:place_of_133_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :place_of_133_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_place_of_133 ;
  lemon:objOfProp           :arg1_place_of_133 ;
  lemon:condition           :place_of_133_sense_1_condition .

:place_of_133_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Place> .

:arg2_place_of_133 lemon:marker :place_of_133_form_preposition .
## Prepositions ##
:place_of_133_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

