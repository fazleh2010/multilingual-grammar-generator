@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :document_505_sense_1_res ;
  lemon:entry    :document_505 .

:document_505 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :document_505_form ;
  lemon:otherForm  :document_505_singular ;
  lemon:otherForm  :document_505_plural ;
  lemon:sense          :document_505_sense_1 ;
  lemon:synBehavior    :document_505_predFrame .

:document_505_form
 lemon:writtenRep "document"@en .

:document_505_singular a    lemon:Form ;
  lemon:writtenRep "document"@en ;
  lexinfo:number   lexinfo:singular .

:document_505_plural a   lemon:Form ;
  lemon:writtenRep "documents"@en ;
  lexinfo:number    lexinfo:plural .

:document_505_sense_1 a  lemon:LexicalSense ;
  lemon:reference :document_505_sense_1_res ;
  lemon:isA       :document_505_PredSynArg .

:document_505_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Document> .