@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :atmosphere_of_2 ;
  lemon:entry    :atmosphere_of_2_form_preposition .

:atmosphere_of_2 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :atmosphere_of_2_form ;
  lemon:otherForm  :atmosphere_of_2_singular ;
  lemon:otherForm  :atmosphere_of_2_plural ;
  lemon:sense          :atmosphere_of_2_sense_1 ;
  lemon:synBehavior    :atmosphere_of_2_nounpp .

:atmosphere_of_2_form a lemon:Form ;
  lemon:writtenRep "atmosphere"@en .

:atmosphere_of_2_singular a    lemon:Form ;
  lemon:writtenRep "atmosphere"@en ;
  lexinfo:number   lexinfo:singular .

:atmosphere_of_2_plural a   lemon:Form ;
  lemon:writtenRep "atmospheres"@en ;
  lexinfo:number    lexinfo:plural .

:atmosphere_of_2_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_atmosphere_of_2 ;
  lexinfo:prepositionalAdjunct :arg2_atmosphere_of_2 .

:atmosphere_of_2_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :atmosphere_of_2_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/atmosphereComposition> ;
  lemon:subjOfProp          :arg2_atmosphere_of_2 ;
  lemon:objOfProp           :arg1_atmosphere_of_2 ;
  lemon:condition           :atmosphere_of_2_sense_1_condition .

:atmosphere_of_2_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/CelestialBody> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/SoccerClub> .

:arg2_atmosphere_of_2 lemon:marker :atmosphere_of_2_form_preposition .
## Prepositions ##
:atmosphere_of_2_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

