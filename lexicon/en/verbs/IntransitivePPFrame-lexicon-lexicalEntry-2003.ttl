@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-2003 ;
  lemon:entry    :form_lexicalEntry-2003_preposition .

:lexicalEntry-2003 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-2003 ;
  lemon:otherForm      :form_lexicalEntry-2003_past ;
  lemon:sense          :lexicalEntry-2003_sense_1 ;
  lemon:synBehavior    :lexicalEntry-2003_frame .

:form_lexicalEntry-2003 a           lemon:Form ;
  lemon:writtenRep     "born"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-2003 a      lemon:Form ;
  lemon:writtenRep "born"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-2003_past a lemon:Form ;
  lemon:writtenRep  "born"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-2003perfect a   lemon:Form ;
  lemon:writtenRep "born"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-2003_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-2003_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-2003_obj .

:lexicalEntry-2003_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2003_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/movement> ;
  lemon:subjOfProp  :lexicalEntry-2003_obj ;
  lemon:objOfProp   :lexicalEntry-2003_subj ;
  lemon:condition   :lexicalEntry-2003_sense_1_condition .

:lexicalEntry-2003_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Artist> ;
  lemon:propertyRange  <http://www.w3.org/2002/07/owl#Thing> .


:lexicalEntry-2003_obj lemon:marker :form_lexicalEntry-2003_preposition .

## Prepositions ##
:form_lexicalEntry-2003_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

