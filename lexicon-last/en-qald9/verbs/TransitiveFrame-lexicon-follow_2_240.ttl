@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_follow_2_240 ;
  lemon:entry    :follow_2_240ed ;
  lemon:entry    :form_follow_2_240_preposition .

:to_follow_2_240 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_follow_2_240 ;
  lemon:otherForm      :form_follow_2_240s ;
  lemon:otherForm      :form_follow_2_240ed ;
  lemon:otherForm      :form_follow_2_240perfect ;
  lemon:sense          :follow_2_240_sense_1 ;
  lemon:synBehavior    :follow_2_240_frame_transitive .

:form_follow_2_240 a         lemon:Form ;
  lemon:writtenRep     "follow"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_follow_2_240s a    lemon:Form ;
  lemon:writtenRep "follows"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_follow_2_240ed a   lemon:Form ;
  lemon:writtenRep "followed"@en ;
  lexinfo:tense    lexinfo:past .

:form_follow_2_240perfect a   lemon:Form ;
  lemon:writtenRep "followed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:follow_2_240_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :follow_2_240_subj ;
  lexinfo:directObject     :follow_2_240_obj .

:follow_2_240_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :follow_2_240_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/subsequentWork> ;
  lemon:subjOfProp  :follow_2_240_obj ;
  lemon:objOfProp   :follow_2_240_subj ;
  lemon:condition   :follow_2_240_sense_1_condition .


:follow_2_240_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Work> .


:follow_2_240_obj lemon:marker :form_follow_2_240_preposition .

## Prepositions ##
:form_follow_2_240_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

