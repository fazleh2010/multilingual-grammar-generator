@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :radioprogram_of_244 ;
  lemon:entry    :radioprogram_of_244_form_preposition .

:radioprogram_of_244 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :radioprogram_of_244_form ;
  lemon:otherForm  :radioprogram_of_244_singular ;
  lemon:otherForm  :radioprogram_of_244_plural ;
  lemon:sense          :radioprogram_of_244_sense_1 ;
  lemon:synBehavior    :radioprogram_of_244_nounpp .

:radioprogram_of_244_form a lemon:Form ;
  lemon:writtenRep "Radio Program"@en .

:radioprogram_of_244_singular a    lemon:Form ;
  lemon:writtenRep "Radio Program"@en ;
  lexinfo:number   lexinfo:singular .

:radioprogram_of_244_plural a   lemon:Form ;
  lemon:writtenRep "Radio Programs"@en ;
  lexinfo:number    lexinfo:plural .

:radioprogram_of_244_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_radioprogram_of_244 ;
  lexinfo:prepositionalAdjunct :arg2_radioprogram_of_244 .

:radioprogram_of_244_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :radioprogram_of_244_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_radioprogram_of_244 ;
  lemon:objOfProp           :arg1_radioprogram_of_244 ;
  lemon:condition           :radioprogram_of_244_sense_1_condition .

:radioprogram_of_244_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/RadioProgram> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/RadioProgram> .

:arg2_radioprogram_of_244 lemon:marker :radioprogram_of_244_form_preposition .
## Prepositions ##
:radioprogram_of_244_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

