@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :racingdriver_of_76 ;
  lemon:entry    :racingdriver_of_76_form_preposition .

:racingdriver_of_76 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :racingdriver_of_76_form ;
  lemon:otherForm  :racingdriver_of_76_singular ;
  lemon:otherForm  :racingdriver_of_76_plural ;
  lemon:sense          :racingdriver_of_76_sense_1 ;
  lemon:synBehavior    :racingdriver_of_76_nounpp .

:racingdriver_of_76_form a lemon:Form ;
  lemon:writtenRep "Racing Driver"@en .

:racingdriver_of_76_singular a    lemon:Form ;
  lemon:writtenRep "Racing Driver"@en ;
  lexinfo:number   lexinfo:singular .

:racingdriver_of_76_plural a   lemon:Form ;
  lemon:writtenRep "Racing Drivers"@en ;
  lexinfo:number    lexinfo:plural .

:racingdriver_of_76_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_racingdriver_of_76 ;
  lexinfo:prepositionalAdjunct :arg2_racingdriver_of_76 .

:racingdriver_of_76_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :racingdriver_of_76_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_racingdriver_of_76 ;
  lemon:objOfProp           :arg1_racingdriver_of_76 ;
  lemon:condition           :racingdriver_of_76_sense_1_condition .

:racingdriver_of_76_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/RacingDriver> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/RacingDriver> .

:arg2_racingdriver_of_76 lemon:marker :racingdriver_of_76_form_preposition .
## Prepositions ##
:racingdriver_of_76_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

