@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-3713 ;
  lemon:entry    :lexicalEntry-3713ed ;
  lemon:entry    :form_lexicalEntry-3713_preposition .

:to_lexicalEntry-3713 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-3713 ;
  lemon:otherForm      :form_lexicalEntry-3713s ;
  lemon:otherForm      :form_lexicalEntry-3713ed ;
  lemon:otherForm      :form_lexicalEntry-3713perfect ;
  lemon:sense          :lexicalEntry-3713_sense_1 ;
  lemon:synBehavior    :lexicalEntry-3713_frame_transitive .

:form_lexicalEntry-3713 a         lemon:Form ;
  lemon:writtenRep     "develop"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-3713s a    lemon:Form ;
  lemon:writtenRep "develops"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-3713ed a   lemon:Form ;
  lemon:writtenRep "developed"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-3713perfect a   lemon:Form ;
  lemon:writtenRep "developed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-3713_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-3713_subj ;
  lexinfo:directObject     :lexicalEntry-3713_obj .

:lexicalEntry-3713_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3713_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/composer> ;
  lemon:subjOfProp  :lexicalEntry-3713_obj ;
  lemon:objOfProp   :lexicalEntry-3713_subj ;
  lemon:condition   :lexicalEntry-3713_sense_1_condition .


:lexicalEntry-3713_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .

:lexicalEntry-3713ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-3713ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-3713ed_by ;
  lemon:synBehavior    :lexicalEntry-3713ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-3713ed_ontomap .

:form_lexicalEntry-3713ed_canonical a lemon:Form ;
  lemon:writtenRep         "develop"@en .

:form_lexicalEntry-3713ed_by a    lemon:Form ;
  lemon:writtenRep     "develop"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-3713ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-3713ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-3713ed_obj .

:lexicalEntry-3713ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3713ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/composer> ;
  lemon:subjOfProp  :lexicalEntry-3713ed_subj ;
  lemon:objOfProp   :lexicalEntry-3713ed_obj ;
  lemon:condition   :lexicalEntry-3713_sense_1_condition .
:lexicalEntry-3713ed_obj lemon:marker :form_lexicalEntry-3713_preposition .

## Prepositions ##
:form_lexicalEntry-3713_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

