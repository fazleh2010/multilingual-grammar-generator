@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :runtime_of_1_41 ;
  lemon:entry    :runtime_of_1_41_form_preposition .

:runtime_of_1_41 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :runtime_of_1_41_form ;
  lemon:otherForm  :runtime_of_1_41_singular ;
  lemon:otherForm  :runtime_of_1_41_plural ;
  lemon:sense          :runtime_of_1_41_sense_1 ;
  lemon:synBehavior    :runtime_of_1_41_nounpp .

:runtime_of_1_41_form a lemon:Form ;
  lemon:writtenRep "runtime"@en .

:runtime_of_1_41_singular a    lemon:Form ;
  lemon:writtenRep "runtime"@en ;
  lexinfo:number   lexinfo:singular .

:runtime_of_1_41_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:runtime_of_1_41_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_runtime_of_1_41 ;
  lexinfo:prepositionalAdjunct :arg2_runtime_of_1_41 .

:runtime_of_1_41_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :runtime_of_1_41_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/runtime> ;
  lemon:subjOfProp          :arg2_runtime_of_1_41 ;
  lemon:objOfProp           :arg1_runtime_of_1_41 ;
  lemon:condition           :runtime_of_1_41_sense_1_condition .

:runtime_of_1_41_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2_runtime_of_1_41 lemon:marker :runtime_of_1_41_form_preposition .
## Prepositions ##
:runtime_of_1_41_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

