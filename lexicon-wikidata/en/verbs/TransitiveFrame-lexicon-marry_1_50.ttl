@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_marry_1_50 ;
  lemon:entry    :marry_1_50ed ;
  lemon:entry    :form_marry_1_50_preposition .

:to_marry_1_50 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_marry_1_50 ;
  lemon:otherForm      :form_marry_1_50s ;
  lemon:otherForm      :form_marry_1_50ed ;
  lemon:otherForm      :form_marry_1_50perfect ;
  lemon:sense          :marry_1_50_sense_1 ;
  lemon:synBehavior    :marry_1_50_frame_transitive .

:form_marry_1_50 a         lemon:Form ;
  lemon:writtenRep     "marry"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_marry_1_50s a    lemon:Form ;
  lemon:writtenRep "marries"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_marry_1_50ed a   lemon:Form ;
  lemon:writtenRep "married"@en ;
  lexinfo:tense    lexinfo:past .

:form_marry_1_50perfect a   lemon:Form ;
  lemon:writtenRep "married"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:marry_1_50_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :marry_1_50_subj ;
  lexinfo:directObject     :marry_1_50_obj .

:marry_1_50_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :marry_1_50_sense_1 ;
  lemon:reference   <wd:P26> ;
  lemon:subjOfProp  :marry_1_50_obj ;
  lemon:objOfProp   :marry_1_50_subj ;
  lemon:condition   :marry_1_50_sense_1_condition .


:marry_1_50_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <wd:Q19088> ;
  lemon:propertyRange  <wd:Q19088> .


:marry_1_50_obj lemon:marker :form_marry_1_50_preposition .

## Prepositions ##
:form_marry_1_50_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "to"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

