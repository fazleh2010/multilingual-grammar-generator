@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_paint_40 ;
  lemon:entry    :paint_40ed ;
  lemon:entry    :form_paint_40_preposition .

:to_paint_40 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_paint_40 ;
  lemon:otherForm      :form_paint_40s ;
  lemon:otherForm      :form_paint_40ed ;
  lemon:otherForm      :form_paint_40perfect ;
  lemon:sense          :paint_40_sense_1 ;
  lemon:synBehavior    :paint_40_frame_transitive .

:form_paint_40 a         lemon:Form ;
  lemon:writtenRep     "paint"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_paint_40s a    lemon:Form ;
  lemon:writtenRep "paints"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_paint_40ed a   lemon:Form ;
  lemon:writtenRep "painted"@en ;
  lexinfo:tense    lexinfo:past .

:form_paint_40perfect a   lemon:Form ;
  lemon:writtenRep "painted"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:paint_40_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :paint_40_subj ;
  lexinfo:directObject     :paint_40_obj .

:paint_40_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :paint_40_sense_1 ;
  lemon:reference   <wd:P50> ;
  lemon:subjOfProp  :paint_40_obj ;
  lemon:objOfProp   :paint_40_subj ;
  lemon:condition   :paint_40_sense_1_condition .


:paint_40_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <wd:Q7397> ;
  lemon:propertyRange  <wd:Q19088> .


:paint_40_obj lemon:marker :form_paint_40_preposition .

## Prepositions ##
:form_paint_40_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

