@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-4431 ;
  lemon:entry    :form_lexicalEntry-4431_preposition .

:lexicalEntry-4431 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-4431 ;
  lemon:otherForm      :form_lexicalEntry-4431_past ;
  lemon:sense          :lexicalEntry-4431_sense_1 ;
  lemon:synBehavior    :lexicalEntry-4431_frame .

:form_lexicalEntry-4431 a           lemon:Form ;
  lemon:writtenRep     "publish"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-4431 a      lemon:Form ;
  lemon:writtenRep "publishes"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-4431_past a lemon:Form ;
  lemon:writtenRep  "published"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-4431perfect a   lemon:Form ;
  lemon:writtenRep "published"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-4431_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-4431_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-4431_obj .

:lexicalEntry-4431_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-4431_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/abbreviation> ;
  lemon:subjOfProp  :lexicalEntry-4431_obj ;
  lemon:objOfProp   :lexicalEntry-4431_subj ;
  lemon:condition   :lexicalEntry-4431_sense_1_condition .

:lexicalEntry-4431_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://www.w3.org/2002/07/owl#Thing> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#string> .


:lexicalEntry-4431_obj lemon:marker :form_lexicalEntry-4431_preposition .

## Prepositions ##
:form_lexicalEntry-4431_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

