@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :president_of ;
  lemon:entry    :president_of_form_preposition .

:president_of a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :president_of_form ;
  lemon:otherForm  :president_of_singular ;
  lemon:otherForm  :president_of_plural ;
  lemon:sense          :president_of_sense_1 ;
  lemon:sense          :president_of_sense_2 ;
  lemon:synBehavior    :president_of_nounpp .

:president_of_form a lemon:Form ;
  lemon:writtenRep "president"@en .

:president_of_singular a    lemon:Form ;
  lemon:writtenRep "president"@en ;
  lexinfo:number   lexinfo:singular .

:president_of_plural a   lemon:Form ;
  lemon:writtenRep "presidents"@en ;
  lexinfo:number    lexinfo:plural .

:president_of_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:president_of_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :president_of_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/leader> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :president_of_sense_1_condition .

:president_of_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:president_of_sense_2 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :president_of_sense_2 ;
  lemon:reference           <http://dbpedia.org/ontology/leader> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :president_of_sense_2_condition .

:president_of_sense_2_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Country> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:arg2 lemon:marker :president_of_form_preposition .
## Prepositions ##
:president_of_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

