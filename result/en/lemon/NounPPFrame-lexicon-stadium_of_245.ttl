@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :stadium_of_245 ;
  lemon:entry    :stadium_of_245_form_preposition .

:stadium_of_245 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :stadium_of_245_form ;
  lemon:otherForm  :stadium_of_245_singular ;
  lemon:otherForm  :stadium_of_245_plural ;
  lemon:sense          :stadium_of_245_sense_1 ;
  lemon:synBehavior    :stadium_of_245_nounpp .

:stadium_of_245_form a lemon:Form ;

:stadium_of_245_singular a    lemon:Form ;
  lemon:writtenRep "stadium"@en ;
  lexinfo:number   lexinfo:singular .

:stadium_of_245_plural a   lemon:Form ;
  lemon:writtenRep "stadiums"@en ;
  lexinfo:number    lexinfo:plural .

:stadium_of_245_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_stadium_of_245 ;
  lexinfo:prepositionalAdjunct :arg2_stadium_of_245 .

:stadium_of_245_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :stadium_of_245_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_stadium_of_245 ;
  lemon:objOfProp           :arg1_stadium_of_245 ;
  lemon:condition           :stadium_of_245_sense_1_condition .

:stadium_of_245_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Stadium> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Stadium> .

:arg2_stadium_of_245 lemon:marker :stadium_of_245_form_preposition .
## Prepositions ##
:stadium_of_245_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

