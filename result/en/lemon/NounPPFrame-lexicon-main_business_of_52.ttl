@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :main_business_of_52 ;
  lemon:entry    :main_business_of_52_form_preposition .

:main_business_of_52 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :main_business_of_52_form ;
  lemon:otherForm  :main_business_of_52_singular ;
  lemon:otherForm  :main_business_of_52_plural ;
  lemon:sense          :main_business_of_52_sense_1 ;
  lemon:synBehavior    :main_business_of_52_nounpp .

:main_business_of_52_form a lemon:Form ;
  lemon:writtenRep "main_business_of"@en .

:main_business_of_52_singular a    lemon:Form ;
  lemon:writtenRep "main_business_of"@en ;
  lexinfo:number   lexinfo:singular .

:main_business_of_52_plural a   lemon:Form ;
  lemon:writtenRep "main businesses"@en ;
  lexinfo:number    lexinfo:plural .

:main_business_of_52_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_main_business_of_52 ;
  lexinfo:prepositionalAdjunct :arg2_main_business_of_52 .

:main_business_of_52_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :main_business_of_52_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/owner> ;
  lemon:subjOfProp          :arg2_main_business_of_52 ;
  lemon:objOfProp           :arg1_main_business_of_52 ;
  lemon:condition           :main_business_of_52_sense_1_condition .

:main_business_of_52_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Band> .

:arg2_main_business_of_52 lemon:marker :main_business_of_52_form_preposition .
## Prepositions ##
:main_business_of_52_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

