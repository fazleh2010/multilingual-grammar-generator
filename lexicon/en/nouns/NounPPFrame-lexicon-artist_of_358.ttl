@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :artist_of_358 ;
  lemon:entry    :artist_of_358_form_preposition .

:artist_of_358 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :artist_of_358_form ;
  lemon:otherForm  :artist_of_358_singular ;
  lemon:otherForm  :artist_of_358_plural ;
  lemon:sense          :artist_of_358_sense_1 ;
  lemon:synBehavior    :artist_of_358_nounpp .

:artist_of_358_form a lemon:Form ;
  lemon:writtenRep "artist"@en .

:artist_of_358_singular a    lemon:Form ;
  lemon:writtenRep "artist"@en ;
  lexinfo:number   lexinfo:singular .

:artist_of_358_plural a   lemon:Form ;
  lemon:writtenRep "artists"@en ;
  lexinfo:number    lexinfo:plural .

:artist_of_358_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_artist_of_358 ;
  lexinfo:prepositionalAdjunct :arg2_artist_of_358 .

:artist_of_358_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :artist_of_358_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_artist_of_358 ;
  lemon:objOfProp           :arg1_artist_of_358 ;
  lemon:condition           :artist_of_358_sense_1_condition .

:artist_of_358_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Artist> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Artist> .

:arg2_artist_of_358 lemon:marker :artist_of_358_form_preposition .
## Prepositions ##
:artist_of_358_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

