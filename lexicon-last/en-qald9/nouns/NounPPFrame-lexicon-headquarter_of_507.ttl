@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :headquarter_of_507 ;
  lemon:entry    :headquarter_of_507_form_preposition .

:headquarter_of_507 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :headquarter_of_507_form ;
  lemon:otherForm  :headquarter_of_507_singular ;
  lemon:otherForm  :headquarter_of_507_plural ;
  lemon:sense          :headquarter_of_507_sense_1 ;
  lemon:synBehavior    :headquarter_of_507_nounpp .

:headquarter_of_507_form a lemon:Form ;
  lemon:writtenRep "headquarter"@en .

:headquarter_of_507_singular a    lemon:Form ;
  lemon:writtenRep "headquarter"@en ;
  lexinfo:number   lexinfo:singular .

:headquarter_of_507_plural a   lemon:Form ;
  lemon:writtenRep "headquarters"@en ;
  lexinfo:number    lexinfo:plural .

:headquarter_of_507_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_headquarter_of_507 ;
  lexinfo:prepositionalAdjunct :arg2_headquarter_of_507 .

:headquarter_of_507_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :headquarter_of_507_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/location> ;
  lemon:subjOfProp          :arg1_headquarter_of_507 ;
  lemon:objOfProp           :arg2_headquarter_of_507 ;
  lemon:condition           :headquarter_of_507_sense_1_condition .

:headquarter_of_507_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Place> .

:arg2_headquarter_of_507 lemon:marker :headquarter_of_507_form_preposition .
## Prepositions ##
:headquarter_of_507_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

