@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :case_568_sense_1_res ;
  lemon:entry    :case_568 .

:case_568 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :case_568_form ;
  lemon:otherForm  :case_568_singular ;
  lemon:otherForm  :case_568_plural ;
  lemon:sense          :case_568_sense_1 ;
  lemon:synBehavior    :case_568_predFrame .

:case_568_form
 lemon:writtenRep "case"@en .

:case_568_singular a    lemon:Form ;
  lemon:writtenRep "case"@en ;
  lexinfo:number   lexinfo:singular .

:case_568_plural a   lemon:Form ;
  lemon:writtenRep "cases"@en ;
  lexinfo:number    lexinfo:plural .

:case_568_sense_1 a  lemon:LexicalSense ;
  lemon:reference :case_568_sense_1_res ;
  lemon:isA       :case_568_PredSynArg .

:case_568_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Case> .