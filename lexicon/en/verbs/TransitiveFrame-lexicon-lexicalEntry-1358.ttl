@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-1358 ;
  lemon:entry    :lexicalEntry-1358ed ;
  lemon:entry    :form_lexicalEntry-1358_preposition .

:to_lexicalEntry-1358 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-1358 ;
  lemon:otherForm      :form_lexicalEntry-1358s ;
  lemon:otherForm      :form_lexicalEntry-1358ed ;
  lemon:otherForm      :form_lexicalEntry-1358perfect ;
  lemon:sense          :lexicalEntry-1358_sense_1 ;
  lemon:synBehavior    :lexicalEntry-1358_frame_transitive .

:form_lexicalEntry-1358 a         lemon:Form ;
  lemon:writtenRep     "based"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-1358s a    lemon:Form ;
  lemon:writtenRep "based"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-1358ed a   lemon:Form ;
  lemon:writtenRep "based"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-1358perfect a   lemon:Form ;
  lemon:writtenRep "based"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-1358_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-1358_subj ;
  lexinfo:directObject     :lexicalEntry-1358_obj .

:lexicalEntry-1358_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1358_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/headquarter> ;
  lemon:subjOfProp  :lexicalEntry-1358_obj ;
  lemon:objOfProp   :lexicalEntry-1358_subj ;
  lemon:condition   :lexicalEntry-1358_sense_1_condition .


:lexicalEntry-1358_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Organisation> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/PopulatedPlace> .

:lexicalEntry-1358ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-1358ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-1358ed_by ;
  lemon:synBehavior    :lexicalEntry-1358ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-1358ed_ontomap .

:form_lexicalEntry-1358ed_canonical a lemon:Form ;
  lemon:writtenRep         "based"@en .

:form_lexicalEntry-1358ed_by a    lemon:Form ;
  lemon:writtenRep     "based"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-1358ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-1358ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-1358ed_obj .

:lexicalEntry-1358ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-1358ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/headquarter> ;
  lemon:subjOfProp  :lexicalEntry-1358ed_subj ;
  lemon:objOfProp   :lexicalEntry-1358ed_obj ;
  lemon:condition   :lexicalEntry-1358_sense_1_condition .
:lexicalEntry-1358ed_obj lemon:marker :form_lexicalEntry-1358_preposition .

## Prepositions ##
:form_lexicalEntry-1358_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

