@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-3675 ;
  lemon:entry    :lexicalEntry-3675ed ;
  lemon:entry    :form_lexicalEntry-3675_preposition .

:to_lexicalEntry-3675 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-3675 ;
  lemon:otherForm      :form_lexicalEntry-3675s ;
  lemon:otherForm      :form_lexicalEntry-3675ed ;
  lemon:otherForm      :form_lexicalEntry-3675perfect ;
  lemon:sense          :lexicalEntry-3675_sense_1 ;
  lemon:synBehavior    :lexicalEntry-3675_frame_transitive .

:form_lexicalEntry-3675 a         lemon:Form ;
  lemon:writtenRep     "aired on"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-3675s a    lemon:Form ;
  lemon:writtenRep "aired on"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-3675ed a   lemon:Form ;
  lemon:writtenRep "aired on"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-3675perfect a   lemon:Form ;
  lemon:writtenRep "aired on"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-3675_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-3675_subj ;
  lexinfo:directObject     :lexicalEntry-3675_obj .

:lexicalEntry-3675_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3675_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/completionDate> ;
  lemon:subjOfProp  :lexicalEntry-3675_obj ;
  lemon:objOfProp   :lexicalEntry-3675_subj ;
  lemon:condition   :lexicalEntry-3675_sense_1_condition .


:lexicalEntry-3675_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Ship> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .

:lexicalEntry-3675ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-3675ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-3675ed_by ;
  lemon:synBehavior    :lexicalEntry-3675ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-3675ed_ontomap .

:form_lexicalEntry-3675ed_canonical a lemon:Form ;
  lemon:writtenRep         "aired on"@en .

:form_lexicalEntry-3675ed_by a    lemon:Form ;
  lemon:writtenRep     "aired on"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-3675ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-3675ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-3675ed_obj .

:lexicalEntry-3675ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3675ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/completionDate> ;
  lemon:subjOfProp  :lexicalEntry-3675ed_subj ;
  lemon:objOfProp   :lexicalEntry-3675ed_obj ;
  lemon:condition   :lexicalEntry-3675_sense_1_condition .
:lexicalEntry-3675ed_obj lemon:marker :form_lexicalEntry-3675_preposition .

## Prepositions ##
:form_lexicalEntry-3675_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

