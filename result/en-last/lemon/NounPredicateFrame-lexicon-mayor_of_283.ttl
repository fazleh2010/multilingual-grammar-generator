@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :mayor_of_283_sense_1_res ;
  lemon:entry    :mayor_of_283 .

:mayor_of_283 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :mayor_of_283_form ;
  lemon:otherForm  :mayor_of_283_singular ;
  lemon:otherForm  :mayor_of_283_plural ;
  lemon:sense          :mayor_of_283_sense_1 ;
  lemon:synBehavior    :mayor_of_283_predFrame .

:mayor_of_283_form
 lemon:writtenRep "Mayor"@en .

:mayor_of_283_singular a    lemon:Form ;
  lemon:writtenRep "Mayor"@en ;
  lexinfo:number   lexinfo:singular .

:mayor_of_283_plural a   lemon:Form ;
  lemon:writtenRep "Mayors"@en ;
  lexinfo:number    lexinfo:plural .

:mayor_of_283_sense_1 a  lemon:LexicalSense ;
  lemon:reference :mayor_of_283_sense_1_res ;
  lemon:isA       :mayor_of_283_PredSynArg .

:mayor_of_283_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Mayor> .
