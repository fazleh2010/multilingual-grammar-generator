@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :monument_of_70 ;
  lemon:entry    :monument_of_70_form_preposition .

:monument_of_70 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :monument_of_70_form ;
  lemon:otherForm  :monument_of_70_singular ;
  lemon:otherForm  :monument_of_70_plural ;
  lemon:sense          :monument_of_70_sense_1 ;
  lemon:synBehavior    :monument_of_70_nounpp .

:monument_of_70_form a lemon:Form ;

:monument_of_70_singular a    lemon:Form ;
  lemon:writtenRep "Monument"@en ;
  lexinfo:number   lexinfo:singular .

:monument_of_70_plural a   lemon:Form ;
  lemon:writtenRep "Monuments"@en ;
  lexinfo:number    lexinfo:plural .

:monument_of_70_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_monument_of_70 ;
  lexinfo:prepositionalAdjunct :arg2_monument_of_70 .

:monument_of_70_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :monument_of_70_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_monument_of_70 ;
  lemon:objOfProp           :arg1_monument_of_70 ;
  lemon:condition           :monument_of_70_sense_1_condition .

:monument_of_70_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Monument> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Monument> .

:arg2_monument_of_70 lemon:marker :monument_of_70_form_preposition .
## Prepositions ##
:monument_of_70_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

