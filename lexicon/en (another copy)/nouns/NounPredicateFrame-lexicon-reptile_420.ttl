@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :reptile_420_sense_1_res ;
  lemon:entry    :reptile_420 .

:reptile_420 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :reptile_420_form ;
  lemon:otherForm  :reptile_420_singular ;
  lemon:otherForm  :reptile_420_plural ;
  lemon:sense          :reptile_420_sense_1 ;
  lemon:synBehavior    :reptile_420_predFrame .

:reptile_420_form
 lemon:writtenRep "reptile"@en .

:reptile_420_singular a    lemon:Form ;
  lemon:writtenRep "reptile"@en ;
  lexinfo:number   lexinfo:singular .

:reptile_420_plural a   lemon:Form ;
  lemon:writtenRep "reptiles"@en ;
  lexinfo:number    lexinfo:plural .

:reptile_420_sense_1 a  lemon:LexicalSense ;
  lemon:reference :reptile_420_sense_1_res ;
  lemon:isA       :reptile_420_PredSynArg .

:reptile_420_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Reptile> .