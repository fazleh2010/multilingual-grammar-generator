@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-2114 ;
  lemon:entry    :lexicalEntry-2114ed ;
  lemon:entry    :form_lexicalEntry-2114_preposition .

:to_lexicalEntry-2114 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-2114 ;
  lemon:otherForm      :form_lexicalEntry-2114s ;
  lemon:otherForm      :form_lexicalEntry-2114ed ;
  lemon:otherForm      :form_lexicalEntry-2114perfect ;
  lemon:sense          :lexicalEntry-2114_sense_1 ;
  lemon:synBehavior    :lexicalEntry-2114_frame_transitive .

:form_lexicalEntry-2114 a         lemon:Form ;
  lemon:writtenRep     "direct"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-2114s a    lemon:Form ;
  lemon:writtenRep "directs"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-2114ed a   lemon:Form ;
  lemon:writtenRep "directed"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-2114perfect a   lemon:Form ;
  lemon:writtenRep "directed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-2114_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-2114_subj ;
  lexinfo:directObject     :lexicalEntry-2114_obj .

:lexicalEntry-2114_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2114_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/musicComposer> ;
  lemon:subjOfProp  :lexicalEntry-2114_obj ;
  lemon:objOfProp   :lexicalEntry-2114_subj ;
  lemon:condition   :lexicalEntry-2114_sense_1_condition .


:lexicalEntry-2114_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Film> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .

:lexicalEntry-2114ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-2114ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-2114ed_by ;
  lemon:synBehavior    :lexicalEntry-2114ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-2114ed_ontomap .

:form_lexicalEntry-2114ed_canonical a lemon:Form ;
  lemon:writtenRep         "direct"@en .

:form_lexicalEntry-2114ed_by a    lemon:Form ;
  lemon:writtenRep     "direct"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-2114ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-2114ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-2114ed_obj .

:lexicalEntry-2114ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-2114ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/musicComposer> ;
  lemon:subjOfProp  :lexicalEntry-2114ed_subj ;
  lemon:objOfProp   :lexicalEntry-2114ed_obj ;
  lemon:condition   :lexicalEntry-2114_sense_1_condition .
:lexicalEntry-2114ed_obj lemon:marker :form_lexicalEntry-2114_preposition .

## Prepositions ##
:form_lexicalEntry-2114_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

