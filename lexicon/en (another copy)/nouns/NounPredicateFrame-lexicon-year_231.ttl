@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :year_231_sense_1_res ;
  lemon:entry    :year_231 .

:year_231 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :year_231_form ;
  lemon:otherForm  :year_231_singular ;
  lemon:otherForm  :year_231_plural ;
  lemon:sense          :year_231_sense_1 ;
