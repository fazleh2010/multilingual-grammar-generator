@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :german_300_sense_1_res ;
  lemon:entry    :german_300 .

:german_300 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :german_300_lemma ;
  lemon:sense          :german_300_sense_1 ;
  lemon:synBehavior    :german_300_attrFrame, :german_300_predFrame .

:german_300_lemma lemon:writtenRep "swiss"@en .

:german_300_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :german_300_PredSynArg .

:german_300_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :german_300_AttrSynArg .

:german_300_sense_1 a  lemon:LexicalSense ;
  lemon:reference :german_300_sense_1_res ;
  lemon:isA       :german_300_AttrSynArg, :german_300_PredSynArg .

:german_300_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Germany> .
