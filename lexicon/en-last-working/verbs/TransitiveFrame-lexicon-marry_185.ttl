@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_marry_185 ;
  lemon:entry    :marry_185ed ;
  lemon:entry    :form_marry_185_preposition .

:to_marry_185 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_marry_185 ;
  lemon:otherForm      :form_marry_185s ;
  lemon:otherForm      :form_marry_185ed ;
  lemon:otherForm      :form_marry_185perfect ;
  lemon:sense          :marry_185_sense_1 ;
  lemon:synBehavior    :marry_185_frame_transitive .

:form_marry_185 a         lemon:Form ;
  lemon:writtenRep     "marry"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_marry_185s a    lemon:Form ;
  lemon:writtenRep "marries"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_marry_185ed a   lemon:Form ;
  lemon:writtenRep "married"@en ;
  lexinfo:tense    lexinfo:past .

:form_marry_185perfect a   lemon:Form ;
  lemon:writtenRep "married"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:marry_185_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :marry_185_subj ;
  lexinfo:directObject     :marry_185_obj .

:marry_185_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :marry_185_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/spouse> ;
  lemon:subjOfProp  :marry_185_obj ;
  lemon:objOfProp   :marry_185_subj ;
  lemon:condition   :marry_185_sense_1_condition .


:marry_185_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Person> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:marry_185_obj lemon:marker :form_marry_185_preposition .

## Prepositions ##
:form_marry_185_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "to"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

