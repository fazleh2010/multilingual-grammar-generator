@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :barbadian_940_sense_1_res ;
  lemon:entry    :barbadian_940 .

:barbadian_940 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :barbadian_940_lemma ;
  lemon:sense          :barbadian_940_sense_1 ;
  lemon:synBehavior    :barbadian_940_attrFrame, :barbadian_940_predFrame .

:barbadian_940_lemma lemon:writtenRep "barbadian"@en .

:barbadian_940_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :barbadian_940_PredSynArg .

:barbadian_940_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :barbadian_940_AttrSynArg .

:barbadian_940_sense_1 a  lemon:LexicalSense ;
  lemon:reference :barbadian_940_sense_1_res ;
  lemon:isA       :barbadian_940_AttrSynArg, :barbadian_940_PredSynArg .

:barbadian_940_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Barbados> .
