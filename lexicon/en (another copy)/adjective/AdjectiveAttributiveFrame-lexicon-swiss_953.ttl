@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :swiss_953_sense_1_res ;
  lemon:entry    :swiss_953 .

:swiss_953 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :swiss_953_lemma ;
  lemon:sense          :swiss_953_sense_1 ;
  lemon:synBehavior    :swiss_953_attrFrame, :swiss_953_predFrame .

:swiss_953_lemma lemon:writtenRep "swiss"@en .

:swiss_953_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :swiss_953_PredSynArg .

:swiss_953_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :swiss_953_AttrSynArg .

:swiss_953_sense_1 a  lemon:LexicalSense ;
  lemon:reference :swiss_953_sense_1_res ;
  lemon:isA       :swiss_953_AttrSynArg, :swiss_953_PredSynArg .

:swiss_953_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Switzerland> .
