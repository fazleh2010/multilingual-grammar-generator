@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :region_580_sense_1_res ;
  lemon:entry    :region_580 .

:region_580 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :region_580_form ;
  lemon:otherForm  :region_580_singular ;
  lemon:otherForm  :region_580_plural ;
  lemon:sense          :region_580_sense_1 ;
  lemon:synBehavior    :region_580_predFrame .

:region_580_form
 lemon:writtenRep "region"@en .

:region_580_singular a    lemon:Form ;
  lemon:writtenRep "region"@en ;
  lexinfo:number   lexinfo:singular .

:region_580_plural a   lemon:Form ;
  lemon:writtenRep "regions"@en ;
  lexinfo:number    lexinfo:plural .

:region_580_sense_1 a  lemon:LexicalSense ;
  lemon:reference :region_580_sense_1_res ;
  lemon:isA       :region_580_PredSynArg .

:region_580_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Region> .