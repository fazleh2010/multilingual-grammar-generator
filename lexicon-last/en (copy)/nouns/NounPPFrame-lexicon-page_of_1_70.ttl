@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :page_of_1_70 ;
  lemon:entry    :page_of_1_70_form_preposition .

:page_of_1_70 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :page_of_1_70_form ;
  lemon:otherForm  :page_of_1_70_singular ;
  lemon:otherForm  :page_of_1_70_plural ;
  lemon:sense          :page_of_1_70_sense_1 ;
  lemon:synBehavior    :page_of_1_70_nounpp .

:page_of_1_70_form a lemon:Form ;
  lemon:writtenRep "page"@en .

:page_of_1_70_singular a    lemon:Form ;
  lemon:writtenRep "page"@en ;
  lexinfo:number   lexinfo:singular .

:page_of_1_70_plural a   lemon:Form ;
  lemon:writtenRep "pages"@en ;
  lexinfo:number    lexinfo:plural .

:page_of_1_70_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_page_of_1_70 ;
  lexinfo:prepositionalAdjunct :arg2_page_of_1_70 .

:page_of_1_70_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :page_of_1_70_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/numberOfPages> ;
  lemon:subjOfProp          :arg2_page_of_1_70 ;
  lemon:objOfProp           :arg1_page_of_1_70 ;
  lemon:condition           :page_of_1_70_sense_1_condition .

:page_of_1_70_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#positiveInteger> .

:arg2_page_of_1_70 lemon:marker :page_of_1_70_form_preposition .
## Prepositions ##
:page_of_1_70_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

