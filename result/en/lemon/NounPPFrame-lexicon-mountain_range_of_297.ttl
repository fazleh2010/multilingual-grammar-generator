@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :mountain_range_of_297 ;
  lemon:entry    :mountain_range_of_297_form_preposition .

:mountain_range_of_297 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :mountain_range_of_297_form ;
  lemon:otherForm  :mountain_range_of_297_singular ;
  lemon:otherForm  :mountain_range_of_297_plural ;
  lemon:sense          :mountain_range_of_297_sense_1 ;
  lemon:synBehavior    :mountain_range_of_297_nounpp .

:mountain_range_of_297_form a lemon:Form ;

:mountain_range_of_297_singular a    lemon:Form ;
  lemon:writtenRep "mountain range"@en ;
  lexinfo:number   lexinfo:singular .

:mountain_range_of_297_plural a   lemon:Form ;
  lemon:writtenRep "mountain ranges"@en ;
  lexinfo:number    lexinfo:plural .

:mountain_range_of_297_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_mountain_range_of_297 ;
  lexinfo:prepositionalAdjunct :arg2_mountain_range_of_297 .

:mountain_range_of_297_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :mountain_range_of_297_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_mountain_range_of_297 ;
  lemon:objOfProp           :arg1_mountain_range_of_297 ;
  lemon:condition           :mountain_range_of_297_sense_1_condition .

:mountain_range_of_297_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/MountainRange> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/MountainRange> .

:arg2_mountain_range_of_297 lemon:marker :mountain_range_of_297_form_preposition .
## Prepositions ##
:mountain_range_of_297_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

