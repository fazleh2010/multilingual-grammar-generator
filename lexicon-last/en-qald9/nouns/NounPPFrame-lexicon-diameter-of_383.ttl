@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :diameter-of_383 ;
  lemon:entry    :diameter-of_383_form_preposition .

:diameter-of_383 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :diameter-of_383_form ;
  lemon:otherForm  :diameter-of_383_singular ;
  lemon:otherForm  :diameter-of_383_plural ;
  lemon:sense          :diameter-of_383_sense_1 ;
  lemon:synBehavior    :diameter-of_383_nounpp .

:diameter-of_383_form a lemon:Form ;
  lemon:writtenRep "diameter"@en .

:diameter-of_383_singular a    lemon:Form ;
  lemon:writtenRep "diameter"@en ;
  lexinfo:number   lexinfo:singular .

:diameter-of_383_plural a   lemon:Form ;
  lemon:writtenRep "diameters"@en ;
  lexinfo:number    lexinfo:plural .

:diameter-of_383_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_diameter-of_383 ;
  lexinfo:prepositionalAdjunct :arg2_diameter-of_383 .

:diameter-of_383_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :diameter-of_383_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/diameter> ;
  lemon:subjOfProp          :arg1_diameter-of_383 ;
  lemon:objOfProp           :arg2_diameter-of_383 ;
  lemon:condition           :diameter-of_383_sense_1_condition .

:diameter-of_383_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Device> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2_diameter-of_383 lemon:marker :diameter-of_383_form_preposition .
## Prepositions ##
:diameter-of_383_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

