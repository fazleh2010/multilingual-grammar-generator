@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :muscle_of_162 ;
  lemon:entry    :muscle_of_162_form_preposition .

:muscle_of_162 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :muscle_of_162_form ;
  lemon:otherForm  :muscle_of_162_singular ;
  lemon:otherForm  :muscle_of_162_plural ;
  lemon:sense          :muscle_of_162_sense_1 ;
  lemon:synBehavior    :muscle_of_162_nounpp .

:muscle_of_162_form a lemon:Form ;
  lemon:writtenRep "Muscle"@en .

:muscle_of_162_singular a    lemon:Form ;
  lemon:writtenRep "Muscle"@en ;
  lexinfo:number   lexinfo:singular .

:muscle_of_162_plural a   lemon:Form ;
  lemon:writtenRep "Muscles"@en ;
  lexinfo:number    lexinfo:plural .

:muscle_of_162_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_muscle_of_162 ;
  lexinfo:prepositionalAdjunct :arg2_muscle_of_162 .

:muscle_of_162_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :muscle_of_162_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_muscle_of_162 ;
  lemon:objOfProp           :arg1_muscle_of_162 ;
  lemon:condition           :muscle_of_162_sense_1_condition .

:muscle_of_162_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Muscle> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Muscle> .

:arg2_muscle_of_162 lemon:marker :muscle_of_162_form_preposition .
## Prepositions ##
:muscle_of_162_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

