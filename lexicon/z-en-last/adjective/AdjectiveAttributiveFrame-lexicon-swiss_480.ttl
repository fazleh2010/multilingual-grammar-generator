@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :swiss_480_sense_1_res ;
  lemon:entry    :swiss_480 .

:swiss_480 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :swiss_480_lemma ;
  lemon:sense          :swiss_480_sense_1 ;
  lemon:synBehavior    :swiss_480_attrFrame, :swiss_480_predFrame .

:swiss_480_lemma lemon:writtenRep "swiss"@en .

:swiss_480_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :swiss_480_PredSynArg .

:swiss_480_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :swiss_480_AttrSynArg .

:swiss_480_sense_1 a  lemon:LexicalSense ;
  lemon:reference :swiss_480_sense_1_res ;
  lemon:isA       :swiss_480_AttrSynArg, :swiss_480_PredSynArg .

:swiss_480_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Switzerland> .
