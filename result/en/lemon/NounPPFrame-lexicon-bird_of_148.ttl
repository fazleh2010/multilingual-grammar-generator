@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :bird_of_148 ;
  lemon:entry    :bird_of_148_form_preposition .

:bird_of_148 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :bird_of_148_form ;
  lemon:otherForm  :bird_of_148_singular ;
  lemon:otherForm  :bird_of_148_plural ;
  lemon:sense          :bird_of_148_sense_1 ;
  lemon:synBehavior    :bird_of_148_nounpp .

:bird_of_148_form a lemon:Form ;

:bird_of_148_singular a    lemon:Form ;
  lemon:writtenRep "Bird"@en ;
  lexinfo:number   lexinfo:singular .

:bird_of_148_plural a   lemon:Form ;
  lemon:writtenRep "Birds"@en ;
  lexinfo:number    lexinfo:plural .

:bird_of_148_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_bird_of_148 ;
  lexinfo:prepositionalAdjunct :arg2_bird_of_148 .

:bird_of_148_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :bird_of_148_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_bird_of_148 ;
  lemon:objOfProp           :arg1_bird_of_148 ;
  lemon:condition           :bird_of_148_sense_1_condition .

:bird_of_148_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Bird> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Bird> .

:arg2_bird_of_148 lemon:marker :bird_of_148_form_preposition .
## Prepositions ##
:bird_of_148_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

