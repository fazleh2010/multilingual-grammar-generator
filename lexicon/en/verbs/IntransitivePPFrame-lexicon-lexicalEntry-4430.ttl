@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-4430 ;
  lemon:entry    :form_lexicalEntry-4430_preposition .

:lexicalEntry-4430 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-4430 ;
  lemon:otherForm      :form_lexicalEntry-4430_past ;
  lemon:sense          :lexicalEntry-4430_sense_1 ;
  lemon:synBehavior    :lexicalEntry-4430_frame .

:form_lexicalEntry-4430 a           lemon:Form ;
  lemon:writtenRep     "publish"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-4430 a      lemon:Form ;
  lemon:writtenRep "publishes"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-4430_past a lemon:Form ;
  lemon:writtenRep  "published"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-4430perfect a   lemon:Form ;
  lemon:writtenRep "published"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-4430_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-4430_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-4430_obj .

:lexicalEntry-4430_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-4430_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/abbreviation> ;
  lemon:subjOfProp  :lexicalEntry-4430_obj ;
  lemon:objOfProp   :lexicalEntry-4430_subj ;
  lemon:condition   :lexicalEntry-4430_sense_1_condition .

:lexicalEntry-4430_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://www.w3.org/2002/07/owl#Thing> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#string> .


:lexicalEntry-4430_obj lemon:marker :form_lexicalEntry-4430_preposition .

## Prepositions ##
:form_lexicalEntry-4430_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

