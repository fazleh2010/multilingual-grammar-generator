@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-3037 ;
  lemon:entry    :form_lexicalEntry-3037_preposition .

:lexicalEntry-3037 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-3037 ;
  lemon:otherForm      :form_lexicalEntry-3037_past ;
  lemon:sense          :lexicalEntry-3037_sense_1 ;
  lemon:synBehavior    :lexicalEntry-3037_frame .

:form_lexicalEntry-3037 a           lemon:Form ;
  lemon:writtenRep     "produce"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-3037 a      lemon:Form ;
  lemon:writtenRep "produces"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-3037_past a lemon:Form ;
  lemon:writtenRep  "produced"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-3037perfect a   lemon:Form ;
  lemon:writtenRep "produced"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-3037_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-3037_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-3037_obj .

:lexicalEntry-3037_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3037_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/wineProduced> ;
  lemon:subjOfProp  :lexicalEntry-3037_obj ;
  lemon:objOfProp   :lexicalEntry-3037_subj ;
  lemon:condition   :lexicalEntry-3037_sense_1_condition .

:lexicalEntry-3037_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Location> .


:lexicalEntry-3037_obj lemon:marker :form_lexicalEntry-3037_preposition .

## Prepositions ##
:form_lexicalEntry-3037_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

