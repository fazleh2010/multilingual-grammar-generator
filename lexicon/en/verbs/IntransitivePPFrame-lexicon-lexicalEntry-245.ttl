@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-245 ;
  lemon:entry    :form_lexicalEntry-245_preposition .

:lexicalEntry-245 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-245 ;
  lemon:otherForm      :form_lexicalEntry-245_past ;
  lemon:sense          :lexicalEntry-245_sense_1 ;
  lemon:synBehavior    :lexicalEntry-245_frame .

:form_lexicalEntry-245 a           lemon:Form ;
  lemon:writtenRep     "publish"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-245 a      lemon:Form ;
  lemon:writtenRep "publishes"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-245_past a lemon:Form ;
  lemon:writtenRep  "published"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-245perfect a   lemon:Form ;
  lemon:writtenRep "published"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-245_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-245_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-245_obj .

:lexicalEntry-245_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-245_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/creator> ;
  lemon:subjOfProp  :lexicalEntry-245_obj ;
  lemon:objOfProp   :lexicalEntry-245_subj ;
  lemon:condition   :lexicalEntry-245_sense_1_condition .

:lexicalEntry-245_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Agent> .


:lexicalEntry-245_obj lemon:marker :form_lexicalEntry-245_preposition .

## Prepositions ##
:form_lexicalEntry-245_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

