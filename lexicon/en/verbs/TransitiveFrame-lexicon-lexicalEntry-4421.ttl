@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-4421 ;
  lemon:entry    :lexicalEntry-4421ed ;
  lemon:entry    :form_lexicalEntry-4421_preposition .

:to_lexicalEntry-4421 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-4421 ;
  lemon:otherForm      :form_lexicalEntry-4421s ;
  lemon:otherForm      :form_lexicalEntry-4421ed ;
  lemon:otherForm      :form_lexicalEntry-4421perfect ;
  lemon:sense          :lexicalEntry-4421_sense_1 ;
  lemon:synBehavior    :lexicalEntry-4421_frame_transitive .

:form_lexicalEntry-4421 a         lemon:Form ;
  lemon:writtenRep     "design"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-4421s a    lemon:Form ;
  lemon:writtenRep "designs"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-4421ed a   lemon:Form ;
  lemon:writtenRep "designed"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-4421perfect a   lemon:Form ;
  lemon:writtenRep "designed"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-4421_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-4421_subj ;
  lexinfo:directObject     :lexicalEntry-4421_obj .

:lexicalEntry-4421_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-4421_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/architect> ;
  lemon:subjOfProp  :lexicalEntry-4421_obj ;
  lemon:objOfProp   :lexicalEntry-4421_subj ;
  lemon:condition   :lexicalEntry-4421_sense_1_condition .


:lexicalEntry-4421_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/ArchitecturalStructure> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Architect> .

:lexicalEntry-4421ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-4421ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-4421ed_by ;
  lemon:synBehavior    :lexicalEntry-4421ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-4421ed_ontomap .

:form_lexicalEntry-4421ed_canonical a lemon:Form ;
  lemon:writtenRep         "design"@en .

:form_lexicalEntry-4421ed_by a    lemon:Form ;
  lemon:writtenRep     "design"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-4421ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-4421ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-4421ed_obj .

:lexicalEntry-4421ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-4421ed_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/architect> ;
  lemon:subjOfProp  :lexicalEntry-4421ed_subj ;
  lemon:objOfProp   :lexicalEntry-4421ed_obj ;
  lemon:condition   :lexicalEntry-4421_sense_1_condition .
:lexicalEntry-4421ed_obj lemon:marker :form_lexicalEntry-4421_preposition .

## Prepositions ##
:form_lexicalEntry-4421_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

