@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-366 ;
  lemon:entry    :form_lexicalEntry-366_preposition .

:lexicalEntry-366 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-366 ;
  lemon:otherForm      :form_lexicalEntry-366_past ;
  lemon:sense          :lexicalEntry-366_sense_1 ;
  lemon:synBehavior    :lexicalEntry-366_frame .

:form_lexicalEntry-366 a           lemon:Form ;
  lemon:writtenRep     "produce"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_lexicalEntry-366 a      lemon:Form ;
  lemon:writtenRep "produces"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_lexicalEntry-366_past a lemon:Form ;
  lemon:writtenRep  "produced"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_lexicalEntry-366perfect a   lemon:Form ;
  lemon:writtenRep "produced"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-366_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :lexicalEntry-366_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-366_obj .

:lexicalEntry-366_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-366_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/date> ;
  lemon:subjOfProp  :lexicalEntry-366_obj ;
  lemon:objOfProp   :lexicalEntry-366_subj ;
  lemon:condition   :lexicalEntry-366_sense_1_condition .

:lexicalEntry-366_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Event> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:lexicalEntry-366_obj lemon:marker :form_lexicalEntry-366_preposition .

## Prepositions ##
:form_lexicalEntry-366_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

