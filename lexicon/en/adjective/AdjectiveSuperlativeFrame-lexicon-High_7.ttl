@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix oils:     <http://lemon-model.net/oils#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :High_7 ;
  lemon:entry    :form_High_7_preposition ;
  lemon:entry    :High_7_res .

:High_7 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_High_7 ;
  lemon:otherForm      :form_High_7_comperative ;
  lemon:otherForm      :form_High_7_superlative ;
  lemon:sense          :High_7_sense_1 ;
  lemon:synBehavior    :High_7_predFrame .

:form_High_7 lemon:writtenRep "high"@en .

:form_High_7_comperative lemon:writtenRep "higher"@en .

:form_High_7_superlative lemon:writtenRep "highest"@en .

:High_7_predFrame a        lexinfo:AdjectiveSuperlativeFrame ;
  lexinfo:copulativeSubject :High_7_PredSynArg .

:High_7_sense_1 a  lemon:LexicalSense ;
  lemon:reference :High_7_res ;
  lemon:isA       :High_7_PredSynArg ;
  lemon:condition :High_7_sense_1_condition .

:High_7_res a   oils:CovariantScalar ;
  oils:boundTo  <http://dbpedia.org/ontology/elevation> ;
  oils:degree   <http://lemon-model.net/oils/high> .

:High_7_sense_1_condition a lemon:condition ;
  lemon:propertyDomain   <http://www.w3.org/2002/07/owl#Thing> ;
  lemon:propertyRange    <http://dbpedia.org/ontology/Mountain> .## Prepositions ##
:form_High_7_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

