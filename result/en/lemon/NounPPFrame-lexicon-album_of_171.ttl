@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :album_of_171 ;
  lemon:entry    :album_of_171_form_preposition .

:album_of_171 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :album_of_171_form ;
  lemon:otherForm  :album_of_171_singular ;
  lemon:otherForm  :album_of_171_plural ;
  lemon:sense          :album_of_171_sense_1 ;
  lemon:synBehavior    :album_of_171_nounpp .

:album_of_171_form a lemon:Form ;

:album_of_171_singular a    lemon:Form ;
  lemon:writtenRep "album"@en ;
  lexinfo:number   lexinfo:singular .

:album_of_171_plural a   lemon:Form ;
  lemon:writtenRep "albums"@en ;
  lexinfo:number    lexinfo:plural .

:album_of_171_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_album_of_171 ;
  lexinfo:prepositionalAdjunct :arg2_album_of_171 .

:album_of_171_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :album_of_171_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_album_of_171 ;
  lemon:objOfProp           :arg1_album_of_171 ;
  lemon:condition           :album_of_171_sense_1_condition .

:album_of_171_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Album> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Album> .

:arg2_album_of_171 lemon:marker :album_of_171_form_preposition .
## Prepositions ##
:album_of_171_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

