@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :surname_331_sense_1_res ;
  lemon:entry    :surname_331 .

:surname_331 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :surname_331_form ;
  lemon:otherForm  :surname_331_singular ;
  lemon:otherForm  :surname_331_plural ;
  lemon:sense          :surname_331_sense_1 ;
  lemon:synBehavior    :surname_331_predFrame .

:surname_331_form
 lemon:writtenRep "surname"@en .

:surname_331_singular a    lemon:Form ;
  lemon:writtenRep "surname"@en ;
  lexinfo:number   lexinfo:singular .

:surname_331_plural a   lemon:Form ;
  lemon:writtenRep "surnames"@en ;
  lexinfo:number    lexinfo:plural .

:surname_331_sense_1 a  lemon:LexicalSense ;
  lemon:reference :surname_331_sense_1_res ;
  lemon:isA       :surname_331_PredSynArg .

:surname_331_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Surname> .