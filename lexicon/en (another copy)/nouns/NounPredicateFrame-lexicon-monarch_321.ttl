@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :monarch_321_sense_1_res ;
  lemon:entry    :monarch_321 .

:monarch_321 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :monarch_321_form ;
  lemon:otherForm  :monarch_321_singular ;
  lemon:otherForm  :monarch_321_plural ;
  lemon:sense          :monarch_321_sense_1 ;
  lemon:synBehavior    :monarch_321_predFrame .

:monarch_321_form
 lemon:writtenRep "monarch"@en .

:monarch_321_singular a    lemon:Form ;
  lemon:writtenRep "monarch"@en ;
  lexinfo:number   lexinfo:singular .

:monarch_321_plural a   lemon:Form ;
  lemon:writtenRep "monarchs"@en ;
  lexinfo:number    lexinfo:plural .

:monarch_321_sense_1 a  lemon:LexicalSense ;
  lemon:reference :monarch_321_sense_1_res ;
  lemon:isA       :monarch_321_PredSynArg .

:monarch_321_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Monarch> .