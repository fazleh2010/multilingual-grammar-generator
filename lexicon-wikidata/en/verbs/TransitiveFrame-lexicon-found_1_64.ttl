@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_found_1_64 ;
  lemon:entry    :found_1_64ed ;
  lemon:entry    :form_found_1_64_preposition .

:to_found_1_64 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_found_1_64 ;
  lemon:otherForm      :form_found_1_64s ;
  lemon:otherForm      :form_found_1_64ed ;
  lemon:otherForm      :form_found_1_64perfect ;
  lemon:sense          :found_1_64_sense_1 ;
  lemon:synBehavior    :found_1_64_frame_transitive .

:form_found_1_64 a         lemon:Form ;
  lemon:writtenRep     "found"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_found_1_64s a    lemon:Form ;
  lemon:writtenRep "found"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_found_1_64ed a   lemon:Form ;
  lemon:writtenRep "founded"@en ;
  lexinfo:tense    lexinfo:past .

:form_found_1_64perfect a   lemon:Form ;
  lemon:writtenRep "founded"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:found_1_64_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :found_1_64_subj ;
  lexinfo:directObject     :found_1_64_obj .

:found_1_64_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :found_1_64_sense_1 ;
  lemon:reference   <wd:P112> ;
  lemon:subjOfProp  :found_1_64_obj ;
  lemon:objOfProp   :found_1_64_subj ;
  lemon:condition   :found_1_64_sense_1_condition .


:found_1_64_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <wd:Q476028> ;
  lemon:propertyRange  <wd:Q19088> .


:found_1_64_obj lemon:marker :form_found_1_64_preposition .

## Prepositions ##
:form_found_1_64_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

