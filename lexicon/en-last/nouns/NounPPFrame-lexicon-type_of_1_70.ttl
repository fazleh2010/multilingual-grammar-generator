@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :type_of_1_70 ;
  lemon:entry    :type_of_1_70_form_preposition .

:type_of_1_70 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :type_of_1_70_form ;
  lemon:otherForm  :type_of_1_70_singular ;
  lemon:otherForm  :type_of_1_70_plural ;
  lemon:sense          :type_of_1_70_sense_1 ;
  lemon:synBehavior    :type_of_1_70_nounpp .

:type_of_1_70_form a lemon:Form ;
  lemon:writtenRep "type"@en .

:type_of_1_70_singular a    lemon:Form ;
  lemon:writtenRep "type"@en ;
  lexinfo:number   lexinfo:singular .

:type_of_1_70_plural a   lemon:Form ;
  lemon:writtenRep "types"@en ;
  lexinfo:number    lexinfo:plural .

:type_of_1_70_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_type_of_1_70 ;
  lexinfo:prepositionalAdjunct :arg2_type_of_1_70 .

:type_of_1_70_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :type_of_1_70_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/class> ;
  lemon:subjOfProp          :arg2_type_of_1_70 ;
  lemon:objOfProp           :arg1_type_of_1_70 ;
  lemon:condition           :type_of_1_70_sense_1_condition .

:type_of_1_70_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Insect> ;
  lemon:propertyRange   <http://www.w3.org/2002/07/owl#Thing> .

:arg2_type_of_1_70 lemon:marker :type_of_1_70_form_preposition .
## Prepositions ##
:type_of_1_70_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

