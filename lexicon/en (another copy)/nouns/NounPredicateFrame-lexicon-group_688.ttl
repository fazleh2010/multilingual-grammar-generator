@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :group_688_sense_1_res ;
  lemon:entry    :group_688 .

:group_688 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :group_688_form ;
  lemon:otherForm  :group_688_singular ;
  lemon:otherForm  :group_688_plural ;
  lemon:sense          :group_688_sense_1 ;
  lemon:synBehavior    :group_688_predFrame .

:group_688_form
 lemon:writtenRep "group"@en .

:group_688_singular a    lemon:Form ;
  lemon:writtenRep "group"@en ;
  lexinfo:number   lexinfo:singular .

:group_688_plural a   lemon:Form ;
  lemon:writtenRep "groups"@en ;
  lexinfo:number    lexinfo:plural .

:group_688_sense_1 a  lemon:LexicalSense ;
  lemon:reference :group_688_sense_1_res ;
  lemon:isA       :group_688_PredSynArg .

:group_688_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Group> .