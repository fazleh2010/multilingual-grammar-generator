@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :legislature_256_sense_1_res ;
  lemon:entry    :legislature_256 .

:legislature_256 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :legislature_256_form ;
  lemon:otherForm  :legislature_256_singular ;
  lemon:otherForm  :legislature_256_plural ;
  lemon:sense          :legislature_256_sense_1 ;
  lemon:synBehavior    :legislature_256_predFrame .

:legislature_256_form
 lemon:writtenRep "legislature"@en .

:legislature_256_singular a    lemon:Form ;
  lemon:writtenRep "legislature"@en ;
  lexinfo:number   lexinfo:singular .

:legislature_256_plural a   lemon:Form ;
  lemon:writtenRep "legislatures"@en ;
  lexinfo:number    lexinfo:plural .

:legislature_256_sense_1 a  lemon:LexicalSense ;
  lemon:reference :legislature_256_sense_1_res ;
  lemon:isA       :legislature_256_PredSynArg .

:legislature_256_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Legislature> .