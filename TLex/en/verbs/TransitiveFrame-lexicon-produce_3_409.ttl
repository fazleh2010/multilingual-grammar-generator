@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_produce_3_409 ;
  lemon:entry    :produce_3_409ed ;
  lemon:entry    :form_produce_3_409_preposition .

:to_produce_3_409 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_produce_3_409 ;
  lemon:otherForm      :form_produce_3_409s ;
  lemon:otherForm      :form_produce_3_409ed ;
  lemon:otherForm      :form_produce_3_409perfect ;
  lemon:sense          :produce_3_409_sense_1 ;
  lemon:synBehavior    :produce_3_409_frame_transitive .

:form_produce_3_409 a         lemon:Form ;
  lemon:writtenRep     "produce"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_produce_3_409s a    lemon:Form ;
  lemon:writtenRep "produces"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_produce_3_409ed a   lemon:Form ;
  lemon:writtenRep "produced"@en ;
  lexinfo:tense    lexinfo:past .

:form_produce_3_409perfect a   lemon:Form ;
  lemon:writtenRep "produced"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:produce_3_409_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :produce_3_409_subj ;
  lexinfo:directObject     :produce_3_409_obj .

:produce_3_409_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :produce_3_409_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/executiveProducer> ;
  lemon:subjOfProp  :produce_3_409_obj ;
  lemon:objOfProp   :produce_3_409_subj ;
  lemon:condition   :produce_3_409_sense_1_condition .


:produce_3_409_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/TelevisionShow> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:produce_3_409_obj lemon:marker :form_produce_3_409_preposition .

## Prepositions ##
:form_produce_3_409_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

