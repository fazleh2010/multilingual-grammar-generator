@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :tv-show-with_402 ;
  lemon:entry    :tv-show-with_402_form_preposition .

:tv-show-with_402 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :tv-show-with_402_form ;
  lemon:otherForm  :tv-show-with_402_singular ;
  lemon:otherForm  :tv-show-with_402_plural ;
  lemon:sense          :tv-show-with_402_sense_1 ;
  lemon:synBehavior    :tv-show-with_402_nounpp .

:tv-show-with_402_form a lemon:Form ;
  lemon:writtenRep "TV show"@en .

:tv-show-with_402_singular a    lemon:Form ;
  lemon:writtenRep "TV show"@en ;
  lexinfo:number   lexinfo:singular .

:tv-show-with_402_plural a   lemon:Form ;
  lemon:writtenRep "TV show"@en ;
  lexinfo:number    lexinfo:plural .

:tv-show-with_402_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_tv-show-with_402 ;
  lexinfo:prepositionalAdjunct :arg2_tv-show-with_402 .

:tv-show-with_402_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :tv-show-with_402_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/starring> ;
  lemon:subjOfProp          :arg2_tv-show-with_402 ;
  lemon:objOfProp           :arg1_tv-show-with_402 ;
  lemon:condition           :tv-show-with_402_sense_1_condition .

:tv-show-with_402_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Actor> .

:arg2_tv-show-with_402 lemon:marker :tv-show-with_402_form_preposition .
## Prepositions ##
:tv-show-with_402_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "with"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

