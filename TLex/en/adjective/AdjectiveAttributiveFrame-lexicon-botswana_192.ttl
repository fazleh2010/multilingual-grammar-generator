@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :botswana_192_sense_1_res ;
  lemon:entry    :botswana_192 .

:botswana_192 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :botswana_192_lemma ;
  lemon:sense          :botswana_192_sense_1 ;
  lemon:synBehavior    :botswana_192_attrFrame, :botswana_192_predFrame .

:botswana_192_lemma lemon:writtenRep "botswana"@en .

:botswana_192_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :botswana_192_PredSynArg .

:botswana_192_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :botswana_192_AttrSynArg .

:botswana_192_sense_1 a  lemon:LexicalSense ;
  lemon:reference :botswana_192_sense_1_res ;
  lemon:isA       :botswana_192_AttrSynArg, :botswana_192_PredSynArg .

:botswana_192_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Botswana> .
