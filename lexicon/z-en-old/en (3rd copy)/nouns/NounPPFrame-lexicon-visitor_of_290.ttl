@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :visitor_of_290 ;
  lemon:entry    :visitor_of_290_form_preposition .

:visitor_of_290 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :visitor_of_290_form ;
  lemon:otherForm  :visitor_of_290_singular ;
  lemon:otherForm  :visitor_of_290_plural ;
  lemon:sense          :visitor_of_290_sense_1 ;
  lemon:synBehavior    :visitor_of_290_nounpp .

:visitor_of_290_form a lemon:Form ;
  lemon:writtenRep "visitor"@en .

:visitor_of_290_singular a    lemon:Form ;
  lemon:writtenRep "visitor"@en ;
  lexinfo:number   lexinfo:singular .

:visitor_of_290_plural a   lemon:Form ;
  lemon:writtenRep "visitors"@en ;
  lexinfo:number    lexinfo:plural .

:visitor_of_290_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :null ;
  lexinfo:prepositionalAdjunct :null .

:visitor_of_290_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :visitor_of_290_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/numberOfVisitors> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :visitor_of_290_sense_1_condition .

:visitor_of_290_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Museum> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#nonNegativeInteger> .

:null lemon:marker :visitor_of_290_form_preposition .
## Prepositions ##
:visitor_of_290_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

