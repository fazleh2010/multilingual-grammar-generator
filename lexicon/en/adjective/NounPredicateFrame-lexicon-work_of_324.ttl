@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :work_of_324_sense_1_res ;
  lemon:entry    :work_of_324_sense_2_res ;
  lemon:entry    :work_of_324 .

:work_of_324 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :work_of_324_form ;
  lemon:otherForm  :work_of_324_singular ;
  lemon:otherForm  :work_of_324_plural ;
  lemon:sense          :work_of_324_sense_1 ;
  lemon:sense          :work_of_324_sense_2 ;
  lemon:synBehavior    :work_of_324_predFrame .

:work_of_324_form
 lemon:writtenRep "work"@en .

:work_of_324_singular a    lemon:Form ;
  lemon:writtenRep "work"@en ;
  lexinfo:number   lexinfo:singular .

:work_of_324_plural a   lemon:Form ;
  lemon:writtenRep "works"@en ;
  lexinfo:number    lexinfo:plural .

:work_of_324_sense_1 a  lemon:LexicalSense ;
  lemon:reference :work_of_324_sense_1_res ;
  lemon:isA       :work_of_324_PredSynArg .

:work_of_324_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Ship> .
:work_of_324_sense_2 a  lemon:LexicalSense ;
  lemon:reference :work_of_324_sense_2_res ;
  lemon:isA       :work_of_324_PredSynArg .

:work_of_324_sense_2_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Work> .
