@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :aruban_204_sense_1_res ;
  lemon:entry    :aruban_204 .

:aruban_204 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :aruban_204_lemma ;
  lemon:sense          :aruban_204_sense_1 ;
  lemon:synBehavior    :aruban_204_attrFrame, :aruban_204_predFrame .

:aruban_204_lemma lemon:writtenRep "aruban"@en .

:aruban_204_predFrame a        lexinfo:AdjectivePredicateFrame ;
  lexinfo:copulativeSubject :aruban_204_PredSynArg .

:aruban_204_attrFrame a     lexinfo:AdjectiveAttributiveFrame ;
  lexinfo:attributiveArg :aruban_204_AttrSynArg .

:aruban_204_sense_1 a  lemon:LexicalSense ;
  lemon:reference :aruban_204_sense_1_res ;
  lemon:isA       :aruban_204_AttrSynArg, :aruban_204_PredSynArg .

:aruban_204_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://dbpedia.org/ontology/country> ;
  owl:hasValue   <http://dbpedia.org/resource/Aruba> .
