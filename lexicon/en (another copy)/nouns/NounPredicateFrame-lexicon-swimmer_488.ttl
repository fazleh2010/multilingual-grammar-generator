@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :swimmer_488_sense_1_res ;
  lemon:entry    :swimmer_488 .

:swimmer_488 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :swimmer_488_form ;
  lemon:otherForm  :swimmer_488_singular ;
  lemon:otherForm  :swimmer_488_plural ;
  lemon:sense          :swimmer_488_sense_1 ;
  lemon:synBehavior    :swimmer_488_predFrame .

:swimmer_488_form
 lemon:writtenRep "swimmer"@en .

:swimmer_488_singular a    lemon:Form ;
  lemon:writtenRep "swimmer"@en ;
  lexinfo:number   lexinfo:singular .

:swimmer_488_plural a   lemon:Form ;
  lemon:writtenRep "swimmers"@en ;
  lexinfo:number    lexinfo:plural .

:swimmer_488_sense_1 a  lemon:LexicalSense ;
  lemon:reference :swimmer_488_sense_1_res ;
  lemon:isA       :swimmer_488_PredSynArg .

:swimmer_488_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/Swimmer> .