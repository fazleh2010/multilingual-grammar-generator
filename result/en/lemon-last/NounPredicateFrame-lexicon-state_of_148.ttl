@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :state_of_148_sense_1_res ;
  lemon:entry    :state_of_148 .

:state_of_148 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :state_of_148_form ;
  lemon:otherForm  :state_of_148_singular ;
  lemon:otherForm  :state_of_148_plural ;
  lemon:sense          :state_of_148_sense_1 ;
  lemon:synBehavior    :state_of_148_predFrame .

:state_of_148_form
 lemon:writtenRep "state"@en .

:state_of_148_singular a    lemon:Form ;
  lemon:writtenRep "state"@en ;
  lexinfo:number   lexinfo:singular .

:state_of_148_plural a   lemon:Form ;
  lemon:writtenRep "states"@en ;
  lexinfo:number    lexinfo:plural .

:state_of_148_sense_1 a  lemon:LexicalSense ;
  lemon:reference :state_of_148_sense_1_res ;
  lemon:isA       :state_of_148_PredSynArg .

:state_of_148_sense_1_res a   owl:Restriction ;
  owl:onProperty <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  owl:hasValue   <http://dbpedia.org/ontology/State> .
