@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :store_of_859 ;
  lemon:entry    :store_of_859_form_preposition .

:store_of_859 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :store_of_859_form ;
  lemon:otherForm  :store_of_859_singular ;
  lemon:otherForm  :store_of_859_plural ;
  lemon:sense          :store_of_859_sense_1 ;
  lemon:synBehavior    :store_of_859_nounpp .

:store_of_859_form a lemon:Form ;
  lemon:writtenRep "store"@en .

:store_of_859_singular a    lemon:Form ;
  lemon:writtenRep "store"@en ;
  lexinfo:number   lexinfo:singular .

:store_of_859_plural a   lemon:Form ;
  lemon:writtenRep "stores"@en ;
  lexinfo:number    lexinfo:plural .

:store_of_859_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_store_of_859 ;
  lexinfo:prepositionalAdjunct :arg2_store_of_859 .

:store_of_859_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :store_of_859_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/numberOfLocations> ;
  lemon:subjOfProp          :arg2_store_of_859 ;
  lemon:objOfProp           :arg1_store_of_859 ;
  lemon:condition           :store_of_859_sense_1_condition .

:store_of_859_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Company> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#nonNegativeInteger> .

:arg2_store_of_859 lemon:marker :store_of_859_form_preposition .
## Prepositions ##
:store_of_859_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

