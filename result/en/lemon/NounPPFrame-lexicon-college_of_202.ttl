@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :college_of_202 ;
  lemon:entry    :college_of_202_form_preposition .

:college_of_202 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :college_of_202_form ;
  lemon:otherForm  :college_of_202_singular ;
  lemon:otherForm  :college_of_202_plural ;
  lemon:sense          :college_of_202_sense_1 ;
  lemon:synBehavior    :college_of_202_nounpp .

:college_of_202_form a lemon:Form ;
  lemon:writtenRep "College"@en .

:college_of_202_singular a    lemon:Form ;
