@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :lexicalEntry-2007 ;
  lemon:entry    :lexicalEntry-2007_form_preposition .

:lexicalEntry-2007 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :lexicalEntry-2007_form ;
  lemon:otherForm  :lexicalEntry-2007_singular ;
  lemon:otherForm  :lexicalEntry-2007_plural ;
  lemon:sense          :lexicalEntry-2007_sense_1 ;
  lemon:synBehavior    :lexicalEntry-2007_nounpp .

:lexicalEntry-2007_form a lemon:Form ;
  lemon:writtenRep "painting"@en .

:lexicalEntry-2007_singular a    lemon:Form ;
  lemon:writtenRep "painting"@en ;
  lexinfo:number   lexinfo:singular .

:lexicalEntry-2007_plural a   lemon:Form ;
  lemon:writtenRep "painting"@en ;
  lexinfo:number    lexinfo:plural .

:lexicalEntry-2007_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1 ;
  lexinfo:prepositionalAdjunct :arg2 .

:lexicalEntry-2007_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :lexicalEntry-2007_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/movement> ;
  lemon:subjOfProp          :arg2 ;
  lemon:objOfProp           :arg1 ;
  lemon:condition           :lexicalEntry-2007_sense_1_condition .

:lexicalEntry-2007_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Artist> ;
  lemon:propertyRange   <http://www.w3.org/2002/07/owl#Thing> .

:arg2 lemon:marker :lexicalEntry-2007_form_preposition .
## Prepositions ##
:lexicalEntry-2007_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep ""@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

