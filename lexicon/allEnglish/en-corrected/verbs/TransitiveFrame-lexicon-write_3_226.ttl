@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_write_3_226 ;
  lemon:entry    :write_3_226ed ;
  lemon:entry    :form_write_3_226_preposition .

:to_write_3_226 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_write_3_226 ;
  lemon:otherForm      :form_write_3_226s ;
  lemon:otherForm      :form_write_3_226ed ;
  lemon:otherForm      :form_write_3_226perfect ;
  lemon:sense          :write_3_226_sense_1 ;
  lemon:synBehavior    :write_3_226_frame_transitive .

:form_write_3_226 a         lemon:Form ;
  lemon:writtenRep     "write"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_write_3_226s a    lemon:Form ;
  lemon:writtenRep "writes"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_write_3_226ed a   lemon:Form ;
  lemon:writtenRep "wrote"@en ;
  lexinfo:tense    lexinfo:past .

:form_write_3_226perfect a   lemon:Form ;
  lemon:writtenRep "wrote"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:write_3_226_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :write_3_226_subj ;
  lexinfo:directObject     :write_3_226_obj .

:write_3_226_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :write_3_226_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/writer> ;
  lemon:subjOfProp  :write_3_226_obj ;
  lemon:objOfProp   :write_3_226_subj ;
  lemon:condition   :write_3_226_sense_1_condition .


:write_3_226_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange  <http://dbpedia.org/ontology/Person> .


:write_3_226_obj lemon:marker :form_write_3_226_preposition .

## Prepositions ##
:form_write_3_226_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

