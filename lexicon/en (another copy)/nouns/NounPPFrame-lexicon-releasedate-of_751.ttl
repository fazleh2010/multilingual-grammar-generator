@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :releasedate-of_751 ;
  lemon:entry    :releasedate-of_751_form_preposition .

:releasedate-of_751 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :releasedate-of_751_form ;
  lemon:otherForm  :releasedate-of_751_singular ;
  lemon:otherForm  :releasedate-of_751_plural ;
  lemon:sense          :releasedate-of_751_sense_1 ;
  lemon:synBehavior    :releasedate-of_751_nounpp .

:releasedate-of_751_form a lemon:Form ;
  lemon:writtenRep "release date"@en .

:releasedate-of_751_singular a    lemon:Form ;
  lemon:writtenRep "release date"@en ;
  lexinfo:number   lexinfo:singular .

:releasedate-of_751_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:releasedate-of_751_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_releasedate-of_751 ;
  lexinfo:prepositionalAdjunct :arg2_releasedate-of_751 .

:releasedate-of_751_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :releasedate-of_751_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/releaseDate> ;
  lemon:subjOfProp          :arg2_releasedate-of_751 ;
  lemon:objOfProp           :arg1_releasedate-of_751 ;
  lemon:condition           :releasedate-of_751_sense_1_condition .

:releasedate-of_751_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Software> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2_releasedate-of_751 lemon:marker :releasedate-of_751_form_preposition .
## Prepositions ##
:releasedate-of_751_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

