@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :completiondate-of_330 ;
  lemon:entry    :completiondate-of_330_form_preposition .

:completiondate-of_330 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :completiondate-of_330_form ;
  lemon:otherForm  :completiondate-of_330_singular ;
  lemon:otherForm  :completiondate-of_330_plural ;
  lemon:sense          :completiondate-of_330_sense_1 ;
  lemon:synBehavior    :completiondate-of_330_nounpp .

:completiondate-of_330_form a lemon:Form ;
  lemon:writtenRep "completion date"@en .

:completiondate-of_330_singular a    lemon:Form ;
  lemon:writtenRep "completion date"@en ;
  lexinfo:number   lexinfo:singular .

:completiondate-of_330_plural a   lemon:Form ;
  lemon:writtenRep "XX"@en ;
  lexinfo:number    lexinfo:plural .

:completiondate-of_330_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_completiondate-of_330 ;
  lexinfo:prepositionalAdjunct :arg2_completiondate-of_330 .

:completiondate-of_330_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :completiondate-of_330_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/completionDate> ;
  lemon:subjOfProp          :arg2_completiondate-of_330 ;
  lemon:objOfProp           :arg1_completiondate-of_330 ;
  lemon:condition           :completiondate-of_330_sense_1_condition .

:completiondate-of_330_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Ship> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#date> .

:arg2_completiondate-of_330 lemon:marker :completiondate-of_330_form_preposition .
## Prepositions ##
:completiondate-of_330_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

