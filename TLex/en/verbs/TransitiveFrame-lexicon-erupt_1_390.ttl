@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_erupt_1_390 ;
  lemon:entry    :erupt_1_390ed ;
  lemon:entry    :form_erupt_1_390_preposition .

:to_erupt_1_390 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_erupt_1_390 ;
  lemon:otherForm      :form_erupt_1_390s ;
  lemon:otherForm      :form_erupt_1_390ed ;
  lemon:otherForm      :form_erupt_1_390perfect ;
  lemon:sense          :erupt_1_390_sense_1 ;
  lemon:synBehavior    :erupt_1_390_frame_transitive .

:form_erupt_1_390 a         lemon:Form ;
  lemon:writtenRep     "erupt"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_erupt_1_390s a    lemon:Form ;
  lemon:writtenRep "erupts"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_erupt_1_390ed a   lemon:Form ;
  lemon:writtenRep "erupted"@en ;
  lexinfo:tense    lexinfo:past .

:form_erupt_1_390perfect a   lemon:Form ;
  lemon:writtenRep "erupted"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:erupt_1_390_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :erupt_1_390_obj ;
  lexinfo:directObject     :erupt_1_390_subj .

:erupt_1_390_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :erupt_1_390_sense_1 ;
  lemon:reference   <http://dbpedia.org/ontology/eruptionYear> ;
  lemon:subjOfProp  :erupt_1_390_obj ;
  lemon:objOfProp   :erupt_1_390_subj ;
  lemon:condition   :erupt_1_390_sense_1_condition .


:erupt_1_390_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Volcano> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#gYear> .


:erupt_1_390_obj lemon:marker :form_erupt_1_390_preposition .

## Prepositions ##
:form_erupt_1_390_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

