@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :to_lexicalEntry-3257 ;
  lemon:entry    :lexicalEntry-3257ed ;
  lemon:entry    :form_lexicalEntry-3257_preposition .

:to_lexicalEntry-3257 a           lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_lexicalEntry-3257 ;
  lemon:otherForm      :form_lexicalEntry-3257s ;
  lemon:otherForm      :form_lexicalEntry-3257ed ;
  lemon:otherForm      :form_lexicalEntry-3257perfect ;
  lemon:sense          :lexicalEntry-3257_sense_1 ;
  lemon:synBehavior    :lexicalEntry-3257_frame_transitive .

:form_lexicalEntry-3257 a         lemon:Form ;
  lemon:writtenRep     "held"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .

:form_lexicalEntry-3257s a    lemon:Form ;
  lemon:writtenRep "held"@en ;
  lexinfo:person   lexinfo:secondPerson .

:form_lexicalEntry-3257ed a   lemon:Form ;
  lemon:writtenRep "held"@en ;
  lexinfo:tense    lexinfo:past .

:form_lexicalEntry-3257perfect a   lemon:Form ;
  lemon:writtenRep "held"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:lexicalEntry-3257_frame_transitive a lexinfo:TransitiveFrame ;
  lexinfo:subject          :lexicalEntry-3257_subj ;
  lexinfo:directObject     :lexicalEntry-3257_obj .

:lexicalEntry-3257_sense_1 a   lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3257_sense_1 ;
  lemon:reference   <http://dbpedia.org/property/date> ;
  lemon:subjOfProp  :lexicalEntry-3257_obj ;
  lemon:objOfProp   :lexicalEntry-3257_subj ;
  lemon:condition   :lexicalEntry-3257_sense_1_condition .


:lexicalEntry-3257_sense_1_condition a    lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/MilitaryConflict> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .

:lexicalEntry-3257ed a            lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_lexicalEntry-3257ed_canonical ;
  lemon:otherForm      :form_lexicalEntry-3257ed_by ;
  lemon:synBehavior    :lexicalEntry-3257ed_frame_adjectivepp ;
  lemon:sense          :lexicalEntry-3257ed_ontomap .

:form_lexicalEntry-3257ed_canonical a lemon:Form ;
  lemon:writtenRep         "held"@en .

:form_lexicalEntry-3257ed_by a    lemon:Form ;
  lemon:writtenRep     "held"@en ;
  lexinfo:verbFormMood lexinfo:participle .


:lexicalEntry-3257ed_frame_adjectivepp a  lexinfo:AdjectivePPFrame ;
  lexinfo:copulativeSubject    :lexicalEntry-3257ed_subj ;
  lexinfo:prepositionalAdjunct :lexicalEntry-3257ed_obj .

:lexicalEntry-3257ed_ontomap a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :lexicalEntry-3257ed_ontomap ;
  lemon:reference   <http://dbpedia.org/property/date> ;
  lemon:subjOfProp  :lexicalEntry-3257ed_subj ;
  lemon:objOfProp   :lexicalEntry-3257ed_obj ;
  lemon:condition   :lexicalEntry-3257_sense_1_condition .
:lexicalEntry-3257ed_obj lemon:marker :form_lexicalEntry-3257_preposition .

## Prepositions ##
:form_lexicalEntry-3257_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "by"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

