:racecourse_600_form
 lemon:writtenRep "race course"@en .

:racecourse_600_singular a    lemon:Form ;
  lemon:writtenRep "race course"@en ;
  lexinfo:number   lexinfo:singular .

:racecourse_600_plural a   lemon:Form ;
