@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix oils:     <http://lemon-model.net/oils#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :high_8_96 ;
  lemon:entry    :form_high_8_96_preposition ;
  lemon:entry    :high_8_96_res .

:high_8_96 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:adjective ;
  lemon:canonicalForm  :form_high_8_96 ;
  lemon:otherForm      :form_high_8_96_comperative ;
  lemon:otherForm      :form_high_8_96_superlative ;
  lemon:sense          :high_8_96_sense_1 ;
  lemon:synBehavior    :high_8_96_predFrame .

:form_high_8_96 lemon:writtenRep "high"@en .

:form_high_8_96_comperative lemon:writtenRep "higher"@en .

:form_high_8_96_superlative lemon:writtenRep "highest"@en .

:high_8_96_predFrame a        lexinfo:AdjectiveSuperlativeFrame ;
  lexinfo:copulativeSubject :high_8_96_PredSynArg .

:high_8_96_sense_1 a  lemon:LexicalSense ;
  lemon:reference :high_8_96_res ;
  lemon:isA       :high_8_96_PredSynArg ;
  lemon:condition :high_8_96_sense_1_condition .

:high_8_96_res a   oils:CovariantScalar ;
  oils:boundTo  <http://dbpedia.org/property/elevationM> ;
  oils:degree   <http://lemon-model.net/oils/high> .

:high_8_96_sense_1_condition a lemon:condition ;
  lemon:propertyDomain   <http://dbpedia.org/ontology/Mountain> ;
  lemon:propertyRange    <http://www.w3.org/2001/XMLSchema#double> .

## Prepositions ##
:form_high_8_96_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "in"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

