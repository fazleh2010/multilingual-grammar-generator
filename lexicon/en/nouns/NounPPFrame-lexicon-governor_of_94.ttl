:governor_of_94_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :null ;
  lexinfo:prepositionalAdjunct :null .

:governor_of_94_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :governor_of_94_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/governor> ;
  lemon:subjOfProp          :null ;
  lemon:objOfProp           :null ;
  lemon:condition           :governor_of_94_sense_1_condition .

:governor_of_94_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Place> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Person> .

:null lemon:marker :governor_of_94_form_preposition .
## Prepositions ##
:governor_of_94_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
