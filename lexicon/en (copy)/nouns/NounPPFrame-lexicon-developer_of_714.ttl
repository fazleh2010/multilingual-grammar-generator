@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :developer_of_714 ;
  lemon:entry    :developer_of_714_form_preposition .

:developer_of_714 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :developer_of_714_form ;
  lemon:otherForm  :developer_of_714_singular ;
  lemon:otherForm  :developer_of_714_plural ;
  lemon:sense          :developer_of_714_sense_1 ;
  lemon:synBehavior    :developer_of_714_nounpp .

:developer_of_714_form a lemon:Form ;
  lemon:writtenRep "developer"@en .

:developer_of_714_singular a    lemon:Form ;
  lemon:writtenRep "developer"@en ;
  lexinfo:number   lexinfo:singular .

:developer_of_714_plural a   lemon:Form ;
  lemon:writtenRep "developers"@en ;
  lexinfo:number    lexinfo:plural .

:developer_of_714_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_developer_of_714 ;
  lexinfo:prepositionalAdjunct :arg2_developer_of_714 .

:developer_of_714_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :developer_of_714_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/developer> ;
  lemon:subjOfProp          :arg2_developer_of_714 ;
  lemon:objOfProp           :arg1_developer_of_714 ;
  lemon:condition           :developer_of_714_sense_1_condition .

:developer_of_714_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Work> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/Agent> .

:arg2_developer_of_714 lemon:marker :developer_of_714_form_preposition .
## Prepositions ##
:developer_of_714_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

