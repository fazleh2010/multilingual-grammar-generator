@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :birthplace_of_112 ;
  lemon:entry    :birthplace_of_112_form_preposition .

:birthplace_of_112 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :birthplace_of_112_form ;
  lemon:otherForm  :birthplace_of_112_singular ;
  lemon:otherForm  :birthplace_of_112_plural ;
  lemon:sense          :birthplace_of_112_sense_1 ;
  lemon:synBehavior    :birthplace_of_112_nounpp .

:birthplace_of_112_form a lemon:Form ;
  lemon:writtenRep "birthplace"@en .

:birthplace_of_112_singular a    lemon:Form ;
  lemon:writtenRep "birthplace"@en ;
  lexinfo:number   lexinfo:singular .

:birthplace_of_112_plural a   lemon:Form ;
  lemon:writtenRep "birthplaces"@en ;
  lexinfo:number    lexinfo:plural .

:birthplace_of_112_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_birthplace_of_112 ;
  lexinfo:prepositionalAdjunct :arg2_birthplace_of_112 .

:birthplace_of_112_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :birthplace_of_112_sense_1 ;
  lemon:reference           <wd:P19> ;
  lemon:subjOfProp          :arg2_birthplace_of_112 ;
  lemon:objOfProp           :arg1_birthplace_of_112 ;
  lemon:condition           :birthplace_of_112_sense_1_condition .

:birthplace_of_112_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <wd:Q19088> ;
  lemon:propertyRange   <wd:Q3957> .

:arg2_birthplace_of_112 lemon:marker :birthplace_of_112_form_preposition .
## Prepositions ##
:birthplace_of_112_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

