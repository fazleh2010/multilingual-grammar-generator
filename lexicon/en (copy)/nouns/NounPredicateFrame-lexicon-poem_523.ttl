@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :poem_523_sense_1_res ;
  lemon:entry    :poem_523 .

:poem_523 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :poem_523_form ;
  lemon:otherForm  :poem_523_singular ;
  lemon:otherForm  :poem_523_plural ;
  lemon:sense          :poem_523_sense_1 ;
