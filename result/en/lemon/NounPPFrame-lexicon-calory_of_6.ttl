@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :calory_of_6 ;
  lemon:entry    :calory_of_6_form_preposition .

:calory_of_6 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :calory_of_6_form ;
  lemon:otherForm  :calory_of_6_singular ;
  lemon:otherForm  :calory_of_6_plural ;
  lemon:sense          :calory_of_6_sense_1 ;
  lemon:synBehavior    :calory_of_6_nounpp .

:calory_of_6_form a lemon:Form ;
  lemon:writtenRep "calory"@en .

:calory_of_6_singular a    lemon:Form ;
  lemon:writtenRep "calory"@en ;
  lexinfo:number   lexinfo:singular .

:calory_of_6_plural a   lemon:Form ;
  lemon:writtenRep "calories"@en ;
  lexinfo:number    lexinfo:plural .

:calory_of_6_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg2_calory_of_6 ;
  lexinfo:prepositionalAdjunct :arg1_calory_of_6 .

:calory_of_6_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :calory_of_6_sense_1 ;
  lemon:reference           <http://dbpedia.org/property/calories> ;
  lemon:subjOfProp          :arg1_calory_of_6 ;
  lemon:objOfProp           :arg2_calory_of_6 ;
  lemon:condition           :calory_of_6_sense_1_condition .

:calory_of_6_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Food> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#integer> .

:arg1_calory_of_6 lemon:marker :calory_of_6_form_preposition .
## Prepositions ##
:calory_of_6_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

