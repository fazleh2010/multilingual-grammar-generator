@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :start_at_3_320 ;
  lemon:entry    :form_start_at_3_320_preposition .

:start_at_3_320 a             lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:verb ;
  lemon:canonicalForm  :form_start_at_3_320 ;
  lemon:otherForm      :form_start_at_3_320_present3rd ;
  lemon:otherForm      :form_start_at_3_320_past ;
  lemon:otherForm      :form_start_at_3_320_perfect ;
  lemon:sense          :start_at_3_320_sense_1 ;
  lemon:synBehavior    :start_at_3_320_frame .

:form_start_at_3_320 a           lemon:Form ;
  lemon:writtenRep     "start"@en ;
  lexinfo:verbFormMood lexinfo:infinitive .


:form_start_at_3_320_present3rd a      lemon:Form ;
  lemon:writtenRep "starts"@en ;
  lexinfo:number   lexinfo:singular ;
  lexinfo:person   lexinfo:thirdPerson ;
  lexinfo:tense    lexinfo:present .

:form_start_at_3_320_past a lemon:Form ;
  lemon:writtenRep  "started"@en ;
  lexinfo:number    lexinfo:singular ;
  lexinfo:person    lexinfo:thirdPerson ;
  lexinfo:tense     lexinfo:past .


:form_start_at_3_320_perfect a   lemon:Form ;
  lemon:writtenRep "started"@en ;
  lexinfo:tense    lexinfo:perfect ;
  lexinfo:person   lexinfo:thirdPerson .

:start_at_3_320_frame a  lexinfo:IntransitivePPFrame ;
  lexinfo:subject              :start_at_3_320_subj ;
  lexinfo:prepositionalAdjunct :start_at_3_320_obj .

:start_at_3_320_sense_1 a     lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping :start_at_3_320_ontomap ;
  lemon:reference   <http://dbpedia.org/ontology/date> ;
  lemon:subjOfProp  :start_at_3_320_obj ;
  lemon:objOfProp   :start_at_3_320_subj ;
  lemon:condition   :start_at_3_320_sense_1_condition .

:start_at_3_320_sense_1_condition a      lemon:condition ;
  lemon:propertyDomain <http://dbpedia.org/ontology/Event> ;
  lemon:propertyRange  <http://www.w3.org/2001/XMLSchema#date> .


:start_at_3_320_obj lemon:marker :form_start_at_3_320_preposition .

## Prepositions ##
:form_start_at_3_320_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "at"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

