@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :budget_of_59 ;
  lemon:entry    :budget_of_59_form_preposition .

:budget_of_59 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :budget_of_59_form ;
  lemon:otherForm  :budget_of_59_singular ;
  lemon:otherForm  :budget_of_59_plural ;
  lemon:sense          :budget_of_59_sense_1 ;
  lemon:synBehavior    :budget_of_59_nounpp .

:budget_of_59_form a lemon:Form ;
  lemon:writtenRep "budget"@en .

:budget_of_59_singular a    lemon:Form ;
  lemon:writtenRep "budget"@en ;
  lexinfo:number   lexinfo:singular .

:budget_of_59_plural a   lemon:Form ;
  lemon:writtenRep "budgets"@en ;
  lexinfo:number    lexinfo:plural .

:budget_of_59_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_budget_of_59 ;
  lexinfo:prepositionalAdjunct :arg2_budget_of_59 .

:budget_of_59_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :budget_of_59_sense_1 ;
  lemon:reference           <http://dbpedia.org/ontology/budget> ;
  lemon:subjOfProp          :arg2_budget_of_59 ;
  lemon:objOfProp           :arg1_budget_of_59 ;
  lemon:condition           :budget_of_59_sense_1_condition .

:budget_of_59_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/Film> ;
  lemon:propertyRange   <http://www.w3.org/2001/XMLSchema#double> .

:arg2_budget_of_59 lemon:marker :budget_of_59_form_preposition .
## Prepositions ##
:budget_of_59_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

