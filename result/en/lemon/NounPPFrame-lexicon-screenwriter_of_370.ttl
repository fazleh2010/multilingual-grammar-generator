@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :screenwriter_of_370 ;
  lemon:entry    :screenwriter_of_370_form_preposition .

:screenwriter_of_370 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :screenwriter_of_370_form ;
  lemon:otherForm  :screenwriter_of_370_singular ;
  lemon:otherForm  :screenwriter_of_370_plural ;
  lemon:sense          :screenwriter_of_370_sense_1 ;
  lemon:synBehavior    :screenwriter_of_370_nounpp .

:screenwriter_of_370_form a lemon:Form ;

:screenwriter_of_370_singular a    lemon:Form ;
  lemon:writtenRep "Screen Writer"@en ;
  lexinfo:number   lexinfo:singular .

:screenwriter_of_370_plural a   lemon:Form ;
  lemon:writtenRep "Screen Writers"@en ;
  lexinfo:number    lexinfo:plural .

:screenwriter_of_370_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_screenwriter_of_370 ;
  lexinfo:prepositionalAdjunct :arg2_screenwriter_of_370 .

:screenwriter_of_370_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :screenwriter_of_370_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_screenwriter_of_370 ;
  lemon:objOfProp           :arg1_screenwriter_of_370 ;
  lemon:condition           :screenwriter_of_370_sense_1_condition .

:screenwriter_of_370_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/ScreenWriter> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/ScreenWriter> .

:arg2_screenwriter_of_370 lemon:marker :screenwriter_of_370_form_preposition .
## Prepositions ##
:screenwriter_of_370_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

