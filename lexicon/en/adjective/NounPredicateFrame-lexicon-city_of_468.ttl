@prefix :        <http://localhost:8080/#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :city_of_468_sense_1_res ;
  lemon:entry    :city_of_468_sense_2_res ;
  lemon:entry    :city_of_468 .

:city_of_468 a  lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
 lemon:canonicalForm  :city_of_468_form ;
  lemon:otherForm  :city_of_468_singular ;
  lemon:otherForm  :city_of_468_plural ;
  lemon:sense          :city_of_468_sense_1 ;
  lemon:sense          :city_of_468_sense_2 ;
  lemon:synBehavior    :city_of_468_predFrame .

:city_of_468_form
 lemon:writtenRep "city"@en .

:city_of_468_singular a    lemon:Form ;
  lemon:writtenRep "city"@en ;
  lexinfo:number   lexinfo:singular .

:city_of_468_plural a   lemon:Form ;
  lemon:writtenRep "citys"@en ;
