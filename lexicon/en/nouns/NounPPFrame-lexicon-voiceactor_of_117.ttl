@prefix :        <http://localhost:8080/lexicon#> .

@prefix lexinfo: <http://www.lexinfo.net/ontology/2.0/lexinfo#> .
@prefix lemon:   <http://lemon-model.net/lemon#> .

@base            <http://localhost:8080#> .

:lexicon_en a    lemon:Lexicon ;
  lemon:language "en" ;
  lemon:entry    :voiceactor_of_117 ;
  lemon:entry    :voiceactor_of_117_form_preposition .

:voiceactor_of_117 a       lemon:LexicalEntry ;
  lexinfo:partOfSpeech lexinfo:noun ;
  lemon:canonicalForm  :voiceactor_of_117_form ;
  lemon:otherForm  :voiceactor_of_117_singular ;
  lemon:otherForm  :voiceactor_of_117_plural ;
  lemon:sense          :voiceactor_of_117_sense_1 ;
  lemon:synBehavior    :voiceactor_of_117_nounpp .

:voiceactor_of_117_form a lemon:Form ;
  lemon:writtenRep "VoiceActor"@en .

:voiceactor_of_117_singular a    lemon:Form ;
  lemon:writtenRep "VoiceActor"@en ;
  lexinfo:number   lexinfo:singular .

:voiceactor_of_117_plural a   lemon:Form ;
  lemon:writtenRep "VoiceActors"@en ;
  lexinfo:number    lexinfo:plural .

:voiceactor_of_117_nounpp a        lexinfo:NounPPFrame ;
  lexinfo:copulativeArg        :arg1_voiceactor_of_117 ;
  lexinfo:prepositionalAdjunct :arg2_voiceactor_of_117 .

:voiceactor_of_117_sense_1 a lemon:OntoMap, lemon:LexicalSense ;
  lemon:ontoMapping         :voiceactor_of_117_sense_1 ;
  lemon:reference           <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> ;
  lemon:subjOfProp          :arg2_voiceactor_of_117 ;
  lemon:objOfProp           :arg1_voiceactor_of_117 ;
  lemon:condition           :voiceactor_of_117_sense_1_condition .

:voiceactor_of_117_sense_1_condition a lemon:condition ;
  lemon:propertyDomain  <http://dbpedia.org/ontology/VoiceActor> ;
  lemon:propertyRange   <http://dbpedia.org/ontology/VoiceActor> .

:arg2_voiceactor_of_117 lemon:marker :voiceactor_of_117_form_preposition .
## Prepositions ##
:voiceactor_of_117_form_preposition a                  lemon:SynRoleMarker ;
  lemon:canonicalForm  [ lemon:writtenRep "of"@en ] ;
  lexinfo:partOfSpeech lexinfo:preposition .

